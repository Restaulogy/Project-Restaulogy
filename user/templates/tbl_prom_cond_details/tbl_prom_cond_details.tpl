{include file='header.tpl'}<div class="wrapper"><h1>"{$prom_detail.title}" {$_lang.tbl_prom_cond_details.listing_title}</h1>{if $error_msg}	<div class="biz_center">{$error_msg}</div><!--/center-->{/if}<form name="frm_tbl_prom_cond_details" id="frm_tbl_prom_cond_details"  method="POST" action="{$page_url}" >{if $result_found gt 0 && $tbl_prom_cond_detailslist}	<table class="listTable">		<tr><th class="bigListItem" colspan="2">{$_lang.tbl_prom_cond_details.title}</th></tr>		{foreach from=$tbl_prom_cond_detailslist item=tbl_prom_cond_detailsitem}			<tr>			<td style="width:2%;">				<label for="sel_prom_cond_details[{$tbl_prom_cond_detailsitem.prcndtl_id}]" data-mini="true" style="width:23px;"><input type="checkbox" data-inline='true' data-mini='true' id="sel_prom_cond_details[{$tbl_prom_cond_detailsitem.prcndtl_id}]" name="sel_prom_cond_details[{$tbl_prom_cond_detailsitem.prcndtl_id}]" />&nbsp;</label>     	</td>			<td class="bigListItem"><a href="{$page_url}&mode={$smarty.const.MODE_VIEW}&prcndtl_id={$tbl_prom_cond_detailsitem.prcndtl_id}">{$tbl_prom_cond_detailsitem.prmcon_title}</a></td>				<!--<td class="actionListItem"><a href="{$page_url}&mode={$smarty.const.MODE_UPDATE}&prcndtl_id={$tbl_prom_cond_detailsitem.prcndtl_id}" class="editIcon"></a><a class="deleteIcon" href="#" onclick="deletetbl_prom_cond_details({$tbl_prom_cond_detailsitem.prcndtl_id});"></a>{if $tbl_prom_cond_detailsitem.isActive eq 1}<a href="{$page_url}&action={$smarty.const.ACTION_DEACTIVATE}&prcndtl_id={$tbl_prom_cond_detailsitem.prcndtl_id}" class="deactiveIcon" title="{$_lang.tbl_prom_cond_details.DEACTIVATE.BTN_LBL}"></a>{else}<a href="{$page_url}&action={$smarty.const.ACTION_ACTIVATE}&prcndtl_id={$tbl_prom_cond_detailsitem.prcndtl_id}" class="activeIcon" title="{$_lang.tbl_prom_cond_details.ACTIVATE.BTN_LBL}"></a>{/if}</td>-->			</tr>	{/foreach}	</table>{else}	<div class="error">{$_lang.tbl_prom_cond_details.no_record_found}</div>{/if}{if $pagination}	<div class="biz_center">{$pagination}</div><!--/center-->{/if}<input type="hidden" id="action" name="action" value=""/></form><input data-inline="true" data-icon="briefcase" type="button" id="sel_all_prom_cond_details" name="sel_all_prom_cond_details" value="{$_lang.main.toggle}" onclick="javascript:$('input[type=checkbox]').click();" /><input data-inline="true" data-icon="delete" type="button" id="del_sel_all_tbl_prom_cond_details" name="del_sel_all_tbl_prom_cond_details" value="{$_lang.tbl_prom_cond_details.DELETE.BTN_LBL}" onclick="deletetbl_prom_cond_details();" /><div class="biz_center"><input data-icon="add-item" data-inline="true" onclick="window.location.href='{$page_url}&mode={$smarty.const.MODE_CREATE}'" type="button" value="{$_lang.tbl_prom_cond_details.CREATE.BTN_LBL}"/></div><!--/center--></div>{include file="tbl_prom_cond_details/js.tpl"}{include file="footer.tpl"}