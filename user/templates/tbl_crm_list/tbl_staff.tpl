{include file='header.tpl'}<div class="wrapper"><h1>{$_lang.tbl_staff.crm_list_title}</h1>{if $error_msg}	<center>{$error_msg}</center>{/if}{include file="tbl_crm_list/prom_cond_disc_tab.tpl"}<form name="frm_tbl_stafflist" id="frm_tbl_stafflist"  method="POST" action="{$page_url}" onsubmit="return validate_email_sms_form();" >{if $result_found gt 0 && $tbl_stafflist}<div style="width: 100%; overflow-x: auto;">	<table class="listTable">		<tr>        <th class="bigListItem" colspan="2">{$_lang.tbl_staff.crm_list}</th>        <th>{$_lang.tbl_staff.label.staff_email}</th>        <th >            {$_lang.tbl_staff.label.staff_phone}		</th>		</tr>		{foreach from=$tbl_stafflist item=tbl_staffitem}			<tr>			<td style="width:2%;">			<label for="sel_tbl_staff[{$tbl_staffitem.crm_id}]" data-mini="true" style="width:23px;">                <input type="checkbox" data-inline='true' data-mini='true' id="sel_tbl_staff[{$tbl_staffitem.crm_id}]" name="sel_tbl_staff[{$tbl_staffitem.crm_id}]" {if $smarty.session._sel_crm_id_fr_email && in_array($tbl_staffitem.crm_id,$smarty.session._sel_crm_id_fr_email)} checked="checked" {/if} onchange="add_to_sess_now({$tbl_staffitem.crm_id},this);" />&nbsp;            </label>            </td>				<td ><a href="#">{$tbl_staffitem.customer_name}</a></td>			<td >{if $tbl_staffitem.usr_without_email eq 0}{$tbl_staffitem.crm_cust_email}{/if} </td>			<td >{$tbl_staffitem.crm_cust_phone}</td>						<!--            <td class="actionListItem"><a href="{$page_url}?mode={$smarty.const.MODE_VIEW}&staff_id={$tbl_staffitem.staff_id}" class="detailIcon"></a>            <a href="{$page_url}?mode={$smarty.const.MODE_UPDATE}&staff_id={$tbl_staffitem.staff_id}" class="editIcon"></a>            <a class="deleteIcon" href="#" onclick="deletetbl_staff({$tbl_staffitem.staff_id});"></a>            {if $tbl_staffitem.isActive eq 1}<a href="{$page_url}?action={$smarty.const.ACTION_DEACTIVATE}&staff_id={$tbl_staffitem.staff_id}" class="deactiveIcon" title="{$_lang.tbl_staff.DEACTIVATE.BTN_LBL}"></a>            {else}<a href="{$page_url}?action={$smarty.const.ACTION_ACTIVATE}&staff_id={$tbl_staffitem.staff_id}" class="activeIcon" title="{$_lang.tbl_staff.ACTIVATE.BTN_LBL}"></a>            {/if}            </td>            -->            		</tr>	{/foreach}	</table></div>{else}	<div class="error">{$_lang.tbl_staff.no_record_found}</div>{/if}{if $pagination}	<center>{$pagination}</center>{/if}<input type='hidden' name='action' id='action' value='' /><input type='hidden' name='recipent_sel' id='recipent_sel' value="{if $smarty.session._sel_crm_id_fr_email && $smarty.session._sel_crm_id_fr_email neq ''}{$smarty.session._sel_crm_id_fr_email|@count}{/if}" /><input type='hidden' name='rest_name' id='rest_name' value='{$rest_info[$smarty.const.RESTAURENT_NAME]}' />{include file="tbl_crm_list/promotions.tpl"}<div class="biz_center">    {if $result_found gt 0 && $tbl_stafflist}    <table>        <tr>            <td valign="top" >				<fieldset data-role="controlgroup" style='width:25px;border-radius:3px;'>					<input type="checkbox" value="1" data-mini="true" data-inline='true' id="_is_all_selected" name="_is_all_selected" {if $_is_all_selected eq 1} checked="checked" {/if} data-iconpos="notext" onclick="giveFlashMsg(this);" />  			        <label for="_is_all_selected" data="test"></label>				</fieldset>            </td>            <td valign="middle" style='width:93px;font-weight:bold;vertical-align: middle;'>                Select All            </td>            <td valign="middle" style='vertical-align:top;padding-top:6px;'>                <!-- <input data-inline="true" data-icon="briefcase" type="button" id="sel_all_tbl_staff" name="sel_all_tbl_staff" value="{$_lang.main.toggle}" onclick="javascript:$('input[type=checkbox]').click();" /> -->                {jqmbutton onclick="open_prom_sel_dialogue('{$tab_sms_email}');" icon="add-item" value="Email/SMS Promotion"}                {*jqmbutton onclick="open_prom_sel_dialogue(1);" icon="add-item" value="SMS Promotion"*}            </td>        </tr>    </table>    {/if}    <!--    <input data-icon="add-item" data-inline="true" onclick="window.location.href='{$page_url}?mode={$smarty.const.MODE_CREATE}'" type="button" value="{$_lang.tbl_crm.CREATE.BTN_LBL}"/>    <input data-icon="search" data-inline="true" onclick="$('#popup_csv_import').show();" type="button" value="CSV Import"/>    -->    </div></form>{include file="tbl_crm_list/js.tpl"}{include file="tbl_crm_list/csv_import.tpl"}</div>{include file='footercontent.tpl'}</body></html>