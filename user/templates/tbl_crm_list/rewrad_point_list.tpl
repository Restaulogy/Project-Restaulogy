{include file='header.tpl'}<div class="wrapper"  ><h1>{$_lang.biz_rewards.listing_title}</h1>{if $rpt_to_show=='rewrad_point_list'}    {include file="tbl_qrcode_log/tabs.tpl"}{else}    {include file="biz_rewards/tabs.tpl"}{/if}<a href="#" style="float:right;" data-role="button" data-icon="search" data-iconpos="notext" onclick="$('#filter_for_rwards_lst').toggle();"></a>{if $filt_text neq ''}	<div style="width:99%;padding:5px;border-bottom:1px dotted #ccc;font-style: italic;"><span class="info">Search results &laquo; </span><span class="notice">{$filt_text}</span><span class="info">&raquo;</span></div><br>{/if}{if $error_msg}	<center>{$error_msg}</center>{/if}{include file='tbl_crm_list/search.tpl'}{* include file='tbl_crm_list/quick_search.tpl' *}<form name="frm_tbl_stafflist" id="frm_tbl_stafflist"  method="POST" action="{$page_url}"  ><!-- onsubmit="return validate_email_sms_form();" -->{if $result_found gt 0 && $tbl_stafflist}<div style="width: 100%; overflow-x: auto;">	<table class="listTable" >		<tr>        <th class="bigListItem" colspan="2">{$_lang.tbl_staff.crm_list}</th>        <th>{$_lang.tbl_staff.label.staff_email}</th>        <th>{$_lang.tbl_staff.label.staff_phone}</th>        <th >            {$_lang.biz_rewards.total_points}		</th>		<th >            {$_lang.biz_rewards.redeemed_points}		</th>		<th >            {$_lang.biz_rewards.balance_points}		</th>		<th >            Action		</th>        <th >            &nbsp;		</th>		</tr>		{foreach from=$tbl_stafflist item=tbl_staffitem}		 <tr>            <td style="width:2%;">            <label for="sel_tbl_staff[{$tbl_staffitem.staff_id}]" data-mini="true" style="width:23px;">                <input type="checkbox" data-inline='true' data-mini='true' id="sel_tbl_staff[{$tbl_staffitem.staff_id}]" name="sel_tbl_staff[{$tbl_staffitem.staff_id}]" {if $smarty.session._sel_crm_id_fr_email && in_array($tbl_staffitem.staff_id,$smarty.session._sel_crm_id_fr_email)} checked="checked" {/if} onchange="add_to_sess_now({$tbl_staffitem.staff_id},this);" />&nbsp;            </label>                        </td>			<td style='cursor:pointer' onclick="window.open('{$website}/user/customer_rewards.php?manager_cust_sess_id={$tbl_staffitem.staff_member_id}');">{$tbl_staffitem.staff_full_name}</td>			<td style='cursor:pointer' onclick="window.open('{$website}/user/customer_rewards.php?manager_cust_sess_id={$tbl_staffitem.staff_member_id}');">{if $tbl_staffitem.usr_without_email eq 0}{$tbl_staffitem.staff_email}{/if}</td>			<td style='cursor:pointer' onclick="window.open('{$website}/user/customer_rewards.php?manager_cust_sess_id={$tbl_staffitem.staff_member_id}');">{$tbl_staffitem.staff_phone}</td>			<td style='cursor:pointer' onclick="window.open('{$website}/user/customer_rewards.php?manager_cust_sess_id={$tbl_staffitem.staff_member_id}');">{$tbl_staffitem.total_points|round:"0"}</td>			<td style='cursor:pointer' onclick="window.open('{$website}/user/customer_rewards.php?manager_cust_sess_id={$tbl_staffitem.staff_member_id}');">{$tbl_staffitem.redeem_points|round:"0"}</td>			<td style='cursor:pointer' onclick="window.open('{$website}/user/customer_rewards.php?manager_cust_sess_id={$tbl_staffitem.staff_member_id}');">{$tbl_staffitem.balance_points|round:"0"}</td>			<td > <input type="button" data-icon="add-item" data-inline="true" name="add_visit" onclick="set_cust_id({$tbl_staffitem.staff_member_id},{$tbl_staffitem.pts_earlier_today},{$tbl_staffitem.staff_award_multiplier})" value="{$_lang.biz_checkins.create_title}" />            </td>            <td>            <input type="button" data-icon="add-item" data-inline="true" name="edit_cust" onclick="window.location.href= '{$website}/user/tbl_staff.php?mode=UPDATE&staff_id={$tbl_staffitem.staff_id}';" value="Edit Profile" />            </td>		</tr>	{/foreach}	</table></div>{else}    {if $filt_text neq ''}	   <div class="error">{$_lang.tbl_staff.no_record_found}</div>	{else}        <div class="error">Please use filter option to check list.</div>	{/if}{/if}{if $pagination}	<center>{$pagination}</center>{/if}<input type='hidden' name='action' id='action' value='' /><input type='hidden' name='recipent_sel' id='recipent_sel' value="{if $smarty.session._sel_crm_id_fr_email && $smarty.session._sel_crm_id_fr_email neq ''}{$smarty.session._sel_crm_id_fr_email|@count}{/if}" /><input type='hidden' name='rest_name' id='rest_name' value='{$rest_info[$smarty.const.RESTAURENT_NAME]}' />{include file="tbl_crm_list/promotions.tpl"}{include file="tbl_crm_list/menu_items.tpl"}<!--<div id='_all_sel_ppoup1' style="display:none;position:fixed;width:145px;z-index:900;top: 50%;left: 50%;margin-left:-120px;margin-top:-150px;border:1px solid #FF9600;" class='ui-body-a'>    <div data-role="header" data-theme="a" class="ui-corner-top">        <h1>Message</h1>    </div>    <div data-role="content" data-theme="a" class="ui-corner-bottom ui-content" style="padding:5px;">        <b class='info'> All contacts selected </b>    </div></div>--><div class="biz_center">    {if ($sesslife) && ($Global_member.member_role_id eq $smarty.const.ROLE_ADMIN)}        <table>        <tr>            <td style='width:23px;'>            	{if $result_found gt 0 && $tbl_stafflist}                <label style="width:23px;" >                    <input type="checkbox" value="1" data-mini="true" data-inline='true' id="_is_all_selected" name="_is_all_selected" {if $_is_all_selected eq 1} checked="checked" {/if} onclick="giveFlashMsg(this);" /> &nbsp;                </label>                {/if}            </td>            <td style='width:93px;font-weight:bold;'>                 {if $result_found gt 0 && $tbl_stafflist} Select All{/if}            </td>            <td >                 <!--                 <input data-inline="true" data-icon="briefcase" type="button" id="sel_all_tbl_staff" name="sel_all_tbl_staff" value="{$_lang.main.toggle}" onclick="javascript:$('input[type=checkbox]').click();" />                 -->                {if $result_found gt 0 && $tbl_stafflist}                {jqmbutton onclick="open_prom_sel_dialogue('{$tab_sms_email}');" icon="star" value="Email/SMS Promotion"}                {jqmbutton onclick="open_prom_sel_dialogue('{$tab_sms_email}','menus');" icon="star" value="Email/SMS Menu"}				                <input data-inline="true" data-icon="delete" type="button" id="del_sel_all_tbl_staff" name="del_sel_all_tbl_staff" value="{$_lang.tbl_staff.BANN_USER.BTN_LBL}" onclick="softdeletetbl_staff();" />				{/if}                <input data-inline="true" data-icon="male-user" type="button" value="Join Now" onclick="location.href='{$website}/user/cust_login.php'" />                <input data-icon="search" data-inline="true" onclick="$('#popup_csv_import').show();" type="button" value="CSV Import"/>                 <input data-inline="true" data-icon="home" type="button" value="Back" onclick="location.href='{$website}/user/index.php'" />            </td>        </tr>        </table>    {else}    	 <input data-inline="true" data-icon="home" type="button" value="Back" onclick="location.href='{$website}/user/index.php'" />     	{/if}</div></form>{include file="tbl_crm_list/csv_import.tpl"}{include file="tbl_crm_list/add_visit.tpl"}{include file="tbl_crm_list/js.tpl"}</div>{include file='footercontent.tpl'}</body></html>