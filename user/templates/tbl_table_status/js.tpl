{literal}<script type="text/javascript">function deletetbl_table_status(){	var varId = $("input:checked").length;	if(varId > 0){		if(confirm("{/literal}{$_lang.tbl_table_status.DELETE.CONFIRM_MSG}{literal}")==true){									$('#action').val("{/literal}{$smarty.const.ACTION_DELETE}{literal}");			$('#frm_tbl_table_statuslist').submit();		}	}else{		alert("{/literal}{$_lang.main.select_ids.empty}{literal}");	}}function validatetbl_table_status(){	$("#tbl_sts_id_err").html("");	$("#tbl_sts_name_err").html("");	$("#tbl_sts_desc_err").html("");	$("#tbl_sts_sort_order_err").html("");	$("#tbl_sts_start_date_err").html("");	$("#tbl_sts_end_date_err").html("");	var isErr = true;	if(elemById("action").value=="update"){		if(IsNonEmpty(elemById("tbl_sts_id").value)==false){			$("#tbl_sts_id_err").html("{/literal}{$_lang.tbl_table_status.not_empty_msg.tbl_sts_id}{literal}");			isErr = false;		}	}	if(IsNonEmpty(elemById("tbl_sts_name").value)==false){		$("#tbl_sts_name_err").html("{/literal}{$_lang.tbl_table_status.not_empty_msg.tbl_sts_name}{literal}");		isErr = false;	}	if(IsNonEmpty(elemById("tbl_sts_desc").value)==false){		$("#tbl_sts_desc_err").html("{/literal}{$_lang.tbl_table_status.not_empty_msg.tbl_sts_desc}{literal}");		isErr = false;	}	if(IsNonEmpty(elemById("tbl_sts_sort_order").value)==false){		$("#tbl_sts_sort_order_err").html("{/literal}{$_lang.tbl_table_status.not_empty_msg.tbl_sts_sort_order}{literal}");		isErr = false;	}		if(isInt(elemById("tbl_sts_sort_order").value)==false){		$("#tbl_sts_sort_order_err").html("{/literal}{$_lang.tbl_service_stage.invalid_input_msg.tbl_sts_sort_order}{literal}");		isErr = false;	}/*	if(IsNonEmpty(elemById("tbl_sts_start_date").value)==false){		$("#tbl_sts_start_date_err").html("{/literal}{$_lang.tbl_table_status.not_empty_msg.tbl_sts_start_date}{literal}");		isErr = false;	}*//*	if(IsNonEmpty(elemById("tbl_sts_end_date").value)==false){		$("#tbl_sts_end_date_err").html("{/literal}{$_lang.tbl_table_status.not_empty_msg.tbl_sts_end_date}{literal}");		isErr = false;	}*/	if(isErr == false){		alert("{/literal}{$_lang.messages.revise_form}{literal}");	}	return isErr;}//..function</script>{/literal}