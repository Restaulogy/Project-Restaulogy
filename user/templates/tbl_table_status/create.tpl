{include file="header.tpl"}<div class="wrapper"><h1>{$_lang.tbl_table_status.create_title}</h1>{if $error_msg}	<center>{$error_msg}</center>{/if}<form name="frmcreatetbl_table_status" id="frmcreatetbl_table_status" onsubmit="return validatetbl_table_status();" method="POST" action="{$page_url}">	<input type="hidden" name="tbl_sts_id" id="tbl_sts_id" value="0"/>	<div style="display:none;" class="error" id="tbl_sts_id_err"></div>	<div class="field-row">		<label for="tbl_sts_name">{$_lang.tbl_table_status.label.tbl_sts_name}</label>		<input type="text" name="tbl_sts_name" id="tbl_sts_name" value="{$smarty.post.tbl_sts_name}"/>		<div class="error" id="tbl_sts_name_err"></div>	</div>	<div class="field-row">		<label for="tbl_sts_desc">{$_lang.tbl_table_status.label.tbl_sts_desc}</label>		<input type="text" name="tbl_sts_desc" id="tbl_sts_desc" value="{$smarty.post.tbl_sts_desc}"/>		<div class="error" id="tbl_sts_desc_err"></div>	</div>	<div class="field-row">		<label for="tbl_sts_sort_order">{$_lang.tbl_table_status.label.tbl_sts_sort_order}</label>		<select name="tbl_sts_sort_order" id="tbl_sts_sort_order">        	{for $foo=1 to 20}                <option value="{$foo}" {if $smarty.post.tbl_sts_sort_order eq $foo} selected="selected" {/if}> {$foo} </option>            {/for}        </select>		<div class="error" id="tbl_sts_sort_order_err"></div>	</div>	<!--	<div class="field-row">		<label for="tbl_sts_start_date">{$_lang.tbl_table_status.label.tbl_sts_start_date}</label>		<input type="text" name="tbl_sts_start_date" id="tbl_sts_start_date" value="{$smarty.post.tbl_sts_start_date}"/>		<div class="error" id="tbl_sts_start_date_err"></div>	</div>	-->	<!--	<div class="field-row">		<label for="tbl_sts_end_date">{$_lang.tbl_table_status.label.tbl_sts_end_date}</label>		<input type="text" name="tbl_sts_end_date" id="tbl_sts_end_date" value="{$smarty.post.tbl_sts_end_date}"/>		<div class="error" id="tbl_sts_end_date_err"></div>	</div>	-->	<input type="hidden" id="action" name="action" value="{$smarty.const.ACTION_CREATE}"/>	<center><input  data-inline="true" data-icon="save"  type="submit" value="{$_lang.save_lbl}"/> <input data-inline="true" data-icon="delete" type="reset" value="{$_lang.cancel_lbl}" onclick="window.location.href='{$page_url}'"/>	</center></form>{include file="tbl_table_status/js.tpl"}</div>{include file="footer.tpl"}