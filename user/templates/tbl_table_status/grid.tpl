{include file='header.tpl'}<div class="wrapper"><h1>{$_lang.tbl_table_status.listing_title}</h1>{if $error_msg}	<center>{$error_msg}</center>{/if}{if $result_found gt 0 && $tbl_table_statuslist}	<table class="biz_data_grid">		<tr>			<th class="{if $smarty.get.sort_on eq 'tbl_sts_id'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=tbl_sts_id&sort_by={$new_sort}">{$_lang.tbl_table_status.label.tbl_sts_id}</a></th>			<th class="{if $smarty.get.sort_on eq 'tbl_sts_name'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=tbl_sts_name&sort_by={$new_sort}">{$_lang.tbl_table_status.label.tbl_sts_name}</a></th>			<th class="{if $smarty.get.sort_on eq 'tbl_sts_desc'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=tbl_sts_desc&sort_by={$new_sort}">{$_lang.tbl_table_status.label.tbl_sts_desc}</a></th>			<th class="{if $smarty.get.sort_on eq 'tbl_sts_sort_order'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=tbl_sts_sort_order&sort_by={$new_sort}">{$_lang.tbl_table_status.label.tbl_sts_sort_order}</a></th>			<th class="{if $smarty.get.sort_on eq 'tbl_sts_start_date'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=tbl_sts_start_date&sort_by={$new_sort}">{$_lang.tbl_table_status.label.tbl_sts_start_date}</a></th>			<th class="{if $smarty.get.sort_on eq 'tbl_sts_end_date'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=tbl_sts_end_date&sort_by={$new_sort}">{$_lang.tbl_table_status.label.tbl_sts_end_date}</a></th>		</tr>		{foreach from=$tbl_table_statuslist item=tbl_table_statusitem}		<tr class="{cycle values="odd,even"}">			<td>{$tbl_table_statusitem.tbl_sts_id}</td>			<td>{$tbl_table_statusitem.tbl_sts_name}</td>			<td>{$tbl_table_statusitem.tbl_sts_desc}</td>			<td>{$tbl_table_statusitem.tbl_sts_sort_order}</td>			<td>{$tbl_table_statusitem.tbl_sts_start_date}</td>			<td>{$tbl_table_statusitem.tbl_sts_end_date}</td>		</tr>	{/foreach}		<tfoot>			<tr>				<td colspan="6">					# {$result_found}&nbsp;&nbsp;&nbsp;{if $pagination neq ""}{$pagination}{/if}					<select onchange="changePage('{$navigationURL}',this.value,{$smarty.request.limit});">					{if $allPageCount gt 1}						{for $foo=1 to $allPageCount}							<option value="{$foo}" {if $foo eq $currentPage}selected="selected"{/if}>{$foo}</option>						{/for}					{else}						<option value="1" disabled="disabled">1</option>					{/if}					</select>				</td>			</tr>		</tfoot>	</table>{else}	<div class="error">{$_lang.tbl_table_status.no_record_found}</div>{/if}<input onclick="window.location.href='{$page_url}?mode={$smarty.const.MODE_CREATE}'" type="button" value="{$_lang.tbl_table_status.CREATE.BTN_LBL}"/></div>{include file="tbl_table_status/js.tpl"}{include file="footer.tpl"}