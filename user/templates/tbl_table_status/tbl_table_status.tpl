{include file='header.tpl'}<div class="wrapper"><h1>{$_lang.tbl_table_status.listing_title}</h1>{if $error_msg}	<center>{$error_msg}</center>{/if}<form name="frm_tbl_table_statuslist" id="frm_tbl_table_statuslist"  method="POST" action="{$page_url}" >{if $result_found gt 0 && $tbl_table_statuslist}	<table class="listTable">		<tr><th class="bigListItem" colspan="2">{$_lang.tbl_table_status.title}</th>		</tr>		{foreach from=$tbl_table_statuslist item=tbl_table_statusitem}			<tr>				<td style="width:2%;">				<label for="sel_tbl_table_status[{$tbl_table_statusitem.tbl_sts_id}]" data-mini="true" style="width:23px;"><input type="checkbox" data-inline='true' data-mini='true' id="sel_tbl_table_status[{$tbl_table_statusitem.tbl_sts_id}]" name="sel_tbl_table_status[{$tbl_table_statusitem.tbl_sts_id}]" />&nbsp;</label>                </td>									<td class="bigListItem"><a href="{$page_url}?mode={$smarty.const.MODE_UPDATE}&tbl_sts_id={$tbl_table_statusitem.tbl_sts_id}">{$tbl_table_statusitem.tbl_sts_name} ({$tbl_table_statusitem.tbl_sts_sort_order})</a></td>				<!--<td class="actionListItem"><a href="{$page_url}?mode={$smarty.const.MODE_VIEW}&tbl_sts_id={$tbl_table_statusitem.tbl_sts_id}" class="detailIcon"></a><a href="{$page_url}?mode={$smarty.const.MODE_UPDATE}&tbl_sts_id={$tbl_table_statusitem.tbl_sts_id}" class="editIcon"></a><a class="deleteIcon" href="#" onclick="deletetbl_table_status({$tbl_table_statusitem.tbl_sts_id});"></a>{if $tbl_table_statusitem.isActive eq 1}<a href="{$page_url}?action={$smarty.const.ACTION_DEACTIVATE}&tbl_sts_id={$tbl_table_statusitem.tbl_sts_id}" class="deactiveIcon" title="{$_lang.tbl_table_status.DEACTIVATE.BTN_LBL}"></a>{else}<a href="{$page_url}?action={$smarty.const.ACTION_ACTIVATE}&tbl_sts_id={$tbl_table_statusitem.tbl_sts_id}" class="activeIcon" title="{$_lang.tbl_table_status.ACTIVATE.BTN_LBL}"></a>{/if}</td>-->			</tr>	{/foreach}	</table>{else}	<div class="error">{$_lang.tbl_table_status.no_record_found}</div>{/if}{if $pagination}	<center>{$pagination}</center>{/if}<center><input type='hidden' name='action' id='action' value='' /></form><input data-inline="true" data-icon="briefcase" type="button" id="sel_all_tbl_table_status" name="sel_all_tbl_table_status" value="{$_lang.main.toggle}" onclick="javascript:$('input[type=checkbox]').click();" /><input data-inline="true" data-icon="delete" type="button" id="del_sel_all_tbl_table_status" name="del_sel_all_tbl_table_status" value="{$_lang.tbl_table_status.DELETE.BTN_LBL}" onclick="deletetbl_table_status({$tbl_table_statusitem.tbl_sts_id});" /><input data-inline="true" data-icon="add-item"   onclick="window.location.href='{$page_url}?mode={$smarty.const.MODE_CREATE}'" type="button" value="{$_lang.tbl_table_status.CREATE.BTN_LBL}"/>	</center></div>{include file="tbl_table_status/js.tpl"}{include file="footer.tpl"}