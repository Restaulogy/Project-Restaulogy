{include file='header.tpl'}<div class="wrapper">{*include file='menu_nav_bar.tpl'*}<h1>{$_lang.tbl_menu.listing_title}</h1>{if $error_msg}	<center>{$error_msg}</center>{/if}{if $result_found gt 0 && $tbl_menulist}<form name="frm_tbl_menu" id="frm_tbl_menu" method="POST" action="{$page_url}">	<table class="listTable">		<tr>            <th class="{if $smarty.get.sort_on eq 'menu_name'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}" colspan="3">            <a href="{$page_url}?sort_on=menu_name&sort_by={$new_sort}">            {$_lang.tbl_menu.title}            </a>            </th>             <th style="width:5%;">                {$_lang.main.status}			</th>            <th>{$_lang.tbl_sub_menu.title}</th>		 </tr>		{foreach from=$tbl_menulist item=tbl_menuitem}			<tr>             <td style="width:3%;">					 <label for="sel_menu[{$tbl_menuitem.menu_id}]" data-mini="true" style="width:23px;"><input type="checkbox" data-inline='true' data-mini='true' id="sel_menu[{$tbl_menuitem.menu_id}]" name="sel_menu[{$tbl_menuitem.menu_id}]" />&nbsp;</label>			</td>		 			<td style="width:4%;">			 {if $tbl_menuitem.menu_image neq ''}			     <img width="40" height="40" src="{$website}/uploads/menu/{$tbl_menuitem.menu_image}" alt="{$tbl_menuitem.menu_name}" />             {else}                 <img width="40" height="40" src="{$website}/images/no_dish.png" />             {/if}            </td>            <td class="bigListItem" style="width:90%;">                <a href="{$page_url}?mode={$smarty.const.MODE_UPDATE}&menu_id={$tbl_menuitem.menu_id}">{$tbl_menuitem.menu_name}</a><br>              <small>{$tbl_menuitem.menu_desc}</small>           </td>		   <td style="width:3%;">{if $smarty.session.member_role_id eq $smarty.const.ROLE_ADMIN || $smarty.session.member_role_id eq $smarty.const.ROLE_MANAGER}<!--			<a href="{$page_url}?mode={$smarty.const.MODE_UPDATE}&menu_id={$tbl_menuitem.menu_id}" class="editIcon"></a>			<a class="deleteIcon" href="#" onclick="deletetbl_menu({$tbl_menuitem.menu_id});"></a>			{if $tbl_menuitem.isActive eq 1}				<a href="{$page_url}?action={$smarty.const.ACTION_DEACTIVATE}&menu_id={$tbl_menuitem.menu_id}" class="deactiveIcon" title="{$_lang.tbl_menu.DEACTIVATE.BTN_LBL}"></a>			{else}				<a href="{$page_url}?action={$smarty.const.ACTION_ACTIVATE}&menu_id={$tbl_menuitem.menu_id}" class="activeIcon" title="{$_lang.tbl_menu.ACTIVATE.BTN_LBL}"></a>			{/if}			<a target="_blank" href="{$page_url}?mode={$smarty.const.MODE_VIEW}&menu_id={$tbl_menuitem.menu_id}&is_preview=1" class="statsIcon"></a>-->            {if $tbl_menuitem.isActive eq 1}                <a href="#" class="activeIcon" title="{$_lang.tbl_menu.ACTIVATE.BTN_LBL}"></a>			{else}				<a href="#" class="deactiveIcon" title="{$_lang.tbl_menu.DEACTIVATE.BTN_LBL}"></a>			{/if}            </td>            <td>                <a title="forward" class="forwardIcon" href="{$website}/user/tbl_sub_menu.php?submnu_menu={$tbl_menuitem.menu_id}&is_forward=true"></a>    			<!--<input onclick="window.location.href='{$website}/user/tbl_sub_menu.php?submnu_menu={$tbl_menuitem.menu_id}&is_forward=true}';" data-inline="true" data-min="true" data-icon="arrow-r" type="button" data-iconpos="top" value="{$_lang.tbl_sub_menu.title}"/>-->    {/if}				</td>			</tr>	{/foreach}	</table>{else}	<div class="error">{$_lang.tbl_menu.no_record_found}</div>{/if}{if $pagination}	<center>{$pagination}</center>{/if}<input type="hidden" id="action" name="{$smarty.const.ACTION_TITLE}"/></form>{if $smarty.session.member_role_id eq $smarty.const.ROLE_ADMIN || $smarty.session.member_role_id eq $smarty.const.ROLE_MANAGER  } <div class="biz_center"> {if $result_found gt 0 && $tbl_menulist} 	<input data-inline="true" data-icon="briefcase" type="button" value="{$_lang.main.toggle}" onclick="javascript:$('input[type=checkbox]').click();" /><input data-inline="true" data-icon="inactive" type="button" value="{$_lang.tbl_dishes.DEACTIVATE.BTN_LBL}" onclick="actiontbl_menu('{$smarty.const.ACTION_DEACTIVATE}');"  /><input data-inline="true" data-icon="active" type="button" value="{$_lang.tbl_dishes.ACTIVATE.BTN_LBL}" onclick="actiontbl_menu('{$smarty.const.ACTION_ACTIVATE}');" /><!--<input data-inline="true" data-icon="delete" type="button" value="{$_lang.tbl_dishes.DELETE.BTN_LBL}" onclick="actiontbl_menu('{$smarty.const.ACTION_DELETE}');" /> -->{/if}	<input onclick="window.location.href='{$page_url}?mode={$smarty.const.MODE_CREATE}'" data-inline="true" data-icon="add-item" type="button" value="{$_lang.tbl_menu.CREATE.BTN_LBL}"/>	</div>  {/if}</div>{include file="tbl_menu/js.tpl"}{include file="footer.tpl"}