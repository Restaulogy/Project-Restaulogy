{include file='header.tpl'}<div class="wrapper"><h1>{$_lang.tbl_menu.listing_title}</h1>{if $error_msg}	<center>{$error_msg}</center>{/if}{if $result_found gt 0 && $tbl_menulist}	<table class="biz_data_grid">		<tr>			<th class="{if $smarty.get.sort_on eq 'menu_name'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=menu_name&sort_by={$new_sort}">{$_lang.tbl_menu.label.menu_name}</a></th>			<th class="{if $smarty.get.sort_on eq 'menu_category'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=menu_category&sort_by={$new_sort}">{$_lang.tbl_menu.label.menu_category}</a></th>			<th class="{if $smarty.get.sort_on eq 'menu_desc'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=menu_desc&sort_by={$new_sort}">{$_lang.tbl_menu.label.menu_desc}</a></th>			<th class="{if $smarty.get.sort_on eq 'menu_image'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=menu_image&sort_by={$new_sort}">{$_lang.tbl_menu.label.menu_image}</a></th>			<th class="{if $smarty.get.sort_on eq 'menu_start_timing'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=menu_start_timing&sort_by={$new_sort}">{$_lang.tbl_menu.label.menu_start_timing}</a></th>			<th class="{if $smarty.get.sort_on eq 'menu_end_timing'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=menu_end_timing&sort_by={$new_sort}">{$_lang.tbl_menu.label.menu_end_timing}</a></th>			<th class="action_header">Action</th>		</tr>		{foreach from=$tbl_menulist item=tbl_menuitem}		<tr class="{cycle values="odd,even"}">			<td><a href="{$page_url}?mode={$smarty.const.MODE_VIEW}&menu_id={$tbl_menuitem.menu_id}">{$tbl_menuitem.menu_name}</a></td>			<td>{$tbl_menuitem.menu_category}</td>			<td>{$tbl_menuitem.menu_desc}</td>			<td>{$tbl_menuitem.menu_image}</td>			<td>{$tbl_menuitem.menu_start_timing}</td>			<td>{$tbl_menuitem.menu_end_timing}</td>			<td><a href="{$page_url}?mode={$smarty.const.MODE_UPDATE}&menu_id={$tbl_menuitem.menu_id}" class="editIcon"></a><a class="deleteIcon" href="#" onclick="deletetbl_menu({$tbl_menuitem.menu_id});"></a>{if $tbl_menuitem.isActive eq 1}<a href="{$page_url}?action={$smarty.const.ACTION_DEACTIVATE}&menu_id={$tbl_menuitem.menu_id}" class="deactiveIcon" title="{$_lang.tbl_menu.DEACTIVATE.BTN_LBL}"></a>{else}<a href="{$page_url}?action={$smarty.const.ACTION_ACTIVATE}&menu_id={$tbl_menuitem.menu_id}" class="activeIcon" title="{$_lang.tbl_menu.ACTIVATE.BTN_LBL}"></a>{/if}</td>		</tr>	{/foreach}		<tfoot>			<tr>				<td colspan="9">					# {$result_found}&nbsp;&nbsp;&nbsp;{if $pagination neq ""}{$pagination}{/if}					<select onchange="changePage('{$navigationURL}',this.value,{$smarty.request.limit});">					{if $allPageCount gt 1}						{for $foo=1 to $allPageCount}							<option value="{$foo}" {if $foo eq $currentPage}selected="selected"{/if}>{$foo}</option>						{/for}					{else}						<option value="1" disabled="disabled">1</option>					{/if}					</select>				</td>			</tr>		</tfoot>	</table>{else}	<div class="error">{$_lang.tbl_menu.no_record_found}</div>{/if}<input onclick="window.location.href='{$page_url}?mode={$smarty.const.MODE_CREATE}'" type="button" value="{$_lang.tbl_menu.CREATE.BTN_LBL}"/></div>{include file="tbl_menu/js.tpl"}{include file="footer.tpl"}