{include file='header.tpl'}<div class="wrapper"><h1>{$_lang.tbl_rest_groups.listing_title}</h1>{if $error_msg}	<center>{$error_msg}</center>{/if}{if $result_found gt 0 && $tbl_rest_groupslist}	<table class="listTable">		<tr><th class="bigListItem">{$_lang.tbl_rest_groups.title}</th>		<th class="actionListItem"></th></tr>		{foreach from=$tbl_rest_groupslist item=tbl_rest_groupsitem}			<tr>				<td class="bigListItem"><a href="{$page_url}?mode={$smarty.const.MODE_UPDATE}&rstgrp_id={$tbl_rest_groupsitem.rstgrp_id}">{$tbl_rest_groupsitem.rstgrp_group}</a></td>				<td class="actionListItem"><a class="deleteIcon" href="#" onclick="deletetbl_rest_groups({$tbl_rest_groupsitem.rstgrp_id});"></a>{if $tbl_rest_groupsitem.isActive eq 1}<a href="{$page_url}?action={$smarty.const.ACTION_DEACTIVATE}&rstgrp_id={$tbl_rest_groupsitem.rstgrp_id}" class="deactiveIcon" title="{$_lang.tbl_rest_groups.DEACTIVATE.BTN_LBL}"></a>{else}<a href="{$page_url}?action={$smarty.const.ACTION_ACTIVATE}&rstgrp_id={$tbl_rest_groupsitem.rstgrp_id}" class="activeIcon" title="{$_lang.tbl_rest_groups.ACTIVATE.BTN_LBL}"></a>{/if}</td>			</tr>	{/foreach}	</table>{else}	<div class="error">{$_lang.tbl_rest_groups.no_record_found}</div>{/if}{if $pagination}	<center>{$pagination}</center>{/if}<center><input data-icon="add-item" data-inline="true" onclick="window.location.href='{$page_url}?mode={$smarty.const.MODE_CREATE}'" type="button" value="{$_lang.tbl_rest_groups.CREATE.BTN_LBL}"/></center></div>{include file="tbl_rest_groups/js.tpl"}{include file="footer.tpl"}