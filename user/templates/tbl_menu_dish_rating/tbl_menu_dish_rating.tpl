{include file='header.tpl'}<div class="wrapper"><h1>{$_lang.tbl_menu_dish_rating.listing_title}</h1>{if $error_msg}	<center>{$error_msg}</center>{/if}{if $result_found gt 0 && $tbl_menu_dish_ratinglist}	<table class="listTable">		<tr><th class="bigListItem">{$_lang.tbl_menu_dish_rating.title}</th>		<th class="actionListItem"></th></tr>		{foreach from=$tbl_menu_dish_ratinglist item=tbl_menu_dish_ratingitem}			<tr>				<td class="bigListItem"><a href="{$page_url}?mode={$smarty.const.MODE_VIEW}&mnu_dsh_rate_id={$tbl_menu_dish_ratingitem.mnu_dsh_rate_id}">{$tbl_menu_dish_ratingitem.mnu_dsh_rate_type}</a></td>				<td class="actionListItem"><a href="{$page_url}?mode={$smarty.const.MODE_UPDATE}&mnu_dsh_rate_id={$tbl_menu_dish_ratingitem.mnu_dsh_rate_id}" class="editIcon"></a><a class="deleteIcon" href="#" onclick="deletetbl_menu_dish_rating({$tbl_menu_dish_ratingitem.mnu_dsh_rate_id});"></a>{if $tbl_menu_dish_ratingitem.isActive eq 1}<a href="{$page_url}?action={$smarty.const.ACTION_DEACTIVATE}&mnu_dsh_rate_id={$tbl_menu_dish_ratingitem.mnu_dsh_rate_id}" class="deactiveIcon" title="{$_lang.tbl_menu_dish_rating.DEACTIVATE.BTN_LBL}"></a>{else}<a href="{$page_url}?action={$smarty.const.ACTION_ACTIVATE}&mnu_dsh_rate_id={$tbl_menu_dish_ratingitem.mnu_dsh_rate_id}" class="activeIcon" title="{$_lang.tbl_menu_dish_rating.ACTIVATE.BTN_LBL}"></a>{/if}</td>			</tr>	{/foreach}	</table>{else}	<div class="error">{$_lang.tbl_menu_dish_rating.no_record_found}</div>{/if}{if $pagination}	<center>{$pagination}</center>{/if}<input onclick="window.location.href='{$page_url}?mode={$smarty.const.MODE_CREATE}'" type="button" value="{$_lang.tbl_menu_dish_rating.CREATE.BTN_LBL}"/></div>{include file="tbl_menu_dish_rating/js.tpl"}{include file="footer.tpl"}