{literal}<script type="text/javascript">function deletetbl_menu_dish_rating(varId){	if(varId > 0){		if(confirm("{/literal}{$_lang.tbl_menu_dish_rating.DELETE.CONFIRM_MSG}{literal}")==true){			window.location.href="{/literal}{$page_url}{literal}?action=delete&mnu_dsh_rate_id="+varId;		}	}}function validatetbl_menu_dish_rating(){	$("#mnu_dsh_rate_id_err").html("");	$("#mnu_dsh_rate_type_err").html("");	$("#mnu_dsh_rate_enty_id_err").html("");	$("#mnu_dsh_rate_star_err").html("");	$("#mnu_dsh_rate_description_err").html("");	$("#mnu_dsh_rate_by_cust_err").html("");	$("#mnu_dsh_rate_created_on_err").html("");	var isErr = true;	if(elemById("action").value=="update"){		if(IsNonEmpty(elemById("mnu_dsh_rate_id").value)==false){			$("#mnu_dsh_rate_id_err").html("{/literal}{$_lang.tbl_menu_dish_rating.not_empty_msg.mnu_dsh_rate_id}{literal}");			isErr = false;		}	}	if(IsNonEmpty(elemById("mnu_dsh_rate_type").value)==false){		$("#mnu_dsh_rate_type_err").html("{/literal}{$_lang.tbl_menu_dish_rating.not_empty_msg.mnu_dsh_rate_type}{literal}");		isErr = false;	}	if(IsNonEmpty(elemById("mnu_dsh_rate_enty_id").value)==false){		$("#mnu_dsh_rate_enty_id_err").html("{/literal}{$_lang.tbl_menu_dish_rating.not_empty_msg.mnu_dsh_rate_enty_id}{literal}");		isErr = false;	}	if(IsNonEmpty(elemById("mnu_dsh_rate_star").value)==false){		$("#mnu_dsh_rate_star_err").html("{/literal}{$_lang.tbl_menu_dish_rating.not_empty_msg.mnu_dsh_rate_star}{literal}");		isErr = false;	}	if(IsNonEmpty(elemById("mnu_dsh_rate_description").value)==false){		$("#mnu_dsh_rate_description_err").html("{/literal}{$_lang.tbl_menu_dish_rating.not_empty_msg.mnu_dsh_rate_description}{literal}");		isErr = false;	}	if(IsNonEmpty(elemById("mnu_dsh_rate_by_cust").value)==false){		$("#mnu_dsh_rate_by_cust_err").html("{/literal}{$_lang.tbl_menu_dish_rating.not_empty_msg.mnu_dsh_rate_by_cust}{literal}");		isErr = false;	}	if(IsNonEmpty(elemById("mnu_dsh_rate_created_on").value)==false){		$("#mnu_dsh_rate_created_on_err").html("{/literal}{$_lang.tbl_menu_dish_rating.not_empty_msg.mnu_dsh_rate_created_on}{literal}");		isErr = false;	}	if(isErr == false){		alert("{/literal}{$_lang.messages.revise_form}{literal}");	}	return isErr;}//..function</script>{/literal}