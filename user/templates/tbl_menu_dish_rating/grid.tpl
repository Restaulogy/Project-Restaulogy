{include file='header.tpl'}<div class="wrapper"><h1>{$_lang.tbl_menu_dish_rating.listing_title}</h1>{if $error_msg}	<center>{$error_msg}</center>{/if}{if $result_found gt 0 && $tbl_menu_dish_ratinglist}	<table class="biz_data_grid">		<tr>			<th class="{if $smarty.get.sort_on eq 'mnu_dsh_rate_type'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=mnu_dsh_rate_type&sort_by={$new_sort}">{$_lang.tbl_menu_dish_rating.label.mnu_dsh_rate_type}</a></th>			<th class="{if $smarty.get.sort_on eq 'mnu_dsh_rate_enty_id'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=mnu_dsh_rate_enty_id&sort_by={$new_sort}">{$_lang.tbl_menu_dish_rating.label.mnu_dsh_rate_enty_id}</a></th>			<th class="{if $smarty.get.sort_on eq 'mnu_dsh_rate_star'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=mnu_dsh_rate_star&sort_by={$new_sort}">{$_lang.tbl_menu_dish_rating.label.mnu_dsh_rate_star}</a></th>			<th class="{if $smarty.get.sort_on eq 'mnu_dsh_rate_description'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=mnu_dsh_rate_description&sort_by={$new_sort}">{$_lang.tbl_menu_dish_rating.label.mnu_dsh_rate_description}</a></th>			<th class="{if $smarty.get.sort_on eq 'mnu_dsh_rate_by_cust'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=mnu_dsh_rate_by_cust&sort_by={$new_sort}">{$_lang.tbl_menu_dish_rating.label.mnu_dsh_rate_by_cust}</a></th>			<th class="{if $smarty.get.sort_on eq 'mnu_dsh_rate_created_on'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=mnu_dsh_rate_created_on&sort_by={$new_sort}">{$_lang.tbl_menu_dish_rating.label.mnu_dsh_rate_created_on}</a></th>			<th class="action_header">Action</th>		</tr>		{foreach from=$tbl_menu_dish_ratinglist item=tbl_menu_dish_ratingitem}		<tr class="{cycle values="odd,even"}">			<td><a href="{$page_url}?mode={$smarty.const.MODE_VIEW}&mnu_dsh_rate_id={$tbl_menu_dish_ratingitem.mnu_dsh_rate_id}">{$tbl_menu_dish_ratingitem.mnu_dsh_rate_type}</a></td>			<td>{$tbl_menu_dish_ratingitem.mnu_dsh_rate_enty_id}</td>			<td>{$tbl_menu_dish_ratingitem.mnu_dsh_rate_star}</td>			<td>{$tbl_menu_dish_ratingitem.mnu_dsh_rate_description}</td>			<td>{$tbl_menu_dish_ratingitem.mnu_dsh_rate_by_cust}</td>			<td>{$tbl_menu_dish_ratingitem.mnu_dsh_rate_created_on}</td>			<td><a href="{$page_url}?mode={$smarty.const.MODE_UPDATE}&mnu_dsh_rate_id={$tbl_menu_dish_ratingitem.mnu_dsh_rate_id}" class="editIcon"></a><a class="deleteIcon" href="#" onclick="deletetbl_menu_dish_rating({$tbl_menu_dish_ratingitem.mnu_dsh_rate_id});"></a>{if $tbl_menu_dish_ratingitem.isActive eq 1}<a href="{$page_url}?action={$smarty.const.ACTION_DEACTIVATE}&mnu_dsh_rate_id={$tbl_menu_dish_ratingitem.mnu_dsh_rate_id}" class="deactiveIcon" title="{$_lang.tbl_menu_dish_rating.DEACTIVATE.BTN_LBL}"></a>{else}<a href="{$page_url}?action={$smarty.const.ACTION_ACTIVATE}&mnu_dsh_rate_id={$tbl_menu_dish_ratingitem.mnu_dsh_rate_id}" class="activeIcon" title="{$_lang.tbl_menu_dish_rating.ACTIVATE.BTN_LBL}"></a>{/if}</td>		</tr>	{/foreach}		<tfoot>			<tr>				<td colspan="7">					# {$result_found}&nbsp;&nbsp;&nbsp;{if $pagination neq ""}{$pagination}{/if}					<select onchange="changePage('{$navigationURL}',this.value,{$smarty.request.limit});">					{if $allPageCount gt 1}						{for $foo=1 to $allPageCount}							<option value="{$foo}" {if $foo eq $currentPage}selected="selected"{/if}>{$foo}</option>						{/for}					{else}						<option value="1" disabled="disabled">1</option>					{/if}					</select>				</td>			</tr>		</tfoot>	</table>{else}	<div class="error">{$_lang.tbl_menu_dish_rating.no_record_found}</div>{/if}<input onclick="window.location.href='{$page_url}?mode={$smarty.const.MODE_CREATE}'" type="button" value="{$_lang.tbl_menu_dish_rating.CREATE.BTN_LBL}"/></div>{include file="tbl_menu_dish_rating/js.tpl"}{include file="footer.tpl"}