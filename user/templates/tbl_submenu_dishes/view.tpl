ding-toggle').addClass('biz_ui_selected').trigger("create");	}else{		$(this).find('a.ui-collapsible-heading-toggle').removeClass('biz_ui_selected').trigger("create");	}}); $(function(){	/*$('#menu_start_timing').scroller({ preset: 'time', timeFormat:'HH:ii'});*/	$("#menu_start_timing").scroller({ preset: 'time', timeFormat: 'HH:ii:ss', timeWheels: 'HHii', animate: 'pop'});	$("#menu_end_timing").scroller({ preset: 'time', timeFormat: 'HH:ii:ss', timeWheels: 'HHii', animate: 'pop'});	/*$('#menu_end_timing').scroller({ preset: 'time' }); */});	function deleteDish_opt_price(sbopt_pr_id){	 	if(sbopt_pr_id > 0){ 		if(confirm("{/literal}{$_lang.tbl_sbmnu_dish_opt_price.DELETE.CONFIRM_MSG}{literal}")==true){				var info = {action:'deleteDish_opt_price',var1:sbopt_pr_id};				ajaxian(info,function(res){		 			window.location.reload();		 		});			} 		} 	} 	function updateDish_opt_price(opt_val_id, isReset){		/*alert(elemById("price_" + opt_val_id).value);		alert(elemById("price_id_" + opt_val_id).value); */				 if(IsNumeric(elemById("price_" + opt_val_id).value)){ 			elemById("sbmdopt_pr_option_value").value = opt_val_id;  		 if(isReset){		 	elemById("sbmdopt_pr_price").value = "0.00";		 }else{		 	elemById("sbmdopt_pr_price").value = elemById("price_" + opt_val_id).value;		 }			 elemById("sbmdopt_pr_id").value = elemById("price_id_" + opt_val_id).value; 			 			if(elemById("sbmdopt_pr_id").value>0){ 				elemById("opt_price_action").value = "UPDATE";			}else{				elemById("opt_price_action").value = "CREATE";			}			 			$("#btn_submit_tbl_sbmnu_dish_opt_price").trigger('click');		}else{			alert("Please Enter Proper Price");			return false;		}		return true;		} </script>{/literal}