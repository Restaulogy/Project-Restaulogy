{include file='header.tpl'}<div class="wrapper"><h1>{$_lang.tbl_submenu_dishes.listing_title}</h1>{if $error_msg}	<center>{$error_msg}</center>{/if}{if $result_found gt 0 && $tbl_submenu_disheslist}	<table class="biz_data_grid">		<tr>			<th class="{if $smarty.get.sort_on eq 'sbmnu_dish_submenu'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=sbmnu_dish_submenu&sort_by={$new_sort}">{$_lang.tbl_submenu_dishes.label.sbmnu_dish_submenu}</a></th>			<th class="{if $smarty.get.sort_on eq 'sbmnu_dish_dish'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=sbmnu_dish_dish&sort_by={$new_sort}">{$_lang.tbl_submenu_dishes.label.sbmnu_dish_dish}</a></th>			<th class="{if $smarty.get.sort_on eq 'sbmnu_dish_price'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=sbmnu_dish_price&sort_by={$new_sort}">{$_lang.tbl_submenu_dishes.label.sbmnu_dish_price}</a></th>			<th class="{if $smarty.get.sort_on eq 'sbmnu_dish_display_order'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=sbmnu_dish_display_order&sort_by={$new_sort}">{$_lang.tbl_submenu_dishes.label.sbmnu_dish_display_order}</a></th>			<th class="{if $smarty.get.sort_on eq 'sbmnu_dish_desc'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=sbmnu_dish_desc&sort_by={$new_sort}">{$_lang.tbl_submenu_dishes.label.sbmnu_dish_desc}</a></th>			<th class="action_header">Action</th>		</tr>		{foreach from=$tbl_submenu_disheslist item=tbl_submenu_dishesitem}		<tr class="{cycle values="odd,even"}">			<td><a href="{$page_url}?mode={$smarty.const.MODE_VIEW}&sbmnu_dish_id={$tbl_submenu_dishesitem.sbmnu_dish_id}">{$tbl_submenu_dishesitem.sbmnu_dish_submenu}</a></td>			<td>{$tbl_submenu_dishesitem.sbmnu_dish_dish}</td>			<td>{$tbl_submenu_dishesitem.sbmnu_dish_price}</td>			<td>{$tbl_submenu_dishesitem.sbmnu_dish_display_order}</td>			<td>{$tbl_submenu_dishesitem.sbmnu_dish_desc}</td>			<td><a href="{$page_url}?mode={$smarty.const.MODE_UPDATE}&sbmnu_dish_id={$tbl_submenu_dishesitem.sbmnu_dish_id}" class="editIcon"></a><a class="deleteIcon" href="#" onclick="deletetbl_submenu_dishes({$tbl_submenu_dishesitem.sbmnu_dish_id});"></a>{if $tbl_submenu_dishesitem.isActive eq 1}<a href="{$page_url}?action={$smarty.const.ACTION_DEACTIVATE}&sbmnu_dish_id={$tbl_submenu_dishesitem.sbmnu_dish_id}" class="deactiveIcon" title="{$_lang.tbl_submenu_dishes.DEACTIVATE.BTN_LBL}"></a>{else}<a href="{$page_url}?action={$smarty.const.ACTION_ACTIVATE}&sbmnu_dish_id={$tbl_submenu_dishesitem.sbmnu_dish_id}" class="activeIcon" title="{$_lang.tbl_submenu_dishes.ACTIVATE.BTN_LBL}"></a>{/if}</td>		</tr>	{/foreach}		<tfoot>			<tr>				<td colspan="8">					# {$result_found}&nbsp;&nbsp;&nbsp;{if $pagination neq ""}{$pagination}{/if}					<select onchange="changePage('{$navigationURL}',this.value,{$smarty.request.limit});">					{if $allPageCount gt 1}						{for $foo=1 to $allPageCount}							<option value="{$foo}" {if $foo eq $currentPage}selected="selected"{/if}>{$foo}</option>						{/for}					{else}						<option value="1" disabled="disabled">1</option>					{/if}					</select>				</td>			</tr>		</tfoot>	</table>{else}	<div class="error">{$_lang.tbl_submenu_dishes.no_record_found}</div>{/if}<input onclick="window.location.href='{$page_url}?mode={$smarty.const.MODE_CREATE}'" type="button" value="{$_lang.tbl_submenu_dishes.CREATE.BTN_LBL}"/></div>{include file="tbl_submenu_dishes/js.tpl"}{include file="footer.tpl"}