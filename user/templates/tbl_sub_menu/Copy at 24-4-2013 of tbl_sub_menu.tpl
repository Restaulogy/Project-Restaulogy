{include file='header.tpl'}<div class="wrapper">{include file='menu_nav_bar.tpl'}<!--<h1>"{$mnu_deatils.menu_name}" - <small style="font-size:12px;"> {$_lang.tbl_sub_menu.listing_title} </small></h1>-->{if $error_msg}	<center>{$error_msg}</center>{/if}<div id="add_tbl_submenu" style="display:none;padding:5px;">	{include file="tbl_sub_menu/add.tpl"}</div><div class="field-row clearfix">	<button type="button" id="btn_add_tbl_submenu" onclick="clear_sub_menu_form();$('#add_tbl_submenu').show();$('#btn_add_tbl_submenu').hide();">Add New</button></div><div class="clearfix"></div><br> {if $result_found gt 0 && $tbl_sub_menulist}	<table class="listTable">		<tr><th class="bigListItem">"{$mnu_deatils.menu_name}" - <small style="font-size:12px;"> {$_lang.tbl_sub_menu.listing_title} </small> </th>		<th class="actionListItem"></th></tr>		{foreach from=$tbl_sub_menulist item=tbl_sub_menuitem}			<tr>				<td class="bigListItem"><a href="{$page_url}&mode={$smarty.const.MODE_VIEW}&submnu_id={$tbl_sub_menuitem.submnu_id}">{$tbl_sub_menuitem.submnu_name}</a></td>				<td class="actionListItem">				<a href="{$page_url}&mode={$smarty.const.MODE_UPDATE}&submnu_id={$tbl_sub_menuitem.submnu_id}" class="editIcon"></a>				<a class="deleteIcon" href="#" onclick="deletetbl_sub_menu({$tbl_sub_menuitem.submnu_id});"></a>				{if $tbl_sub_menuitem.isActive eq 1}<a href="{$page_url}&action={$smarty.const.ACTION_DEACTIVATE}&submnu_id={$tbl_sub_menuitem.submnu_id}" class="deactiveIcon" title="{$_lang.tbl_sub_menu.DEACTIVATE.BTN_LBL}"></a>{else}<a href="{$page_url}&action={$smarty.const.ACTION_ACTIVATE}&submnu_id={$tbl_sub_menuitem.submnu_id}" class="activeIcon" title="{$_lang.tbl_sub_menu.ACTIVATE.BTN_LBL}"></a>{/if}				<a class="forwardIcon" href="{$website}/user/tbl_submenu_dishes.php?sbmnu_dish_submenu={$tbl_sub_menuitem.submnu_id}"></a>				</td>			</tr>	{/foreach}	</table>{else}	<div class="error">{$_lang.tbl_sub_menu.no_record_found}</div>{/if}{if $pagination}	<center>{$pagination}</center>{/if}<!--<input onclick="window.location.href='{$page_url}&mode={$smarty.const.MODE_CREATE}'" type="button" value="{$_lang.tbl_sub_menu.CREATE.BTN_LBL}"/>--></div>{include file="tbl_sub_menu/js.tpl"}{include file="footer.tpl"}