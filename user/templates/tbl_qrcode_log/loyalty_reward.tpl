<table class="listTable">    <tr>		<td class="bigListItem">            <table class="listTable">                <tr><th class="bigListItem">Search</th></tr>            	<tr>            		<td class="bigListItem">                        {include file='tbl_qrcode_log/search.tpl'}                    </td>            	</tr>            </table>        </td>	</tr>	<tr>		<td class="bigListItem">            <table class="listTable">                <tr><th class="bigListItem" colspan="2"> Loyalty Reward Info </th></tr>            	<tr>            		<td><b>New members</b></td><td>{$report_img.new_joins}</td>            	</tr>            	<tr>            		<td><b>Current members</b></td><td>{$report_img.existing_member}</td>            	</tr>            	<tr>            		<td><b>Points earned</b></td><td>{$report_img.points_dt_range|round:0}</td>            	</tr>            	<tr>            		<td><b>Points redeemed</b></td><td>{$report_img.redeemed_dt_range}</td>            	</tr>            	<tr>            		<td><b>Amount spent ($)</b></td><td>{$report_img.amt_spent_dt_range}</td>            	</tr>            </table>        </td>	</tr>	<tr>		<td class="bigListItem">            <table class="listTable">                <tr><th class="bigListItem">A] Repeat visits</th></tr>            	<tr>            		<td class="bigListItem">                        <div class="biz_center">                         {if $report_img neq "" && $report_img.repeat_visit_report.img_src neq ""}                        	 <img src="data:image/png;base64,{$report_img.repeat_visit_report.img_src}" width="90%;"/>                         {else}                        	 <img src="{$website}/images/_graphics/no_record.png"/>                         {/if}                        </div>                    </td>            	</tr>            </table>        </td>	</tr>		<tr>		<td class="bigListItem">            <table class="listTable">                <tr><th class="bigListItem">B] Points awarded table-wise</th></tr>            	<tr>            		<td class="bigListItem">                        <div class="biz_center">                         {if $report_img neq "" && $report_img.table_report.img_src neq ""}                        	 <img src="data:image/png;base64,{$report_img.table_report.img_src}" width="90%;"/>                         {else}                        	 <img src="{$website}/images/_graphics/no_record.png"/>                         {/if}                        </div>                    </td>            	</tr>            </table>        </td>	</tr>		<tr>		<td class="bigListItem">            <table class="listTable">                <tr><th class="bigListItem">C] Points awarded date-wise</th></tr>            	<tr>            		<td class="bigListItem">                        <div class="biz_center">                         {if $report_img neq "" && $report_img.day_report.img_src neq ""}                        	 <img src="data:image/png;base64,{$report_img.day_report.img_src}" width="90%;"/>                         {else}                        	 <img src="{$website}/images/_graphics/no_record.png"/>                         {/if}                        </div>                    </td>            	</tr>            </table>        </td>	</tr>		<tr>		<td class="bigListItem">            <table class="listTable">                <tr><th class="bigListItem">D] Points wise memebrs</th></tr>            	<tr>            		<td class="bigListItem">                        <div class="biz_center">                         {if $report_img neq "" && $report_img.pointwise_member_report.img_src neq ""}                        	 <img src="data:image/png;base64,{$report_img.pointwise_member_report.img_src}" width="90%;"/>                         {else}                        	 <img src="{$website}/images/_graphics/no_record.png"/>                         {/if}                        </div>                    </td>            	</tr>            </table>        </td>	</tr></table>