{if $result_found gt 0 && $tbl_qrcode_loglist}	<table class="biz_data_grid">		<tr>        {if $search_groupby neq ''}            <th class="{if $smarty.get.sort_on eq 'dy_wk_mnth'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=dy_wk_mnth&search_groupby=$search_groupby&sort_by={$new_sort}">Group</a></th>			<th class="{if $smarty.get.sort_on eq 'no_of_recs'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=no_of_recs&search_groupby=$search_groupby&sort_by={$new_sort}">Total</a></th>        {else}			<th class="{if $smarty.get.sort_on eq 'qrlog_member_id'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=qrlog_member_id&sort_by={$new_sort}">{$_lang.tbl_qrcode_log.label.qrlog_member_id}</a></th>			<th class="{if $smarty.get.sort_on eq 'qrlog_tbl_id'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=qrlog_tbl_id&sort_by={$new_sort}">{$_lang.tbl_qrcode_log.label.qrlog_tbl_id}</a></th>			<th class="{if $smarty.get.sort_on eq 'qrlog_in_time'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=qrlog_in_time&sort_by={$new_sort}">{$_lang.tbl_qrcode_log.label.qrlog_in_time}</a></th>		{/if}        </tr> 		{foreach from=$tbl_qrcode_loglist item=tbl_qrcode_logitem}		<tr class="{cycle values="odd,even"}">            {if $search_groupby neq ''}                <td>{$tbl_qrcode_logitem.dy_wk_mnth}</td>    			<td>{$tbl_qrcode_logitem.no_of_recs}</td>            {else}    			<!--                <td><a href="{$page_url}?mode={$smarty.const.MODE_VIEW}&qrlog_id={$tbl_qrcode_logitem.qrlog_id}">{$tbl_qrcode_logitem.qrlog_member_id}</a></td>                -->                <td>{$tbl_qrcode_logitem.qrlog_member_id}</td>    			<td>{$tbl_qrcode_logitem.qrlog_table_number}</td>    			<td>{$tbl_qrcode_logitem.qrlog_in_time}</td>			{/if}        </tr>	   {/foreach}		<tfoot>			<tr>				<td colspan="5">					# {$result_found}&nbsp;&nbsp;&nbsp;{if $pagination neq ""}{$pagination}{/if}					<select onchange="changePage('{$navigationURL}',this.value,{$smarty.request.limit});">					{if $allPageCount gt 1}						{for $foo=1 to $allPageCount}							<option value="{$foo}" {if $foo eq $currentPage}selected="selected"{/if}>{$foo}</option>						{/for}					{else}						<option value="1" disabled="disabled">1</option>					{/if}					</select>				</td>			</tr>		</tfoot>	</table>		<span class="error"> Total:    {if $search_groupby neq ''}        {$result_found_group}    {else}        {$result_found}    {/if}    </span>{else}	<div class="error">{$_lang.tbl_qrcode_log.no_record_found}</div>{/if}