{literal}<script type="text/javascript">function validDtRange(frmDtId,toDtId){    if(frmDtId){}else{		frmDtId = 'search_from_dt';	}	if(toDtId){}else{		toDtId = 'search_to_dt';	}	if(new Date($('#'+ toDtId).val()).getTime() < new Date($('#' + frmDtId).val()).getTime()){	//if($('#'+ toDtId).val() < $('#' + frmDtId).val()){		alert('End date should be greter than start date');		return false;	}	return true;}function deletetbl_qrcode_log(varId){	if(varId > 0){		if(confirm("{/literal}{$_lang.tbl_qrcode_log.DELETE.CONFIRM_MSG}{literal}")==true){			window.location.href="{/literal}{$page_url}{literal}?action=delete&qrlog_id="+varId;		}	}}function validatetbl_qrcode_log(){	$("#qrlog_id_err").html("");	$("#qrlog_member_id_err").html("");	$("#qrlog_tbl_id_err").html("");	$("#qrlog_in_time_err").html("");	$("#qrlog_out_time_err").html("");	var isErr = true;	if(elemById("action").value=="update"){		if(IsNonEmpty(elemById("qrlog_id").value)==false){			$("#qrlog_id_err").html("{/literal}{$_lang.tbl_qrcode_log.not_empty_msg.qrlog_id}{literal}");			isErr = false;		}	}	if(IsNonEmpty(elemById("qrlog_member_id").value)==false){		$("#qrlog_member_id_err").html("{/literal}{$_lang.tbl_qrcode_log.not_empty_msg.qrlog_member_id}{literal}");		isErr = false;	}	if(IsNonEmpty(elemById("qrlog_tbl_id").value)==false){		$("#qrlog_tbl_id_err").html("{/literal}{$_lang.tbl_qrcode_log.not_empty_msg.qrlog_tbl_id}{literal}");		isErr = false;	}	if(IsNonEmpty(elemById("qrlog_in_time").value)==false){		$("#qrlog_in_time_err").html("{/literal}{$_lang.tbl_qrcode_log.not_empty_msg.qrlog_in_time}{literal}");		isErr = false;	}	if(IsNonEmpty(elemById("qrlog_out_time").value)==false){		$("#qrlog_out_time_err").html("{/literal}{$_lang.tbl_qrcode_log.not_empty_msg.qrlog_out_time}{literal}");		isErr = false;	}	if(isErr == false){		alert("{/literal}{$_lang.messages.revise_form}{literal}");	}	return isErr;}//..function</script>{/literal}