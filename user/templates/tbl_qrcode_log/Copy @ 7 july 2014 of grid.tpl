{include file='header.tpl'}<div class="wrapper"><h1>{$_lang.tbl_qrcode_log.listing_title}</h1><a href="#" style="float:right;" data-role="button" data-icon="search" data-iconpos="notext" onclick="$('#filter_for_qrcodescan').toggle();"></a>{if $filt_text neq ''}	<div style="width:99%;padding:5px;border-bottom:1px dotted #ccc;font-style: italic;"><span class="info">Search results &laquo; </span><span class="notice">{$filt_text}</span><span class="info">&raquo;</span></div><br>{/if}{if $error_msg}	<center>{$error_msg}</center>{/if}{include file='tbl_qrcode_log/search.tpl'}{if $result_found gt 0 && $tbl_qrcode_loglist}	<table class="biz_data_grid">		<tr>			<th class="{if $smarty.get.sort_on eq 'qrlog_member_id'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=qrlog_member_id&sort_by={$new_sort}">{$_lang.tbl_qrcode_log.label.qrlog_member_id}</a></th>			<th class="{if $smarty.get.sort_on eq 'qrlog_tbl_id'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=qrlog_tbl_id&sort_by={$new_sort}">{$_lang.tbl_qrcode_log.label.qrlog_tbl_id}</a></th>			<th class="{if $smarty.get.sort_on eq 'qrlog_in_time'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=qrlog_in_time&sort_by={$new_sort}">{$_lang.tbl_qrcode_log.label.qrlog_in_time}</a></th>			<!--            <th class="{if $smarty.get.sort_on eq 'qrlog_out_time'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=qrlog_out_time&sort_by={$new_sort}">{$_lang.tbl_qrcode_log.label.qrlog_out_time}</a></th>			<th class="action_header">Action</th>            -->        </tr>		{foreach from=$tbl_qrcode_loglist item=tbl_qrcode_logitem}		<tr class="{cycle values="odd,even"}">			<td><a href="{$page_url}?mode={$smarty.const.MODE_VIEW}&qrlog_id={$tbl_qrcode_logitem.qrlog_id}">{$tbl_qrcode_logitem.qrlog_member_id}</a></td>			<td>{$tbl_qrcode_logitem.qrlog_table_number}</td>			<td>{$tbl_qrcode_logitem.qrlog_in_time}</td>			<!--            <td>{$tbl_qrcode_logitem.qrlog_out_time}</td>			<td><a href="{$page_url}?mode={$smarty.const.MODE_UPDATE}&qrlog_id={$tbl_qrcode_logitem.qrlog_id}" class="editIcon"></a><a class="deleteIcon" href="#" onclick="deletetbl_qrcode_log({$tbl_qrcode_logitem.qrlog_id});"></a>{if $tbl_qrcode_logitem.isActive eq 1}<a href="{$page_url}?action={$smarty.const.ACTION_DEACTIVATE}&qrlog_id={$tbl_qrcode_logitem.qrlog_id}" class="deactiveIcon" title="{$_lang.tbl_qrcode_log.DEACTIVATE.BTN_LBL}"></a>{else}<a href="{$page_url}?action={$smarty.const.ACTION_ACTIVATE}&qrlog_id={$tbl_qrcode_logitem.qrlog_id}" class="activeIcon" title="{$_lang.tbl_qrcode_log.ACTIVATE.BTN_LBL}"></a>{/if}</td>			-->        </tr>	{/foreach}		<tfoot>			<tr>				<td colspan="5">					# {$result_found}&nbsp;&nbsp;&nbsp;{if $pagination neq ""}{$pagination}{/if}					<select onchange="changePage('{$navigationURL}',this.value,{$smarty.request.limit});">					{if $allPageCount gt 1}						{for $foo=1 to $allPageCount}							<option value="{$foo}" {if $foo eq $currentPage}selected="selected"{/if}>{$foo}</option>						{/for}					{else}						<option value="1" disabled="disabled">1</option>					{/if}					</select>				</td>			</tr>		</tfoot>	</table>		<br>	<span class="info"> Total:{$result_found}</span>{else}	<div class="error">{$_lang.tbl_qrcode_log.no_record_found}</div>{/if}<!--<center><input data-icon="add-item" data-inline="true" onclick="window.location.href='{$page_url}?mode={$smarty.const.MODE_CREATE}'" type="button" value="{$_lang.tbl_qrcode_log.CREATE.BTN_LBL}"/></center>--></div>{include file="tbl_qrcode_log/js.tpl"}{include file="footercontent.tpl"}{literal}<script type="text/javascript">      $(function(){  $("#search_from_dt, #search_to_dt").scroller({ display:'bubble', preset: 'date', dateFormat: 'mm/dd/yyyy', timeWheels: 'yyyymmdd', animate: 'slidevertical'});});</script>{/literal}</body></html>