{include file="header.tpl"}<div class="wrapper"><h1>{$_lang.tbl_qrcode_log.create_title}</h1>{if $error_msg}	<center>{$error_msg}</center>{/if}<form name="frmcreatetbl_qrcode_log" id="frmcreatetbl_qrcode_log" onsubmit="return validatetbl_qrcode_log();" method="POST" action="{$page_url}">	<input type="hidden" name="Array" id="Array" value="0"/>	<div style="display:none;" class="error" id="Array_err"></div>	<div class="field-row">		<label for="qrlog_member_id">{$_lang.tbl_qrcode_log.label.qrlog_member_id}</label>		<input maxlength="11" id="qrlog_member_id" type="text" value="{$smarty.post.qrlog_member_id}" name="qrlog_member_id"/> 		<div class="error" id="qrlog_member_id_err"></div>	</div>	<div class="field-row">		<label for="qrlog_tbl_id">{$_lang.tbl_qrcode_log.label.qrlog_tbl_id}</label>		<input maxlength="11" id="qrlog_tbl_id" type="text" value="{$smarty.post.qrlog_tbl_id}" name="qrlog_tbl_id"/> 		<div class="error" id="qrlog_tbl_id_err"></div>	</div>	<div class="field-row">		<label for="qrlog_in_time">{$_lang.tbl_qrcode_log.label.qrlog_in_time}</label>		<input  id="qrlog_in_time" type="text" value="{$smarty.post.qrlog_in_time}" name="qrlog_in_time"/> 		<div class="error" id="qrlog_in_time_err"></div>	</div>	<div class="field-row">		<label for="qrlog_out_time">{$_lang.tbl_qrcode_log.label.qrlog_out_time}</label>		<input  id="qrlog_out_time" type="text" value="{$smarty.post.qrlog_out_time}" name="qrlog_out_time"/> 		<div class="error" id="qrlog_out_time_err"></div>	</div>	<center><input type="hidden" id="action" name="action" value="{$smarty.const.ACTION_CREATE}"/><input data-inline="true" data-icon="save" type="submit" value="{$_lang.save_lbl}"/> <input data-inline="true" data-icon="delete" type="reset" value="{$_lang.cancel_lbl}" onclick="window.location.href='{$page_url}'"/></center></form>{include file="tbl_qrcode_log/js.tpl"}</div>{include file="footer.tpl"}