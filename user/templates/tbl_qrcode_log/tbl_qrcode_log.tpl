{include file='header.tpl'}<div class="wrapper"><h1>{$_lang.tbl_qrcode_log.listing_title}</h1>{if $error_msg}	<center>{$error_msg}</center>{/if}{if $result_found gt 0 && $tbl_qrcode_loglist}	<table class="listTable">		<tr><th class="bigListItem">{$_lang.tbl_qrcode_log.title}</th>		<th class="actionListItem"></th></tr>		{foreach from=$tbl_qrcode_loglist item=tbl_qrcode_logitem}			<tr>				<td class="bigListItem"><a href="{$page_url}?mode={$smarty.const.MODE_VIEW}&qrlog_id={$tbl_qrcode_logitem.qrlog_id}">{$tbl_qrcode_logitem.qrlog_member_id}</a></td>				<td class="actionListItem"><a href="{$page_url}?mode={$smarty.const.MODE_UPDATE}&qrlog_id={$tbl_qrcode_logitem.qrlog_id}" class="editIcon"></a><a class="deleteIcon" href="#" onclick="deletetbl_qrcode_log({$tbl_qrcode_logitem.qrlog_id});"></a>{if $tbl_qrcode_logitem.isActive eq 1}<a href="{$page_url}?action={$smarty.const.ACTION_DEACTIVATE}&qrlog_id={$tbl_qrcode_logitem.qrlog_id}" class="deactiveIcon" title="{$_lang.tbl_qrcode_log.DEACTIVATE.BTN_LBL}"></a>{else}<a href="{$page_url}?action={$smarty.const.ACTION_ACTIVATE}&qrlog_id={$tbl_qrcode_logitem.qrlog_id}" class="activeIcon" title="{$_lang.tbl_qrcode_log.ACTIVATE.BTN_LBL}"></a>{/if}</td>			</tr>	{/foreach}	</table>{else}	<div class="error">{$_lang.tbl_qrcode_log.no_record_found}</div>{/if}{if $pagination}	<center>{$pagination}</center>{/if}<center><input data-icon="add-item" data-inline="true" onclick="window.location.href='{$page_url}?mode={$smarty.const.MODE_CREATE}'" type="button" value="{$_lang.tbl_qrcode_log.CREATE.BTN_LBL}"/></center></div>{include file="tbl_qrcode_log/js.tpl"}{include file="footer.tpl"}