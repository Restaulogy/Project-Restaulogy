{include file="header.tpl"}<div class="wrapper"><h1>{$_lang.tbl_qrcode_log.title}</h1>{if $error_msg}	<center>{$error_msg}</center>{/if}<form name="frmupdatetbl_qrcode_log" id="frmupdatetbl_qrcode_log" onsubmit="return validatetbl_qrcode_log();" method="POST" action="{$page_url}" style="{if $isUpdate eq 1}{else}display:none;{/if}">	<input type="hidden" name="qrlog_id" id="qrlog_id" value="{$tbl_qrcode_loginfo.qrlog_id}"/>	<div style="display:none;" class="error" id="qrlog_id_err"></div>	<div class="field-row">		<label for="qrlog_member_id">{$_lang.tbl_qrcode_log.label.qrlog_member_id}</label>		<input type="text" name="qrlog_member_id" id="qrlog_member_id" value="{$tbl_qrcode_loginfo.qrlog_member_id}"/>		<div class="error" id="qrlog_member_id_err"></div>	</div>	<div class="field-row">		<label for="qrlog_tbl_id">{$_lang.tbl_qrcode_log.label.qrlog_tbl_id}</label>		<input type="text" name="qrlog_tbl_id" id="qrlog_tbl_id" value="{$tbl_qrcode_loginfo.qrlog_tbl_id}"/>		<div class="error" id="qrlog_tbl_id_err"></div>	</div>	<div class="field-row">		<label for="qrlog_in_time">{$_lang.tbl_qrcode_log.label.qrlog_in_time}</label>		<input type="text" name="qrlog_in_time" id="qrlog_in_time" value="{$tbl_qrcode_loginfo.qrlog_in_time}"/>		<div class="error" id="qrlog_in_time_err"></div>	</div>	<div class="field-row">		<label for="qrlog_out_time">{$_lang.tbl_qrcode_log.label.qrlog_out_time}</label>		<input type="text" name="qrlog_out_time" id="qrlog_out_time" value="{$tbl_qrcode_loginfo.qrlog_out_time}"/>		<div class="error" id="qrlog_out_time_err"></div>	</div>	<center><input type="hidden" id="action" name="action" value="{$smarty.const.ACTION_UPDATE}"/> <input  data-inline="true" data-icon="save"  type="submit" value="{$_lang.save_lbl}"/> <input data-inline="true" data-icon="delete" type="reset" onclick="$('#tbl_qrcode_log_view').show();$('#frmupdatetbl_qrcode_log').hide();" value="{$_lang.cancel_lbl}"/></center></form><div id="tbl_qrcode_log_view" class="description" style="{if $isUpdate eq 1}display:none;{/if}">	<table class="listTable">		<tr><th class="fieldItem">{$_lang.field_title}</th><th class="valueItem">{$_lang.value_title}</th></tr>		<tr><td class="fieldItem">{$_lang.tbl_qrcode_log.label.qrlog_id}<i>:</i></td><td class="valueItem">{$tbl_qrcode_loginfo.qrlog_id}</td></tr>		<tr><td class="fieldItem">{$_lang.tbl_qrcode_log.label.qrlog_member_id}<i>:</i></td><td class="valueItem">{$tbl_qrcode_loginfo.qrlog_member_id}</td></tr>		<tr><td class="fieldItem">{$_lang.tbl_qrcode_log.label.qrlog_tbl_id}<i>:</i></td><td class="valueItem">{$tbl_qrcode_loginfo.qrlog_tbl_id}</td></tr>		<tr><td class="fieldItem">{$_lang.tbl_qrcode_log.label.qrlog_in_time}<i>:</i></td><td class="valueItem">{$tbl_qrcode_loginfo.qrlog_in_time}</td></tr>		<tr><td class="fieldItem">{$_lang.tbl_qrcode_log.label.qrlog_out_time}<i>:</i></td><td class="valueItem">{$tbl_qrcode_loginfo.qrlog_out_time}</td></tr>	<tr><td class="fieldItem">{$_lang.tbl_qrcode_log.label.isActive}<i>:</i></td><td class="valueItem">{if $tbl_qrcode_loginfo.isActive eq 1}{$_lang.tbl_qrcode_log.label.isActive_yes}{else}{$_lang.tbl_qrcode_log.label.isActive_no}{/if}</td></tr>	</table>	<center><input data-inline="true" data-icon="edit" type="button" value="{$_lang.tbl_qrcode_log.UPDATE.BTN_LBL}" onclick="$('#tbl_qrcode_log_view').hide();$('#frmupdatetbl_qrcode_log').show();"/><input type="button" value="{$_lang.cancel_lbl}" data-inline="true" data-icon="delete" onclick="window.location.href='{$page_url}'"/></center></div>{include file="tbl_qrcode_log/js.tpl"}</div>{include file="footer.tpl"}