{include file='header.tpl'}<div class="wrapper"><h1>{if $flt_cust neq ""}"{$flt_cust}" - {$_lang.tbl_cust_favorites.title}{else if $Global_member.member_role_id eq $smarty.const.ROLE_CUSTOMER}{$_lang.tbl_cust_favorites.customer_title}{else}{$_lang.tbl_cust_favorites.listing_title}{/if}</h1>{if $error_msg}	<center>{$error_msg}</center>{/if}{if $flt_cust neq ""}	{include file="filter_customer_navbar.tpl"}{/if}<form name="frm_tbl_cust_favorites" id="frm_tbl_cust_favorites"  method="POST" action="{$page_url}" >{if $result_found gt 0 && $tbl_cust_favoriteslist}	<table class="biz_data_grid">		<tr>			<!--<th class="{if $smarty.get.sort_on eq 'cust_fav_post_id'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=cust_fav_post_id&sort_by={$new_sort}">{$_lang.tbl_cust_favorites.label.cust_fav_post_id}</a></th>-->			<!--<th class="{if $smarty.get.sort_on eq 'cust_fav_post_type'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=cust_fav_post_type&sort_by={$new_sort}">{$_lang.tbl_cust_favorites.label.cust_fav_post_type}</a></th>-->			{if $Global_member.member_role_id eq $smarty.const.ROLE_CUSTOMER}			<th>&nbsp;</th>			{/if}			<th>Post</th>			<th class="{if $smarty.get.sort_on eq 'cust_fav_desc'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=cust_fav_desc&sort_by={$new_sort}">{$_lang.tbl_cust_favorites.label.cust_fav_desc}</a></th>						{if $Global_member.member_role_id eq $smarty.const.ROLE_MANAGER}			<th class="{if $smarty.get.sort_on eq 'cust_fav_user'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=cust_fav_user&sort_by={$new_sort}">{$_lang.tbl_cust_favorites.label.cust_fav_user}</a></th>			{/if}			<th class="{if $smarty.get.sort_on eq 'cust_fav_start_date'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=cust_fav_start_date&sort_by={$new_sort}">{$_lang.tbl_cust_favorites.label.cust_fav_start_date}</a></th>			<!--<th class="action_header">Action</th>-->		</tr>	<!--href="{$page_url}?mode={$smarty.const.MODE_VIEW}&cust_fav_id={$tbl_cust_favoritesitem.cust_fav_id}"-->		{foreach from=$tbl_cust_favoriteslist item=tbl_cust_favoritesitem}				<tr class="{cycle values="odd,even"}" ><!--			<td>{$tbl_cust_favoritesitem.cust_fav_post_id}</td>-->			<!--<td>{$tbl_cust_favoritesitem.type_title}</td>-->			{if $Global_member.member_role_id eq $smarty.const.ROLE_CUSTOMER}			<td style="width:2%;">				<label for="sel_cust_favorites[{$tbl_cust_favoritesitem.cust_fav_id}]" data-mini="true" style="width:23px;"><input type="checkbox" data-inline='true' data-mini='true' id="sel_cust_favorites[{$tbl_cust_favoritesitem.cust_fav_id}]" name="sel_cust_favorites[{$tbl_cust_favoritesitem.cust_fav_id}]" />&nbsp;</label>     		</td>			{/if}			<td><a href="#" onclick="lnkPostType({$tbl_cust_favoritesitem.cust_fav_post_id},'{$tbl_cust_favoritesitem.cust_fav_post_type}');">{$tbl_cust_favoritesitem.post_title}</a></td>			<td><a href="#" onclick='popupViewFavorite("{$tbl_cust_favoritesitem.cust_fav_desc|replace:'"':"'"}");'>{if  $tbl_cust_favoritesitem.cust_fav_desc neq ""}{$tbl_cust_favoritesitem.cust_fav_desc|truncate:20}{else}--{/if}</a></td>			{if $Global_member.member_role_id eq $smarty.const.ROLE_MANAGER}			<td>{$tbl_cust_favoritesitem.customer}</td>			{/if}			<td>{$tbl_cust_favoritesitem.friendly_start_date}</td>			 			<!--<td><a href="{$page_url}?mode={$smarty.const.MODE_UPDATE}&cust_fav_id={$tbl_cust_favoritesitem.cust_fav_id}" class="editIcon"></a><a class="deleteIcon" href="#" onclick="deletetbl_cust_favorites({$tbl_cust_favoritesitem.cust_fav_id});"></a>{if $tbl_cust_favoritesitem.isActive eq 1}<a href="{$page_url}?action={$smarty.const.ACTION_DEACTIVATE}&cust_fav_id={$tbl_cust_favoritesitem.cust_fav_id}" class="deactiveIcon" title="{$_lang.tbl_cust_favorites.DEACTIVATE.BTN_LBL}"></a>{else}<a href="{$page_url}?action={$smarty.const.ACTION_ACTIVATE}&cust_fav_id={$tbl_cust_favoritesitem.cust_fav_id}" class="activeIcon" title="{$_lang.tbl_cust_favorites.ACTIVATE.BTN_LBL}"></a>{/if}</td>-->		</tr>	{/foreach}		<tfoot>			<tr>				<td colspan="7">					# {$result_found}&nbsp;&nbsp;&nbsp;{if $pagination neq ""}{$pagination}{/if}					<select onchange="changePage('{$navigationURL}',this.value,{$smarty.request.limit});">					{if $allPageCount gt 1}						{for $foo=1 to $allPageCount}							<option value="{$foo}" {if $foo eq $currentPage}selected="selected"{/if}>{$foo}</option>						{/for}					{else}						<option value="1" disabled="disabled">1</option>					{/if}					</select>				</td>			</tr>		</tfoot>	</table>{else}	<div class="error">{$_lang.tbl_cust_favorites.no_record_found}</div>{/if} <input type="hidden" id="action" name="action" value=""/></form>{if $Global_member.member_role_id eq $smarty.const.ROLE_CUSTOMER}<center>    {if $result_found gt 0 && $tbl_cust_favoriteslist}    	<input data-inline="true" data-icon="briefcase" type="button" id="sel_all_table_type" name="sel_all_table_type" value="{$_lang.main.toggle}" onclick="javascript:$('input[type=checkbox]').click();" />    	<input data-inline="true" data-icon="delete" type="button" id="del_sel_all_tbl_cust_favorites" name="del_sel_all_tbl_cust_favorites" value="{$_lang.tbl_cust_favorites.DELETE.BTN_LBL}" onclick="deletetbl_cust_favorites();" />    {/if}   	<input data-inline="true" data-icon="add-item" onclick="window.location.href='{$website}/user/tbl_menu.php'" type="button" value="{$_lang.tbl_cust_favorites.CREATE.BTN_LBL}"/></center>{/if}</div><div data-role="popup" id="popupView" data-dismissible="false" data-theme="a1" data-overlay-theme="g" style="width:250px;">	<div data-role="header">		<h1>{$_lang.tbl_cust_favorites.label.cust_fav_desc}</h1>	</div>	<div data-role="content" style="padding: 5px;"> 					<div id="vew_cust_fav_desc"></div>		<div class="clearfix line_break"></div>				<div class="biz_center"> 			<input type="button" onclick="$('#popupView').popup('close');" data-inline="true" data-icon="delete" value="{$_lang.close_lbl}" />		</div>	</div></div>{include file="tbl_cust_favorites/js.tpl"}{include file="footer.tpl"}