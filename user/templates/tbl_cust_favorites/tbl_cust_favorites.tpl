{include file='header.tpl'}<div class="wrapper"><h1>{$_lang.tbl_cust_favorites.listing_title}</h1>{if $error_msg}	<center>{$error_msg}</center>{/if}{if $result_found gt 0 && $tbl_cust_favoriteslist}	<table class="listTable">		<tr><th class="bigListItem">{$_lang.tbl_cust_favorites.title}</th>		<th class="actionListItem"></th></tr>		{foreach from=$tbl_cust_favoriteslist item=tbl_cust_favoritesitem}			<tr>				<td class="bigListItem"><a href="{$page_url}?mode={$smarty.const.MODE_VIEW}&cust_fav_id={$tbl_cust_favoritesitem.cust_fav_id}">{$tbl_cust_favoritesitem.cust_fav_post_id}</a></td>				<td class="actionListItem"><a href="{$page_url}?mode={$smarty.const.MODE_UPDATE}&cust_fav_id={$tbl_cust_favoritesitem.cust_fav_id}" class="editIcon"></a><a class="deleteIcon" href="#" onclick="deletetbl_cust_favorites({$tbl_cust_favoritesitem.cust_fav_id});"></a>{if $tbl_cust_favoritesitem.isActive eq 1}<a href="{$page_url}?action={$smarty.const.ACTION_DEACTIVATE}&cust_fav_id={$tbl_cust_favoritesitem.cust_fav_id}" class="deactiveIcon" title="{$_lang.tbl_cust_favorites.DEACTIVATE.BTN_LBL}"></a>{else}<a href="{$page_url}?action={$smarty.const.ACTION_ACTIVATE}&cust_fav_id={$tbl_cust_favoritesitem.cust_fav_id}" class="activeIcon" title="{$_lang.tbl_cust_favorites.ACTIVATE.BTN_LBL}"></a>{/if}</td>			</tr>	{/foreach}	</table>{else}	<div class="error">{$_lang.tbl_cust_favorites.no_record_found}</div>{/if}{if $pagination}	<center>{$pagination}</center>{/if}<center><input data-icon="add-item" data-inline="true" onclick="window.location.href='{$page_url}?mode={$smarty.const.MODE_CREATE}'" type="button" value="{$_lang.tbl_cust_favorites.CREATE.BTN_LBL}"/></center></div>{include file="tbl_cust_favorites/js.tpl"}{include file="footer.tpl"}