{literal}<script type="text/javascript">function deletetbl_cust_favorites(varId){	if(varId > 0){		if(confirm("{/literal}{$_lang.tbl_cust_favorites.DELETE.CONFIRM_MSG}{literal}")==true){			window.location.href="{/literal}{$page_url}{literal}?action=delete&cust_fav_id="+varId;		}	}else{		var varId = $("input:checked").length;		if(varId > 0){			if(confirm("{/literal}{$_lang.tbl_cust_favorites.DELETE.CONFIRM_MSG}{literal}")==true){					$('#action').val("{/literal}{$smarty.const.ACTION_DELETE}{literal}");				$('#frm_tbl_cust_favorites').submit();			}		}else{			alert("{/literal}{$_lang.main.select_ids.empty}{literal}");		}	}} function validatetbl_cust_favorites(){	$("#cust_fav_id_err").html("");	$("#cust_fav_post_id_err").html("");	$("#cust_fav_post_type_err").html("");	$("#cust_fav_desc_err").html("");	$("#cust_fav_user_err").html("");	$("#cust_fav_start_date_err").html("");	$("#cust_fav_end_date_err").html("");	var isErr = true;	if(elemById("action").value=="update"){		if(IsNonEmpty(elemById("cust_fav_id").value)==false){			$("#cust_fav_id_err").html("{/literal}{$_lang.tbl_cust_favorites.not_empty_msg.cust_fav_id}{literal}");			isErr = false;		}	}	if(IsNonEmpty(elemById("cust_fav_post_id").value)==false){		$("#cust_fav_post_id_err").html("{/literal}{$_lang.tbl_cust_favorites.not_empty_msg.cust_fav_post_id}{literal}");		isErr = false;	}	if(IsNonEmpty(elemById("cust_fav_post_type").value)==false){		$("#cust_fav_post_type_err").html("{/literal}{$_lang.tbl_cust_favorites.not_empty_msg.cust_fav_post_type}{literal}");		isErr = false;	}	if(IsNonEmpty(elemById("cust_fav_desc").value)==false){		$("#cust_fav_desc_err").html("{/literal}{$_lang.tbl_cust_favorites.not_empty_msg.cust_fav_desc}{literal}");		isErr = false;	}	if(IsNonEmpty(elemById("cust_fav_user").value)==false){		$("#cust_fav_user_err").html("{/literal}{$_lang.tbl_cust_favorites.not_empty_msg.cust_fav_user}{literal}");		isErr = false;	}/*	if(IsNonEmpty(elemById("cust_fav_start_date").value)==false){		$("#cust_fav_start_date_err").html("{/literal}{$_lang.tbl_cust_favorites.not_empty_msg.cust_fav_start_date}{literal}");		isErr = false;	}*//*	if(IsNonEmpty(elemById("cust_fav_end_date").value)==false){		$("#cust_fav_end_date_err").html("{/literal}{$_lang.tbl_cust_favorites.not_empty_msg.cust_fav_end_date}{literal}");		isErr = false;	}*/	if(isErr == false){		alert("{/literal}{$_lang.messages.revise_form}{literal}");	}	return isErr;}//..functionfunction lnkPostType(post_id,post_type){	if((post_id > 0) && (IsNonEmpty(post_type))){	var strURL = '';	switch(post_type){		case 'SUBMENU_DISH' : strURL = '{/literal}{$website}{literal}/user/tbl_submenu_dishes.php?sbmnu_dish_id='+ post_id + '&is_preview=1';		break;	}	if(IsNonEmpty(strURL)){		window.open(strURL);	}	}}function popupCustFavorites(post_id,post_type){	if((post_id > 0) && (IsNonEmpty(post_type))){		elemById('cust_fav_post_id').value= post_id;	 	elemById('cust_fav_post_type').value= post_type; 		$('#popupAddFavorites').popup('open');	}else{		alert('Please Select Post');	}  }function saveCustFavorites(){   $('#cust_fav_desc_err').html('');		/*if(IsNonEmpty(elemById('cust_fav_desc').value)){*/		var info = {};		info['action'] = 'add2CustFavorites';		info['var1'] = elemById('cust_fav_post_id').value;		info['var2'] = elemById('cust_fav_post_type').value;		info['var3'] = elemById('cust_fav_desc').value; 		 		$.ajax({	        type     : "POST",	        url      :website + "/ajax/custom_functions.php" ,	        dataType : "json", 					data		 : info,					async		 : false,	    		success  : function(data) {						if(data){								window.location.reload();						}					},error: function(objResponse){						alert(objResponse.responseText);					}  		});	/*}else{		$('#cust_fav_desc_err').html('{/literal}{$_lang.messages.validation.not_empty|sprintf:"description"}{literal}');	}*/} function deleteCustFavorites(cust_fav_id){	 	if(cust_fav_id > 0){	 if(confirm('Are you sure to remove from favorites')){			var info ={};			 info['action']='removeCustFavorites';			 info['var1']= cust_fav_id; 			$.ajax({	        type     : "POST",	        url      :website + "/ajax/custom_functions.php" ,	        dataType : "json", 					data		 : info,					async		 : false,	    		success  : function(data) {						if(data){window.location.reload();}					},error: function(objResponse){						alert(objResponse.responseText);					}  		}											);		}	}  } function popupViewFavorite(cust_fav_desc){	$('#vew_cust_fav_desc').html(cust_fav_desc).trigger('refresh'); 	$('#popupView').popup('open'); }</script>{/literal}