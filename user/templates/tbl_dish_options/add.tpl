<div data-role="popup" id="popupAddDishOption" data-overlay-theme="a" data-theme="c" data-dismissible="false" style="width:300px;" class="ui-corner-all">    <div data-role="header" data-theme="a" class="ui-corner-top">        <h6>{$_lang.tbl_dish_options.create_title}</h6>		<a href="#" data-rel="back" data-role="button" data-icon="delete" data-iconpos="notext" class="ui-btn-right">Close</a>    </div>    <div data-role="content" class="ui-corner-bottom ui-content" style="padding:5px;"> <form name="frmcreatetbl_dish_options" id="frmcreatetbl_dish_options" onsubmit="return validatetbl_dish_options();" method="POST" action="{$page_url}">	<input type="hidden" name="dish_opt_id" id="dish_opt_id" value="0"/>	<div style="display:none;" class="error" id="dish_opt_id_err"></div>	<div class="field-row" style="display:none">		<label for="dish_opt_dish_id">{$_lang.tbl_dish_options.label.dish_opt_dish_id}</label>		<input type="text" name="dish_opt_dish_id" id="dish_opt_dish_id" value="{$smarty.session[$smarty.const.SES_DISH]}"/>		<div class="error" id="dish_opt_dish_id_err"></div>	</div>		<div class="field-row" style="display:none">		<label for="dish_opt_submenu">{$_lang.tbl_dish_options.label.dish_opt_submenu}</label>		<input type="text" name="dish_opt_submenu" id="dish_opt_submenu" value="{$smarty.session[$smarty.const.SES_SUB_MENU]}"/>		<div class="error" id="dish_opt_submenu_err"></div>	</div>	<div class="field-row">		<label for="dish_opt_name">{$_lang.tbl_dish_options.label.dish_opt_name}</label>		<input type="text" name="dish_opt_name" id="dish_opt_name" value="{$smarty.post.dish_opt_name}"/>		<div class="error" id="dish_opt_name_err"></div>	</div>	<div class="field-row">		<label for="dish_opt_type">{$_lang.tbl_dish_options.label.dish_opt_type}</label>		 		<select name="dish_opt_type" id="dish_opt_type">		{assign var="option_caption" value=$_lang.tbl_dish_options.label.select_dish_opt_type} 		{assign var="options" value=$optionTypeList}		{assign var="option_selected" value=$smarty.request.dish_opt_type}		{include file="control/optionlist.tpl"} 		</select>		 		<div class="error" id="dish_opt_type_err"></div>	</div> 	<div class="field-row">		<label for="dish_opt_mandotory">{$_lang.tbl_dish_options.label.dish_opt_mandotory}{html_input type="checkbox" value="1" name="dish_opt_mandotory" id="dish_opt_mandotory"}</label>	</div>			 	<!--	<div class="field-row">		<label for="dish_opt_start_date">{$_lang.tbl_dish_options.label.dish_opt_start_date}</label>		<input type="text" name="dish_opt_start_date" id="dish_opt_start_date" value="{$smarty.post.dish_opt_start_date}"/>		<div class="error" id="dish_opt_start_date_err"></div>	</div>	-->	<!--	<div class="field-row">		<label for="dish_opt_end_date">{$_lang.tbl_dish_options.label.dish_opt_end_date}</label>		<input type="text" name="dish_opt_end_date" id="dish_opt_end_date" value="{$smarty.post.dish_opt_end_date}"/>		<div class="error" id="dish_opt_end_date_err"></div>	</div>	--><center>	<input type="hidden" id="action" name="action" value="{$smarty.const.ACTION_CREATE}"/><input data-inline="true" data-icon="save" type="submit" value="{$_lang.save_lbl}"/> <input data-icon="delete" data-inline="true" type="reset" onclick="$('#popupAddDishOption').popup('close');" value="{$_lang.cancel_lbl}"/></center></form> 	</div></div>