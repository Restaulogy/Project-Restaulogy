{include file='header.tpl'}<div class="wrapper">{if $currDishInfo}	<h1>"{$currDishInfo.dish_name}" - {$_lang.tbl_dish_options.listing_title}</h1>{elseif $currSubmenuInfo}	<h1>"{$currSubmenuInfo.submnu_name}" - {$_lang.tbl_dish_options.listing_title}</h1>{/if}{**include file="dish_navbar.tpl"**}{if $error_msg}	<center>{$error_msg}</center>{/if}   {include file="tbl_dish_options/add.tpl"}   <div class="clearfix"></div><br/> <form name="frm_tbl_dish_options" id="frm_tbl_dish_options" method="POST" action="{$page_url}" >{if $result_found gt 0 && $tbl_dish_optionslist}	<table class="listTable">		<tr><th class="bigListItem" colspan="2" style="width:90%;">{if $currDishInfo}"{$currDishInfo.dish_name}"{elseif $currSubmenuInfo}"{$currSubmenuInfo.submnu_name}"{/if} - {$_lang.tbl_dish_options.title}</th>		<th style="width:5%;">                {$_lang.main.status}		</th>		<th class="actionListItem">Dish Option Values</th>        </tr>		{foreach from=$tbl_dish_optionslist item=tbl_dish_optionsitem} 			<tr>				<td style="width:2%;">						<label for="sel_dish_options[{$tbl_dish_optionsitem.dish_opt_id}]" data-mini="true" style="width:23px;"><input type="checkbox" data-inline='true' data-mini='true' id="sel_dish_options[{$tbl_dish_optionsitem.dish_opt_id}]" name="sel_dish_options[{$tbl_dish_optionsitem.dish_opt_id}]" />&nbsp;</label>     	  </td>				<td class="valueItem"><a href="{$page_url}?mode={$smarty.const.MODE_UPDATE}&dish_opt_id={$tbl_dish_optionsitem.dish_opt_id}">{$tbl_dish_optionsitem.dish_opt_name}</a><br /><small>{$tbl_dish_optionsitem.values|replace:";":", "}</small></td>				<td class="actionListItem">                 {if $tbl_dish_optionsitem.isActive eq 1}                    <a href="#" class="activeIcon" title="{$_lang.tbl_dish_options.ACTIVATE.BTN_LBL}"></a>                {else}                    <a href="#" class="deactiveIcon" title="{$_lang.tbl_dish_options.DEACTIVATE.BTN_LBL}"></a>                {/if}                </td>                <td>				<a href="{$website}/user/tbl_dish_options_values.php?dish_opt_val_option_id={$tbl_dish_optionsitem.dish_opt_id}" target="_blank" class="forwardIcon" title="{$_lang.tbl_dish_options.FORWARD.BTN_LBL}"></a>								</td>				<!--                <td class="actionListItem">				<a href="{$page_url}?mode={$smarty.const.MODE_UPDATE}&dish_opt_id={$tbl_dish_optionsitem.dish_opt_id}" class="editIcon"></a>                <a class="deleteIcon" href="#" onclick="deletetbl_dish_options({$tbl_dish_optionsitem.dish_opt_id});"></a>                {if $tbl_dish_optionsitem.isActive eq 1}                    <a href="{$page_url}?action={$smarty.const.ACTION_DEACTIVATE}&dish_opt_id={$tbl_dish_optionsitem.dish_opt_id}" class="deactiveIcon" title="{$_lang.tbl_dish_options.DEACTIVATE.BTN_LBL}"></a>                {else}                    <a href="{$page_url}?action={$smarty.const.ACTION_ACTIVATE}&dish_opt_id={$tbl_dish_optionsitem.dish_opt_id}" class="activeIcon" title="{$_lang.tbl_dish_options.ACTIVATE.BTN_LBL}"></a>                {/if}				<a href="{$website}/user/tbl_dish_options_values.php?dish_opt_val_option_id={$tbl_dish_optionsitem.dish_opt_id}" class="forwardIcon" title="{$_lang.tbl_dish_options.FORWARD.BTN_LBL}"></a>								</td>                -->			</tr>	{/foreach}	</table>{else}	<div class="error">{$_lang.tbl_dish_options.no_record_found}</div>{/if}{if $pagination}	<div class="biz_center">{$pagination}</div>{/if}<input type="hidden" id="action" name="action" value=""/></form><div class="biz_center"> {if $result_found gt 0 && $tbl_dish_optionslist}{jqmbutton type="button" icon="briefcase" value="{$_lang.main.toggle}" onclick="javascript:$('input[type=checkbox]').click();"}{jqmbutton icon="inactive" value="{$_lang.tbl_dish_options.DEACTIVATE.BTN_LBL}" onclick="actiontbl_dish_options('{$smarty.const.ACTION_DEACTIVATE}');"} {jqmbutton icon="active" value="{$_lang.tbl_dish_options.ACTIVATE.BTN_LBL}" onclick="actiontbl_dish_options('{$smarty.const.ACTION_ACTIVATE}');"} {/if}{*jqmbutton type="delete" onclick="actiontbl_dish_options('{$smarty.const.ACTION_DELETE}');"*}  {jqmbutton type="button" icon="add-item" value="{$_lang.tbl_dish_options.CREATE.BTN_LBL}" onclick="$('#popupAddDishOption').popup('open');"}   </div><!--<input onclick="window.location.href='{$page_url}?mode={$smarty.const.MODE_CREATE}'" type="button" value="{$_lang.tbl_dish_options.CREATE.BTN_LBL}"/>--></div>{include file="tbl_dish_options/js.tpl"}{include file="footer.tpl"}