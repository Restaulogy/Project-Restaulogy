{include file="header.tpl"}<div class="wrapper"><h1>{$_lang.tbl_food_pairing.create_title} for {$food_pair_main_dish_detail.dish_name}</h1>{if $error_msg}	<center>{$error_msg}</center>{/if}<form name="frmcreatetbl_food_pairing" id="frmcreatetbl_food_pairing" onsubmit="return validatetbl_food_pairing();" method="POST" action="{$page_url}">	<input type="hidden" name="Array" id="Array" value="0"/>	<div style="display:none;" class="error" id="Array_err"></div>	<div class="biz_hidden">		<label for="food_pair_main_dish">{$_lang.tbl_food_pairing.label.food_pair_main_dish}</label>		<input id="food_pair_main_dish" type="text" value="{$food_pair_main_dish}" name="food_pair_main_dish"/>		<div class="error" id="food_pair_main_dish_err"></div>	</div>	<div class="field-row">		<label for="food_pair_paired_dish">{$_lang.tbl_food_pairing.label.food_pair_paired_dish}</label>		<select id="food_pair_paired_dish" name="food_pair_paired_dish[]" multiple="multiple" data-native-menu="false">         <option value="" data-placeholder="true">Select dishes </option>		 {foreach $lst_dishes as $_eachdish}		 	<option value="{$_eachdish@key}" {if $smarty.request.food_pair_paired_dish eq $_eachdish@key}selected="selected"{/if}>{$_eachdish}</option>		 {/foreach}		 </select>		<div class="error" id="food_pair_paired_dish_err"></div>	</div>	<!--	<div class="field-row">		<label for="food_pair_start_date">{$_lang.tbl_food_pairing.label.food_pair_start_date}</label>		<input  id="food_pair_start_date" type="text" value="{$smarty.post.food_pair_start_date}" name="food_pair_start_date"/> 		<div class="error" id="food_pair_start_date_err"></div>	</div>	-->	<!--	<div class="field-row">		<label for="food_pair_end_date">{$_lang.tbl_food_pairing.label.food_pair_end_date}</label>		<input  id="food_pair_end_date" type="text" value="{$smarty.post.food_pair_end_date}" name="food_pair_end_date"/> 		<div class="error" id="food_pair_end_date_err"></div>	</div>	-->	<div class="biz_center">    <input type="hidden" id="action" name="action" value="{$smarty.const.ACTION_CREATE}"/>    <input data-inline="true" data-icon="save" type="submit" value="{$_lang.save_lbl}"/>    <input data-inline="true" data-icon="delete" type="reset" value="{$_lang.cancel_lbl}" onclick="window.location.href='{$page_url}'"/>    </div></form>{include file="tbl_food_pairing/js.tpl"}</div>{include file="footer.tpl"}