{include file="header.tpl"}<div class="wrapper"><h1>{$_lang.tbl_food_pairing.title}</h1>{if $error_msg}	<center>{$error_msg}</center>{/if}<form name="frmupdatetbl_food_pairing" id="frmupdatetbl_food_pairing" onsubmit="return validatetbl_food_pairing();" method="POST" action="{$page_url}" style="{if $isUpdate eq 1}{else}display:none;{/if}">	<input type="hidden" name="food_pair_id" id="food_pair_id" value="{$tbl_food_pairinginfo.food_pair_id}"/>	<div style="display:none;" class="error" id="food_pair_id_err"></div>	<div class="biz_hidden">		<label for="food_pair_main_dish">{$_lang.tbl_food_pairing.label.food_pair_main_dish}</label>		<input type="text" name="food_pair_main_dish" id="food_pair_main_dish" value="{$food_pair_main_dish}"/>		<div class="error" id="food_pair_main_dish_err"></div>	</div>	<div class="field-row">		<label for="food_pair_paired_dish">{$_lang.tbl_food_pairing.label.food_pair_paired_dish}</label>		<select id="food_pair_paired_dish" name="food_pair_paired_dish[]" multiple="multiple" data-native-menu="false">         <option value="" data-placeholder="true">Select dishes </option>		 {foreach $lst_dishes as $_eachdish}		 	<option value="{$_eachdish@key}" {if $tbl_food_pairinginfo.food_pair_paired_dish &&  in_array($_eachdish@key,$tbl_food_pairinginfo.food_pair_paired_dish)}selected="selected"{/if}>{$_eachdish}</option>		 {/foreach}		 </select>		<div class="error" id="food_pair_paired_dish_err"></div>	</div>	<!--	<div class="field-row">		<label for="food_pair_start_date">{$_lang.tbl_food_pairing.label.food_pair_start_date}</label>		<input type="text" name="food_pair_start_date" id="food_pair_start_date" value="{$tbl_food_pairinginfo.food_pair_start_date}"/>		<div class="error" id="food_pair_start_date_err"></div>	</div>	-->	<!--	<div class="field-row">		<label for="food_pair_end_date">{$_lang.tbl_food_pairing.label.food_pair_end_date}</label>		<input type="text" name="food_pair_end_date" id="food_pair_end_date" value="{$tbl_food_pairinginfo.food_pair_end_date}"/>		<div class="error" id="food_pair_end_date_err"></div>	</div>	-->	<div class="biz_center">    <input type="hidden" id="action" name="action" value="{$smarty.const.ACTION_UPDATE}"/>    <input  data-inline="true" data-icon="save"  type="submit" value="{$_lang.save_lbl}"/>    <input type="button" value="{$_lang.cancel_lbl}" data-inline="true" data-icon="delete" onclick="window.location.href='{$page_url}'"/>    </div>    </form><div id="tbl_food_pairing_view" class="description" style="{if $isUpdate eq 1}display:none;{/if}">	<table class="listTable">		<tr><th class="fieldItem">{$_lang.field_title}</th><th class="valueItem">{$_lang.value_title}</th></tr>		<tr><td class="fieldItem">{$_lang.tbl_food_pairing.label.food_pair_id}<i>:</i></td><td class="valueItem">{$tbl_food_pairinginfo.food_pair_id}</td></tr>		<tr><td class="fieldItem">{$_lang.tbl_food_pairing.label.food_pair_main_dish}<i>:</i></td><td class="valueItem">{$tbl_food_pairinginfo.food_pair_main_dish}</td></tr>		<tr><td class="fieldItem">{$_lang.tbl_food_pairing.label.food_pair_paired_dish}<i>:</i></td><td class="valueItem">{$tbl_food_pairinginfo.food_pair_paired_dish}</td></tr>	<!--		<tr><td class="fieldItem">{$_lang.tbl_food_pairing.label.food_pair_start_date}<i>:</i></td><td class="valueItem">{$tbl_food_pairinginfo.food_pair_start_date}</td></tr>	-->	<!--		<tr><td class="fieldItem">{$_lang.tbl_food_pairing.label.food_pair_end_date}<i>:</i></td><td class="valueItem">{$tbl_food_pairinginfo.food_pair_end_date}</td></tr>	-->	<tr><td class="fieldItem">{$_lang.tbl_food_pairing.label.isActive}<i>:</i></td><td class="valueItem">{if $tbl_food_pairinginfo.isActive eq 1}{$_lang.tbl_food_pairing.label.isActive_yes}{else}{$_lang.tbl_food_pairing.label.isActive_no}{/if}</td></tr>	</table>	<center><input data-inline="true" data-icon="edit" type="button" value="{$_lang.tbl_food_pairing.UPDATE.BTN_LBL}" onclick="$('#tbl_food_pairing_view').hide();$('#frmupdatetbl_food_pairing').show();"/><input type="button" value="{$_lang.cancel_lbl}" data-inline="true" data-icon="delete" onclick="window.location.href='{$page_url}'"/></center></div>{include file="tbl_food_pairing/js.tpl"}</div>{include file="footer.tpl"}