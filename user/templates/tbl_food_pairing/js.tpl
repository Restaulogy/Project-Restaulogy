{literal}<script type="text/javascript">function actiontbl_food_pairing(action){	var varId = $("input:checked").length;	var isValid = 0;	if(varId > 0){	if(action != ""){		switch (action) {			case '{/literal}{$smarty.const.ACTION_DELETE}{literal}':			if(confirm("{/literal}{$_lang.tbl_food_pairing.DELETE.CONFIRM_MSG}{literal}")==true){				isValid = 1;			}			break;			case '{/literal}{$smarty.const.ACTION_DEACTIVATE}{literal}':			isValid = 1;			break;			case '{/literal}{$smarty.const.ACTION_ACTIVATE}{literal}':			isValid = 1;			break;		}		if(isValid){		  $('#action').val(action);		  $('#frm_tbl_food_pairing').submit();		}	}	}else{			alert("{/literal}{$_lang.main.select_ids.empty}{literal}"); 	}}function deletetbl_food_pairing(varId){	if(varId > 0){		if(confirm("{/literal}{$_lang.tbl_food_pairing.DELETE.CONFIRM_MSG}{literal}")==true){			window.location.href="{/literal}{$page_url}{literal}?action=delete&food_pair_id="+varId;		}	}}function validatetbl_food_pairing(){	$("#food_pair_id_err").html("");	$("#food_pair_main_dish_err").html("");	$("#food_pair_paired_dish_err").html("");	$("#food_pair_start_date_err").html("");	$("#food_pair_end_date_err").html("");	var isErr = true;	if(elemById("action").value=="update"){		if(IsNonEmpty(elemById("food_pair_id").value)==false){			$("#food_pair_id_err").html("{/literal}{$_lang.tbl_food_pairing.not_empty_msg.food_pair_id}{literal}");			isErr = false;		}	}	if(IsNonEmpty(elemById("food_pair_main_dish").value)==false){		$("#food_pair_main_dish_err").html("{/literal}{$_lang.tbl_food_pairing.not_empty_msg.food_pair_main_dish}{literal}");		isErr = false;	}	if(IsNonEmpty(elemById("food_pair_paired_dish").value)==false){		$("#food_pair_paired_dish_err").html("{/literal}{$_lang.tbl_food_pairing.not_empty_msg.food_pair_paired_dish}{literal}");		isErr = false;	}/*	if(IsNonEmpty(elemById("food_pair_start_date").value)==false){		$("#food_pair_start_date_err").html("{/literal}{$_lang.tbl_food_pairing.not_empty_msg.food_pair_start_date}{literal}");		isErr = false;	}*//*	if(IsNonEmpty(elemById("food_pair_end_date").value)==false){		$("#food_pair_end_date_err").html("{/literal}{$_lang.tbl_food_pairing.not_empty_msg.food_pair_end_date}{literal}");		isErr = false;	}*/	if(isErr == false){		alert("{/literal}{$_lang.messages.revise_form}{literal}");	}	return isErr;}//..function</script>{/literal}