{include file="header.tpl"}<div class="wrapper"><h1>{$_lang.tbl_restaurent.create_title}</h1>{if $error_msg}	<center>{$error_msg}</center>{/if}<form name="frmcreatetbl_restaurent" id="frmcreatetbl_restaurent" onsubmit="return validatetbl_restaurent();" enctype="multipart/form-data" method="POST" action="{$page_url}">	<input type="hidden" name="restaurent_id" id="restaurent_id" value="0"/>	<div style="display:none;" class="error" id="restaurent_id_err"></div>	<div class="field-row" style="display:none;">		<label for="restaurent_owner">{$_lang.tbl_restaurent.label.restaurent_owner}</label>		<input type="text" name="restaurent_owner" id="restaurent_owner" value="{$smarty.post.restaurent_owner}"/>		<div class="error" id="restaurent_owner_err"></div>	</div>	<div class="field-row">		<label for="restaurent_name">{$_lang.tbl_restaurent.label.restaurent_name}</label>		<input type="text" name="restaurent_name" id="restaurent_name" value="{$smarty.post.restaurent_name}"/>		<div class="error" id="restaurent_name_err"></div>	</div>	<div class="field-row">		<label for="restaurent_address">{$_lang.tbl_restaurent.label.restaurent_address}</label>		<textarea name="restaurent_address" id="restaurent_address">{$smarty.post.restaurent_address}</textarea>		<div class="error" id="restaurent_address_err"></div>	</div>	<div class="field-row" style="display:none;">		<label for="restaurent_country">{$_lang.tbl_restaurent.label.restaurent_country}</label>		<input type="text" name="restaurent_country" id="restaurent_country" value="{if $smarty.request.restaurent_country}{$smarty.request.restaurent_country}{else}US{/if}"/>		<div class="error" id="restaurent_country_err"></div>	</div>	<div class="field-row">		<label for="restaurent_state">{$_lang.tbl_restaurent.label.restaurent_state}</label>		<select id="restaurent_state" name="restaurent_state" onchange="change_city_by_state('restaurent_state','restaurent_city',{if $smarty.request.restaurent_city}{$smarty.request.restaurent_city}{else}0{/if});">			<option value="">{$_lang.select_states}</option>			{if $state_list}			{foreach $state_list as $state}				<option value="{$state@key}" {if $tbl_restaurentinfo.restaurent_state eq $state@key}selected="selected"{/if}>{$state}</option>			{/foreach}			{/if}		</select>		 		<div class="error" id="restaurent_state_err"></div>	</div>	<div class="field-row">		<label for="restaurent_city">{$_lang.tbl_restaurent.label.restaurent_city}</label>		<select id="restaurent_city" name="restaurent_city">			<option value="">{$_lang.select_states}</option>			{if $city_list}			{foreach $city_list as $city}				<option value="{$city@key}" {if $smarty.request.restaurent_city eq $city@key}selected="selected"{/if}>{$city}</option>			{/foreach}			{/if}		</select>		<div class="error" id="restaurent_city_err"></div>	</div>	<div class="field-row">		<label for="restaurent_zip">{$_lang.tbl_restaurent.label.restaurent_zip}</label>		<input type="text" name="restaurent_zip" id="restaurent_zip" value="{$smarty.post.restaurent_zip}"/>		<div class="error" id="restaurent_zip_err"></div>	</div>	<div class="field-row">		<label for="restaurent_phone_1">{$_lang.tbl_restaurent.label.restaurent_phone_1}</label>		<input type="text" name="restaurent_phone_1" id="restaurent_phone_1" value="{$smarty.post.restaurent_phone_1}"/>		<div class="error" id="restaurent_phone_1_err"></div>	</div>	<div class="field-row">		<label for="restaurent_phone_2">{$_lang.tbl_restaurent.label.restaurent_phone_2}</label>		<input type="text" name="restaurent_phone_2" id="restaurent_phone_2" value="{$smarty.post.restaurent_phone_2}"/>		<div class="error" id="restaurent_phone_2_err"></div>	</div>		<div class="field-row">		<label for="restaurent_email">{$_lang.tbl_restaurent.label.restaurent_email}</label>		<input type="text" name="restaurent_email" id="restaurent_email" value="{$smarty.request.restaurent_email}"/>		<div class="error" id="restaurent_email_err"></div>	</div>		<div class="field-row">		<label for="restaurent_website">{$_lang.tbl_restaurent.label.restaurent_website}</label>		<input type="text" name="restaurent_website" id="restaurent_website" value="{$smarty.request.restaurent_website}"/>		<div class="error" id="restaurent_website_err"></div>	</div>		<div class="field-row">		<label for="restaurent_description">{$_lang.tbl_restaurent.label.restaurent_description}</label>		<textarea type="text" name="restaurent_description" id="restaurent_description" >{$smarty.request.restaurent_description}</textarea>		<div class="error" id="restaurent_description_err"></div>	</div>		<div class="field-row">		<label for="restaurent_img">{$_lang.tbl_restaurent.label.restaurent_img}</label>		<input type="file" name="restaurent_img" id="restaurent_img" /> 			<div class="error" id="restaurent_img_err"></div>	</div>	<div class="field-row">		<label for="restaurent_is_paypal">		<input type="checkbox" name="restaurent_is_paypal" id="restaurent_is_paypal" value="{if $smarty.request.restaurent_is_paypal neq ''}{$smarty.request.restaurent_is_paypal}{else}1{/if}"/>{$_lang.tbl_restaurent.label.restaurent_is_paypal}</label>		<div class="error" id="restaurent_is_paypal_err"></div>	</div>	 	<div class="field-row" id="paypal_info" style="display:none;">		<label for="restaurent_paypal_email">{$_lang.tbl_restaurent.label.restaurent_paypal_email}</label>		<input type="text" name="restaurent_paypal_email" id="restaurent_paypal_email" value="{$smarty.post.restaurent_paypal_email}"/>		<div class="error" id="restaurent_paypal_email_err"></div>	</div>	  <table style="width:100%">		<tr>			<td><label for="restaurent_online_ord_start">{$_lang.tbl_restaurent.label.restaurent_online_ord_start}</label></td>			<td>&nbsp;</td>			<td><label for="restaurent_online_ord_end">{$_lang.tbl_restaurent.label.restaurent_online_ord_end}</label></td>		</tr>		<tr>			<td><input type="text" readonly="readonly" name="restaurent_online_ord_start" id="restaurent_online_ord_start" value="{$smarty.post.restaurent_online_ord_start}"/></td>			<td>&nbsp;</td>			<td><input type="text" readonly="readonly" name="restaurent_online_ord_end" id="restaurent_online_ord_end" value="{$smarty.post.restaurent_online_ord_end}"/></td>		</tr>		<tr>			<td><div class="error" id="restaurent_online_ord_start_err"></div></td>			<td>&nbsp;</td>			<td><div class="error" id="restaurent_online_ord_end_err"></div></td>		</tr>		<!--		<tr>			<td><label for="restaurent_lat">{$_lang.tbl_restaurent.label.restaurent_lat}</label></td>			<td>&nbsp;</td>			<td><label for="restaurent_lng">{$_lang.tbl_restaurent.label.restaurent_lng}</label></td>		</tr>		<tr>			<td><input type="text" readonly="readonly" name="restaurent_lat" id="restaurent_lat" value="{$smarty.post.restaurent_lat}"/></td>			<td>&nbsp;</td>			<td><input type="text" readonly="readonly" name="restaurent_lng" id="restaurent_lng" value="{$smarty.post.restaurent_lng}"/></td>		</tr>		<tr>			<td><div class="error" id="restaurent_lat_err"></div></td>			<td>&nbsp;</td>			<td><div class="error" id="restaurent_lng_err"></div></td>		</tr>-->	</table>	<!--	<div class="field-row">		<label for="restaurent_start_date">{$_lang.tbl_restaurent.label.restaurent_start_date}</label>		<input type="text" name="restaurent_start_date" id="restaurent_start_date" value="{$smarty.post.restaurent_start_date}"/>		<div class="error" id="restaurent_start_date_err"></div>	</div>	-->	<!--	<div class="field-row">		<label for="restaurent_end_date">{$_lang.tbl_restaurent.label.restaurent_end_date}</label>		<input type="text" name="restaurent_end_date" id="restaurent_end_date" value="{$smarty.post.restaurent_end_date}"/>		<div class="error" id="restaurent_end_date_err"></div>	</div>	--><center>	<input type="hidden" id="action" name="action" value="{$smarty.const.ACTION_CREATE}"/><input data-inline="true" data-icon="save" type="submit" value="{$_lang.save_lbl}"/> <input data-inline="true" data-icon="delete" type="reset" value="{$_lang.cancel_lbl}" onclick="window.location.href='{$page_url}'"/></center></form></div>{include file="footercontent.tpl"}{include file="tbl_restaurent/js.tpl"}{literal}    <script>        $(function(){        	$("#restaurent_online_ord_start").scroller({ preset: 'time', timeFormat: 'HH:ii:ss', timeWheels: 'HHii', animate: 'pop'});        	$("#restaurent_online_ord_end").scroller({ preset: 'time', timeFormat: 'HH:ii:ss', timeWheels: 'HHii', animate: 'pop'});        })    </script>{/literal}</body></html>