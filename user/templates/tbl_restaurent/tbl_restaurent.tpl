{include file='header.tpl'}<div class="wrapper"><h1>{$_lang.tbl_restaurent.listing_title}</h1>{if $error_msg}	<center>{$error_msg}</center>{/if}{if $result_found gt 0 && $tbl_restaurentlist}<table class="listTable">	<tr>		<th class="bigListItem">{$_lang.tbl_restaurent.title}</th>		<!-- <th class="actionListItem"></th> -->	</tr>	{foreach from=$tbl_restaurentlist item=tbl_restaurentitem}	 {if $Global_member.member_role_id eq $smarty.const.ROLE_DEV OR $smarty.session[$smarty.const.SES_RESTAURANT] eq $tbl_restaurentitem.restaurent_id}		<tr>			<td class="bigListItem"><a href="{$page_url}?mode={$smarty.const.MODE_UPDATE}&restaurent_id={$tbl_restaurentitem.restaurent_id}">{$tbl_restaurentitem.restaurent_name}</a></td>			<!--			<td class="actionListItem">            <a href="{$page_url}?mode={$smarty.const.MODE_UPDATE}&restaurent_id={$tbl_restaurentitem.restaurent_id}" class="editIcon"></a><a class="deleteIcon" href="#" onclick="deletetbl_restaurent({$tbl_restaurentitem.restaurent_id});"></a>{if $tbl_restaurentitem.isActive eq 1}<a href="{$page_url}?action={$smarty.const.ACTION_DEACTIVATE}&restaurent_id={$tbl_restaurentitem.restaurent_id}" class="deactiveIcon" title="{$_lang.tbl_restaurent.DEACTIVATE.BTN_LBL}"></a>{else}<a href="{$page_url}?action={$smarty.const.ACTION_ACTIVATE}&restaurent_id={$tbl_restaurentitem.restaurent_id}" class="activeIcon" title="{$_lang.tbl_restaurent.ACTIVATE.BTN_LBL}"></a>{/if}            </td>            -->		</tr>	 {/if}{/foreach}</table>{else}	<div class="error">{$_lang.tbl_restaurent.no_record_found}</div>{/if}{if $pagination}	<center>{$pagination}</center>{/if}{if $Global_member.member_role_id eq $smarty.const.ROLE_DEV }<!--<input data-role="button" onclick="window.location.href='{$page_url}?mode={$smarty.const.MODE_CREATE}'" type="button" value="{$_lang.tbl_restaurent.CREATE.BTN_LBL}"/>-->{jqmbutton icon='add-item' value={$_lang.tbl_restaurent.CREATE.BTN_LBL} onclick="window.location.href='{$page_url}?mode={$smarty.const.MODE_CREATE}'"}{/if}</div>{include file="tbl_restaurent/js.tpl"}{include file="footer.tpl"}