{include file='header.tpl'}<div class="wrapper"><h1>{$_lang.tbl_restaurent.listing_title}</h1>{if $error_msg}	<center>{$error_msg}</center>{/if}{if $result_found gt 0 && $tbl_restaurentlist}	<table class="biz_data_grid">		<tr>			<th class="{if $smarty.get.sort_on eq 'restaurent_owner'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=restaurent_owner&sort_by={$new_sort}">{$_lang.tbl_restaurent.label.restaurent_owner}</a></th>			<th class="{if $smarty.get.sort_on eq 'restaurent_name'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=restaurent_name&sort_by={$new_sort}">{$_lang.tbl_restaurent.label.restaurent_name}</a></th>			<th class="{if $smarty.get.sort_on eq 'restaurent_address'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=restaurent_address&sort_by={$new_sort}">{$_lang.tbl_restaurent.label.restaurent_address}</a></th>			<th class="{if $smarty.get.sort_on eq 'restaurent_country'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=restaurent_country&sort_by={$new_sort}">{$_lang.tbl_restaurent.label.restaurent_country}</a></th>			<th class="{if $smarty.get.sort_on eq 'restaurent_state'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=restaurent_state&sort_by={$new_sort}">{$_lang.tbl_restaurent.label.restaurent_state}</a></th>			<th class="{if $smarty.get.sort_on eq 'restaurent_city'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=restaurent_city&sort_by={$new_sort}">{$_lang.tbl_restaurent.label.restaurent_city}</a></th>			<th class="{if $smarty.get.sort_on eq 'restaurent_zip'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=restaurent_zip&sort_by={$new_sort}">{$_lang.tbl_restaurent.label.restaurent_zip}</a></th>			<th class="{if $smarty.get.sort_on eq 'restaurent_phone_1'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=restaurent_phone_1&sort_by={$new_sort}">{$_lang.tbl_restaurent.label.restaurent_phone_1}</a></th>			<th class="{if $smarty.get.sort_on eq 'restaurent_phone_2'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=restaurent_phone_2&sort_by={$new_sort}">{$_lang.tbl_restaurent.label.restaurent_phone_2}</a></th>			<th class="{if $smarty.get.sort_on eq 'restaurent_is_paypal'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=restaurent_is_paypal&sort_by={$new_sort}">{$_lang.tbl_restaurent.label.restaurent_is_paypal}</a></th>			<th class="{if $smarty.get.sort_on eq 'restaurent_paypal_email'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=restaurent_paypal_email&sort_by={$new_sort}">{$_lang.tbl_restaurent.label.restaurent_paypal_email}</a></th>			<th class="action_header">Action</th>		</tr>		{foreach from=$tbl_restaurentlist item=tbl_restaurentitem}		<tr class="{cycle values="odd,even"}">			<td><a href="{$page_url}?mode={$smarty.const.MODE_VIEW}&restaurent_id={$tbl_restaurentitem.restaurent_id}">{$tbl_restaurentitem.restaurent_owner}</a></td>			<td>{$tbl_restaurentitem.restaurent_name}</td>			<td>{$tbl_restaurentitem.restaurent_address}</td>			<td>{$tbl_restaurentitem.restaurent_country}</td>			<td>{$tbl_restaurentitem.restaurent_state}</td>			<td>{$tbl_restaurentitem.restaurent_city}</td>			<td>{$tbl_restaurentitem.restaurent_zip}</td>			<td>{$tbl_restaurentitem.restaurent_phone_1}</td>			<td>{$tbl_restaurentitem.restaurent_phone_2}</td>			<td>{$tbl_restaurentitem.restaurent_is_paypal}</td>			<td>{$tbl_restaurentitem.restaurent_paypal_email}</td>			<td><a href="{$page_url}?mode={$smarty.const.MODE_UPDATE}&restaurent_id={$tbl_restaurentitem.restaurent_id}" class="editIcon"></a><a class="deleteIcon" href="#" onclick="deletetbl_restaurent({$tbl_restaurentitem.restaurent_id});"></a>{if $tbl_restaurentitem.isActive eq 1}<a href="{$page_url}?action={$smarty.const.ACTION_DEACTIVATE}&restaurent_id={$tbl_restaurentitem.restaurent_id}" class="deactiveIcon" title="{$_lang.tbl_restaurent.DEACTIVATE.BTN_LBL}"></a>{else}<a href="{$page_url}?action={$smarty.const.ACTION_ACTIVATE}&restaurent_id={$tbl_restaurentitem.restaurent_id}" class="activeIcon" title="{$_lang.tbl_restaurent.ACTIVATE.BTN_LBL}"></a>{/if}</td>		</tr>	{/foreach}		<tfoot>			<tr>				<td colspan="14">					# {$result_found}&nbsp;&nbsp;&nbsp;{if $pagination neq ""}{$pagination}{/if}					<select onchange="changePage('{$navigationURL}',this.value,{$smarty.request.limit});">					{if $allPageCount gt 1}						{for $foo=1 to $allPageCount}							<option value="{$foo}" {if $foo eq $currentPage}selected="selected"{/if}>{$foo}</option>						{/for}					{else}						<option value="1" disabled="disabled">1</option>					{/if}					</select>				</td>			</tr>		</tfoot>	</table>{else}	<div class="error">{$_lang.tbl_restaurent.no_record_found}</div>{/if}<input onclick="window.location.href='{$page_url}?mode={$smarty.const.MODE_CREATE}'" type="button" value="{$_lang.tbl_restaurent.CREATE.BTN_LBL}"/></div>{include file="tbl_restaurent/js.tpl"}{include file="footer.tpl"}