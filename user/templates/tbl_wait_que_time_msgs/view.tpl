{include file="header.tpl"}<div class="wrapper"><h1>"{$table_type_info.tbl_type_name} ({$est_time_grinfo.wt_est_time_party_size})" - {$_lang.tbl_wait_que_time_msgs.title}</h1>{if $error_msg}	<center>{$error_msg}</center>{/if}<form name="frmupdatetbl_wait_que_time_msgs" id="frmupdatetbl_wait_que_time_msgs" onsubmit="return validatetbl_wait_que_time_msgs();" method="POST" action="{$page_url}" style="{if $isUpdate eq 1}{else}display:none;{/if}">	<input type="hidden" name="wtq_time_msg_id" id="wtq_time_msg_id" value="{$tbl_wait_que_time_msgsinfo.wtq_time_msg_id}"/>	<div style="display:none;" class="error" id="wtq_time_msg_id_err"></div> 	<div class="field-row">		<label for="wtq_time_msg_text">{$_lang.tbl_wait_que_time_msgs.label.wtq_time_msg_text}</label>		<textarea name="wtq_time_msg_text" id="wtq_time_msg_text">{$tbl_wait_que_time_msgsinfo.wtq_time_msg_text}</textarea> 		<div class="error" id="wtq_time_msg_text_err"></div>	</div>	<div class="field-row">		<label for="wtq_time_msg_est_time">{$_lang.tbl_wait_que_time_msgs.label.wtq_time_msg_est_time}</label>		<select name="wtq_time_msg_est_time" id="wtq_time_msg_est_time" onchange="change_limit_by_group(this.value);">  		<option value="">Select Estimation Time Group</option>		{foreach $est_time_group as $group}  			<option value="{$group.wt_est_time_id}" {if $tbl_wait_que_time_msgsinfo.wtq_time_msg_est_time eq $group.wt_est_time_id}selected="selected"{/if}>{$group.tbl_type_name} ({$group.wt_est_time_party_size} Members)		{/foreach}		</select> 		<div class="error" id="wtq_time_msg_est_time_err"></div>	</div>		<div class="field-row">		<label for="wtq_time_msg_time">{$_lang.tbl_wait_que_time_msgs.label.wtq_time_msg_time}</label>		<select name="wtq_time_msg_time" id="wtq_time_msg_time">		<option value="">Select Time</option>		{for $i=5 to $tbl_wait_que_time_msgsinfo.est_time_group.wt_est_time_value step=5} 		<option value="{$i}" {if $tbl_wait_que_time_msgsinfo.wtq_time_msg_time eq $i}select