{include file="header.tpl"}<div class="wrapper"><h1>"{$table_type_info.tbl_type_name} ({$est_time_grinfo.wt_est_time_party_size})" - {$_lang.tbl_wait_que_time_msgs.create_title}</h1>{if $error_msg}	<center>{$error_msg}</center>{/if}<form name="frmcreatetbl_wait_que_time_msgs" id="frmcreatetbl_wait_que_time_msgs" onsubmit="return validatetbl_wait_que_time_msgs();" method="POST" action="{$page_url}">	<input type="hidden" name="wtq_time_msg_id" id="wtq_time_msg_id" value="0"/>	<div style="display:none;" class="error" id="wtq_time_msg_id_err"></div>		<div class="field-row">		<label for="wtq_time_msg_text">{$_lang.tbl_wait_que_time_msgs.label.wtq_time_msg_text}</label>		<textarea name="wtq_time_msg_text" id="wtq_time_msg_text">{$smarty.post.wtq_time_msg_text}</textarea> 		<div class="error" id="wtq_time_msg_text_err"></div>	</div>	<div class="field-row">		<label for="wtq_time_msg_est_time">{$_lang.tbl_wait_que_time_msgs.label.wtq_time_msg_est_time}</label>				{if $smarty.session[$smarty.const.SES_EST_TIME_GR] gt 0}		<input type="hidden" name="wtq_time_msg_est_time" id="wtq_time_msg_est_time" value="{$smarty.session[$smarty.const.SES_EST_TIME_GR]}"/> 		{$table_type_info.tbl_type_name} ({$est_time_grinfo.wt_est_time_party_size} Members)		{else}		<select name="wtq_time_msg_est_time" id="wtq_time_msg_est_time"  onchange="change_limit_by_group(this.value);">  		<option value="">Select Estimation Time Group</option>		{foreach $est_time_group as $group}			<option value="{$group.wt_est_time_id}" {if $smarty.post.wtq_time_msg_est_time eq $group.wt_est_time_id}selected="selected"{/if}>{$group.tbl_type_name} ({$group.wt_est_time_party_size} Members)</option>		{/foreach}		</select>		{/if} 		<div class="error" id="wtq_time_msg_est_time_err"></div>	</div>			<div class="field-row">		<label for="wtq_time_msg_time">{$_lang.tbl_wait_que_time_msgs.label.wtq_time_msg_time}</label> 		 <select name="wtq_time_msg_time" id="wtq_time_msg_time">		 	<option value="">Select Time</option>			{for $i=5 to $est_time_grinfo.wt_est_time_value step=5} 			<option value="{$i}">{$i}</option> 			{/for}		 </select>		<div class="error" id="wtq_time_msg_time_err"></div>	</div>		<!--	<div class="field-row">		<label for="wtq_time_start_date">{$_lang.tbl_wait_que_time_msgs.label.wtq_time_start_date}</label>		<input type="text" name="wtq_time_start_date" id="wtq_time_start_date" value="{$smarty.post.wtq_time_start_date}"/>		<div class="error" id="wtq_time_start_date_err"></div>	</div>	-->	<!--	<div class="field-row">		<label for="wtq_time_end_date">{$_lang.tbl_wait_que_time_msgs.label.wtq_time_end_date}</label>		<input type="text" name="wtq_time_end_date" id="wtq_time_end_date" value="{$smarty.post.wtq_time_end_date}"/>		<div class="error" id="wtq_time_end_date_err"></div>	</div>	-->	<div class="biz_center">					<input type="hidden" id="action" name="action" value="{$smarty.const.ACTION_CREATE}"/><input data-inline="true" data-icon="save" type="submit" value="{$_lang.save_lbl}"/> 			<input data-inline="true" data-icon="delete" type="reset" value="{$_lang.cancel_lbl}" onclick="window.location.href='{$page_url}'"/>			</div></form></div>{include file="footercontent.tpl"}{include file="tbl_wait_que_time_msgs/js.tpl"}</body></html>