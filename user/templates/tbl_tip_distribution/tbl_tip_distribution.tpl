{include file='header.tpl'}<div class="wrapper"><h1>{$_lang.tbl_tip_distribution.listing_title}</h1>{if $error_msg}	<div class="biz_center">{$error_msg}</div>{/if}{include file="tbl_tip/tabs.tpl"} {if $result_found gt 0 && $tbl_tip_distributionlist}<form name="frm_tbl_tip_distribution" id="frm_tbl_tip_distribution"  method="POST" action="{$page_url}" >	<table class="listTable">		<tr>				<th style="width:2%;"></th>				<th style="width:98%;">{$_lang.tbl_tip_distribution.title}</th>                <th class="actionListItem">Paid</th>		</tr>		{foreach from=$tbl_tip_distributionlist item=tbl_tip_distributionitem}			<tr>				<td style="width:2%;">					<label for="sel_tip_dis{$tbl_tip_distributionitem.tip_dis_id}" data-mini="true" style="width:23px;" ><input type="checkbox" data-inline='true' data-mini='true' id="sel_tip_dis{$tbl_tip_distributionitem.tip_dis_id}" name="sel_tip_dis[{$tbl_tip_distributionitem.tip_dis_id}]" />&nbsp;</label>				</td>				<td>                    <a href="{$page_url}?mode={$smarty.const.MODE_UPDATE}&tip_dis_id={$tbl_tip_distributionitem.tip_dis_id}">{$tbl_tip_distributionitem.tip_dis_name}</a>                </td>                <td class="actionListItem">                    {if $tbl_tip_distributionitem.tip_dis_is_paid eq 1}                         <a href="{$page_url}?action=MKUNPAID&tip_dis_id={$tbl_tip_distributionitem.tip_dis_id}" class="activeIcon" title="{$_lang.tbl_tip_distribution.mkunpaid.BTN_LBL}"></a>                    {else}                        <a href="{$page_url}?action=MKPAID&tip_dis_id={$tbl_tip_distributionitem.tip_dis_id}" class="deactiveIcon" title="{$_lang.tbl_tip_distribution.mkpaid.BTN_LBL}"></a>                    {/if}                </td>								<!--<td class="actionListItem"><a href="{$page_url}?mode={$smarty.const.MODE_UPDATE}&tip_dis_id={$tbl_tip_distributionitem.tip_dis_id}" class="editIcon"></a><a class="deleteIcon" href="#" onclick="deletetbl_tip_distribution({$tbl_tip_distributionitem.tip_dis_id});"></a>{if $tbl_tip_distributionitem.isActive eq 1}<a href="{$page_url}?action={$smarty.const.ACTION_DEACTIVATE}&tip_dis_id={$tbl_tip_distributionitem.tip_dis_id}" class="deactiveIcon" title="{$_lang.tbl_tip_distribution.DEACTIVATE.BTN_LBL}"></a>{else}<a href="{$page_url}?action={$smarty.const.ACTION_ACTIVATE}&tip_dis_id={$tbl_tip_distributionitem.tip_dis_id}" class="activeIcon" title="{$_lang.tbl_tip_distribution.ACTIVATE.BTN_LBL}"></a>{/if}</td>-->			</tr>	{/foreach}	</table>	{html_input type="hidden" name="action"}</form>{else}	<div class="error">{$_lang.tbl_tip_distribution.no_record_found}</div>{/if}{if $pagination}	<div class="biz_center">{$pagination}</div>{/if}<div class="biz_center">{if $result_found gt 0  && $tbl_tip_distributionlist}	{jqmbutton icon="briefcase" value="{$_lang.main.toggle}" onclick="javascript:$('input[type=checkbox]').click();"} 	{jqmbutton type="delete" onclick="actiontbl_tip_distribution('{$smarty.const.ACTION_DELETE}');"}	{*jqmbutton icon="inactive" value="{$_lang.tbl_tip_distribution.DEACTIVATE.BTN_LBL}" onclick="actiontbl_tip_distribution('{$smarty.const.ACTION_DEACTIVATE}');"*}	{*jqmbutton icon="active" value="{$_lang.tbl_tip_distribution.ACTIVATE.BTN_LBL}" onclick="actiontbl_tip_distribution('{$smarty.const.ACTION_ACTIVATE}');"*}	{***jqmbutton icon="bar-chart" value="Report" onclick="window.location.href='{$website}/user/tbl_tip_distribution.php?{$smarty.const.MODE_TITLE}={$smarty.const.MODE_REPORT}'"***}{/if}	{jqmbutton icon="add-item" value="{$_lang.tbl_tip_distribution.CREATE.BTN_LBL}" onclick="window.location.href='{$page_url}?{$smarty.const.MODE_TITLE}={$smarty.const.MODE_CREATE}'"}</div></div>{include file="footercontent.tpl"}{include file="tbl_tip_distribution/js.tpl"}</body></html>