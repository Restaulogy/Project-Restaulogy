{include file="header.tpl"}<div class="wrapper"><h1>{$_lang.tbl_tip_distribution.title}</h1>{if $error_msg}	<div class="biz_center">{$error_msg}</div>{/if}{include file="tbl_tip/tabs.tpl"}<form name="frmupdatetbl_tip_distribution" id="frmupdatetbl_tip_distribution" onsubmit="return validatetbl_tip_distribution();" method="POST" action="{$page_url}" style="{if $isUpdate eq 1}{else}display:none;{/if}">	<input type="hidden" name="tip_dis_id" id="tip_dis_id" value="{$tbl_tip_distributioninfo.tip_dis_id}"/>	<div style="display:none;" class="error" id="tip_dis_id_err"></div> <fieldset style="border: 1px solid #ccc;margin:5px;padding:5px;">  <legend style='font-weight:bold;'>{$_lang.tbl_tip_distribution.step_1} </legend>    <table class="biz_hidden">		<tr >			<td style="width:48%;"><label for="tip_dis_period_from">{$_lang.tbl_tip_distribution.label.tip_dis_period} : {$tbl_tip_distributioninfo.tip_dis_period_from|date_format:$smarty.const.HTML5_DAY_FORMAT} - {$tbl_tip_distributioninfo.tip_dis_period_to|date_format:$smarty.const.HTML5_DAY_FORMAT}</label></td>			<td style="width:4%;">&nbsp;</td>			<td style="width:48%;"><label for="tip_dis_period_to">{$_lang.tbl_tip_distribution.label.tip_dis_period_to} </label></td>		</tr>				<tr >			<td  style="width:48%;"><label>{$tbl_tip_distributioninfo.tip_dis_period_from|date_format:$smarty.const.HTML5_DAY_FORMAT} - {$tbl_tip_distributioninfo.tip_dis_period_to|date_format:$smarty.const.HTML5_DAY_FORMAT}</label>            <input class="biz_hidden" id="tip_dis_period_from" type="text" value="{$tbl_tip_distributioninfo.tip_dis_period_from}" name="tip_dis_period_from" readonly="readonly"/>             </td>			 <td style="width:4%;">&nbsp;</td>			<td style="width:48%;"><label>{$tbl_tip_distributioninfo.tip_dis_period_to|date_format:$smarty.const.HTML5_DAY_FORMAT}</label>            <input class="biz_hidden" id="tip_dis_period_to" type="text" value="{$tbl_tip_distributioninfo.tip_dis_period_to}" name="tip_dis_period_to" readonly="readonly"/>            </td>		</tr>				<tr>			<td style="width:48%;"><div class="error" id="tip_dis_period_from_err"></div></td>			<td style="width:4%;">&nbsp;</td>			<td style="width:48%;"><div class="error" id="tip_dis_period_to_err"></div></td>		</tr>	</table>		<div class="field-row">        <label for="tip_dis_period_from">        {$_lang.tbl_tip_distribution.label.tip_dis_period} : {$tbl_tip_distributioninfo.tip_dis_period_from|date_format:$smarty.const.HTML5_DAY_FORMAT} - {$tbl_tip_distributioninfo.tip_dis_period_to|date_format:$smarty.const.HTML5_DAY_FORMAT}        </label>	</div>	<div class="field-row">		<label for="tip_dis_total_amnt">{$_lang.tbl_tip_distribution.label.tip_dis_total_amnt} : {$tbl_tip_distributioninfo.tip_dis_total_amnt}</label>		<input maxlength="7" id="tip_dis_total_amnt" type="hidden" value="{$tbl_tip_distributioninfo.tip_dis_total_amnt}" name="tip_dis_total_amnt" readonly="readonly"/>		<div class="error" id="tip_dis_total_amnt_err"></div>	</div>		<div class="biz_center">		{jqmbutton value="{$_lang.tbl_tip_distribution.tip_collected_detail}" icon="bar-chart" onclick="show_distrution();"}	</div>	<div class="info">{$_lang.tbl_tip_distribution.tip_paid_alreadydone}</div>  </fieldset>  <br>  <fieldset style="border: 1px solid #ccc;margin:5px;padding:5px;">  <legend style='font-weight:bold;'>{$_lang.tbl_tip_distribution.step_2}</legend>	<div class="field-row">		<label for="tip_dis_name">{$_lang.tbl_tip_distribution.label.tip_dis_name}</label>		<input maxlength="250" id="tip_dis_name" type="text" value="{$tbl_tip_distributioninfo.tip_dis_name}" name="tip_dis_name"/>		<div class="error" id="tip_dis_name_err"></div>	</div>	<div class="field-row">		<label for="tip_dis_type">{$_lang.tbl_tip_distribution.label.tip_dis_type}</label><select name="tip_dis_type" id="tip_dis_type" onclick="change_distrution(this.value);">	{foreach $disttypelist as $disttype}		<option value="{$disttype@key}" {if $tbl_tip_distributioninfo.tip_dis_type eq $disttype@key}selected="selected"{/if}>{$disttype}</option>	{/foreach}</select>		<div class="error" id="tip_dis_type_err"></div>	</div>	<div class="field-row" id="shared_among_id" {if $tbl_tip_distributioninfo.tip_dis_type neq 'SHARED'}style="display: none;"{/if}>		<label for="tip_dis_shared_among">{$_lang.tbl_tip_distribution.label.tip_dis_shared_among}</label>		<select name="tip_dis_shared_among[]" id="tip_dis_shared_among" multiple="multiple" data-native-menu="false">				<option value="">Select Employee</option>			{foreach $employeelist as $employee}				<option value="{$employee@key}" {if in_array($employee@key, $tbl_tip_distributioninfo.shared_list)}selected="selected"{/if}>{$employee}</option>			{/foreach}		</select>		<!--<textarea name="tip_dis_shared_among"  id="tip_dis_shared_among">{$smarty.post.tip_dis_shared_among}</textarea>-->		<div class="error" id="tip_dis_shared_among_err"></div>	</div>	<!--	<div class="field-row">		<label for="tip_dis_start_date">{$_lang.tbl_tip_distribution.label.tip_dis_start_date}</label>		<input type="text" name="tip_dis_start_date" id="tip_dis_start_date" value="{$tbl_tip_distributioninfo.tip_dis_start_date}"/>		<div class="error" id="tip_dis_start_date_err"></div>	</div>	-->	<!--	<div class="field-row">		<label for="tip_dis_end_date">{$_lang.tbl_tip_distribution.label.tip_dis_end_date}</label>		<input type="text" name="tip_dis_end_date" id="tip_dis_end_date" value="{$tbl_tip_distributioninfo.tip_dis_end_date}"/>		<div class="error" id="tip_dis_end_date_err"></div>	</div>	--></fieldset>	<div class="biz_center"><input type="hidden" id="action" name="action" value="{$smarty.const.ACTION_UPDATE}"/> <input  data-inline="true" data-icon="save"  type="submit" value="{$_lang.save_lbl}"/> <input type="button" value="{$_lang.cancel_lbl}" data-inline="true" data-icon="delete" onclick="window.location.href='{$page_url}'"/>{jqmbutton icon="search" value="{$_lang.tbl_tip_distribution.tip_paid_detail}" onclick="detail_distribution({$tbl_tip_distributioninfo.tip_dis_id});"}</div></form><div id="tbl_tip_distribution_view" class="description" style="{if $isUpdate eq 1}display:none;{/if}">	<table class="listTable">		<tr><th class="fieldItem">{$_lang.field_title}</th><th class="valueItem">{$_lang.value_title}</th></tr>		<tr><td class="fieldItem">{$_lang.tbl_tip_distribution.label.tip_dis_id}<i>:</i></td><td class="valueItem">{$tbl_tip_distributioninfo.tip_dis_id}</td></tr>		<tr><td class="fieldItem">{$_lang.tbl_tip_distribution.label.tip_dis_name}<i>:</i></td><td class="valueItem">{$tbl_tip_distributioninfo.tip_dis_name}</td></tr>		<tr><td class="fieldItem">{$_lang.tbl_tip_distribution.label.tip_dis_total_amnt}<i>:</i></td><td class="valueItem">{$tbl_tip_distributioninfo.tip_dis_total_amnt}</td></tr>		<tr><td class="fieldItem">{$_lang.tbl_tip_distribution.label.tip_dis_type}<i>:</i></td><td class="valueItem">{$tbl_tip_distributioninfo.tip_dis_type}</td></tr>		<tr><td class="fieldItem">{$_lang.tbl_tip_distribution.label.tip_dis_shared_among}<i>:</i></td><td class="valueItem">{$tbl_tip_distributioninfo.tip_dis_shared_among}</td></tr>		<tr><td class="fieldItem">{$_lang.tbl_tip_distribution.label.tip_dis_period_from}<i>:</i></td><td class="valueItem">{$tbl_tip_distributioninfo.tip_dis_period_from}</td></tr>		<tr><td class="fieldItem">{$_lang.tbl_tip_distribution.label.tip_dis_period_to}<i>:</i></td><td class="valueItem">{$tbl_tip_distributioninfo.tip_dis_period_to}</td></tr>	<!--		<tr><td class="fieldItem">{$_lang.tbl_tip_distribution.label.tip_dis_start_date}<i>:</i></td><td class="valueItem">{$tbl_tip_distributioninfo.tip_dis_start_date}</td></tr>	-->	<!--		<tr><td class="fieldItem">{$_lang.tbl_tip_distribution.label.tip_dis_end_date}<i>:</i></td><td class="valueItem">{$tbl_tip_distributioninfo.tip_dis_end_date}</td></tr>	-->	<tr><td class="fieldItem">{$_lang.tbl_tip_distribution.label.isActive}<i>:</i></td><td class="valueItem">{if $tbl_tip_distributioninfo.isActive eq 1}{$_lang.tbl_tip_distribution.label.isActive_yes}{else}{$_lang.tbl_tip_distribution.label.isActive_no}{/if}</td></tr>	</table>	<div class="biz_center"><input data-inline="true" data-icon="edit" type="button" value="{$_lang.tbl_tip_distribution.UPDATE.BTN_LBL}" onclick="$('#tbl_tip_distribution_view').hide();$('#frmupdatetbl_tip_distribution').show();"/><input type="button" value="{$_lang.cancel_lbl}" data-inline="true" data-icon="delete" onclick="window.location.href='{$page_url}'"/></div></div>{include file="tbl_tip_distribution/js.tpl"}</div>{include file="footer.tpl"}