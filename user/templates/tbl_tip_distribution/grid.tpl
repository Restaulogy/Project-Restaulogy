{include file='header.tpl'}<div class="wrapper"><h1>{$_lang.tbl_tip_distribution.listing_title}</h1>{if $error_msg}	<div class="biz_center">{$error_msg}</div>{/if}{if $result_found gt 0 && $tbl_tip_distributionlist}	<table class="biz_data_grid">		<tr>			<th class="{if $smarty.get.sort_on eq 'tip_dis_name'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=tip_dis_name&sort_by={$new_sort}">{$_lang.tbl_tip_distribution.label.tip_dis_name}</a></th>			<th class="{if $smarty.get.sort_on eq 'tip_dis_total_amnt'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=tip_dis_total_amnt&sort_by={$new_sort}">{$_lang.tbl_tip_distribution.label.tip_dis_total_amnt}</a></th>			<th class="{if $smarty.get.sort_on eq 'tip_dis_type'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=tip_dis_type&sort_by={$new_sort}">{$_lang.tbl_tip_distribution.label.tip_dis_type}</a></th>			<th class="{if $smarty.get.sort_on eq 'tip_dis_shared_among'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=tip_dis_shared_among&sort_by={$new_sort}">{$_lang.tbl_tip_distribution.label.tip_dis_shared_among}</a></th>			<th class="{if $smarty.get.sort_on eq 'tip_dis_period_from'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=tip_dis_period_from&sort_by={$new_sort}">{$_lang.tbl_tip_distribution.label.tip_dis_period_from}</a></th>			<th class="{if $smarty.get.sort_on eq 'tip_dis_period_to'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=tip_dis_period_to&sort_by={$new_sort}">{$_lang.tbl_tip_distribution.label.tip_dis_period_to}</a></th>			<th class="action_header">Action</th>		</tr>		{foreach from=$tbl_tip_distributionlist item=tbl_tip_distributionitem}		<tr class="{cycle values="odd,even"}">			<td><a href="{$page_url}?mode={$smarty.const.MODE_VIEW}&tip_dis_id={$tbl_tip_distributionitem.tip_dis_id}">{$tbl_tip_distributionitem.tip_dis_name}</a></td>			<td>{$tbl_tip_distributionitem.tip_dis_total_amnt}</td>			<td>{$tbl_tip_distributionitem.tip_dis_type}</td>			<td>{$tbl_tip_distributionitem.tip_dis_shared_among}</td>			<td>{$tbl_tip_distributionitem.tip_dis_period_from}</td>			<td>{$tbl_tip_distributionitem.tip_dis_period_to}</td>			<td><a href="{$page_url}?mode={$smarty.const.MODE_UPDATE}&tip_dis_id={$tbl_tip_distributionitem.tip_dis_id}" class="editIcon"></a><a class="deleteIcon" href="#" onclick="deletetbl_tip_distribution({$tbl_tip_distributionitem.tip_dis_id});"></a>{if $tbl_tip_distributionitem.isActive eq 1}<a href="{$page_url}?action={$smarty.const.ACTION_DEACTIVATE}&tip_dis_id={$tbl_tip_distributionitem.tip_dis_id}" class="deactiveIcon" title="{$_lang.tbl_tip_distribution.DEACTIVATE.BTN_LBL}"></a>{else}<a href="{$page_url}?action={$smarty.const.ACTION_ACTIVATE}&tip_dis_id={$tbl_tip_distributionitem.tip_dis_id}" class="activeIcon" title="{$_lang.tbl_tip_distribution.ACTIVATE.BTN_LBL}"></a>{/if}</td>		</tr>	{/foreach}		<tfoot>			<tr>				<td colspan="9">					# {$result_found}&nbsp;&nbsp;&nbsp;{if $pagination neq ""}{$pagination}{/if}					<select onchange="changePage('{$navigationURL}',this.value,{$smarty.request.limit});">					{if $allPageCount gt 1}						{for $foo=1 to $allPageCount}							<option value="{$foo}" {if $foo eq $currentPage}selected="selected"{/if}>{$foo}</option>						{/for}					{else}						<option value="1" disabled="disabled">1</option>					{/if}					</select>				</td>			</tr>		</tfoot>	</table>{else}	<div class="error">{$_lang.tbl_tip_distribution.no_record_found}</div>{/if}<div class="biz_center"><input data-icon="add-item" data-inline="true" onclick="window.location.href='{$page_url}?mode={$smarty.const.MODE_CREATE}'" type="button" value="{$_lang.tbl_tip_distribution.CREATE.BTN_LBL}"/></div></div>{include file="tbl_tip_distribution/js.tpl"}{include file="footer.tpl"}