{literal}<script type="text/javascript">function deletetbl_customers(varId){	if(varId > 0){		if(confirm("{/literal}{$_lang.tbl_customers.DELETE.CONFIRM_MSG}{literal}")==true){			window.location.href="{/literal}{$page_url}{literal}?action=delete&customer_id="+varId;		}	}}function validatetbl_customers(){	$("#customer_id_err").html("");	$("#cutomer_name_err").html("");	$("#cutomer_phone_no_err").html("");	$("#customer_email_err").html("");	$("#customer_address_err").html("");	$("#customer_city_err").html("");	$("#customer_zip_err").html("");	$("#cutomer_created_on_err").html("");	$("#customer_end_date_err").html("");	var isErr = true;	if(elemById("action").value=="update"){		if(IsNonEmpty(elemById("customer_id").value)==false){			$("#customer_id_err").html("{/literal}{$_lang.tbl_customers.not_empty_msg.customer_id}{literal}");			isErr = false;		}	}	if(IsNonEmpty(elemById("cutomer_name").value)==false){		$("#cutomer_name_err").html("{/literal}{$_lang.tbl_customers.not_empty_msg.cutomer_name}{literal}");		isErr = false;	}	if(IsNonEmpty(elemById("cutomer_phone_no").value) == false){      	$('#cutomer_phone_no_err').html("{/literal}{$_lang.tbl_customers.not_empty_msg.cutomer_phone_no}{literal}");    	isErr = false;    }else{      	if(isPhoneNumber(elemById("cutomer_phone_no").value) == false){    	  	$('#cutomer_phone_no_err').html("{/literal}{$_lang.tbl_customers.invalid_input_msg.cutomer_phone_no}{literal}");    		isErr = false;      	}    }		if(IsNonEmpty(elemById("customer_email").value) == false){      	$('#customer_email_err').html("{/literal}{$_lang.tbl_customers.not_empty_msg.customer_email}{literal}");    	isErr = false;      }else{      	if(isEmail(elemById("customer_email").value) == false){    	  	$('#customer_email_err').html("{/literal}{$_lang.tbl_customers.invalid_input_msg.customer_email}{literal}");    		isErr = false;      	}      }	if(IsNonEmpty(elemById("customer_address").value)==false){		$("#customer_address_err").html("{/literal}{$_lang.tbl_customers.not_empty_msg.customer_address}{literal}");		isErr = false;	}	if(IsNonEmpty(elemById("customer_city").value)==false){		$("#customer_city_err").html("{/literal}{$_lang.tbl_customers.not_empty_msg.customer_city}{literal}");		isErr = false;	}		if(IsNonEmpty(elemById("customer_zip").value) == false){      	$('#customer_zip_err').html("{/literal}{$_lang.tbl_customers.not_empty_msg.customer_zip}{literal}");    	isErr = false;      }else{      	if(isUSZip(elemById("customer_zip").value) == false){    	  	$('#customer_zip_err').html("{/literal}{$_lang.tbl_customers.invalid_input_msg.customer_zip}{literal}");    		isErr = false;      	}      }	/*	if(IsNonEmpty(elemById("cutomer_created_on").value)==false){		$("#cutomer_created_on_err").html("{/literal}{$_lang.tbl_customers.not_empty_msg.cutomer_created_on}{literal}");		isErr = false;	}	if(IsNonEmpty(elemById("customer_end_date").value)==false){		$("#customer_end_date_err").html("{/literal}{$_lang.tbl_customers.not_empty_msg.customer_end_date}{literal}");		isErr = false;	}	*/	if(isErr == false){		alert("{/literal}{$_lang.messages.revise_form}{literal}");	}	return isErr;}//..function</script>{/literal}