{include file='header.tpl'}<div class="wrapper"><h1>{$_lang.tbl_complaints.listing_title}</h1>{if $error_msg}	<center>{$error_msg}</center>{/if}{if $result_found gt 0 && $tbl_complaintslist}	<table class="listTable">		<tr><th class="bigListItem">{$_lang.tbl_complaints.title}</th>		<th class="actionListItem"></th></tr>		{foreach from=$tbl_complaintslist item=tbl_complaintsitem}			<tr>				<td class="bigListItem"><a href="{$page_url}?mode={$smarty.const.MODE_VIEW}&complnt_id={$tbl_complaintsitem.complnt_id}">{$tbl_complaintsitem.complnt_title}</a></td>				<td class="actionListItem"><a href="{$page_url}?mode={$smarty.const.MODE_UPDATE}&complnt_id={$tbl_complaintsitem.complnt_id}" class="editIcon"></a><a class="deleteIcon" href="#" onclick="deletetbl_complaints({$tbl_complaintsitem.complnt_id});"></a>{if $tbl_complaintsitem.isActive eq 1}<a href="{$page_url}?action={$smarty.const.ACTION_DEACTIVATE}&complnt_id={$tbl_complaintsitem.complnt_id}" class="deactiveIcon" title="{$_lang.tbl_complaints.DEACTIVATE.BTN_LBL}"></a>{else}<a href="{$page_url}?action={$smarty.const.ACTION_ACTIVATE}&complnt_id={$tbl_complaintsitem.complnt_id}" class="activeIcon" title="{$_lang.tbl_complaints.ACTIVATE.BTN_LBL}"></a>{/if}</td>			</tr>	{/foreach}	</table>{else}	<div class="error">{$_lang.tbl_complaints.no_record_found}</div>{/if}{if $pagination}	<center>{$pagination}</center>{/if}<center><input data-icon="add-item" data-inline="true" onclick="window.location.href='{$page_url}?mode={$smarty.const.MODE_CREATE}'" type="button" value="{$_lang.tbl_complaints.CREATE.BTN_LBL}"/></center></div>{include file="tbl_complaints/js.tpl"}{include file="footer.tpl"}