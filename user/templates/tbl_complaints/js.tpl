{literal}<script type="text/javascript">function actiontbl_complaints(action){	var varId = $("input:checked").length;	var isValid = 0;	if(varId > 0){	if(action != ""){		switch (action) {			case '{/literal}{$smarty.const.ACTION_DELETE}{literal}':			if(confirm("{/literal}{$_lang.tbl_complaints.DELETE.CONFIRM_MSG}{literal}")==true){				isValid = 1;			}			break;			case '{/literal}{$smarty.const.ACTION_DEACTIVATE}{literal}':			isValid = 1;			break;			case '{/literal}{$smarty.const.ACTION_ACTIVATE}{literal}':			isValid = 1;			break;		}    	if(isValid){    	   $('#action').val(action);    	   $('#frm_tbl_complaints').submit();    	}	}	}else{			alert("{/literal}{$_lang.main.select_ids.empty}{literal}"); 	}}function deletetbl_complaints(varId){	if(varId > 0){		if(confirm("{/literal}{$_lang.tbl_complaints.DELETE.CONFIRM_MSG}{literal}")==true){			window.location.href="{/literal}{$page_url}{literal}?action=delete&complnt_id="+varId;		}	}}function validatetbl_complaints(){	$("#complnt_id_err").html("");	$("#complnt_title_err").html("");	$("#complnt_description_err").html("");	$("#complnt_email_err").html("");	$("#complnt_phone_err").html("");	$("#complnt_restaurant_err").html("");	$("#complnt_start_date_err").html("");	$("#complnt_end_date_err").html("");	var isErr = true;	if(elemById("action").value=="update"){		if(IsNonEmpty(elemById("complnt_id").value)==false){			$("#complnt_id_err").html("{/literal}{$_lang.tbl_complaints.not_empty_msg.complnt_id}{literal}");			isErr = false;		}	}	/*	if(IsNonEmpty(elemById("complnt_title").value)==false){		$("#complnt_title_err").html("{/literal}{$_lang.tbl_complaints.not_empty_msg.complnt_title}{literal}");		isErr = false;	}    */	if(IsNonEmpty(elemById("complnt_description").value)==false){		$("#complnt_description_err").html("{/literal}{$_lang.tbl_complaints.not_empty_msg.complnt_description}{literal}");		isErr = false;	}    /*	if(IsNonEmpty(elemById("complnt_email").va