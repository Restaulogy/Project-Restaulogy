{include file='header.tpl'}<div class="wrapper"><h1>{$_lang.tbl_shift.listing_title}</h1>{if $error_msg}	<center>{$error_msg}</center>{/if}{include file="tbl_shift/tabs.tpl"}<form name="frm_tbl_shiftlist" id="frm_tbl_shiftlist"  method="POST" action="{$page_url}" >{if $result_found gt 0 && $tbl_shiftlist}	<table class="listTable">		<tr>            <th class="bigListItem" colspan="2">{$_lang.tbl_shift.title}</th>    		<th style="width:5%;">                {$_lang.main.status}    		</th>		</tr>		{foreach from=$tbl_shiftlist item=tbl_shiftitem}						<tr>				<td style="width:2%;">				<label for="sel_tbl_shift[{$tbl_shiftitem.shift_id}]" data-mini="true" style="width:23px;"><input type="checkbox" data-inline='true' data-mini='true' id="sel_tbl_shift[{$tbl_shiftitem.shift_id}]" name="sel_tbl_shift[{$tbl_shiftitem.shift_id}]" />&nbsp;</label>                </td>					<td class="bigListItem"><a href="{$page_url}?mode={$smarty.const.MODE_UPDATE}&shift_id={$tbl_shiftitem.shift_id}">{$tbl_shiftitem.shift_name} ({$tbl_shiftitem.shift_start_time|date_format:"%H:%M"} - {$tbl_shiftitem.shift_end_time|date_format:"%H:%M"})</a></td>				<td class="actionListItem">                    {if $tbl_shiftitem.isActive eq 1}                          <a href="#" class="activeIcon" title="{$_lang.tbl_shift.ACTIVATE.BTN_LBL}"></a>                    {else}                        <a href="#" class="deactiveIcon" title="{$_lang.tbl_shift.DEACTIVATE.BTN_LBL}"></a>                    {/if}				</td>				<!--<td class="actionListItem"><a href="{$page_url}?mode=view&shift_id={$tbl_shiftitem.shift_id}" class="detailIcon"></a><a href="{$page_url}?mode=update&shift_id={$tbl_shiftitem.shift_id}" class="editIcon"></a><a class="deleteIcon" href="#" onclick="deletetbl_shift({$tbl_shiftitem.shift_id});"></a>{if $tbl_shiftitem.isActive eq 1}<a href="{$page_url}?action=deactivate&shift_id={$tbl_shiftitem.shift_id}" class="deactiveIcon" title="{$_lang.tbl_shift.DEACTIVATE.BTN_LBL}"></a>{else}<a href="{$page_url}?action=activate&shift_id={$tbl_shiftitem.shift_id}" class="activeIcon" title="{$_lang.tbl_shift.ACTIVATE.BTN_LBL}"></a>{/if}</td>-->			</tr>	{/foreach}	</table>{else}	<div class="error">{$_lang.tbl_shift.no_record_found}</div>{/if}{if $pagination}	<center>{$pagination}</center>{/if}<input type='hidden' name='action' id='action' value='' /></form><center>{if $result_found gt 0 && $tbl_shiftlist}<input data-inline="true" data-icon="briefcase" type="button" id="sel_all_tbl_shift" name="sel_all_tbl_shift" value="{$_lang.main.toggle}" onclick="javascript:$('input[type=checkbox]').click();" /><!--<input data-inline="true" data-icon="delete" type="button" id="del_sel_all_tbl_shift" name="del_sel_all_tbl_shift" value="{$_lang.tbl_shift.DELETE.BTN_LBL}" onclick="deletetbl_shift({$tbl_shiftitem.shift_id});" />--><input data-inline="true" data-icon="inactive" type="button" value="{$_lang.tbl_shift.DEACTIVATE.BTN_LBL}" onclick="actiontbl_shift('{$smarty.const.ACTION_DEACTIVATE}');"  /><input data-inline="true" data-icon="active" type="button" value="{$_lang.tbl_shift.ACTIVATE.BTN_LBL}" onclick="actiontbl_shift('{$smarty.const.ACTION_ACTIVATE}');" />{/if}<input type="button" data-inline="true" data-icon="add-item" onclick="window.location.href='{$page_url}?mode=new';"  value="{$_lang.tbl_shift.CREATE.BTN_LBL}"/> <!--<a data-role="button" data-inline="true" data-icon="back" href='{$website}/user/tbl_table_shift_assignment.php'>{$_lang.back_str|sprintf:$_lang.tbl_table_shift_assignment.listing_title}</a>--> </center></div> {include file="tbl_shift/js.tpl"}{include file="footer.tpl"}