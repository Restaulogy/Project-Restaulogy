{include file="header.tpl"}<div class="wrapper"><h1>{if $isCustomer}{$_lang.tbl_wait_queue.customer_view_title}{else}{$_lang.tbl_wait_queue.listing_title}{/if}</h1>{if $error_msg}	<center>{$error_msg}</center>{/if} <form name="frmupdatetbl_wait_queue" id="frmupdatetbl_wait_queue" onsubmit="return validatetbl_wait_queue();" method="POST" action="{$page_url}" style="{if $isCustomer eq false && $isUpdate eq 1 && $tbl_wait_queueitem.isActive eq 1}{else}display:none;{/if}">	<input type="hidden" name="wt_que_id" id="wt_que_id" value="{$tbl_wait_queueinfo.wt_que_id}"/>	<div style="display:none;" class="error" id="wt_que_id_err"></div>	<div class="field-row">		<label for="wt_que_customer">{$_lang.tbl_wait_queue.label.wt_que_customer}</label>		<input type="text" name="wt_que_customer" id="wt_que_customer" value="{$tbl_wait_queueinfo.wt_que_customer}"/>		<div class="error" id="wt_que_customer_err"></div>	</div>	<div class="field-row">		<label for="wt_que_cust_ph_no">{$_lang.tbl_wait_queue.label.wt_que_cust_ph_no}</label>		<input type="text" name="wt_que_cust_ph_no" id="wt_que_cust_ph_no" value="{$tbl_wait_queueinfo.wt_que_cust_ph_no}"/>		<div class="error" id="wt_que_cust_ph_no_err"></div>	</div>	<div class="field-row">		<label for="wt_que_party_size">{$_lang.tbl_wait_queue.label.wt_que_party_size}</label>		 		<select name="wt_que_party_size" onclick="calculate_est_time();" id="wt_que_party_size">		<option>Select Party Size</option>		{for $i=1 to 20}		<option value="{$i}" {if $tbl_wait_queueinfo.wt_que_party_size eq $i}selected="selected"{/if}>{$i}</option>		{/for}		</select>		<div class="error" id="wt_que_party_size_err"></div>	</div>	<div class="field-row">		<label for="wt_que_table_type">{$_lang.tbl_wait_queue.label.wt_que_table_type}</label>		<select name="wt_que_table_type" onclick="calculate_est_time();" id="wt_que_table_type">		<option>Select Table Type</option>		{foreach $table_types as $val}			<option value="{$val@key}" {if $tbl_wait_queueinfo.wt_que_table_type eq $val@key}selected="selected"{/if}>{$val}</option>		{/foreach} 		</select>  		<div class="error" id="wt_que_table_type_err"></div>	</div>	<div class="field-row">		<label for="wt_que_table_id">{$_lang.tbl_wait_queue.label.wt_que_table_id}</label>		<input type="text" name="wt_que_table_id" id="wt_que_table_id" value="{$tbl_wait_queueinfo.wt_que_table_id}"/>		<div class="error" id="wt_que_table_id_err"></div>	</div>	<div class="field-row">		<label for="wt_que_exp_time">{$_lang.tbl_wait_queue.label.wt_que_exp_time}</label>		<label style="display:none;" id="lbl_wt_que_exp_time">{$tbl_wait_queueinfo.wt_que_exp_time} Min</label> 		<input type="text" name="wt_que_exp_time" id="wt_que_exp_time" value="{$tbl_wait_queueinfo.wt_que_exp_time}"/>		<div class="error" id="wt_que_exp_time_err"></div>	</div>	 		<!--<div class="field-row">		<label for="wt_que_created_on">{$_lang.tbl_wait_queue.label.wt_que_created_on}</label>		<input type="text" name="wt_que_created_on" id="wt_que_created_on" value="{$tbl_wait_queueinfo.wt_que_created_on}"/>		<div class="error" id="wt_que_created_on_err"></div>	</div>	<div class="field-row">		<label for="wt_que_completed_on">{$_lang.tbl_wait_queue.label.wt_que_completed_on}</label>		<input type="text" name="wt_que_completed_on" id="wt_que_completed_on" value="{$tbl_wait_queueinfo.wt_que_completed_on}"/>		<div class="error" id="wt_que_completed_on_err"></div>	</div>-->	<input type="hidden" name="wt_que_is_cancelled" id="wt_que_is_cancelled" value="{$tbl_wait_queueinfo.wt_que_is_cancelled}"/>	 <input type="hidden" name="wt_que_restaurant" id="wt_que_restaurant" value="{$tbl_wait_queueinfo.wt_que_restaurant}"/>	<input type="hidden" name="wt_que_by_cust" id="wt_que_by_cust" value="{$tbl_wait_queueinfo.wt_que_by_cust}"/>	<input type="hidden" id="action" name="action" value="{$smarty.const.ACTION_UPDATE}"/> 	<center>	<input  data-inline="true" data-icon="save"  type="submit" value="{$_lang.save_lbl}"/> <input data-inline="true" data-icon="delete" type="reset" onclick="$('#tbl_wait_queue_view').show();$('#frmupdatetbl_wait_queue').hide();" value="{$_lang.cancel_lbl}"/></center></form><div id="tbl_wait_queue_view" class="description" style="{if $isUpdate eq 1 && $isCustomer eq false && $tbl_wait_queueitem.isActive eq 1}display:none;{/if}">	<table class="listTable">		<tr><th class="fieldItem">{$_lang.field_title}</th><th class="valueItem">{$_lang.value_title}</th></tr>		<!--<tr><td class="fieldItem">{$_lang.tbl_wait_queue.label.wt_que_id}<i>:</i></td><td class="valueItem">{$tbl_wait_queueinfo.wt_que_id}</td></tr>-->		<tr><td class="fieldItem">{$_lang.tbl_wait_queue.label.wt_que_customer}<i>:</i></td><td class="valueItem">{$tbl_wait_queueinfo.wt_que_customer}</td></tr>		<tr><td class="fieldItem">{$_lang.tbl_wait_queue.label.wt_que_cust_ph_no}<i>:</i></td><td class="valueItem">{$tbl_wait_queueinfo.wt_que_cust_ph_no}</td></tr>		<tr><td class="fieldItem">{$_lang.tbl_wait_queue.label.wt_que_party_size}<i>:</i></td><td class="valueItem">{$tbl_wait_queueinfo.wt_que_party_size}</td></tr>		<tr><td class="fieldItem">{$_lang.tbl_wait_queue.label.wt_que_table_type}<i>:</i></td><td class="valueItem">{$tbl_wait_queueinfo.table_type_info.tbl_type_name}</td></tr>		<tr><td class="fieldItem">{$_lang.tbl_wait_queue.label.wt_que_table_id}<i>:</i></td><td class="valueItem">{if $tbl_wait_queueinfo.wt_que_table_id neq ""}{$tbl_wait_queueinfo.wt_que_table_id}{else}--{/if}</td></tr>		<tr><td class="fieldItem">{$_lang.tbl_wait_queue.label.wt_que_exp_time}<i>:</i></td><td class="valueItem">{$tbl_wait_queueinfo.wt_que_exp_time}</td></tr>		<tr><td class="fieldItem">Status<i>:</i></td><td class="valueItem">{if $tbl_wait_queueinfo.isActive eq 1}Waiting ({$tbl_wait_queueitem.wt_que_cur_remain_time} min) {else}{if $tbl_wait_queueinfo.wt_que_is_cancelled eq 1}Cancelled{else}Seated{/if}{/if}</td></tr> 		<!--<tr><td class="fieldItem">{$_lang.tbl_wait_queue.label.wt_que_restaurant}<i>:</i></td><td class="valueItem">{$tbl_wait_queueinfo.wt_que_restaurant}</td></tr>-->		<tr><td class="fieldItem">{$_lang.tbl_wait_queue.label.wt_que_by_cust}<i>:</i></td><td class="valueItem">{if $tbl_wait_queueinfo.wt_que_by_cust eq 1}Yes{else}No{/if}</td></tr>		<tr><td class="fieldItem">{$_lang.tbl_wait_queue.label.wt_que_created_on}<i>:</i></td><td class="valueItem">{$tbl_wait_queueinfo.friendly_wt_que_created_on}</td></tr>		<tr><td class="fieldItem">{$_lang.tbl_wait_queue.label.wt_que_completed_on}<i>:</i></td><td class="valueItem">		{if $tbl_wait_queueinfo.isActive eq 1}--{else}{$tbl_wait_queueinfo.friendly_wt_que_completed_on}{/if}		</td></tr>	<!--<tr><td class="fieldItem">{$_lang.tbl_wait_queue.label.isActive}<i>:</i></td><td class="valueItem">{if $tbl_wait_queueinfo.isActive eq 1}{$_lang.tbl_wait_queue.label.isActive_yes}{else}{$_lang.tbl_wait_queue.label.isActive_no}{/if}</td></tr>-->	</table>	<center>	{if $isCustomer || $tbl_wait_queueinfo.isActive eq 0}{else}		<input data-inline="true" data-icon="edit" type="button" value="{$_lang.tbl_wait_queue.UPDATE.BTN_LBL}" onclick="$('#tbl_wait_queue_view').hide();$('#frmupdatetbl_wait_queue').show();"/>	{/if}	{if $isCustomer && $tbl_wait_queueinfo.isActive eq 1}	<input type="button" value="{$_lang.tbl_wait_queue.CANCEL.BTN_LBL}" data-inline="true" data-icon="delete" onclick="cancelQue({$tbl_wait_queueinfo.wt_que_id},1)"/>	{else}	<input type="button" value="{$_lang.cancel_lbl}" data-inline="true" data-icon="delete" onclick="window.location.href='{$page_url}'"/>	{/if}	</center></div> </div>{include file="tbl_wait_queue/popupTable.tpl"}{include file="footercontent.tpl"}{include file="tbl_wait_queue/js.tpl"}</body></html>