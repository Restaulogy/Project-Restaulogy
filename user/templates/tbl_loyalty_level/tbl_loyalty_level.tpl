{include file='header.tpl'}<div class="wrapper"><h1>{$_lang.tbl_loyalty_level.listing_title}</h1>{if $error_msg}	<center>{$error_msg}</center>{/if}{if $result_found gt 0 && $tbl_loyalty_levellist}	<table class="listTable">		<tr><th class="bigListItem">{$_lang.tbl_loyalty_level.title}</th>		<th class="actionListItem"></th></tr>		{foreach from=$tbl_loyalty_levellist item=tbl_loyalty_levelitem}			<tr>				<td class="bigListItem">{$tbl_loyalty_levelitem.loylev_seq}.&nbsp; <a href="{$page_url}?mode={$smarty.const.MODE_UPDATE}&loylev_id={$tbl_loyalty_levelitem.loylev_id}">{$tbl_loyalty_levelitem.loylev_level} &nbsp;&nbsp;({$tbl_loyalty_levelitem.loylev_min_pt}-{$tbl_loyalty_levelitem.loylev_max_pt})&nbsp;&nbsp;x&nbsp;{$tbl_loyalty_levelitem.loylev_multiply_factor}&nbsp;</a></td>				<td class="actionListItem">				<a class="editIcon" href="#" onclick="setdefaulttbl_loyalty_level({$tbl_loyalty_levelitem.loylev_id});" title="Set Deatult level"></a>				<a class="deleteIcon" href="#" onclick="deletetbl_loyalty_level({$tbl_loyalty_levelitem.loylev_id});"></a>{if $tbl_loyalty_levelitem.isActive eq 1}<a href="{$page_url}?action={$smarty.const.ACTION_DEACTIVATE}&loylev_id={$tbl_loyalty_levelitem.loylev_id}" class="deactiveIcon" title="{$_lang.tbl_loyalty_level.DEACTIVATE.BTN_LBL}"></a>{else}<a href="{$page_url}?action={$smarty.const.ACTION_ACTIVATE}&loylev_id={$tbl_loyalty_levelitem.loylev_id}" class="activeIcon" title="{$_lang.tbl_loyalty_level.ACTIVATE.BTN_LBL}"></a>{/if}</td>			</tr>	{/foreach}	</table>{else}	<div class="error">{$_lang.tbl_loyalty_level.no_record_found}</div>{/if}{if $pagination}	<center>{$pagination}</center>{/if}<center><input data-icon="add-item" data-inline="true" onclick="window.location.href='{$page_url}?mode={$smarty.const.MODE_CREATE}'" type="button" value="{$_lang.tbl_loyalty_level.CREATE.BTN_LBL}"/></center></div>{include file="tbl_loyalty_level/js.tpl"}{include file="footer.tpl"}