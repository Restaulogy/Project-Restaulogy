{include file='header.tpl'}<div class="wrapper"><h1>{$_lang.tbl_loyalty_level.listing_title}</h1>{if $error_msg}	<center>{$error_msg}</center>{/if}{if $result_found gt 0 && $tbl_loyalty_levellist}	<table class="biz_data_grid">		<tr>			<th class="{if $smarty.get.sort_on eq 'loylev_level'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=loylev_level&sort_by={$new_sort}">{$_lang.tbl_loyalty_level.label.loylev_level}</a></th>			<th class="{if $smarty.get.sort_on eq 'loylev_desc'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=loylev_desc&sort_by={$new_sort}">{$_lang.tbl_loyalty_level.label.loylev_desc}</a></th>			<th class="{if $smarty.get.sort_on eq 'loylev_restaurant'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=loylev_restaurant&sort_by={$new_sort}">{$_lang.tbl_loyalty_level.label.loylev_restaurant}</a></th>			<th class="{if $smarty.get.sort_on eq 'loylev_min_pt'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=loylev_min_pt&sort_by={$new_sort}">{$_lang.tbl_loyalty_level.label.loylev_min_pt}</a></th>			<th class="{if $smarty.get.sort_on eq 'loylev_max_pt'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=loylev_max_pt&sort_by={$new_sort}">{$_lang.tbl_loyalty_level.label.loylev_max_pt}</a></th>			<th class="{if $smarty.get.sort_on eq 'loylev_seq'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=loylev_seq&sort_by={$new_sort}">{$_lang.tbl_loyalty_level.label.loylev_seq}</a></th>			<th class="{if $smarty.get.sort_on eq 'loylev_multiply_factor'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=loylev_multiply_factor&sort_by={$new_sort}">{$_lang.tbl_loyalty_level.label.loylev_multiply_factor}</a></th>			<th class="action_header">Action</th>		</tr>		{foreach from=$tbl_loyalty_levellist item=tbl_loyalty_levelitem}		<tr class="{cycle values="odd,even"}">			<td><a href="{$page_url}?mode={$smarty.const.MODE_VIEW}&loylev_id={$tbl_loyalty_levelitem.loylev_id}">{$tbl_loyalty_levelitem.loylev_level}</a></td>			<td>{$tbl_loyalty_levelitem.loylev_desc}</td>			<td>{$tbl_loyalty_levelitem.loylev_restaurant}</td>			<td>{$tbl_loyalty_levelitem.loylev_min_pt}</td>			<td>{$tbl_loyalty_levelitem.loylev_max_pt}</td>			<td>{$tbl_loyalty_levelitem.loylev_seq}</td>			<td>{$tbl_loyalty_levelitem.loylev_multiply_factor}</td>			<td><a href="{$page_url}?mode={$smarty.const.MODE_UPDATE}&loylev_id={$tbl_loyalty_levelitem.loylev_id}" class="editIcon"></a><a class="deleteIcon" href="#" onclick="deletetbl_loyalty_level({$tbl_loyalty_levelitem.loylev_id});"></a>{if $tbl_loyalty_levelitem.isActive eq 1}<a href="{$page_url}?action={$smarty.const.ACTION_DEACTIVATE}&loylev_id={$tbl_loyalty_levelitem.loylev_id}" class="deactiveIcon" title="{$_lang.tbl_loyalty_level.DEACTIVATE.BTN_LBL}"></a>{else}<a href="{$page_url}?action={$smarty.const.ACTION_ACTIVATE}&loylev_id={$tbl_loyalty_levelitem.loylev_id}" class="activeIcon" title="{$_lang.tbl_loyalty_level.ACTIVATE.BTN_LBL}"></a>{/if}</td>		</tr>	{/foreach}		<tfoot>			<tr>				<td colspan="10">					# {$result_found}&nbsp;&nbsp;&nbsp;{if $pagination neq ""}{$pagination}{/if}					<select onchange="changePage('{$navigationURL}',this.value,{$smarty.request.limit});">					{if $allPageCount gt 1}						{for $foo=1 to $allPageCount}							<option value="{$foo}" {if $foo eq $currentPage}selected="selected"{/if}>{$foo}</option>						{/for}					{else}						<option value="1" disabled="disabled">1</option>					{/if}					</select>				</td>			</tr>		</tfoot>	</table>{else}	<div class="error">{$_lang.tbl_loyalty_level.no_record_found}</div>{/if}<center><input data-icon="add-item" data-inline="true" onclick="window.location.href='{$page_url}?mode={$smarty.const.MODE_CREATE}'" type="button" value="{$_lang.tbl_loyalty_level.CREATE.BTN_LBL}"/></center></div>{include file="tbl_loyalty_level/js.tpl"}{include file="footer.tpl"}