{literal}<script type="text/javascript">function deletetbl_order_promotions(varId){	if(varId > 0){		if(confirm("{/literal}{$_lang.tbl_order_promotions.DELETE.CONFIRM_MSG}{literal}")==true){			window.location.href="{/literal}{$page_url}{literal}?action=delete&ordprom_id="+varId;		}	}}function validatetbl_order_promotions(){	$("#ordprom_id_err").html("");	$("#ordprom_order_err").html("");	$("#ordprom_order_sbmnu_dish_err").html("");	$("#ordprom_promotion_err").html("");	$("#ordprom_discount_err").html("");	$("#ordprom_start_date_err").html("");	$("#ordprom_end_date_err").html("");	var isErr = true;	if(elemById("action").value=="update"){		if(IsNonEmpty(elemById("ordprom_id").value)==false){			$("#ordprom_id_err").html("{/literal}{$_lang.tbl_order_promotions.not_empty_msg.ordprom_id}{literal}");			isErr = false;		}	}	if(IsNonEmpty(elemById("ordprom_order").value)==false){		$("#ordprom_order_err").html("{/literal}{$_lang.tbl_order_promotions.not_empty_msg.ordprom_order}{literal}");		isErr = false;	}	if(IsNonEmpty(elemById("ordprom_order_sbmnu_dish").value)==false){		$("#ordprom_order_sbmnu_dish_err").html("{/literal}{$_lang.tbl_order_promotions.not_empty_msg.ordprom_order_sbmnu_dish}{literal}");		isErr = false;	}	if(IsNonEmpty(elemById("ordprom_promotion").value)==false){		$("#ordprom_promotion_err").html("{/literal}{$_lang.tbl_order_promotions.not_empty_msg.ordprom_promotion}{literal}");		isErr = false;	}	if(IsNonEmpty(elemById("ordprom_discount").value)==false){		$("#ordprom_discount_err").html("{/literal}{$_lang.tbl_order_promotions.not_empty_msg.ordprom_discount}{literal}");		isErr = false;	}/*	if(IsNonEmpty(elemById("ordprom_start_date").value)==false){		$("#ordprom_start_date_err").html("{/literal}{$_lang.tbl_order_promotions.not_empty_msg.ordprom_start_date}{literal}");		isErr = false;	}*//*	if(IsNonEmpty(elemById("ordprom_end_date").value)==false){		$("#ordprom_end_date_err").html("{/literal}{$_lang.tbl_order_promotions.not_empty_msg.ordprom_end_date}{literal}");		isErr = false;	}*/	if(isErr == false){		alert("{/literal}{$_lang.messages.revise_form}{literal}");	}	return isErr;}//..function</script>{/literal}