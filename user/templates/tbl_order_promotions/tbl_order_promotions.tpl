{include file='header.tpl'}<div class="wrapper"><h1>{$_lang.tbl_order_promotions.listing_title}</h1>{if $error_msg}	<center>{$error_msg}</center>{/if}{if $result_found gt 0 && $tbl_order_promotionslist}	<table class="listTable">		<tr><th class="bigListItem">{$_lang.tbl_order_promotions.title}</th>		<th class="actionListItem"></th></tr>		{foreach from=$tbl_order_promotionslist item=tbl_order_promotionsitem}			<tr>				<td class="bigListItem"><a href="{$page_url}?mode={$smarty.const.MODE_VIEW}&ordprom_id={$tbl_order_promotionsitem.ordprom_id}">{$tbl_order_promotionsitem.ordprom_order}</a></td>				<td class="actionListItem"><a href="{$page_url}?mode={$smarty.const.MODE_UPDATE}&ordprom_id={$tbl_order_promotionsitem.ordprom_id}" class="editIcon"></a><a class="deleteIcon" href="#" onclick="deletetbl_order_promotions({$tbl_order_promotionsitem.ordprom_id});"></a>{if $tbl_order_promotionsitem.isActive eq 1}<a href="{$page_url}?action={$smarty.const.ACTION_DEACTIVATE}&ordprom_id={$tbl_order_promotionsitem.ordprom_id}" class="deactiveIcon" title="{$_lang.tbl_order_promotions.DEACTIVATE.BTN_LBL}"></a>{else}<a href="{$page_url}?action={$smarty.const.ACTION_ACTIVATE}&ordprom_id={$tbl_order_promotionsitem.ordprom_id}" class="activeIcon" title="{$_lang.tbl_order_promotions.ACTIVATE.BTN_LBL}"></a>{/if}</td>			</tr>	{/foreach}	</table>{else}	<div class="error">{$_lang.tbl_order_promotions.no_record_found}</div>{/if}{if $pagination}	<center>{$pagination}</center>{/if}<center><input data-icon="add-item" data-inline="true" onclick="window.location.href='{$page_url}?mode={$smarty.const.MODE_CREATE}'" type="button" value="{$_lang.tbl_order_promotions.CREATE.BTN_LBL}"/></center></div>{include file="tbl_order_promotions/js.tpl"}{include file="footer.tpl"}