{literal}<script type="text/javascript">function deletetbl_ord_refund(varId){	if(varId > 0){		if(confirm("{/literal}{$_lang.tbl_ord_refund.DELETE.CONFIRM_MSG}{literal}")==true){			window.location.href="{/literal}{$page_url}{literal}?action=delete&refund_id="+varId;		}	}}function validatetbl_ord_refund(){	$("#refund_id_err").html("");	$("#refund_paypal_txn_id_err").html("");	$("#refund_note_err").html("");	$("#refund_amnt_err").html("");	$("#refund_start_date_err").html("");	$("#refund_end_date_err").html("");	var isErr = true;	if(elemById("action").value=="update"){		if(IsNonEmpty(elemById("refund_id").value)==false){			$("#refund_id_err").html("{/literal}{$_lang.tbl_ord_refund.not_empty_msg.refund_id}{literal}");			isErr = false;		}	}	if(IsNonEmpty(elemById("refund_paypal_txn_id").value)==false){		$("#refund_paypal_txn_id_err").html("{/literal}{$_lang.tbl_ord_refund.not_empty_msg.refund_paypal_txn_id}{literal}");		isErr = false;	}	if(IsNonEmpty(elemById("refund_note").value)==false){		$("#refund_note_err").html("{/literal}{$_lang.tbl_ord_refund.not_empty_msg.refund_note}{literal}");		isErr = false;	}	if(IsNonEmpty(elemById("refund_amnt").value)==false){		$("#refund_amnt_err").html("{/literal}{$_lang.tbl_ord_refund.not_empty_msg.refund_amnt}{literal}");		isErr = false;	}/*	if(IsNonEmpty(elemById("refund_start_date").value)==false){		$("#refund_start_date_err").html("{/literal}{$_lang.tbl_ord_refund.not_empty_msg.refund_start_date}{literal}");		isErr = false;	}*//*	if(IsNonEmpty(elemById("refund_end_date").value)==false){		$("#refund_end_date_err").html("{/literal}{$_lang.tbl_ord_refund.not_empty_msg.refund_end_date}{literal}");		isErr = false;	}*/	if(isErr == false){		alert("{/literal}{$_lang.messages.revise_form}{literal}");	}	return isErr;}//..function</script>{/literal}