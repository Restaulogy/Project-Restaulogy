{include file='header.tpl'}<div class="wrapper"><h1>{$_lang.tbl_ord_refund.listing_title}</h1>{if $error_msg}	<center>{$error_msg}</center>{/if}{if $result_found gt 0 && $tbl_ord_refundlist}	<table class="listTable">		<tr><th class="bigListItem">{$_lang.tbl_ord_refund.title}</th>		<th class="actionListItem"></th></tr>		{foreach from=$tbl_ord_refundlist item=tbl_ord_refunditem}			<tr>				<td class="bigListItem"><a href="{$page_url}?mode={$smarty.const.MODE_VIEW}&refund_id={$tbl_ord_refunditem.refund_id}">{$tbl_ord_refunditem.refund_paypal_txn_id}</a></td>				<td class="actionListItem"><a href="{$page_url}?mode={$smarty.const.MODE_UPDATE}&refund_id={$tbl_ord_refunditem.refund_id}" class="editIcon"></a><a class="deleteIcon" href="#" onclick="deletetbl_ord_refund({$tbl_ord_refunditem.refund_id});"></a>{if $tbl_ord_refunditem.isActive eq 1}<a href="{$page_url}?action={$smarty.const.ACTION_DEACTIVATE}&refund_id={$tbl_ord_refunditem.refund_id}" class="deactiveIcon" title="{$_lang.tbl_ord_refund.DEACTIVATE.BTN_LBL}"></a>{else}<a href="{$page_url}?action={$smarty.const.ACTION_ACTIVATE}&refund_id={$tbl_ord_refunditem.refund_id}" class="activeIcon" title="{$_lang.tbl_ord_refund.ACTIVATE.BTN_LBL}"></a>{/if}</td>			</tr>	{/foreach}	</table>{else}	<div class="error">{$_lang.tbl_ord_refund.no_record_found}</div>{/if}{if $pagination}	<center>{$pagination}</center>{/if}<center><input data-icon="add-item" data-inline="true" onclick="window.location.href='{$page_url}?mode={$smarty.const.MODE_CREATE}'" type="button" value="{$_lang.tbl_ord_refund.CREATE.BTN_LBL}"/></center></div>{include file="tbl_ord_refund/js.tpl"}{include file="footer.tpl"}