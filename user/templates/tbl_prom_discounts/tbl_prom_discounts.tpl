{include file='header.tpl'}<div class="wrapper"><h1>{$_lang.tbl_prom_discounts.listing_title}</h1>{if $error_msg}	<center>{$error_msg}</center>{/if}{if $result_found gt 0 && $tbl_prom_discountslist}	<table class="listTable">		<tr><th class="bigListItem">{$_lang.tbl_prom_discounts.title}</th>		<th class="actionListItem"></th></tr>		{foreach from=$tbl_prom_discountslist item=tbl_prom_discountsitem}			<tr>				<td class="bigListItem"><a href="{$page_url}?mode={$smarty.const.MODE_VIEW}&prmdisc_id={$tbl_prom_discountsitem.prmdisc_id}">{$tbl_prom_discountsitem.prmdisc_promotion}</a></td>				<td class="actionListItem"><a href="{$page_url}?mode={$smarty.const.MODE_UPDATE}&prmdisc_id={$tbl_prom_discountsitem.prmdisc_id}" class="editIcon"></a><a class="deleteIcon" href="#" onclick="deletetbl_prom_discounts({$tbl_prom_discountsitem.prmdisc_id});"></a>{if $tbl_prom_discountsitem.isActive eq 1}<a href="{$page_url}?action={$smarty.const.ACTION_DEACTIVATE}&prmdisc_id={$tbl_prom_discountsitem.prmdisc_id}" class="deactiveIcon" title="{$_lang.tbl_prom_discounts.DEACTIVATE.BTN_LBL}"></a>{else}<a href="{$page_url}?action={$smarty.const.ACTION_ACTIVATE}&prmdisc_id={$tbl_prom_discountsitem.prmdisc_id}" class="activeIcon" title="{$_lang.tbl_prom_discounts.ACTIVATE.BTN_LBL}"></a>{/if}</td>			</tr>	{/foreach}	</table>{else}	<div class="error">{$_lang.tbl_prom_discounts.no_record_found}</div>{/if}{if $pagination}	<center>{$pagination}</center>{/if}<center><input data-icon="add-item" data-inline="true" onclick="window.location.href='{$page_url}?mode={$smarty.const.MODE_CREATE}'" type="button" value="{$_lang.tbl_prom_discounts.CREATE.BTN_LBL}"/></center></div>{include file="tbl_prom_discounts/js.tpl"}{include file="footer.tpl"}