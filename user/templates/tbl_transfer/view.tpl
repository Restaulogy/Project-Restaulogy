{include file="header.tpl"}<div class="wrapper"><h1>{$_lang.tbl_transfer.title}</h1>{if $error_msg}	<center>{$error_msg}</center>{/if}<form name="frmupdatetbl_transfer" id="frmupdatetbl_transfer" onsubmit="return validatetbl_transfer();" method="POST" action="{$page_url}" style="{if $isUpdate eq 1}{else}display:none;{/if}">	<input type="hidden" name="trans_id" id="trans_id" value="{$tbl_transferinfo.trans_id}"/>	<div style="display:none;" class="error" id="trans_id_err"></div>	<div class="field-row">		<label for="trans_table">{$_lang.tbl_transfer.label.trans_table}</label>		<input type="text" name="trans_table" id="trans_table" value="{$tbl_transferinfo.trans_table}"/>		<div class="error" id="trans_table_err"></div>	</div>	<div class="field-row">		<label for="trans_old_server">{$_lang.tbl_transfer.label.trans_old_server}</label>		<input type="text" name="trans_old_server" id="trans_old_server" value="{$tbl_transferinfo.trans_old_server}"/>		<div class="error" id="trans_old_server_err"></div>	</div>	<div class="field-row">		<label for="trans_new_server">{$_lang.tbl_transfer.label.trans_new_server}</label>		<input type="text" name="trans_new_server" id="trans_new_server" value="{$tbl_transferinfo.trans_new_server}"/>		<div class="error" id="trans_new_server_err"></div>	</div>	<div class="field-row">		<label for="trans_created_by">{$_lang.tbl_transfer.label.trans_created_by}</label>		<input type="text" name="trans_created_by" id="trans_created_by" value="{$tbl_transferinfo.trans_created_by}"/>		<div class="error" id="trans_created_by_err"></div>	</div>	<div class="field-row">		<label for="trans_time_from">{$_lang.tbl_transfer.label.trans_time_from}</label>		<input type="text" name="trans_time_from" id="trans_time_from" value="{$tbl_transferinfo.trans_time_from}"/>		<div class="error" id="trans_time_from_err"></div>	</div>	<div class="field-row">		<label for="trans_time_to">{$_lang.tbl_transfer.label.trans_time_to}</label>		<input type="text" name="trans_time_to" id="trans_time_to" value="{$tbl_transferinfo.trans_time_to}"/>		<div class="error" id="trans_time_to_err"></div>	</div>	<!--	<div class="field-row">		<label for="trans_start_date">{$_lang.tbl_transfer.label.trans_start_date}</label>		<input type="text" name="trans_start_date" id="trans_start_date" value="{$tbl_transferinfo.trans_start_date}"/>		<div class="error" id="trans_start_date_err"></div>	</div>	-->	<!--	<div class="field-row">		<label for="trans_end_date">{$_lang.tbl_transfer.label.trans_end_date}</label>		<input type="text" name="trans_end_date" id="trans_end_date" value="{$tbl_transferinfo.trans_end_date}"/>		<div class="error" id="trans_end_date_err"></div>	</div>	-->	<center><input type="hidden" id="action" name="action" value="{$smarty.const.ACTION_UPDATE}"/> <input  data-inline="true" data-icon="save"  type="submit" value="{$_lang.save_lbl}"/> <input data-inline="true" data-icon="delete" type="reset" onclick="$('#tbl_transfer_view').show();$('#frmupdatetbl_transfer').hide();" value="{$_lang.cancel_lbl}"/></center></form><div id="tbl_transfer_view" class="description" style="{if $isUpdate eq 1}display:none;{/if}">	<table class="listTable">		<tr><th class="fieldItem">{$_lang.field_title}</th><th class="valueItem">{$_lang.value_title}</th></tr>		<tr><td class="fieldItem">{$_lang.tbl_transfer.label.trans_id}<i>:</i></td><td class="valueItem">{$tbl_transferinfo.trans_id}</td></tr>		<tr><td class="fieldItem">{$_lang.tbl_transfer.label.trans_table}<i>:</i></td><td class="valueItem">{$tbl_transferinfo.trans_table}</td></tr>		<tr><td class="fieldItem">{$_lang.tbl_transfer.label.trans_old_server}<i>:</i></td><td class="valueItem">{$tbl_transferinfo.trans_old_server}</td></tr>		<tr><td class="fieldItem">{$_lang.tbl_transfer.label.trans_new_server}<i>:</i></td><td class="valueItem">{$tbl_transferinfo.trans_new_server}</td></tr>		<tr><td class="fieldItem">{$_lang.tbl_transfer.label.trans_created_by}<i>:</i></td><td class="valueItem">{$tbl_transferinfo.trans_created_by}</td></tr>		<tr><td class="fieldItem">{$_lang.tbl_transfer.label.trans_time_from}<i>:</i></td><td class="valueItem">{$tbl_transferinfo.trans_time_from}</td></tr>		<tr><td class="fieldItem">{$_lang.tbl_transfer.label.trans_time_to}<i>:</i></td><td class="valueItem">{$tbl_transferinfo.trans_time_to}</td></tr>	<!--		<tr><td class="fieldItem">{$_lang.tbl_transfer.label.trans_start_date}<i>:</i></td><td class="valueItem">{$tbl_transferinfo.trans_start_date}</td></tr>	-->	<!--		<tr><td class="fieldItem">{$_lang.tbl_transfer.label.trans_end_date}<i>:</i></td><td class="valueItem">{$tbl_transferinfo.trans_end_date}</td></tr>	-->	<tr><td class="fieldItem">{$_lang.tbl_transfer.label.isActive}<i>:</i></td><td class="valueItem">{if $tbl_transferinfo.isActive eq 1}{$_lang.tbl_transfer.label.isActive_yes}{else}{$_lang.tbl_transfer.label.isActive_no}{/if}</td></tr>	</table>	<center><input data-inline="true" data-icon="edit" type="button" value="{$_lang.tbl_transfer.UPDATE.BTN_LBL}" onclick="$('#tbl_transfer_view').hide();$('#frmupdatetbl_transfer').show();"/><input type="button" value="{$_lang.cancel_lbl}" data-inline="true" data-icon="delete" onclick="window.location.href='{$page_url}'"/></center></div>{include file="tbl_transfer/js.tpl"}</div>{include file="footer.tpl"}