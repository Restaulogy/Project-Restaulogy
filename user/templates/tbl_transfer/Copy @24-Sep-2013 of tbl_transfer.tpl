{include file='header.tpl'}<div class="wrapper"><h1>{$_lang.tbl_transfer.listing_title}</h1>{if $error_msg}	<center>{$error_msg}</center>{/if}{if $result_found gt 0 && $tbl_transferlist}	<table class="listTable">		<tr><th class="bigListItem">{$_lang.tbl_transfer.title}</th>		<th class="actionListItem"></th></tr>		{foreach from=$tbl_transferlist item=tbl_transferitem}			<tr>				<td class="bigListItem"><a href="{$page_url}?mode={$smarty.const.MODE_VIEW}&trans_id={$tbl_transferitem.trans_id}">{$tbl_transferitem.trans_table}</a></td>				<td class="actionListItem"><a href="{$page_url}?mode={$smarty.const.MODE_UPDATE}&trans_id={$tbl_transferitem.trans_id}" class="editIcon"></a><a class="deleteIcon" href="#" onclick="deletetbl_transfer({$tbl_transferitem.trans_id});"></a>{if $tbl_transferitem.isActive eq 1}<a href="{$page_url}?action={$smarty.const.ACTION_DEACTIVATE}&trans_id={$tbl_transferitem.trans_id}" class="deactiveIcon" title="{$_lang.tbl_transfer.DEACTIVATE.BTN_LBL}"></a>{else}<a href="{$page_url}?action={$smarty.const.ACTION_ACTIVATE}&trans_id={$tbl_transferitem.trans_id}" class="activeIcon" title="{$_lang.tbl_transfer.ACTIVATE.BTN_LBL}"></a>{/if}</td>			</tr>	{/foreach}	</table>{else}	<div class="error">{$_lang.tbl_transfer.no_record_found}</div>{/if}{if $pagination}	<center>{$pagination}</center>{/if}<center><input data-icon="add-item" data-inline="true" onclick="window.location.href='{$page_url}?mode={$smarty.const.MODE_CREATE}'" type="button" value="{$_lang.tbl_transfer.CREATE.BTN_LBL}"/></center></div>{include file="tbl_transfer/js.tpl"}{include file="footer.tpl"}