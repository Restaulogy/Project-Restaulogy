{include file='header.tpl'}<div class="wrapper"><h1>{$_lang.tbl_transfer.listing_title}</h1>{if $error_msg}	<center>{$error_msg}</center>{/if}{include file="tbl_transfer/tbl_tabs.tpl"}{if $result_found gt 0 && $tbl_emp_shift_assignmentlist}	<table class="biz_data_grid">		<tr>			<th class="{if $smarty.get.sort_on eq 'trans_table'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=trans_table&sort_by={$new_sort}">{$_lang.tbl_transfer.label.trans_server}</a></th>			<th class="{if $smarty.get.sort_on eq 'trans_table'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=trans_table&sort_by={$new_sort}">{$_lang.tbl_transfer.label.trans_table}</a></th>		</tr>		{foreach from=$tbl_emp_shift_assignmentlist item=tbl_emp_shift_assignmentitem}		<tr class="{cycle values="odd,even"}">			<td><a href="#" onclick="popupemp_shift_assignment({$tbl_emp_shift_assignmentitem.emp_sft_id},0,'{$tbl_emp_shift_assignmentitem.employee_name}');" >{$tbl_emp_shift_assignmentitem.employee_name}</a></td>			<td>{$tbl_emp_shift_assignmentitem.table_list}</td>		</tr>	{/foreach}		<tfoot>			<tr>				<td colspan="9">					# {$result_found}&nbsp;&nbsp;&nbsp;{if $pagination neq ""}{$pagination}{/if}					<select onchange="changePage('{$navigationURL}',this.value,{$smarty.request.limit});">					{if $allPageCount gt 1}						{for $foo=1 to $allPageCount}							<option value="{$foo}" {if $foo eq $currentPage}selected="selected"{/if}>{$foo}</option>						{/for}					{else}						<option value="1" disabled="disabled">1</option>					{/if}					</select>				</td>							</tr>		</tfoot>	</table>{else}	<div class="error">{$_lang.tbl_transfer.no_record_found}</div>{/if}<!--<center><input data-icon="add-item" data-inline="true" onclick="window.location.href='{$page_url}?mode={$smarty.const.MODE_CREATE}'" type="button" value="{$_lang.tbl_transfer.CREATE.BTN_LBL}"/></center>--></div>{include file="tbl_transfer/popupserver.tpl"}{include file="tbl_transfer/js.tpl"}{include file="footercontent.tpl"}{literal}    <script type="text/javascript">        $(function(){ 				         	$("#trans_time_from").scroller({ preset: 'time', timeFormat: 'HH:ii:ss', timeWheels: 'HHii', animate: 'pop'});        	$("#trans_time_to").scroller({ preset: 'time', timeFormat: 'HH:ii:ss', timeWheels: 'HHii', animate: 'pop'});        })    </script>{/literal}</body></html>