{include file='header.tpl'}<div class="wrapper"><h1>{$_lang.tbl_transfer.listing_title}</h1>{if $error_msg}	<div class="biz_center">{$error_msg}</div>{/if}{include file="tbl_transfer/tbl_tabs.tpl"}{if $result_found gt 0 && $tablewiselist}	<table class="biz_data_grid">		<tr>			<th>{$_lang.tbl_transfer.label.trans_table}</th>			<th>{$_lang.tbl_transfer.label.trans_server}</th>		</tr>		{foreach from=$tablewiselist item=empsfttable}		<tr class="{cycle values="odd,even"}">			<td><a href="#" onclick="popupemp_shift_assignment({$empsfttable.emp_sft_id},{$empsfttable.emp_sft_tbl_table},'{$empsfttable.table_number}');" >{$empsfttable.table_number}</a></td>			<td>{$empsfttable.employee_name}</td>		</tr>	{/foreach}		<tfoot>			<tr>				<td colspan="9">					# {$result_found}&nbsp;&nbsp;&nbsp;{if $pagination neq ""}{$pagination}{/if}					<select onchange="changePage('{$navigationURL}',this.value,{$smarty.request.limit});">					{if $allPageCount gt 1}						{for $foo=1 to $allPageCount}							<option value="{$foo}" {if $foo eq $currentPage}selected="selected"{/if}>{$foo}</option>						{/for}					{else}						<option value="1" disabled="disabled">1</option>					{/if}					</select>				</td>							</tr>		</tfoot>	</table>{else}	<div class="error">{$_lang.tbl_transfer.no_record_found}</div>{/if} </div> {include file="tbl_transfer/popupserver.tpl"}{include file="tbl_transfer/js.tpl"}{include file="footercontent.tpl"}{literal}    <script>        $(function(){        	$("#trans_time_from").scroller({ preset: 'time', timeFormat: 'HH:ii:ss', timeWheels: 'HHii', animate: 'pop'});        	$("#trans_time_to").scroller({ preset: 'time', timeFormat: 'HH:ii:ss', timeWheels: 'HHii', animate: 'pop'});        })    </script>{/literal}</body></html>