{include file='header.tpl'}<div class="wrapper"><h1>{$_lang.tbl_transfer.listing_title}</h1>{if $error_msg}	<center>{$error_msg}</center>{/if}{if $result_found gt 0 && $tbl_transferlist}	<table class="biz_data_grid">		<tr>			<th class="{if $smarty.get.sort_on eq 'trans_table'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=trans_table&sort_by={$new_sort}">{$_lang.tbl_transfer.label.trans_table}</a></th>			<th class="{if $smarty.get.sort_on eq 'trans_old_server'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=trans_old_server&sort_by={$new_sort}">{$_lang.tbl_transfer.label.trans_old_server}</a></th>			<th class="{if $smarty.get.sort_on eq 'trans_new_server'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=trans_new_server&sort_by={$new_sort}">{$_lang.tbl_transfer.label.trans_new_server}</a></th>			<th class="{if $smarty.get.sort_on eq 'trans_created_by'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=trans_created_by&sort_by={$new_sort}">{$_lang.tbl_transfer.label.trans_created_by}</a></th>			<th class="{if $smarty.get.sort_on eq 'trans_time_from'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=trans_time_from&sort_by={$new_sort}">{$_lang.tbl_transfer.label.trans_time_from}</a></th>			<th class="{if $smarty.get.sort_on eq 'trans_time_to'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=trans_time_to&sort_by={$new_sort}">{$_lang.tbl_transfer.label.trans_time_to}</a></th>			<th class="action_header">Action</th>		</tr>		{foreach from=$tbl_transferlist item=tbl_transferitem}		<tr class="{cycle values="odd,even"}">			<td><a href="{$page_url}?mode={$smarty.const.MODE_VIEW}&trans_id={$tbl_transferitem.trans_id}">{$tbl_transferitem.trans_table}</a></td>			<td>{$tbl_transferitem.trans_old_server}</td>			<td>{$tbl_transferitem.trans_new_server}</td>			<td>{$tbl_transferitem.trans_created_by}</td>			<td>{$tbl_transferitem.trans_time_from}</td>			<td>{$tbl_transferitem.trans_time_to}</td>			<td><a href="{$page_url}?mode={$smarty.const.MODE_UPDATE}&trans_id={$tbl_transferitem.trans_id}" class="editIcon"></a><a class="deleteIcon" href="#" onclick="deletetbl_transfer({$tbl_transferitem.trans_id});"></a>{if $tbl_transferitem.isActive eq 1}<a href="{$page_url}?action={$smarty.const.ACTION_DEACTIVATE}&trans_id={$tbl_transferitem.trans_id}" class="deactiveIcon" title="{$_lang.tbl_transfer.DEACTIVATE.BTN_LBL}"></a>{else}<a href="{$page_url}?action={$smarty.const.ACTION_ACTIVATE}&trans_id={$tbl_transferitem.trans_id}" class="activeIcon" title="{$_lang.tbl_transfer.ACTIVATE.BTN_LBL}"></a>{/if}</td>		</tr>	{/foreach}		<tfoot>			<tr>				<td colspan="9">					# {$result_found}&nbsp;&nbsp;&nbsp;{if $pagination neq ""}{$pagination}{/if}					<select onchange="changePage('{$navigationURL}',this.value,{$smarty.request.limit});">					{if $allPageCount gt 1}						{for $foo=1 to $allPageCount}							<option value="{$foo}" {if $foo eq $currentPage}selected="selected"{/if}>{$foo}</option>						{/for}					{else}						<option value="1" disabled="disabled">1</option>					{/if}					</select>				</td>			</tr>		</tfoot>	</table>{else}	<div class="error">{$_lang.tbl_transfer.no_record_found}</div>{/if}<center><input data-icon="add-item" data-inline="true" onclick="window.location.href='{$page_url}?mode={$smarty.const.MODE_CREATE}'" type="button" value="{$_lang.tbl_transfer.CREATE.BTN_LBL}"/></center></div>{include file="tbl_transfer/js.tpl"}{include file="footer.tpl"}