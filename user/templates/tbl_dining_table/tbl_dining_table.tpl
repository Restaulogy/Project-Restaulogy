{include file='header.tpl'}<div class="wrapper"><h1>{$_lang.tbl_dining_table.listing_title}</h1> {if $error_msg}	<center>{$error_msg}</center>{/if}{include file="tbl_dining_table/tabs.tpl"}<form name="frm_tbl_dining_tablelist" id="frm_tbl_dining_tablelist"  method="POST" action="{$page_url}" >{if $result_found gt 0 && $tbl_dining_tablelist}	<table class="listTable">		<tr><th class="bigListItem" colspan="2">{$_lang.tbl_dining_table.title}</th>		<th>Table Type</th>		<th>Max. Seat Available</th>		<th style="width:5%;">            {$_lang.main.status}    	</th>		</tr>		{foreach from=$tbl_dining_tablelist item=tbl_dining_tableitem}			<tr>				<td style="width:2%;">				<label for="sel_tbl_dining_table[{$tbl_dining_tableitem.table_id}]" data-mini="true" style="width:23px;"><input type="checkbox" data-inline='true' data-mini='true' id="sel_tbl_dining_table[{$tbl_dining_tableitem.table_id}]" name="sel_tbl_dining_table[{$tbl_dining_tableitem.table_id}]" />&nbsp;</label>                </td>								<td style="width:30%;"><a href="{$page_url}?mode=view&table_id={$tbl_dining_tableitem.table_id}">{$tbl_dining_tableitem.table_number}</a></td>				<td style="width:40%;">					  {$tbl_dining_tableitem.table_type_name}				</td>				<td style="width:28%;">					  {$tbl_dining_tableitem.table_seating_capacity}				</td>				<td style="width:5%;">                    {if $tbl_dining_tableitem.isActive eq 1}                        <a href="#" class="activeIcon" title="{$_lang.tbl_dining_table.ACTIVATE.BTN_LBL}"></a>                    {else}                        <a href="#" class="deactiveIcon" title="{$_lang.tbl_dining_table.DEACTIVATE.BTN_LBL}"></a>                    {/if}				</td>				<!--<td class="actionListItem"><a href="{$page_url}?mode=view&table_id={$tbl_dining_tableitem.table_id}" class="detailIcon"></a><a href="{$page_url}?mode=update&table_id={$tbl_dining_tableitem.table_id}" class="editIcon"></a><a class="deleteIcon" href="#" onclick="deletetbl_dining_table({$tbl_dining_tableitem.table_id});"></a>{if $tbl_dining_tableitem.isActive eq 1}<a href="{$page_url}?action=deactivate&table_id={$tbl_dining_tableitem.table_id}" class="deactiveIcon" title="{$_lang.tbl_dining_table.DEACTIVATE.BTN_LBL}"></a>{else}<a href="{$page_url}?action=activate&table_id={$tbl_dining_tableitem.table_id}" class="activeIcon" title="{$_lang.tbl_dining_table.ACTIVATE.BTN_LBL}"></a>{/if}</td>-->			</tr>	{/foreach}	</table>{else}	<div class="error">{$_lang.tbl_dining_table.no_record_found}</div>{/if}{if $pagination}	<center>{$pagination}</center>{/if}<input type='hidden' name='action' id='action' value='' /></form><center>{if $result_found gt 0 && $tbl_dining_tablelist}    <input data-inline="true" data-icon="briefcase" type="button" id="sel_all_tbl_dining_table" name="sel_all_tbl_dining_table" value="{$_lang.main.toggle}" onclick="javascript:$('input[type=checkbox]').click();" /><input data-inline="true" data-icon="inactive" type="button" value="{$_lang.tbl_dishes.DEACTIVATE.BTN_LBL}" onclick="actiontbl_dining_table('{$smarty.const.ACTION_DEACTIVATE}');" />    <input data-inline="true" data-icon="active" type="button" value="{$_lang.tbl_dishes.ACTIVATE.BTN_LBL}" onclick="actiontbl_dining_table('{$smarty.const.ACTION_ACTIVATE}');" />    <!--        <input data-inline="true" data-icon="delete" type="button" id="del_sel_all_tbl_dining_table" name="del_sel_all_tbl_dining_table" value="{$_lang.tbl_dining_table.DELETE.BTN_LBL}" onclick="deletetbl_dining_table({$tbl_dining_tableitem.table_id});"/>    -->{/if}    <input data-inline="true" data-icon="add-item" onclick="window.location.href='{$page_url}?mode=new'" type="button" value="{$_lang.tbl_dining_table.CREATE.BTN_LBL}" />    <!--    <input type="button" value="{$_lang.lbl_manage}&nbsp;{$_lang.tbl_table_type.title}" data-inline="true" data-icon="configuration" onclick="window.location.href='{$website}/user/tbl_table_type.php'"/>--></center></div>{include file="tbl_dining_table/js.tpl"}{*include file="footer.tpl"*}{if $tbl_layouts}<div id="js_hidden">	{foreach $tbl_layouts as $tbl_layout}		 {$tbl_layout}	{/foreach}</div>{include file='footercontent.tpl'}<script type="text/javascript" src="{$website}/js/html2canvas.min.js"></script>{literal}<script type="text/javascript">  $(function(){	  var params = {};	  {/literal}		{assign var="cnt" value=0}		{foreach $tbl_layouts as $tbl_layout}			{literal}			html2canvas([elemById("{/literal}tbl_layout_{$tbl_layout@key}{literal}")], {	        proxy: "https://html2canvas.appspot.com/query",	        onrendered: function(canvas) {	            var img = canvas.toDataURL("image/png");	            var output = img.replace(/^data:image\/(png|jpg);base64,/, "");	            var output = encodeURIComponent(img);				var Parameters = "&name[{/literal}{$cnt}{literal}]={/literal}{$tbl_layout@key}{literal}&image[{/literal}{$cnt}{literal}]=" + output;				params["{/literal}{$tbl_layout@key}{literal}"] = Parameters;			 }			});			{/literal}			{math assign="cnt" equation="x+1" x=$cnt}		{/foreach}		{literal}		setTimeout(function(){//var str = "filedir=" + "{/literal}{$smarty.const.PATHROOT}{literal}/images/tables";		var str = "filedir=images/tables";		for(i in params){			str = str + params[i];		}	    $.ajax({              type: "POST",              url: "{/literal}{$website}{literal}/ajax/getimgsrc.php",              data: str,    		  		dataType : 'json',              success : function(data){	              //alert(data);								if(data){									for ( i in data){										document.getElementById("drag" + i).src = data[i];									   //$('#drag' + i).attr('src',data[i]).trigger('create');									}								}	                //console.log("screenshot done");									//alert('done');	              },	              error : function (objrespone){	                  alert(objrespone.responseText);	              }          });        $('#js_hidden').hide();		},200);	});	</script>{/literal}{else}	{include file='footercontent.tpl'}{/if}</body>	</html>