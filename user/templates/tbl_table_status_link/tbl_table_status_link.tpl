{include file='header.tpl'}<div class="wrapper"><h1>{$_lang.tbl_table_status_link.listing_title}</h1>{if $error_msg}	<center>{$error_msg}</center>{/if}{if $result_found gt 0 && $tbl_table_status_linklist}	<table class="listTable">		<tr><th class="bigListItem">{$_lang.tbl_table_status_link.title}</th>		<th class="actionListItem"></th></tr>		{foreach from=$tbl_table_status_linklist item=tbl_table_status_linkitem}			<tr>				<td class="bigListItem"><a href="{$page_url}?mode={$smarty.const.MODE_VIEW}&tbl_sts_lnk_id={$tbl_table_status_linkitem.tbl_sts_lnk_id}">{$tbl_table_status_linkitem.tbl_sts_lnk_table_id}</a></td>				<td class="actionListItem"><a href="{$page_url}?mode={$smarty.const.MODE_UPDATE}&tbl_sts_lnk_id={$tbl_table_status_linkitem.tbl_sts_lnk_id}" class="editIcon"></a><a class="deleteIcon" href="#" onclick="deletetbl_table_status_link({$tbl_table_status_linkitem.tbl_sts_lnk_id});"></a>{if $tbl_table_status_linkitem.isActive eq 1}<a href="{$page_url}?action={$smarty.const.ACTION_DEACTIVATE}&tbl_sts_lnk_id={$tbl_table_status_linkitem.tbl_sts_lnk_id}" class="deactiveIcon" title="{$_lang.tbl_table_status_link.DEACTIVATE.BTN_LBL}"></a>{else}<a href="{$page_url}?action={$smarty.const.ACTION_ACTIVATE}&tbl_sts_lnk_id={$tbl_table_status_linkitem.tbl_sts_lnk_id}" class="activeIcon" title="{$_lang.tbl_table_status_link.ACTIVATE.BTN_LBL}"></a>{/if}</td>			</tr>	{/foreach}	</table>{else}	<div class="error">{$_lang.tbl_table_status_link.no_record_found}</div>{/if}{if $pagination}	<center>{$pagination}</center>{/if}<input onclick="window.location.href='{$page_url}?mode={$smarty.const.MODE_CREATE}'" data-inline="true" data-icon="add-item" type="button" value="{$_lang.tbl_table_status_link.CREATE.BTN_LBL}"/></div>{include file="tbl_table_status_link/js.tpl"}{include file="footer.tpl"}