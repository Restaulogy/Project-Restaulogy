{literal}<script type="text/javascript">function editOptionBox(id,value){	if(id > 0){		$('#popupHeader').html('Edit Option Value').trigger('refresh');		$('#dish_opt_val_id').val(id); 		$('#dish_opt_val_value').val(value);		$('#action').val("{/literal}{$smarty.const.ACTION_UPDATE}{literal}");		$('#popupAdd').popup('open'); 	}	}function newOptionBox(){	 $('#popupHeader').html('Add Option Value').trigger('refresh');		$('#dish_opt_val_id').val(''); 		$('#dish_opt_val_value').val('');			$('#action').val("{/literal}{$smarty.const.ACTION_CREATE}{literal}");		$('#popupAdd').popup('open');}function deletetbl_dish_options_values(){ 	var varId = $("input:checked").length;	if(varId > 0){		if(confirm("{/literal}{$_lang.tbl_dish_options_values.DELETE.CONFIRM_MSG}{literal}")==true){				$('#set_action').val("{/literal}{$smarty.const.ACTION_DELETE}{literal}");			$('#frm_tbl_dish_options_values').submit();		}	}else{		alert("{/literal}{$_lang.main.select_ids.empty}{literal}");	}}/*function deletetbl_dish_options_values(varId){	if(varId > 0){		if(confirm("{/literal}{$_lang.tbl_dish_options_values.DELETE.CONFIRM_MSG}{literal}")==true){			window.location.href="{/literal}{$page_url}{literal}?action=delete&dish_opt_val_id="+varId;		}	}}*/function validatetbl_dish_options_values(){	$("#dish_opt_val_id_err").html("");	$("#dish_opt_val_option_id_err").html("");	$("#dish_opt_val_value_err").html("");	var isErr = true;	if(elemById("action").value=="update"){		if(IsNonEmpty(elemById("dish_opt_val_id").value)==false){			$("#dish_opt_val_id_err").html("{/literal}{$_lang.tbl_dish_options_values.not_empty_msg.dish_opt_val_id}{literal}");			isErr = false;		}	}	if(IsNonEmpty(elemById("dish_opt_val_option_id").value)==false){		$("#dish_opt_val_option_id_err").html("{/literal}{$_lang.tbl_dish_options_values.not_empty_msg.dish_opt_val_option_id}{literal}");		isErr = false;	}	if(IsNonEmpty(elemById("dish_opt_val_value").value)==false){		$("#dish_opt_val_value_err").html("{/literal}{$_lang.tbl_dish_options_values.not_empty_msg.dish_opt_val_value}{literal}");		isErr = false;	}	if(isErr == false){		alert("{/literal}{$_lang.messages.revise_form}{literal}");	}	return isErr;}//..function</script>{/literal}