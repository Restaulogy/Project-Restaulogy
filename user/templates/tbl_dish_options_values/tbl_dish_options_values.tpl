{include file='header.tpl'}<div class="wrapper"><h1>"{$currDishOption.dish_opt_name}" - {$_lang.tbl_dish_options_values.listing_title}</h1>{**include file="dish_navbar.tpl"**}{if $error_msg}	<center>{$error_msg}</center>{/if}{include file="tbl_dish_options_values/add.tpl"}  <br/> <form name="frm_tbl_dish_options_values" id="frm_tbl_dish_options_values" method="POST" action="{$page_url}" >{if $result_found gt 0 && $tbl_dish_options_valueslist}	<table class="listTable">		<tr><th class="bigListItem" colspan="2">"{$currDishOption.dish_opt_name}" - {$_lang.tbl_dish_options_values.title}</th>		<!--<th class="actionListItem"></th>--></tr>		{foreach from=$tbl_dish_options_valueslist item=tbl_dish_options_valuesitem}			<tr>				<td style="width:2%;">					<label for="sel_dish_options_values[{$tbl_dish_options_valuesitem.dish_opt_val_id}]" data-mini="true" style="width:23px;"><input type="checkbox" data-inline='true' data-mini='true' id="sel_dish_options_values[{$tbl_dish_options_valuesitem.dish_opt_val_id}]" name="sel_dish_options_values[{$tbl_dish_options_valuesitem.dish_opt_val_id}]" />&nbsp;</label>	     	</td>				<td class="valueItem"><a href="#" onclick="editOptionBox({$tbl_dish_options_valuesitem.dish_opt_val_id},'{$tbl_dish_options_valuesitem.dish_opt_val_value}')">{$tbl_dish_options_valuesitem.dish_opt_val_value}</a></td>				<!--<td class="actionListItem"><a href="{$page_url}?mode={$smarty.const.MODE_UPDATE}&dish_opt_val_id={$tbl_dish_options_valuesitem.dish_opt_val_id}" class="editIcon"></a><a class="deleteIcon" href="#" onclick="deletetbl_dish_options_values({$tbl_dish_options_valuesitem.dish_opt_val_id});"></a>{if $tbl_dish_options_valuesitem.isActive eq 1}<a href="{$page_url}?action={$smarty.const.ACTION_DEACTIVATE}&dish_opt_val_id={$tbl_dish_options_valuesitem.dish_opt_val_id}" class="deactiveIcon" title="{$_lang.tbl_dish_options_values.DEACTIVATE.BTN_LBL}"></a>{else}<a href="{$page_url}?action={$smarty.const.ACTION_ACTIVATE}&dish_opt_val_id={$tbl_dish_options_valuesitem.dish_opt_val_id}" class="activeIcon" title="{$_lang.tbl_dish_options_values.ACTIVATE.BTN_LBL}"></a>{/if}</td>-->			</tr>	{/foreach}	</table>{else}	<div class="error">{$_lang.tbl_dish_options_values.no_record_found}</div>{/if}{if $pagination}	<center>{$pagination}</center>{/if}<input type="hidden" id="set_action" name="action" value=""/></form><div class="biz_center">		<input data-inline="true" data-icon="briefcase" type="button" id="sel_all_dish_options_values" name="sel_all_dish_options_values" value="{$_lang.main.toggle}" onclick="javascript:$('input[type=checkbox]').click();" /> 		<input data-inline="true" data-icon="recycle-full" type="button" id="del_sel_all_tbl_dish_options_values" name="del_sel_all_tbl_dish_options_values" value="{$_lang.tbl_dish_options_values.DELETE.BTN_LBL}" onclick="deletetbl_dish_options_values();" />			<input data-inline="true" data-icon="add-item"  type="button" onclick="newOptionBox();" value="{$_lang.tbl_dish_options_values.CREATE.BTN_LBL}"/> 		<input data-inline="true" data-icon="search" type="button" value="{$_lang.preview_lbl}" class="fright"  onclick="window.open('{$website}/user/tbl_dishes.php?mode={$smarty.const.MODE_VIEW}&is_preview=1&dish_id={$smarty.session[$smarty.const.SES_DISH]}')"/> </div><!--<input onclick="window.location.href='{$page_url}?mode={$smarty.const.MODE_CREATE}'" type="button" value="{$_lang.tbl_dish_options_values.CREATE.BTN_LBL}"/>--></div>{include file="tbl_dish_options_values/js.tpl"}{include file="footer.tpl"}