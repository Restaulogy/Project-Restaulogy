{include file="header.tpl"}<div class="wrapper"><h1>{$_lang.tbl_dish_options_values.create_title}</h1>{if $error_msg}	<center>{$error_msg}</center>{/if}<form name="frmcreatetbl_dish_options_values" id="frmcreatetbl_dish_options_values" onsubmit="return validatetbl_dish_options_values();" method="POST" action="{$page_url}">	<input type="hidden" name="dish_opt_val_id" id="dish_opt_val_id" value="0"/>	<div style="display:none;" class="error" id="dish_opt_val_id_err"></div>	<div class="field-row">		<label for="dish_opt_val_option_id">{$_lang.tbl_dish_options_values.label.dish_opt_val_option_id}</label>		<input type="text" name="dish_opt_val_option_id" id="dish_opt_val_option_id" value="{$smarty.post.dish_opt_val_option_id}"/>		<div class="error" id="dish_opt_val_option_id_err"></div>	</div>	<div class="field-row">		<label for="dish_opt_val_value">{$_lang.tbl_dish_options_values.label.dish_opt_val_value}</label>		<input type="text" name="dish_opt_val_value" id="dish_opt_val_value" value="{$smarty.post.dish_opt_val_value}"/>		<div class="error" id="dish_opt_val_value_err"></div>	</div>	<input type="hidden" id="action" name="action" value="{$smarty.const.ACTION_CREATE}"/><input class="fleft" type="submit" value="{$_lang.save_lbl}"/> <input  class="fright" type="reset" value="{$_lang.cancel_lbl}" onclick="window.location.href='{$page_url}'"/></form>{include file="tbl_dish_options_values/js.tpl"}</div>{include file="footer.tpl"}