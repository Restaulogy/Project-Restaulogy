<div style='padding-right:8px;'>{if $report_data.biz_checkinslist}	<table class="biz_data_grid">		<tr>            <th class="{if $smarty.get.sort_on eq 'chkin_date'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}">Name</th>            <th class="{if $smarty.get.sort_on eq 'chkin_date'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}">Email</th>			<th class="{if $smarty.get.sort_on eq 'chkin_date'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}">Award Points</th>			<th class="{if $smarty.get.sort_on eq 'chkin_date'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}">Promotion</th>			<th class="{if $smarty.get.sort_on eq 'chkin_date'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}">Redeem Points</th>			<th class="{if $smarty.get.sort_on eq 'server_name'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}">Server</th>            <th class="{if $smarty.get.sort_on eq 'chkin_date'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}">Posted</th>            <th class="{if $smarty.get.sort_on eq 'chkin_edit_commnt'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}">Comment</th>            </tr>		{foreach from=$report_data.biz_checkinslist item=biz_checkinsitem}		<tr class="{cycle values="odd,even"}">            <td>{$biz_checkinsitem.name}</td>            <td> {if $biz_checkinsitem.usr_without_email eq 0}{$biz_checkinsitem.email}{/if}</td>            <td>{$biz_checkinsitem.award_points}</td>            <td>{$biz_checkinsitem.promotion}</td>            <td>{$biz_checkinsitem.reward_point}</td>            <td>{$biz_checkinsitem.server_name}</td>			<td>{$biz_checkinsitem.posted_on|date_format:$smarty.const.HTML5_DATETIME_FORMAT}</td>			<td>{$biz_checkinsitem.chkin_edit_commnt}</td>            </tr>	{/foreach}		</table>{else}	<div class="error">{$_lang.biz_checkins.no_record_found}</div>{/if}</div>