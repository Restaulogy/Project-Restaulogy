{literal}<script type="text/javascript">function deletetbl_nutrition(varId){	if(varId > 0){		if(confirm("{/literal}{$_lang.tbl_nutrition.DELETE.CONFIRM_MSG}{literal}")==true){			window.location.href="{/literal}{$page_url}{literal}?action=delete&nutr_id="+varId;		}	}}function validatetbl_nutrition(){	$("#nutr_id_err").html("");	$("#nutr_dish_id_err").html("");	$("#nutr_total_cal_err").html("");	$("#nutr_cal_from_fat_err").html("");	$("#nutr_total_fat_err").html("");	$("#nutr_saturated_fat_err").html("");	$("#nutr_trans_fat_err").html("");	$("#nutr_cholestrol_err").html("");	$("#nutr_sodium_err").html("");	$("#nutr_total_carbohydrate_err").html("");	$("#nutr_sugar_err").html("");	$("#nutr_dietary_fiber_err").html("");	$("#nutr_protein_err").html("");	$("#nutr_start_date_err").html("");	$("#nutr_end_date_err").html("");	var isErr = true;	if(elemById("action").value=="update"){		if(IsNonEmpty(elemById("nutr_id").value)==false){			$("#nutr_id_err").html("{/literal}{$_lang.tbl_nutrition.not_empty_msg.nutr_id}{literal}");			isErr = false;		}	}	if(IsNonEmpty(elemById("nutr_dish_id").value)==false){		$("#nutr_dish_id_err").html("{/literal}{$_lang.tbl_nutrition.not_empty_msg.nutr_dish_id}{literal}");		isErr = false;	}	if(IsNonEmpty(elemById("nutr_total_cal").value)==false){       if(IsNumeric(elemById("nutr_total_cal").value)==false){		  $("#nutr_total_cal_err").html("{/literal}{$_lang.tbl_nutrition.not_empty_msg.nutr_total_cal}{literal}");		isErr = false;		}	}	if(IsNonEmpty(elemById("nutr_cal_from_fat").value)==false){        if(IsNumeric(elemById("nutr_total_cal").value)==false){		  $("#nutr_cal_from_fat_err").html("{/literal}{$_lang.tbl_nutrition.not_empty_msg.nutr_cal_from_fat}{literal}");		isErr = false;		}	}	/*	if(IsNonEmpty(elemById("nutr_start_date").value)==false){		$("#nutr_start_date_err").html("{/literal}{$_lang.tbl_nutrition.not_empty_msg.nutr_start_date}{literal}");		isErr = false;	}*//*	if(IsNonEmpty(elemById("nutr_end_date").value)==false){		$("#nutr_end_date_err").html("{/literal}{$_lang.tbl_nutrition.not_empty_msg.nutr_end_date}{literal}");		isErr = false;	}*/	if(isErr == false){		alert("{/literal}{$_lang.messages.revise_form}{literal}");	}	return isErr;}//..function</script>{/literal}