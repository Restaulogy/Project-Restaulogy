{include file='header.tpl'}<div class="wrapper"><h1>{$_lang.tbl_nutrition.listing_title}</h1>{if $error_msg}	<center>{$error_msg}</center>{/if}{if $result_found gt 0 && $tbl_nutritionlist}	<table class="listTable">		<tr><th class="bigListItem">{$_lang.tbl_nutrition.title}</th>		<th class="actionListItem"></th></tr>		{foreach from=$tbl_nutritionlist item=tbl_nutritionitem}			<tr>				<td class="bigListItem"><a href="{$page_url}?mode={$smarty.const.MODE_VIEW}&nutr_id={$tbl_nutritionitem.nutr_id}">{$tbl_nutritionitem.nutr_dish_id}</a></td>				<td class="actionListItem"><a href="{$page_url}?mode={$smarty.const.MODE_UPDATE}&nutr_id={$tbl_nutritionitem.nutr_id}" class="editIcon"></a><a class="deleteIcon" href="#" onclick="deletetbl_nutrition({$tbl_nutritionitem.nutr_id});"></a>{if $tbl_nutritionitem.isActive eq 1}<a href="{$page_url}?action={$smarty.const.ACTION_DEACTIVATE}&nutr_id={$tbl_nutritionitem.nutr_id}" class="deactiveIcon" title="{$_lang.tbl_nutrition.DEACTIVATE.BTN_LBL}"></a>{else}<a href="{$page_url}?action={$smarty.const.ACTION_ACTIVATE}&nutr_id={$tbl_nutritionitem.nutr_id}" class="activeIcon" title="{$_lang.tbl_nutrition.ACTIVATE.BTN_LBL}"></a>{/if}</td>			</tr>	{/foreach}	</table>{else}	<div class="error">{$_lang.tbl_nutrition.no_record_found}</div>{/if}{if $pagination}	<center>{$pagination}</center>{/if}<center><input data-icon="add-item" data-inline="true" onclick="window.location.href='{$page_url}?mode={$smarty.const.MODE_CREATE}'" type="button" value="{$_lang.tbl_nutrition.CREATE.BTN_LBL}"/></center></div>{include file="tbl_nutrition/js.tpl"}{include file="footer.tpl"}