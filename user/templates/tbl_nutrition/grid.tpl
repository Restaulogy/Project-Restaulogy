{include file='header.tpl'}<div class="wrapper"><h1>{$_lang.tbl_nutrition.listing_title}</h1>{if $error_msg}	<center>{$error_msg}</center>{/if}{if $result_found gt 0 && $tbl_nutritionlist}	<table class="biz_data_grid">		<tr>			<th class="{if $smarty.get.sort_on eq 'nutr_dish_id'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=nutr_dish_id&sort_by={$new_sort}">{$_lang.tbl_nutrition.label.nutr_dish_id}</a></th>			<th class="{if $smarty.get.sort_on eq 'nutr_total_cal'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=nutr_total_cal&sort_by={$new_sort}">{$_lang.tbl_nutrition.label.nutr_total_cal}</a></th>			<th class="{if $smarty.get.sort_on eq 'nutr_cal_from_fat'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=nutr_cal_from_fat&sort_by={$new_sort}">{$_lang.tbl_nutrition.label.nutr_cal_from_fat}</a></th>			<th class="{if $smarty.get.sort_on eq 'nutr_total_fat'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=nutr_total_fat&sort_by={$new_sort}">{$_lang.tbl_nutrition.label.nutr_total_fat}</a></th>			<th class="{if $smarty.get.sort_on eq 'nutr_saturated_fat'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=nutr_saturated_fat&sort_by={$new_sort}">{$_lang.tbl_nutrition.label.nutr_saturated_fat}</a></th>			<th class="{if $smarty.get.sort_on eq 'nutr_trans_fat'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=nutr_trans_fat&sort_by={$new_sort}">{$_lang.tbl_nutrition.label.nutr_trans_fat}</a></th>			<th class="{if $smarty.get.sort_on eq 'nutr_cholestrol'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=nutr_cholestrol&sort_by={$new_sort}">{$_lang.tbl_nutrition.label.nutr_cholestrol}</a></th>			<th class="{if $smarty.get.sort_on eq 'nutr_sodium'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=nutr_sodium&sort_by={$new_sort}">{$_lang.tbl_nutrition.label.nutr_sodium}</a></th>			<th class="{if $smarty.get.sort_on eq 'nutr_total_carbohydrate'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=nutr_total_carbohydrate&sort_by={$new_sort}">{$_lang.tbl_nutrition.label.nutr_total_carbohydrate}</a></th>			<th class="{if $smarty.get.sort_on eq 'nutr_sugar'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=nutr_sugar&sort_by={$new_sort}">{$_lang.tbl_nutrition.label.nutr_sugar}</a></th>			<th class="{if $smarty.get.sort_on eq 'nutr_dietary_fiber'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=nutr_dietary_fiber&sort_by={$new_sort}">{$_lang.tbl_nutrition.label.nutr_dietary_fiber}</a></th>			<th class="{if $smarty.get.sort_on eq 'nutr_protein'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=nutr_protein&sort_by={$new_sort}">{$_lang.tbl_nutrition.label.nutr_protein}</a></th>			<th class="action_header">Action</th>		</tr>		{foreach from=$tbl_nutritionlist item=tbl_nutritionitem}		<tr class="{cycle values="odd,even"}">			<td><a href="{$page_url}?mode={$smarty.const.MODE_VIEW}&nutr_id={$tbl_nutritionitem.nutr_id}">{$tbl_nutritionitem.nutr_dish_id}</a></td>			<td>{$tbl_nutritionitem.nutr_total_cal}</td>			<td>{$tbl_nutritionitem.nutr_cal_from_fat}</td>			<td>{$tbl_nutritionitem.nutr_total_fat}</td>			<td>{$tbl_nutritionitem.nutr_saturated_fat}</td>			<td>{$tbl_nutritionitem.nutr_trans_fat}</td>			<td>{$tbl_nutritionitem.nutr_cholestrol}</td>			<td>{$tbl_nutritionitem.nutr_sodium}</td>			<td>{$tbl_nutritionitem.nutr_total_carbohydrate}</td>			<td>{$tbl_nutritionitem.nutr_sugar}</td>			<td>{$tbl_nutritionitem.nutr_dietary_fiber}</td>			<td>{$tbl_nutritionitem.nutr_protein}</td>			<td><a href="{$page_url}?mode={$smarty.const.MODE_UPDATE}&nutr_id={$tbl_nutritionitem.nutr_id}" class="editIcon"></a><a class="deleteIcon" href="#" onclick="deletetbl_nutrition({$tbl_nutritionitem.nutr_id});"></a>{if $tbl_nutritionitem.isActive eq 1}<a href="{$page_url}?action={$smarty.const.ACTION_DEACTIVATE}&nutr_id={$tbl_nutritionitem.nutr_id}" class="deactiveIcon" title="{$_lang.tbl_nutrition.DEACTIVATE.BTN_LBL}"></a>{else}<a href="{$page_url}?action={$smarty.const.ACTION_ACTIVATE}&nutr_id={$tbl_nutritionitem.nutr_id}" class="activeIcon" title="{$_lang.tbl_nutrition.ACTIVATE.BTN_LBL}"></a>{/if}</td>		</tr>	{/foreach}		<tfoot>			<tr>				<td colspan="15">					# {$result_found}&nbsp;&nbsp;&nbsp;{if $pagination neq ""}{$pagination}{/if}					<select onchange="changePage('{$navigationURL}',this.value,{$smarty.request.limit});">					{if $allPageCount gt 1}						{for $foo=1 to $allPageCount}							<option value="{$foo}" {if $foo eq $currentPage}selected="selected"{/if}>{$foo}</option>						{/for}					{else}						<option value="1" disabled="disabled">1</option>					{/if}					</select>				</td>			</tr>		</tfoot>	</table>{else}	<div class="error">{$_lang.tbl_nutrition.no_record_found}</div>{/if}<center><input data-icon="add-item" data-inline="true" onclick="window.location.href='{$page_url}?mode={$smarty.const.MODE_CREATE}'" type="button" value="{$_lang.tbl_nutrition.CREATE.BTN_LBL}"/></center></div>{include file="tbl_nutrition/js.tpl"}{include file="footer.tpl"}