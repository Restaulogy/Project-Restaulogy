{literal}<script type="text/javascript">function deletetbl_service_category(varId){	if(varId > 0){		if(confirm("{/literal}{$_lang.tbl_service_category.DELETE.CONFIRM_MSG}{literal}")==true){			window.location.href="{/literal}{$page_url}{literal}?action=delete&srvc_cat_id="+varId;		}	}}function validatetbl_service_category(){	$("#srvc_cat_id_err").html("");	$("#srvc_cat_code_err").html("");	$("#srvc_cat_desc_err").html("");	/*$("#srvc_cat_start_date_err").html("");	$("#srvc_cat_end_date_err").html("");*/	var isErr = true;	if(elemById("action").value=="update"){		if(IsNonEmpty(elemById("srvc_cat_id").value)==false){			$("#srvc_cat_id_err").html("{/literal}{$_lang.tbl_service_category.not_empty_msg.srvc_cat_id}{literal}");			isErr = false;		}	}	if(IsNonEmpty(elemById("srvc_cat_code").value)==false){		$("#srvc_cat_code_err").html("{/literal}{$_lang.tbl_service_category.not_empty_msg.srvc_cat_code}{literal}");		isErr = false;	}	if(IsNonEmpty(elemById("srvc_cat_desc").value)==false){		$("#srvc_cat_desc_err").html("{/literal}{$_lang.tbl_service_category.not_empty_msg.srvc_cat_desc}{literal}");		isErr = false;	}	/*if(IsNonEmpty(elemById("srvc_cat_start_date").value)==false){		$("#srvc_cat_start_date_err").html("{/literal}{$_lang.tbl_service_category.not_empty_msg.srvc_cat_start_date}{literal}");		isErr = false;	}	if(IsNonEmpty(elemById("srvc_cat_end_date").value)==false){		$("#srvc_cat_end_date_err").html("{/literal}{$_lang.tbl_service_category.not_empty_msg.srvc_cat_end_date}{literal}");		isErr = false;	}*/	if(isErr == false){		alert("{/literal}{$_lang.messages.revise_form}{literal}");	}	return isErr;}//..function</script>{/literal}