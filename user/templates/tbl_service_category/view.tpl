{include file="header.tpl"}<div class="wrapper"><h1>{$_lang.tbl_service_category.title}</h1>{if $error_msg}	<center>{$error_msg}</center>{/if}<form name="frmupdatetbl_service_category" id="frmupdatetbl_service_category" onsubmit="return validatetbl_service_category();" method="POST" action="{$page_url}" style="{if $isUpdate eq 1}{else}display:none;{/if}">	<input type="hidden" name="srvc_cat_id" id="srvc_cat_id" value="{$tbl_service_categoryinfo.srvc_cat_id}"/>	<div style="display:none;" class="error" id="srvc_cat_id_err"></div>	<label for="srvc_cat_code">{$_lang.tbl_service_category.label.srvc_cat_code}</label>	<input type="text" name="srvc_cat_code" id="srvc_cat_code" value="{$tbl_service_categoryinfo.srvc_cat_code}"/>	<div class="error" id="srvc_cat_code_err"></div>	<label for="srvc_cat_desc">{$_lang.tbl_service_category.label.srvc_cat_desc}</label>	<textarea name="srvc_cat_desc" id="srvc_cat_desc">{$tbl_service_categoryinfo.srvc_cat_desc}</textarea>	<div class="error" id="srvc_cat_desc_err"></div>	<!--<label for="srvc_cat_start_date">{$_lang.tbl_service_category.label.srvc_cat_start_date}</label>	<input type="text" name="srvc_cat_start_date" id="srvc_cat_start_date" value="{$tbl_service_categoryinfo.srvc_cat_start_date}"/>	<div class="error" id="srvc_cat_start_date_err"></div>	<label for="srvc_cat_end_date">{$_lang.tbl_service_category.label.srvc_cat_end_date}</label>	<input type="text" name="srvc_cat_end_date" id="srvc_cat_end_date" value="{$tbl_service_categoryinfo.srvc_cat_end_date}"/>	<div class="error" id="srvc_cat_end_date_err"></div>-->	<input type="hidden" id="action" name="action" value="{$smarty.const.ACTION_UPDATE}"/> <!-- <input class="fright" type="reset" onclick="$('#tbl_service_category_view').show();$('#frmupdatetbl_service_category').hide();" value="{$_lang.cancel_lbl}"/>--></form><div id="tbl_service_category_view" class="description" style="{if $isUpdate eq 1}display:none;{/if}">	<table class="listTable">			<tr class="biz_hidden"><th class="fieldItem">{$_lang.field_title}</th><th class="valueItem">{$_lang.value_title}</th></tr>		<!--<tr><td class="fieldItem">{$_lang.tbl_service_category.label.srvc_cat_id}<i>:</i></td><td class="valueItem">{$tbl_service_categoryinfo.srvc_cat_id}</td></tr>-->		<tr><td class="fieldItem">{$_lang.tbl_service_category.label.srvc_cat_code}<i>:</i></td><td class="valueItem">{$tbl_service_categoryinfo.srvc_cat_code}</td></tr>		<tr><td class="fieldItem">{$_lang.tbl_service_category.label.srvc_cat_desc}<i>:</i></td><td class="valueItem">{$tbl_service_categoryinfo.srvc_cat_desc}</td></tr>		<!--<tr><td class="fieldItem">{$_lang.tbl_service_category.label.srvc_cat_start_date}<i>:</i></td><td class="valueItem">{$tbl_service_categoryinfo.srvc_cat_start_date}</td></tr>		<tr><td class="fieldItem">{$_lang.tbl_service_category.label.srvc_cat_end_date}<i>:</i></td><td class="valueItem">{$tbl_service_categoryinfo.srvc_cat_end_date}</td></tr>-->	<tr><td class="fieldItem">{$_lang.tbl_service_category.label.isActive}<i>:</i></td><td class="valueItem">{if $tbl_service_categoryinfo.isActive eq 1}{$_lang.tbl_service_category.label.isActive_yes}{else}{$_lang.tbl_service_category.label.isActive_no}{/if}</td></tr>	</table> </div><center>{if $isUpdate eq 1}	<input data-inline="true" data-icon="save" type="button" onclick="$('#frmupdatetbl_service_category').submit();" value="{$_lang.save_lbl}"/>{/if}	<input type="button" value="{$_lang.cancel_lbl}" data-inline="true" data-icon="delete" onclick="window.location.href='{$page_url}'"/></center>{include file="tbl_service_category/js.tpl"}</div>{include file="footer.tpl"}