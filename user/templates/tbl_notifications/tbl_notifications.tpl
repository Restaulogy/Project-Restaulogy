{include file='header.tpl'}<div class="wrapper"><h1>{$_lang.tbl_notifications.listing_title}</h1>{if $error_msg}	<center>{$error_msg}</center>{/if}{if $result_found gt 0 && $tbl_notificationslist}	<table class="listTable">		<tr><th class="bigListItem">{$_lang.tbl_notifications.title}</th>		<th class="actionListItem"></th></tr>		{foreach from=$tbl_notificationslist item=tbl_notificationsitem}			<tr>				<td class="bigListItem"><a href="{$page_url}?mode={$smarty.const.MODE_VIEW}&notify_id={$tbl_notificationsitem.notify_id}">{$tbl_notificationsitem.notify_name}</a></td>				<td class="actionListItem"><a href="{$page_url}?mode={$smarty.const.MODE_UPDATE}&notify_id={$tbl_notificationsitem.notify_id}" class="editIcon"></a><a class="deleteIcon" href="#" onclick="deletetbl_notifications({$tbl_notificationsitem.notify_id});"></a>{if $tbl_notificationsitem.isActive eq 1}<a href="{$page_url}?action={$smarty.const.ACTION_DEACTIVATE}&notify_id={$tbl_notificationsitem.notify_id}" class="deactiveIcon" title="{$_lang.tbl_notifications.DEACTIVATE.BTN_LBL}"></a>{else}<a href="{$page_url}?action={$smarty.const.ACTION_ACTIVATE}&notify_id={$tbl_notificationsitem.notify_id}" class="activeIcon" title="{$_lang.tbl_notifications.ACTIVATE.BTN_LBL}"></a>{/if}</td>			</tr>	{/foreach}	</table>{else}	<div class="error">{$_lang.tbl_notifications.no_record_found}</div>{/if}{if $pagination}	<div class="biz_center">{$pagination}</div>{/if}<div class="biz_center"><input data-icon="add-item" data-inline="true" onclick="window.location.href='{$page_url}?mode={$smarty.const.MODE_CREATE}'" type="button" value="{$_lang.tbl_notifications.CREATE.BTN_LBL}"/></div></div>{include file="tbl_notifications/js.tpl"}{include file="footer.tpl"}