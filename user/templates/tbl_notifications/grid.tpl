{include file='header.tpl'}<div class="wrapper"><h1>{$tbl_statusesinfo.status_event}-{$tbl_statusesinfo.status_name}-{$_lang.tbl_notifications.listing_title}</h1>{if $error_msg}	<center>{$error_msg}</center>{/if}{if $result_found gt 0 && $tbl_notificationslist}<form name="frm_tbl_notifications" id="frm_tbl_notifications" method="POST" action="{$page_url}">	<table class="biz_data_grid">		<tr>			<th colspan="2" class="{if $smarty.get.sort_on eq 'notify_name'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}&sort_on=notify_name&sort_by={$new_sort}">{$_lang.tbl_notifications.label.notify_name}</a></th>			<th class="{if $smarty.get.sort_on eq 'notify_message'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}&sort_on=notify_message&sort_by={$new_sort}">{$_lang.tbl_notifications.label.notify_message}</a></th>			<!--            <th>{$_lang.tbl_statuses.label.status_event}</th>			<th class="{if $smarty.get.sort_on eq 'notify_status'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}&sort_on=notify_status&sort_by={$new_sort}">{$_lang.tbl_notifications.label.notify_status}</a></th>			-->			<th class="{if $smarty.get.sort_on eq 'notify_to_role'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}&sort_on=notify_to_role&sort_by={$new_sort}">{$_lang.tbl_notifications.label.notify_to_role}</a></th>			 <!--<th class="{if $smarty.get.sort_on eq 'notify_is_urgent'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}&sort_on=notify_is_urgent&sort_by={$new_sort}">{$_lang.tbl_notifications.label.notify_is_urgent}</a></th>-->                       <th class="{if $smarty.get.sort_on eq 'notify_is_delayed'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}&sort_on=notify_is_delayed&sort_by={$new_sort}">Type{*$_lang.tbl_notifications.label.notify_is_delayed*}</a></th>			<!--<th class="{if $smarty.get.sort_on eq 'notify_restaurent'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}&sort_on=notify_restaurent&sort_by={$new_sort}">{$_lang.tbl_notifications.label.notify_restaurent}</a></th>-->			<!-- <th class="action_header">Action</th> -->        </tr>		{foreach from=$tbl_notificationslist item=tbl_notificationsitem}		<tr class="{cycle values="odd,even"}">            <td style="width:3%;">	               <label for="sel_menu[{$tbl_notificationsitem.notify_id}]" data-mini="true" style="width:23px;"><input type="checkbox" data-inline='true' data-mini='true' id="sel_menu[{$tbl_notificationsitem.notify_id}]" name="sel_menu[{$tbl_notificationsitem.notify_id}]" />&nbsp;</label>			</td>			<td onclick='window.location.href="{$page_url}&mode={$smarty.const.MODE_UPDATE}&notify_id={$tbl_notificationsitem.notify_id}"'>{$tbl_notificationsitem.notify_name}</td>			<td onclick='window.location.href="{$page_url}&mode={$smarty.const.MODE_UPDATE}&notify_id={$tbl_notificationsitem.notify_id}"'>{$tbl_notificationsitem.notify_message}</td>            <!--            <td>{$tbl_notificationsitem.status_event}</td>			<td>{$tbl_notificationsitem.status_name}</td>			-->			<td onclick='window.location.href="{$page_url}&mode={$smarty.const.MODE_UPDATE}&notify_id={$tbl_notificationsitem.notify_id}"'>{$tbl_notificationsitem.member_role_name}</td>			<!--<td onclick='window.location.href="{$page_url}&mode={$smarty.const.MODE_UPDATE}&notify_id={$tbl_notificationsitem.notify_id}"'>{$tbl_notificationsitem.notify_is_urgent}</td>-->						<td onclick='window.location.href="{$page_url}&mode={$smarty.const.MODE_UPDATE}&notify_id={$tbl_notificationsitem.notify_id}"'>{if $tbl_notificationsitem.notify_is_delayed eq 1}On Delay{else}Regular{/if}</td>			<!--<td>{$tbl_notificationsitem.notify_is_delayed}</td>			<td>{$tbl_notificationsitem.notify_restaurent}</td>-->			<!--            <td><a href="{$page_url}&mode={$smarty.const.MODE_UPDATE}&notify_id={$tbl_notificationsitem.notify_id}" class="editIcon"></a><a class="deleteIcon" href="#" onclick="deletetbl_notifications({$tbl_notificationsitem.notify_id});"></a>{if $tbl_notificationsitem.isActive eq 1}<a href="{$page_url}&action={$smarty.const.ACTION_DEACTIVATE}&notify_id={$tbl_notificationsitem.notify_id}" class="deactiveIcon" title="{$_lang.tbl_notifications.DEACTIVATE.BTN_LBL}"></a>{else}<a href="{$page_url}&action={$smarty.const.ACTION_ACTIVATE}&notify_id={$tbl_notificationsitem.notify_id}" class="activeIcon" title="{$_lang.tbl_notifications.ACTIVATE.BTN_LBL}"></a>{/if}</td>            -->        </tr>	{/foreach}		<tfoot>			<tr>				<td colspan="10">					# {$result_found}&nbsp;&nbsp;&nbsp;{if $pagination neq ""}{$pagination}{/if}					<select onchange="changePage('{$navigationURL}',this.value,{$smarty.request.limit});">					{if $allPageCount gt 1}						{for $foo=1 to $allPageCount}							<option value="{$foo}" {if $foo eq $currentPage}selected="selected"{/if}>{$foo}</option>						{/for}					{else}						<option value="1" disabled="disabled">1</option>					{/if}					</select>				</td>			</tr>		</tfoot>	</table>{else}	<div class="error">{$_lang.tbl_notifications.no_record_found}</div>{/if}<input type="hidden" id="action" name="{$smarty.const.ACTION_TITLE}"/></form><div class="biz_center">{if $result_found gt 0 && $tbl_notificationslist}    <input data-inline="true" data-icon="briefcase" type="button" value="{$_lang.main.toggle}" onclick="javascript:$('input[type=checkbox]').click();" />    <input data-inline="true" data-icon="inactive" type="button" value="{$_lang.tbl_dishes.DEACTIVATE.BTN_LBL}" onclick="actiontbl_notifications('{$smarty.const.ACTION_DEACTIVATE}');"  />    <input data-inline="true" data-icon="active" type="button" value="{$_lang.tbl_dishes.ACTIVATE.BTN_LBL}" onclick="actiontbl_notifications('{$smarty.const.ACTION_ACTIVATE}');" />    {if $tbl_statusesinfo.status_control eq 'MANUAL'}        <input data-inline="true" data-icon="delete" type="button" value="{$_lang.tbl_dishes.DELETE.BTN_LBL}" onclick="actiontbl_notifications('{$smarty.const.ACTION_DELETE}');" />    {/if}{/if}		    <input data-icon="add-item" data-inline="true" onclick="window.location.href='{$page_url}&mode={$smarty.const.MODE_CREATE}'" type="button" value="{$_lang.tbl_notifications.CREATE.BTN_LBL}"/>    <input type="button" data-inline="true" data-icon="back"  value="Back to {$_lang.tbl_statuses.listing_title}" onclick="window.location.href='{$website}/user/tbl_statuses.php?status_event={$tbl_statusesinfo.status_event}'"/></div></div>{include file="tbl_notifications/js.tpl"}{include file="footer.tpl"}