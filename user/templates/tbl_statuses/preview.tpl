{include file='header.tpl'}<div class="wrapper"><h1>{$_lang.tbl_statuses.listing_title}</h1>{if $error_msg}	<center>{$error_msg}</center>{/if}{include file="tbl_statuses/tabs.tpl"} <link rel="stylesheet" href="{$website}/css/timeline.css"/>{if $result_found gt 0 && $tbl_statuseslist} 	 	 <ul class="timeline">		{foreach from=$tbl_statuseslist item=tbl_statusesitem}		{if $tbl_statusesitem.status_hidden eq 0} 		<li class="event">			<input type="radio" name="tl-group" data-role="none"/>			<label></label> 			<div class="timeline-content-perspective">				<div class="timeline-content">					<div class="timeline-content-inner">						<h3>{$tbl_statusesitem.status_name} <small>({$tbl_statusesitem.status_control}&nbsp;|&nbsp;{$tbl_statusesitem.status_exp_time}  sec)</small></h3>						<p>   						   {if $tbl_statusesitem.notification_count gt 0}   							 {foreach from=$tbl_statusesitem.notifications item=notification}							  <span>{$notification.member_role_name} : </span>&nbsp;&nbsp;&nbsp;&nbsp;<a href="{$website}/user/tbl_notifications.php?notify_id={$notification.notify_id}&{$smarty.const.MODE_TITLE}={$smarty.const.MODE_UPDATE}&notify_status={$notify_status}" target="_blank">{$notification.notify_message}</a> <br />   							 {/foreach} 							 {else}							 	<span class="error"> No notification associated with this status.</span>							 {/if}						</p>					</div>				</div>			</div>		</li>		{/if}		 {/foreach}	</ul> 	<div class="biz_center">		{jqmbutton onclick="javascript:self.close();" value="Close" icon="delete"} 	</div>		{/if} </div>{include file="tbl_statuses/js.tpl"}{include file="footercontent.tpl"} </body></html> 