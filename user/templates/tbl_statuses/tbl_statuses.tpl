{include file='header.tpl'}<div class="wrapper"><h1>{$_lang.tbl_statuses.listing_title}</h1>{if $error_msg}	<center>{$error_msg}</center>{/if}{if $result_found gt 0 && $tbl_statuseslist}	<table class="listTable">		<tr><th class="bigListItem">{$_lang.tbl_statuses.title}</th>		<th class="actionListItem"></th></tr>		{foreach from=$tbl_statuseslist item=tbl_statusesitem}			<tr>				<td class="bigListItem"><a href="{$page_url}?mode={$smarty.const.MODE_VIEW}&status_id={$tbl_statusesitem.status_id}">{$tbl_statusesitem.status_name}</a></td>				<td class="actionListItem">                <a href="{$page_url}?mode={$smarty.const.MODE_UPDATE}&status_id={$tbl_statusesitem.status_id}" class="editIcon"></a>                <a class="deleteIcon" href="#" onclick="deletetbl_statuses({$tbl_statusesitem.status_id});"></a>                {if $tbl_statusesitem.isActive eq 1}                <a href="{$page_url}?action={$smarty.const.ACTION_DEACTIVATE}&status_id={$tbl_statusesitem.status_id}" class="deactiveIcon" title="{$_lang.tbl_statuses.DEACTIVATE.BTN_LBL}"></a>                {else}                <a href="{$page_url}?action={$smarty.const.ACTION_ACTIVATE}&status_id={$tbl_statusesitem.status_id}" class="activeIcon" title="{$_lang.tbl_statuses.ACTIVATE.BTN_LBL}"></a>                {/if}                </td>			</tr>	{/foreach}	</table>{else}	<div class="error">{$_lang.tbl_statuses.no_record_found}</div>{/if}{if $pagination}	<center>{$pagination}</center>{/if}<center><input data-icon="add-item" data-inline="true" onclick="window.location.href='{$page_url}?mode={$smarty.const.MODE_CREATE}'" type="button" value="{$_lang.tbl_statuses.CREATE.BTN_LBL}"/></center></div>{include file="tbl_statuses/js.tpl"}{include file="footer.tpl"}