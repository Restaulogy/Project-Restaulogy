{include file='header.tpl'}<div class="wrapper"><h1>{$_lang.tbl_emp_shift_assignment.listing_title}</h1>{if $error_msg}	<center>{$error_msg}</center>{/if}{include file="tbl_emp_shift_assignment/tabs.tpl"}{if $result_found gt 0 && $tbl_emp_shift_assignmentlist}	<table class="biz_data_grid">		<tr>			<th class="{if $smarty.get.sort_on eq 'emp_sft_employee'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=emp_sft_employee&sort_by={$new_sort}">{$_lang.tbl_emp_shift_assignment.label.emp_sft_employee}</a></th>			<th class="{if $smarty.get.sort_on eq 'emp_sft_shift'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=emp_sft_shift&sort_by={$new_sort}">{$_lang.tbl_emp_shift_assignment.label.emp_sft_shift}</a></th>			<th class="{if $smarty.get.sort_on eq 'emp_sft_date'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=emp_sft_date&sort_by={$new_sort}">{$_lang.tbl_emp_shift_assignment.label.emp_sft_date}</a></th>			<th class="{if $smarty.get.sort_on eq 'emp_sft_tables'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=emp_sft_tables&sort_by={$new_sort}">{$_lang.tbl_emp_shift_assignment.label.emp_sft_tables}</a></th>			<th class="action_header">Action</th>		</tr>		{foreach from=$tbl_emp_shift_assignmentlist item=tbl_emp_shift_assignmentitem}		<tr class="{cycle values="odd,even"}">			<td><a href="{$page_url}?mode={$smarty.const.MODE_VIEW}&emp_sft_id={$tbl_emp_shift_assignmentitem.emp_sft_id}">{$tbl_emp_shift_assignmentitem.emp_sft_employee}</a></td>			<td>{$tbl_emp_shift_assignmentitem.emp_sft_shift}</td>			<td>{$tbl_emp_shift_assignmentitem.emp_sft_date}</td>			<td>{$tbl_emp_shift_assignmentitem.emp_sft_tables}</td>			<td><a href="{$page_url}?mode={$smarty.const.MODE_UPDATE}&emp_sft_id={$tbl_emp_shift_assignmentitem.emp_sft_id}" class="editIcon"></a><a class="deleteIcon" href="#" onclick="deletetbl_emp_shift_assignment({$tbl_emp_shift_assignmentitem.emp_sft_id});"></a>{if $tbl_emp_shift_assignmentitem.isActive eq 1}<a href="{$page_url}?action={$smarty.const.ACTION_DEACTIVATE}&emp_sft_id={$tbl_emp_shift_assignmentitem.emp_sft_id}" class="deactiveIcon" title="{$_lang.tbl_emp_shift_assignment.DEACTIVATE.BTN_LBL}"></a>{else}<a href="{$page_url}?action={$smarty.const.ACTION_ACTIVATE}&emp_sft_id={$tbl_emp_shift_assignmentitem.emp_sft_id}" class="activeIcon" title="{$_lang.tbl_emp_shift_assignment.ACTIVATE.BTN_LBL}"></a>{/if}</td>		</tr>	{/foreach}		<tfoot>			<tr>				<td colspan="7">					# {$result_found}&nbsp;&nbsp;&nbsp;{if $pagination neq ""}{$pagination}{/if}					<select onchange="changePage('{$navigationURL}',this.value,{$smarty.request.limit});">					{if $allPageCount gt 1}						{for $foo=1 to $allPageCount}							<option value="{$foo}" {if $foo eq $currentPage}selected="selected"{/if}>{$foo}</option>						{/for}					{else}						<option value="1" disabled="disabled">1</option>					{/if}					</select>				</td>			</tr>		</tfoot>	</table>{else}	<div class="error">{$_lang.tbl_emp_shift_assignment.no_record_found}</div>{/if}<center><input data-icon="add-item" data-inline="true" onclick="window.location.href='{$page_url}?mode={$smarty.const.MODE_CREATE}'" type="button" value="{$_lang.tbl_emp_shift_assignment.CREATE.BTN_LBL}"/></center></div><div style="height:150px;background:transparent;">	<select id="sdsa" data-placeholder="Choose a country..."  multiple data-role="none" style="width:100%;" multiple class="chzn-select"> 		<option value="x">x</option>		<option value="y">y</option>		<option value="z">z</option>		<option value="1">1</option>		<option value="2">2</option>	</select>		<br>	<select id="sdsa2" data-placeholder="Choose a country..."  multiple data-role="none" style="width:100%;" multiple class="chzn-select"> 		<option value="x">x</option>		<option value="y">y</option>		<option value="z">z</option>		<option value="1">1</option>		<option value="2">2</option>	</select></div> 		{include file="footercontent.tpl"}{include file="tbl_emp_shift_assignment/js.tpl"}</body></html>