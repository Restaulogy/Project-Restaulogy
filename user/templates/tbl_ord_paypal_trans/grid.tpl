{include file='header.tpl'}<div class="wrapper"><h1>{$_lang.tbl_ord_paypal_trans.listing_title}</h1>{if $error_msg}	<center>{$error_msg}</center>{/if}{if $result_found gt 0 && $tbl_ord_paypal_translist}	<table class="biz_data_grid">		<tr>			<th class="{if $smarty.get.sort_on eq 'paypal_txn_id'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=paypal_txn_id&sort_by={$new_sort}">{$_lang.tbl_ord_paypal_trans.label.paypal_txn_id}</a></th>			<th class="{if $smarty.get.sort_on eq 'paypal_txn_amnt'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=paypal_txn_amnt&sort_by={$new_sort}">{$_lang.tbl_ord_paypal_trans.label.paypal_txn_amnt}</a></th>			<th class="{if $smarty.get.sort_on eq 'paypal_payment_id'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=paypal_payment_id&sort_by={$new_sort}">{$_lang.tbl_ord_paypal_trans.label.paypal_payment_id}</a></th>			<th class="{if $smarty.get.sort_on eq 'paypal_refund_type'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=paypal_refund_type&sort_by={$new_sort}">{$_lang.tbl_ord_paypal_trans.label.paypal_refund_type}</a></th>			<th class="{if $smarty.get.sort_on eq 'paypal_refund_complete'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=paypal_refund_complete&sort_by={$new_sort}">{$_lang.tbl_ord_paypal_trans.label.paypal_refund_complete}</a></th>			<th class="action_header">Action</th>		</tr>		{foreach from=$tbl_ord_paypal_translist item=tbl_ord_paypal_transitem}		<tr class="{cycle values="odd,even"}">			<td><a href="{$page_url}?mode={$smarty.const.MODE_VIEW}&paypal_id={$tbl_ord_paypal_transitem.paypal_id}">{$tbl_ord_paypal_transitem.paypal_txn_id}</a></td>			<td>{$tbl_ord_paypal_transitem.paypal_txn_amnt}</td>			<td>{$tbl_ord_paypal_transitem.paypal_payment_id}</td>			<td>{$tbl_ord_paypal_transitem.paypal_refund_type}</td>			<td>{$tbl_ord_paypal_transitem.paypal_refund_complete}</td>			<td><a href="{$page_url}?mode={$smarty.const.MODE_UPDATE}&paypal_id={$tbl_ord_paypal_transitem.paypal_id}" class="editIcon"></a><a class="deleteIcon" href="#" onclick="deletetbl_ord_paypal_trans({$tbl_ord_paypal_transitem.paypal_id});"></a>{if $tbl_ord_paypal_transitem.isActive eq 1}<a href="{$page_url}?action={$smarty.const.ACTION_DEACTIVATE}&paypal_id={$tbl_ord_paypal_transitem.paypal_id}" class="deactiveIcon" title="{$_lang.tbl_ord_paypal_trans.DEACTIVATE.BTN_LBL}"></a>{else}<a href="{$page_url}?action={$smarty.const.ACTION_ACTIVATE}&paypal_id={$tbl_ord_paypal_transitem.paypal_id}" class="activeIcon" title="{$_lang.tbl_ord_paypal_trans.ACTIVATE.BTN_LBL}"></a>{/if}</td>		</tr>	{/foreach}		<tfoot>			<tr>				<td colspan="8">					# {$result_found}&nbsp;&nbsp;&nbsp;{if $pagination neq ""}{$pagination}{/if}					<select onchange="changePage('{$navigationURL}',this.value,{$smarty.request.limit});">					{if $allPageCount gt 1}						{for $foo=1 to $allPageCount}							<option value="{$foo}" {if $foo eq $currentPage}selected="selected"{/if}>{$foo}</option>						{/for}					{else}						<option value="1" disabled="disabled">1</option>					{/if}					</select>				</td>			</tr>		</tfoot>	</table>{else}	<div class="error">{$_lang.tbl_ord_paypal_trans.no_record_found}</div>{/if}<center><input data-icon="add-item" data-inline="true" onclick="window.location.href='{$page_url}?mode={$smarty.const.MODE_CREATE}'" type="button" value="{$_lang.tbl_ord_paypal_trans.CREATE.BTN_LBL}"/></center></div>{include file="tbl_ord_paypal_trans/js.tpl"}{include file="footer.tpl"}