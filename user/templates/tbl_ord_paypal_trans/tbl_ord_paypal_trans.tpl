{include file='header.tpl'}<div class="wrapper"><h1>{$_lang.tbl_ord_paypal_trans.listing_title}</h1>{if $error_msg}	<center>{$error_msg}</center>{/if}{if $result_found gt 0 && $tbl_ord_paypal_translist}	<table class="listTable">		<tr><th class="bigListItem">{$_lang.tbl_ord_paypal_trans.title}</th>		<th class="actionListItem"></th></tr>		{foreach from=$tbl_ord_paypal_translist item=tbl_ord_paypal_transitem}			<tr>				<td class="bigListItem"><a href="{$page_url}?mode={$smarty.const.MODE_VIEW}&paypal_id={$tbl_ord_paypal_transitem.paypal_id}">{$tbl_ord_paypal_transitem.paypal_txn_id}</a></td>				<td class="actionListItem"><a href="{$page_url}?mode={$smarty.const.MODE_UPDATE}&paypal_id={$tbl_ord_paypal_transitem.paypal_id}" class="editIcon"></a><a class="deleteIcon" href="#" onclick="deletetbl_ord_paypal_trans({$tbl_ord_paypal_transitem.paypal_id});"></a>{if $tbl_ord_paypal_transitem.isActive eq 1}<a href="{$page_url}?action={$smarty.const.ACTION_DEACTIVATE}&paypal_id={$tbl_ord_paypal_transitem.paypal_id}" class="deactiveIcon" title="{$_lang.tbl_ord_paypal_trans.DEACTIVATE.BTN_LBL}"></a>{else}<a href="{$page_url}?action={$smarty.const.ACTION_ACTIVATE}&paypal_id={$tbl_ord_paypal_transitem.paypal_id}" class="activeIcon" title="{$_lang.tbl_ord_paypal_trans.ACTIVATE.BTN_LBL}"></a>{/if}</td>			</tr>	{/foreach}	</table>{else}	<div class="error">{$_lang.tbl_ord_paypal_trans.no_record_found}</div>{/if}{if $pagination}	<center>{$pagination}</center>{/if}<center><input data-icon="add-item" data-inline="true" onclick="window.location.href='{$page_url}?mode={$smarty.const.MODE_CREATE}'" type="button" value="{$_lang.tbl_ord_paypal_trans.CREATE.BTN_LBL}"/></center></div>{include file="tbl_ord_paypal_trans/js.tpl"}{include file="footer.tpl"}