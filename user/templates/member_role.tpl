{include file='header.tpl'}<div class="wrapper"><h1>{$_lang.member_role.listing_title}</h1>{if $error_msg}	<center>{$error_msg}</center>{/if}{if $result_found gt 0 && $member_rolelist}	<table class="listTable">		<tr><th class="bigListItem">{$_lang.member_role.title}</th>		</tr>		{foreach from=$member_rolelist item=member_roleitem}		 			<tr>				<td class="bigListItem"><a href="{$page_url}?mode=update&member_role_id={$member_roleitem.member_role_id}">{$member_roleitem.member_role_name}</a></td>				<!--<td class="actionListItem"><a href="{$page_url}?mode=update&member_role_id={$member_roleitem.member_role_id}" class="editIcon"></a><a class="deleteIcon" href="#" onclick="deletemember_role({$member_roleitem.member_role_id});"></a>{if $member_roleitem.isActive eq 1}<a href="{$page_url}?action=deactivate&member_role_id={$member_roleitem.member_role_id}" class="deactiveIcon" title="{$_lang.member_role.DEACTIVATE.BTN_LBL}"></a>{else}<a href="{$page_url}?action=activate&member_role_id={$member_roleitem.member_role_id}" class="activeIcon" title="{$_lang.member_role.ACTIVATE.BTN_LBL}"></a>{/if}</td>-->			</tr>	{/foreach}	</table>{/if}{if $pagination}	<center>{$pagination}</center>{/if}<center><input data-icon="add-item" data-inline="true" onclick="window.location.href='{$page_url}?mode={$smarty.const.MODE_CREATE}'" type="button" value="{$_lang.member_role.CREATE.BTN_LBL}"/></center></div>{include file="js_member_role.tpl"}{include file="footer.tpl"}