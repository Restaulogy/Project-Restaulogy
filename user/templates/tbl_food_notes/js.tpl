{literal}<script type="text/javascript">function actiontbl_food_notes(action){	var varId = $("input:checked").length;	var isValid = 0;	if(varId > 0){	if(action != ""){		switch (action) {			case '{/literal}{$smarty.const.ACTION_DELETE}{literal}':			if(confirm("{/literal}{$_lang.tbl_allergy_list.DELETE.CONFIRM_MSG}{literal}")==true){				isValid = 1;			}			break;			case '{/literal}{$smarty.const.ACTION_DEACTIVATE}{literal}':			isValid = 1;			break;			case '{/literal}{$smarty.const.ACTION_ACTIVATE}{literal}':			isValid = 1;			break;		}    	if(isValid){    	   $('#action').val(action);    	   $('#frm_tbl_food_notes').submit();    	}	}	}else{			alert("{/literal}{$_lang.main.select_ids.empty}{literal}"); 	}}function deletetbl_food_notes(varId){	if(varId > 0){		if(confirm("{/literal}{$_lang.tbl_food_notes.DELETE.CONFIRM_MSG}{literal}")==true){			window.location.href="{/literal}{$page_url}{literal}?action=delete&fdnote_id="+varId;		}	}}function validatetbl_food_notes(){	$("#fdnote_id_err").html("");	$("#fdnote_number_err").html("");	$("#fdnote_desc_err").html("");	$("#fdnote_start_date_err").html("");	$("#fdnote_end_date_err").html("");	var isErr = true;	if(elemById("action").value=="update"){		if(IsNonEmpty(elemById("fdnote_id").value)==false){			$("#fdnote_id_err").html("{/literal}{$_lang.tbl_food_notes.not_empty_msg.fdnote_id}{literal}");			isErr = false;		}	}	if(IsNonEmpty(elemById("fdnote_number").value)==false){		$("#fdnote_number_err").html("{/literal}{$_lang.tbl_food_notes.not_empty_msg.fdnote_number}{literal}");		isErr = false;	}	if(IsNonEmpty(elemById("fdnote_desc").value)==false){		$("#fdnote_desc_err").html("{/literal}{$_lang.tbl_food_notes.not_empty_msg.fdnote_desc}{literal}");		isErr = false;	}/*	if(IsNonEmpty(elemById("fdnote_start_date").value)==false){		$("#fdnote_start_date_err").html("{/literal}{$_lang.tbl_food_notes.not_empty_msg.fdnote_start_date}{literal}");		isErr = false;	}*//*	if(IsNonEmpty(elemById("fdnote_end_date").value)==false){		$("#fdnote_end_date_err").html("{/literal}{$_lang.tbl_food_notes.not_empty_msg.fdnote_end_date}{literal}");		isErr = false;	}*/	if(isErr == false){		alert("{/literal}{$_lang.messages.revise_form}{literal}");	}	return isErr;}//..function</script>{/literal}