{include file="header.tpl"}<div class="wrapper"><h1>{$_lang.tbl_food_notes.title}</h1>{if $error_msg}	<center>{$error_msg}</center>{/if}<form name="frmupdatetbl_food_notes" id="frmupdatetbl_food_notes" onsubmit="return validatetbl_food_notes();" method="POST" action="{$page_url}" style="{if $isUpdate eq 1}{else}display:none;{/if}">	<input type="hidden" name="fdnote_id" id="fdnote_id" value="{$tbl_food_notesinfo.fdnote_id}"/>	<div style="display:none;" class="error" id="fdnote_id_err"></div>	<div class="field-row">		<label for="fdnote_number">{$_lang.tbl_food_notes.label.fdnote_number}</label>		<input type="text" name="fdnote_number" id="fdnote_number" value="{$tbl_food_notesinfo.fdnote_number}"/>		<div class="error" id="fdnote_number_err"></div>	</div>	<div class="field-row">		<label for="fdnote_desc">{$_lang.tbl_food_notes.label.fdnote_desc}</label>		<input type="text" name="fdnote_desc" id="fdnote_desc" value="{$tbl_food_notesinfo.fdnote_desc}"/>		<div class="error" id="fdnote_desc_err"></div>	</div>	<!--	<div class="field-row">		<label for="fdnote_start_date">{$_lang.tbl_food_notes.label.fdnote_start_date}</label>		<input type="text" name="fdnote_start_date" id="fdnote_start_date" value="{$tbl_food_notesinfo.fdnote_start_date}"/>		<div class="error" id="fdnote_start_date_err"></div>	</div>	-->	<!--	<div class="field-row">		<label for="fdnote_end_date">{$_lang.tbl_food_notes.label.fdnote_end_date}</label>		<input type="text" name="fdnote_end_date" id="fdnote_end_date" value="{$tbl_food_notesinfo.fdnote_end_date}"/>		<div class="error" id="fdnote_end_date_err"></div>	</div>	-->	<center>    <input type="hidden" id="action" name="action" value="{$smarty.const.ACTION_UPDATE}"/>    <input  data-inline="true" data-icon="save"  type="submit" value="{$_lang.save_lbl}"/>    <input data-inline="true" data-icon="delete" type="reset" onclick="$('#tbl_food_notes_view').show();$('#frmupdatetbl_food_notes').hide();" value="{$_lang.cancel_lbl}"/>    </center></form><div id="tbl_food_notes_view" class="description" style="{if $isUpdate eq 1}display:none;{/if}">	<table class="listTable">		<tr><th class="fieldItem">{$_lang.field_title}</th><th class="valueItem">{$_lang.value_title}</th></tr>		<tr><td class="fieldItem">{$_lang.tbl_food_notes.label.fdnote_id}<i>:</i></td><td class="valueItem">{$tbl_food_notesinfo.fdnote_id}</td></tr>		<tr><td class="fieldItem">{$_lang.tbl_food_notes.label.fdnote_number}<i>:</i></td><td class="valueItem">{$tbl_food_notesinfo.fdnote_number}</td></tr>		<tr><td class="fieldItem">{$_lang.tbl_food_notes.label.fdnote_desc}<i>:</i></td><td class="valueItem">{$tbl_food_notesinfo.fdnote_desc}</td></tr>	<!--		<tr><td class="fieldItem">{$_lang.tbl_food_notes.label.fdnote_start_date}<i>:</i></td><td class="valueItem">{$tbl_food_notesinfo.fdnote_start_date}</td></tr>	-->	<!--		<tr><td class="fieldItem">{$_lang.tbl_food_notes.label.fdnote_end_date}<i>:</i></td><td class="valueItem">{$tbl_food_notesinfo.fdnote_end_date}</td></tr>	-->	<tr><td class="fieldItem">{$_lang.tbl_food_notes.label.isActive}<i>:</i></td><td class="valueItem">{if $tbl_food_notesinfo.isActive eq 1}{$_lang.tbl_food_notes.label.isActive_yes}{else}{$_lang.tbl_food_notes.label.isActive_no}{/if}</td></tr>	</table>	<center><input data-inline="true" data-icon="edit" type="button" value="{$_lang.tbl_food_notes.UPDATE.BTN_LBL}" onclick="$('#tbl_food_notes_view').hide();$('#frmupdatetbl_food_notes').show();"/><input type="button" value="{$_lang.cancel_lbl}" data-inline="true" data-icon="delete" onclick="window.location.href='{$page_url}'"/></center></div>{include file="tbl_food_notes/js.tpl"}</div>{include file="footer.tpl"}