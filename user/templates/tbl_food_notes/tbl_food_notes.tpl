{include file='header.tpl'}<div class="wrapper"><h1>{$_lang.tbl_food_notes.listing_title}</h1>{if $error_msg}	<center>{$error_msg}</center>{/if}{if $result_found gt 0 && $tbl_food_noteslist}<form name="frm_tbl_food_notes" id="frm_tbl_food_notes" method="POST" action="{$page_url}">	<table class="listTable">		<tr><th class="bigListItem" colspan="2">{$_lang.tbl_food_notes.title}</th>		<th style="width:5%;">                {$_lang.main.status}		</th>		<!-- <th class="actionListItem"></th> -->        </tr>		{foreach from=$tbl_food_noteslist item=tbl_food_notesitem}			<tr>                <td style="width:3%;" >					 <label for="sel_tbl_food_notes[{$tbl_food_notesitem.fdnote_id}]" data-mini="true" style="width:23px;"><input type="checkbox" data-inline='true' data-mini='true' id="sel_tbl_food_notes[{$tbl_food_notesitem.fdnote_id}]" name="sel_tbl_food_notes[{$tbl_food_notesitem.fdnote_id}]" />&nbsp;</label>			    </td>				<td class="bigListItem"><a href="{$page_url}?mode={$smarty.const.MODE_UPDATE}&fdnote_id={$tbl_food_notesitem.fdnote_id}">{$tbl_food_notesitem.fdnote_number}</a></td>				<td >                    {if $tbl_food_notesitem.isActive eq 1}                            <a href="#" class="activeIcon" title="{$_lang.tbl_food_notes.ACTIVATE.BTN_LBL}"></a>                    {else}                            <a href="#" class="deactiveIcon" title="{$_lang.tbl_food_notes.DEACTIVATE.BTN_LBL}"></a>                    {/if}                </td>                <!--				<td class="actionListItem">                <a class="deleteIcon" href="#" onclick="deletetbl_food_notes({$tbl_food_notesitem.fdnote_id});"></a>                <a href="{$page_url}?mode={$smarty.const.MODE_UPDATE}&fdnote_id={$tbl_food_notesitem.fdnote_id}" class="editIcon"></a>                {if $tbl_food_notesitem.isActive eq 1}                    <a href="{$page_url}?action={$smarty.const.ACTION_DEACTIVATE}&fdnote_id={$tbl_food_notesitem.fdnote_id}" class="deactiveIcon" title="{$_lang.tbl_food_notes.DEACTIVATE.BTN_LBL}"></a>                {else}                    <a href="{$page_url}?action={$smarty.const.ACTION_ACTIVATE}&fdnote_id={$tbl_food_notesitem.fdnote_id}" class="activeIcon" title="{$_lang.tbl_food_notes.ACTIVATE.BTN_LBL}"></a>                {/if}                </td>                -->			</tr>	{/foreach}	</table>{else}	<div class="error">{$_lang.tbl_food_notes.no_record_found}</div>{/if}{if $pagination}	<center>{$pagination}</center>{/if}<input type="hidden" id="action" name="{$smarty.const.ACTION_TITLE}"/></form><div class="biz_center">{if $result_found gt 0 && $tbl_food_noteslist} 	<input data-inline="true" data-icon="briefcase" type="button" value="{$_lang.main.toggle}" onclick="javascript:$('input[type=checkbox]').click();" />    <input data-inline="true" data-icon="inactive" type="button" value="{$_lang.tbl_dishes.DEACTIVATE.BTN_LBL}" onclick="actiontbl_food_notes('{$smarty.const.ACTION_DEACTIVATE}');"  />    <input data-inline="true" data-icon="active" type="button" value="{$_lang.tbl_dishes.ACTIVATE.BTN_LBL}" onclick="actiontbl_food_notes('{$smarty.const.ACTION_ACTIVATE}');" />    <input data-inline="true" data-icon="delete" type="button" value="{$_lang.tbl_dishes.DELETE.BTN_LBL}" onclick="actiontbl_food_notes('{$smarty.const.ACTION_DELETE}');" />{/if}    <input data-icon="add-item" data-inline="true" onclick="window.location.href='{$page_url}?mode={$smarty.const.MODE_CREATE}'" type="button" value="{$_lang.tbl_food_notes.CREATE.BTN_LBL}"/> </div></div>{include file="tbl_food_notes/js.tpl"}{include file="footer.tpl"}