{include file="header.tpl"}<div class="wrapper"><h1>{$_lang.tbl_dishes.create_title}</h1>{if $error_msg}	<center>{$error_msg}</center>{/if}<form name="frmcreatetbl_dishes" id="frmcreatetbl_dishes" onsubmit="return validatetbl_dishes();" method="POST" action="{$page_url}" enctype="multipart/form-data">	<input type="hidden" name="dish_id" id="dish_id" value="0"/>	<div style="display:none;" class="error" id="dish_id_err"></div>	<div class="field-row">		<label for="dish_name">{$_lang.tbl_dishes.label.dish_name}</label>		<input type="text" name="dish_name" id="dish_name" value="{$smarty.post.dish_name}"/>		<div class="error" id="dish_name_err"></div>	</div>	<div class="field-row">		<label for="dish_chef_notes">{$_lang.tbl_dishes.label.dish_chef_notes}</label>		<textarea name="dish_chef_notes" id="dish_chef_notes">{$smarty.post.dish_chef_notes}</textarea>		<div class="error" id="dish_chef_notes_err"></div>	</div>	<div class="field-row">		<label for="dish_ingrad_allergic_contents">{$_lang.tbl_dishes.label.dish_ingrad_allergic_contents}</label>		<textarea name="dish_ingrad_allergic_contents" id="dish_ingrad_allergic_contents">{$smarty.post.dish_ingrad_allergic_contents}</textarea>		<div class="error" id="dish_ingrad_allergic_contents_err"></div>	</div>		<div class="field-row">		<label for="dish_allergy">{$_lang.tbl_dishes.label.dish_allergy}</label>		<textarea name="dish_allergy" id="dish_allergy">{$smarty.post.dish_allergy}</textarea>		<div class="error" id="dish_allergy_err"></div>	</div>		<div class="field-row">		<label for="dish_food_wine_pair">{$_lang.tbl_dishes.label.dish_food_wine_pair}</label>		<textarea name="dish_food_wine_pair" id="dish_food_wine_pair">{$smarty.post.dish_food_wine_pair}</textarea>		<div class="error" id="dish_food_wine_pair_err"></div>	</div>		<div class="field-row">		<label for="dish_nutri_cal_info">{$_lang.tbl_dishes.label.dish_nutri_cal_info}</label>		<textarea name="dish_nutri_cal_info" id="dish_nutri_cal_info">{$smarty.post.dish_nutri_cal_info}</textarea>		<div class="error" id="dish_nutri_cal_info_err"></div>	</div>	<div class="field-row">		<label for="dish_notes">{$_lang.tbl_dishes.label.dish_notes}</label>		<textarea name="dish_notes" id="dish_notes">{$smarty.post.dish_notes}</textarea>		<div class="error" id="dish_notes_err"></div>	</div>	<div class="field-row">		<label for="dish_img">{$_lang.tbl_dishes.label.dish_img}</label>		<input type="file" name="dish_img" id="dish_img" value="{$smarty.post.dish_img}"/>  		<div class="info">{$_lang.tbl_dishes.image_size_info}</div> 		<div class="error" id="dish_img_err"></div>	</div>	<!--	<div class="field-row">		<label for="dish_start_date">{$_lang.tbl_dishes.label.dish_start_date}</label>		<input type="text" name="dish_start_date" id="dish_start_date" value="{$smarty.post.dish_start_date}"/>		<div class="error" id="dish_start_date_err"></div>	</div>	-->	<!--	<div class="field-row">		<label for="dish_end_date">{$_lang.tbl_dishes.label.dish_end_date}</label>		<input type="text" name="dish_end_date" id="dish_end_date" value="{$smarty.post.dish_end_date}"/>		<div class="error" id="dish_end_date_err"></div>	</div>	-->	<input  type="hidden" id="action" name="action" value="{$smarty.const.ACTION_CREATE}"/>	{html_input type="hidden" name="dish_restaurent" value=$smarty.session[$smarty.const.SES_RESTAURANT]}		<div class="biz_center">		<input data-inline="true" data-icon="save" class="fleft" type="submit" value="{$_lang.save_lbl}"/> 		<input data-inline="true" data-icon="delete" class="fright" type="reset" value="{$_lang.cancel_lbl}" onclick="window.location.href='{$page_url}'"/>	</div>	</form>{include file="tbl_dishes/js.tpl"}</div>{include file="footer.tpl"}