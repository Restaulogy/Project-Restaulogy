{include file='header.tpl'}<div class="wrapper"><h1>{$_lang.tbl_dishes.listing_title}</h1>{if $error_msg}	<center>{$error_msg}</center>{/if}{if $result_found gt 0 && $tbl_disheslist}	<table class="biz_data_grid">		<tr>			<th class="{if $smarty.get.sort_on eq 'dish_name'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=dish_name&sort_by={$new_sort}">{$_lang.tbl_dishes.label.dish_name}</a></th>			<th class="{if $smarty.get.sort_on eq 'dish_chef_notes'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=dish_chef_notes&sort_by={$new_sort}">{$_lang.tbl_dishes.label.dish_chef_notes}</a></th>			<th class="{if $smarty.get.sort_on eq 'dish_ingrad_allergic_contents'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=dish_ingrad_allergic_contents&sort_by={$new_sort}">{$_lang.tbl_dishes.label.dish_ingrad_allergic_contents}</a></th>			<th class="{if $smarty.get.sort_on eq 'dish_nutri_cal_info'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=dish_nutri_cal_info&sort_by={$new_sort}">{$_lang.tbl_dishes.label.dish_nutri_cal_info}</a></th>			<th class="{if $smarty.get.sort_on eq 'dish_notes'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=dish_notes&sort_by={$new_sort}">{$_lang.tbl_dishes.label.dish_notes}</a></th>			<th class="{if $smarty.get.sort_on eq 'dish_img'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=dish_img&sort_by={$new_sort}">{$_lang.tbl_dishes.label.dish_img}</a></th>			<th class="action_header">Action</th>		</tr>		{foreach from=$tbl_disheslist item=tbl_dishesitem}		<tr class="{cycle values="odd,even"}">			<td><a href="{$page_url}?mode={$smarty.const.MODE_VIEW}&dish_id={$tbl_dishesitem.dish_id}">{$tbl_dishesitem.dish_name}</a></td>			<td>{$tbl_dishesitem.dish_chef_notes}</td>			<td>{$tbl_dishesitem.dish_ingrad_allergic_contents}</td>			<td>{$tbl_dishesitem.dish_nutri_cal_info}</td>			<td>{$tbl_dishesitem.dish_notes}</td>			<td>{$tbl_dishesitem.dish_img}</td>			<td><a href="{$page_url}?mode={$smarty.const.MODE_UPDATE}&dish_id={$tbl_dishesitem.dish_id}" class="editIcon"></a><a class="deleteIcon" href="#" onclick="deletetbl_dishes({$tbl_dishesitem.dish_id});"></a>{if $tbl_dishesitem.isActive eq 1}<a href="{$page_url}?action={$smarty.const.ACTION_DEACTIVATE}&dish_id={$tbl_dishesitem.dish_id}" class="deactiveIcon" title="{$_lang.tbl_dishes.DEACTIVATE.BTN_LBL}"></a>{else}<a href="{$page_url}?action={$smarty.const.ACTION_ACTIVATE}&dish_id={$tbl_dishesitem.dish_id}" class="activeIcon" title="{$_lang.tbl_dishes.ACTIVATE.BTN_LBL}"></a>{/if}</td>		</tr>	{/foreach}		<tfoot>			<tr>				<td colspan="9">					# {$result_found}&nbsp;&nbsp;&nbsp;{if $pagination neq ""}{$pagination}{/if}					<select onchange="changePage('{$navigationURL}',this.value,{$smarty.request.limit});">					{if $allPageCount gt 1}						{for $foo=1 to $allPageCount}							<option value="{$foo}" {if $foo eq $currentPage}selected="selected"{/if}>{$foo}</option>						{/for}					{else}						<option value="1" disabled="disabled">1</option>					{/if}					</select>				</td>			</tr>		</tfoot>	</table>{else}	<div class="error">{$_lang.tbl_dishes.no_record_found}</div>{/if}<input onclick="window.location.href='{$page_url}?mode={$smarty.const.MODE_CREATE}'" type="button" value="{$_lang.tbl_dishes.CREATE.BTN_LBL}"/></div>{include file="tbl_dishes/js.tpl"}{include file="footer.tpl"}