{include file="header.tpl"}<div class="wrapper"><h1>{$_lang.tbl_dishes.title}</h1>{if $error_msg}	<center>{$error_msg}</center>{/if}<form name="frmupdatetbl_dishes" id="frmupdatetbl_dishes" onsubmit="return validatetbl_dishes();" method="POST" action="{$page_url}" style="{if $isUpdate eq 1}{else}display:none;{/if}" enctype="multipart/form-data">	<input type="hidden" name="dish_id" id="dish_id" value="{$tbl_dishesinfo.dish_id}"/>	<div style="display:none;" class="error" id="dish_id_err"></div>	<div class="field-row">		<label for="dish_name">{$_lang.tbl_dishes.label.dish_name}</label>		<input type="text" name="dish_name" id="dish_name" value="{$tbl_dishesinfo.dish_name}"/>		<div class="error" id="dish_name_err"></div>	</div>	<div class="field-row">		<label for="dish_chef_notes">{$_lang.tbl_dishes.label.dish_chef_notes}</label>		<textarea name="dish_chef_notes" id="dish_chef_notes">{$tbl_dishesinfo.dish_chef_notes}</textarea>		<div class="error" id="dish_chef_notes_err"></div>	</div>	<div class="field-row">		<label for="dish_ingrad_allergic_contents">{$_lang.tbl_dishes.label.dish_ingrad_allergic_contents}</label>		<textarea name="dish_ingrad_allergic_contents" id="dish_ingrad_allergic_contents">{$tbl_dishesinfo.dish_ingrad_allergic_contents}</textarea>		<div class="error" id="dish_ingrad_allergic_contents_err"></div>	</div>	<div class="field-row">		<label for="dish_nutri_cal_info">{$_lang.tbl_dishes.label.dish_nutri_cal_info}</label>        <textarea name="dish_nutri_cal_info" id="dish_nutri_cal_info">{$tbl_dishesinfo.dish_nutri_cal_info}</textarea>		<div class="error" id="dish_nutri_cal_info_err"></div>	</div>	<div class="field-row">		<label for="dish_notes">{$_lang.tbl_dishes.label.dish_notes}</label>		<textarea name="dish_notes" id="dish_notes">{$tbl_dishesinfo.dish_notes}</textarea>		<div class="error" id="dish_notes_err"></div>	</div>	<div class="field-row">		<label for="dish_img">{$_lang.tbl_dishes.label.dish_img}</label>		{if $tbl_dishesinfo.dish_img neq ""}            <a target="_blank" href="{$website}/uploads/dish/{$tbl_dishesinfo.dish_img}"><img width="40" height="40" src="{$website}/uploads/dish/{$tbl_dishesinfo.dish_img}" /></a>        {/if}		<input type="file" name="dish_img" id="dish_img" value="{$tbl_dishesinfo.dish_img}"/>		<div class="error" id="dish_img_err"></div>	</div>	<!--	<div class="field-row">		<label for="dish_start_date">{$_lang.tbl_dishes.label.dish_start_date}</label>		<input type="text" name="dish_start_date" id="dish_start_date" value="{$tbl_dishesinfo.dish_start_date}"/>		<div class="error" id="dish_start_date_err"></div>	</div>	-->	<!--	<div class="field-row">		<label for="dish_end_date">{$_lang.tbl_dishes.label.dish_end_date}</label>		<input type="text" name="dish_end_date" id="dish_end_date" value="{$tbl_dishesinfo.dish_end_date}"/>		<div class="error" id="dish_end_date_err"></div>	</div>	-->	<input type="hidden" id="action" name="action" value="{$smarty.const.ACTION_UPDATE}"/> <input class="fleft" type="submit" value="{$_lang.save_lbl}"/> <input class="fright" type="reset" onclick="$('#tbl_dishes_view').show();$('#frmupdatetbl_dishes').hide();" value="{$_lang.cancel_lbl}"/></form><div id="tbl_dishes_view" class="description" style="{if $isUpdate eq 1}display:none;{/if}">	<table class="listTable">		<tr><th class="fieldItem">{$_lang.field_title}</th><th class="valueItem">{$_lang.value_title}</th></tr>		<!--<tr><td class="fieldItem">{$_lang.tbl_dishes.label.dish_id}<i>:</i></td><td class="valueItem">{$tbl_dishesinfo.dish_id}</td></tr> -->		<tr><td class="fieldItem">{$_lang.tbl_dishes.label.dish_name}<i>:</i></td><td class="valueItem">{$tbl_dishesinfo.dish_name}</td></tr>		<tr><td class="fieldItem">{$_lang.tbl_dishes.label.dish_chef_notes}<i>:</i></td><td class="valueItem">{$tbl_dishesinfo.dish_chef_notes}</td></tr>		<tr><td class="fieldItem">{$_lang.tbl_dishes.label.dish_ingrad_allergic_contents}<i>:</i></td><td class="valueItem">{$tbl_dishesinfo.dish_ingrad_allergic_contents}</td></tr>		<tr><td class="fieldItem">{$_lang.tbl_dishes.label.dish_nutri_cal_info}<i>:</i></td><td class="valueItem">{$tbl_dishesinfo.dish_nutri_cal_info}</td></tr>		<tr><td class="fieldItem">{$_lang.tbl_dishes.label.dish_notes}<i>:</i></td><td class="valueItem">{$tbl_dishesinfo.dish_notes}</td></tr>		<tr><td class="fieldItem">{$_lang.tbl_dishes.label.dish_img}<i>:</i></td><td class="valueItem">        {if $tbl_dishesinfo.dish_img neq ""}            <a target="_blank" href="{$website}/uploads/dish/{$tbl_dishesinfo.dish_img}"><img width="40" height="40" src="{$website}/uploads/dish/{$tbl_dishesinfo.dish_img}" /></a>        {/if}        </td></tr>	<!--		<tr><td class="fieldItem">{$_lang.tbl_dishes.label.dish_start_date}<i>:</i></td><td class="valueItem">{$tbl_dishesinfo.dish_start_date}</td></tr>	-->	<!--		<tr><td class="fieldItem">{$_lang.tbl_dishes.label.dish_end_date}<i>:</i></td><td class="valueItem">{$tbl_dishesinfo.dish_end_date}</td></tr>	-->	<tr><td class="fieldItem">{$_lang.tbl_dishes.label.isActive}<i>:</i></td><td class="valueItem">{if $tbl_dishesinfo.isActive eq 1}{$_lang.tbl_dishes.label.isActive_yes}{else}{$_lang.tbl_dishes.label.isActive_no}{/if}</td></tr>	</table>	<input class="fleft" type="button" value="{$_lang.tbl_dishes.UPDATE.BTN_LBL}" onclick="$('#tbl_dishes_view').hide();$('#frmupdatetbl_dishes').show();"/><input type="button" value="{$_lang.cancel_lbl}" class="fright"  onclick="window.location.href='{$page_url}'"/></div>{include file="tbl_dishes/js.tpl"}</div>{include file="footer.tpl"}