{include file="header.tpl"}<div class="wrapper"><h1>{$tbl_feedbackinfo.recomm_title}</h1>{if $error_msg}	<center>{$error_msg}</center>{/if}<form name="frmupdatetbl_feedback" id="frmupdatetbl_feedback" onsubmit="return validatetbl_feedback();" method="POST" action="{$page_url}" style="{if $isUpdate eq 1}{else}display:none;{/if}">	<input type="hidden" name="id" id="id" value="{$tbl_feedbackinfo.id}"/>	<div style="display:none;" class="error" id="id_err"></div>	<div style="display:none;"> 	<label for="post_id">{$_lang.tbl_feedback.label.post_id}</label>	<input type="text" name="post_id" id="post_id" value="{$tbl_feedbackinfo.id}"/>	<div class="error" id="post_id_err"></div>	<label for="post_title">{$_lang.tbl_feedback.label.post_title}</label>	<input type="text" name="post_title" id="post_title" value="{$tbl_feedbackinfo.name}"/>	<div class="error" id="post_title_err"></div>	<label for="post_type">{$_lang.tbl_feedback.label.post_type}</label>	<input type="text" name="post_type" id="post_type" value="{$smarty.const.DFLT_FEEDBACK_TYPE}"/>	<div class="error" id="post_type_err"></div>	<label for="user_id">{$_lang.tbl_feedback.label.user_id}</label>	<input type="text" name="user_id" id="user_id" value="{$smarty.session.guid}"/>	<div class="error" id="user_id_err"></div>	</div> 	<label for="recomm_title">{$_lang.tbl_feedback.label.recomm_title}</label>	<input type="text" name="recomm_title" id="recomm_title" value="{$tbl_feedbackinfo.recomm_title}"/>	<div class="error" id="recomm_title_err"></div>	<label for="recomm_desc">{$_lang.tbl_feedback.label.recomm_desc}</label>	<input type="text" name="recomm_desc" id="recomm_desc" value="{$tbl_feedbackinfo.recomm_desc}"/>	<div class="error" id="recomm_desc_err"></div>	<label for="recomm_rating">{$_lang.tbl_feedback.label.recomm_rating}</label>	<input type="text" name="recomm_rating" id="recomm_rating" value="{$tbl_feedbackinfo.recomm_rating}"/>	<div class="error" id="recomm_rating_err"></div>	<label for="recomm_QOS_rating">{$_lang.tbl_feedback.label.recomm_QOS_rating}</label>	<input type="text" name="recomm_QOS_rating" id="recomm_QOS_rating" value="{$tbl_feedbackinfo.recomm_QOS_rating}"/>	<div class="error" id="recomm_QOS_rating_err"></div>	<label for="recomm_QOF_rating">{$_lang.tbl_feedback.label.recomm_QOF_rating}</label>	<input type="text" name="recomm_QOF_rating" id="recomm_QOF_rating" value="{$tbl_feedbackinfo.recomm_QOF_rating}"/>	<div class="error" id="recomm_QOF_rating_err"></div>	<label for="recomm_ambience_rating">{$_lang.tbl_feedback.label.recomm_ambience_rating}</label>	<input type="text" name="recomm_ambience_rating" id="recomm_ambience_rating" value="{$tbl_feedbackinfo.recomm_ambience_rating}"/>	<div class="error" id="recomm_ambience_rating_err"></div>	<label for="start_date">{$_lang.tbl_feedback.label.start_date}</label>	<input type="text" name="start_date" id="start_date" value="{$tbl_feedbackinfo.start_date}"/>	<div class="error" id="start_date_err"></div>	<label for="end_date">{$_lang.tbl_feedback.label.end_date}</label>	<input type="text" name="end_date" id="end_date" value="{$tbl_feedbackinfo.end_date}"/>	<div class="error" id="end_date_err"></div>	<center>	<input type="hidden" id="action" name="action" value="{$smarty.const.ACTION_UPDATE}"/> <input data-icon="save" data-inline="true"  type="submit" value="{$_lang.save_lbl}"/> <input data-icon="cancel" data-inline="true" type="reset" onclick="$('#tbl_feedback_view').show();$('#frmupdatetbl_feedback').hide();" value="{$_lang.cancel_lbl}"/>	</center></form><div id="tbl_feedback_view" class="description" style="{if $isUpdate eq 1}display:none;{/if}">	<table class="listTable">		<tr class="biz_hidden"><th class="fieldItem">{$_lang.field_title}</th><th class="valueItem">{$_lang.value_title}</th></tr>		<!--<tr><td class="fieldItem">{$_lang.tbl_feedback.label.id}<i>:</i></td><td class="valueItem">{$tbl_feedbackinfo.id}</td></tr>		<tr><td class="fieldItem">{$_lang.tbl_feedback.label.post_id}<i>:</i></td><td class="valueItem">{$tbl_feedbackinfo.post_id}</td></tr>		<tr><td class="fieldItem">{$_lang.tbl_feedback.label.post_title}<i>:</i></td><td class="valueItem">{$tbl_feedbackinfo.post_title}</td></tr>		<tr><td class="fieldItem">{$_lang.tbl_feedback.label.post_type}<i>:</i></td><td class="valueItem">{$tbl_feedbackinfo.post_type}</td></tr>		<tr><td class="fieldItem">{$_lang.tbl_feedback.label.user_id}<i>:</i></td><td class="valueItem">{$tbl_feedbackinfo.user_id}</td></tr>		<tr><td class="fieldItem">{$_lang.tbl_feedback.label.recomm_title}<i>:</i></td><td class="valueItem">{$tbl_feedbackinfo.recomm_title}</td></tr>-->		<tr><td class="fieldItem">{$_lang.tbl_feedback.label.recomm_desc}<i>:</i></td><td class="valueItem">{$tbl_feedbackinfo.recomm_desc}</td></tr>		<tr><td class="fieldItem">{$_lang.tbl_feedback.label.recomm_rating}<i>:</i></td><td class="valueItem"><img src="{$website}/modules/business_listing/graphics/{$tbl_feedbackinfo.recomm_rating}.gif" alt="{$tbl_feedbackinfo.recomm_rating}"/></td></tr>		<tr><td class="fieldItem">{$_lang.tbl_feedback.label.recomm_QOS_rating}<i>:</i></td><td class="valueItem"><img src="{$website}/modules/business_listing/graphics/{$tbl_feedbackinfo.recomm_QOS_rating}.gif" alt="{$tbl_feedbackinfo.recomm_QOS_rating}"/></td></tr>		<tr><td class="fieldItem">{$_lang.tbl_feedback.label.recomm_QOF_rating}<i>:</i></td><td class="valueItem"><img src="{$website}/modules/business_listing/graphics/{$tbl_feedbackinfo.recomm_QOF_rating}.gif" alt="{$tbl_feedbackinfo.recomm_QOF_rating}"/></td></tr>		<tr><td class="fieldItem">{$_lang.tbl_feedback.label.recomm_ambience_rating}<i>:</i></td><td class="valueItem"><img src="{$website}/modules/business_listing/graphics/{$tbl_feedbackinfo.recomm_ambience_rating}.gif" alt="{$tbl_feedbackinfo.recomm_ambience_rating}"/></td></tr>				<tr><td class="fieldItem">{$_lang.tbl_feedback.label.recomm_email}<i>:</i></td><td class="valueItem">{$tbl_feedbackinfo.recomm_email}</td></tr>		<tr><td class="fieldItem">{$_lang.tbl_feedback.label.recomm_phone}<i>:</i></td><td class="valueItem">{$tbl_feedbackinfo.recomm_phone}</td></tr>				<tr><td class="fieldItem">{$_lang.tbl_feedback.label.recomm_posted_on}<i>:</i></td><td class="valueItem">{$tbl_feedbackinfo.start_date|date_format:"%A, %B %e, %Y"}</td></tr>				<!--<tr><td class="fieldItem">{$_lang.tbl_feedback.label.start_date}<i>:</i></td><td class="valueItem">{$tbl_feedbackinfo.start_date}</td></tr>		<tr><td class="fieldItem">{$_lang.tbl_feedback.label.end_date}<i>:</i></td><td class="valueItem">{$tbl_feedbackinfo.end_date}</td></tr>-->	<!--<tr><td class="fieldItem">{$_lang.tbl_feedback.label.isActive}<i>:</i></td><td class="valueItem">{if $tbl_feedbackinfo.isActive eq 1}{$_lang.tbl_feedback.label.isActive_yes}{else}{$_lang.tbl_feedback.label.isActive_no}{/if}</td></tr>-->	</table>	<center>	<!--<input class="fleft" type="button" value="{$_lang.tbl_feedback.UPDATE.BTN_LBL}" onclick="$('#tbl_feedback_view').hide();$('#frmupdatetbl_feedback').show();"/>--><input data-icon="delete" data-inline="true" type="button" value="{$_lang.cancel_lbl}" class="fright"  onclick="window.location.href='{$page_url}'"/>	</center></div>{include file="tbl_feedback/js.tpl"}</div>{include file="footer.tpl"}