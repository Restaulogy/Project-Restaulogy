{include file='header.tpl'}<div class="wrapper"><h1><center>    {if $my_post eq 0}        {$_lang.tbl_feedback.all_post}	{else}	    {$_lang.tbl_feedback.my_post}    {/if}</center></h1>{if $error_msg}	<center>{$error_msg}</center>{/if}{include file="tbl_feedback/tabs.tpl"}{* include file='tbl_feedback/navbar.tpl' *}{*{if $result_found gt 0 && $tbl_feedbacklist}<form name="frm_tbl_feedback" id="frm_tbl_feedback"  method="POST" action="{$page_url}" >	<table class="listTable">		<tr>        <th class="bigListItem" colspan="2">{$_lang.tbl_feedback.title}</th>        {if $Global_member.member_role_id eq $smarty.const.ROLE_MANAGER || $Global_member.member_role_id eq $smarty.const.ROLE_ADMIN }            <th >                {$_lang.tbl_feedback.label.recomm_table}        	 </th>        	 <th >                {$_lang.tbl_feedback.label.recomm_posted_on}        	 </th>             <th style="width:5%;">                {$_lang.main.status}        	 </th>		{/if}        </tr>		{foreach from=$tbl_feedbacklist item=tbl_feedbackitem}			<tr>                <td style="width:2%;">                {if $Global_member.member_role_id eq $smarty.const.ROLE_MANAGER || $Global_member.member_role_id eq $smarty.const.ROLE_ADMIN }				<label for="sel_tbl_feedback[{$tbl_feedbackitem.id}]" data-mini="true" style="width:23px;"><input type="checkbox" data-inline='true' data-mini='true' id="sel_tbl_feedback[{$tbl_feedbackitem.id}]" name="sel_tbl_feedback[{$tbl_feedbackitem.id}]" />&nbsp;</label>				{else}                    &nbsp;                {/if}                </td>				<td class="bigListItem"><a href="{$page_url}&mode={$smarty.const.MODE_VIEW}&id={$tbl_feedbackitem.id}">                {$tbl_feedbackitem.recomm_title}                <img src="{$website}/modules/business_listing/graphics/{math equation='x * 2' x=$tbl_feedbackitem.recomm_rating}.gif" alt="{math equation='x * 2' x=$tbl_feedbackitem.recomm_rating}"/><br>                <small>{$tbl_feedbackitem.recomm_desc|truncate:60}</small>                </a></td>                                {if $Global_member.member_role_id eq $smarty.const.ROLE_MANAGER || $Global_member.member_role_id eq $smarty.const.ROLE_ADMIN }                <td>                    {$tbl_feedbackitem.recomm_table_number}                </td>                <td>                {* if $smarty.now|date_format:"%Y%m%d" == $tbl_feedbackitem.start_date|date_format:"%Y%m%d"}                    {$tbl_feedbackitem.start_date|date_format:$smarty.const.HTML5_DATETIME_FORMAT}                {else}                    {$tbl_feedbackitem.start_date|date_format:$smarty.const.HTML5_DAY_FORMAT}                {/if *}                {$tbl_feedbackitem.friendly_created_on}                </td>				<td >    				{if $tbl_feedbackitem.isActive eq 1}                        <a href="#" class="activeIcon" title="{$_lang.tbl_feedback.ACTIVATE.BTN_LBL}"></a>                    {else}                        <a href="#" class="deactiveIcon" title="{$_lang.tbl_feedback.DEACTIVATE.BTN_LBL}"></a>    				{/if}    				<!--                    <a href="{$page_url}&mode={$smarty.const.MODE_UPDATE}&id={$tbl_feedbackitem.id}" class="editIcon"></a>                     <a class="deleteIcon" href="#" onclick="deletetbl_feedback({$tbl_feedbackitem.id});"></a>                     -->				</td>				{/if}			</tr>	{/foreach}	</table>{else}	<div class="error">{$_lang.tbl_feedback.no_record_found}</div>{/if}{if $pagination}	<center>{$pagination}</center>{/if}<input type='hidden' name='action' id='action' value='' /></form>*}{if $restaurentinfo.rest_review_systems}  {include file="tbl_feedback/add_site_review.tpl"}{/if}<div class="biz_center" >{if $result_found gt 0 && $tbl_feedbacklist && ($Global_member.member_role_id eq $smarty.const.ROLE_MANAGER || $Global_member.member_role_id eq $smarty.const.ROLE_ADMIN)}    <input data-inline="true" data-icon="briefcase" type="button" id="sel_all_tbl_feedback" name="sel_all_tbl_feedback" value="{$_lang.main.toggle}" onclick="javascript:$('input[type=checkbox]').click();" />    <input data-inline="true" data-icon="inactive" type="button" value="{$_lang.tbl_dishes.DEACTIVATE.BTN_LBL}" onclick="actiontbl_feedback('{$smarty.const.ACTION_DEACTIVATE}');" />    <input data-inline="true" data-icon="active" type="button" value="{$_lang.tbl_dishes.ACTIVATE.BTN_LBL}" onclick="actiontbl_feedback('{$smarty.const.ACTION_ACTIVATE}');" />    <input data-inline="true" data-icon="recycle-full" type="button"  value="{$_lang.tbl_dishes.DELETE.BTN_LBL}" onclick="actiontbl_feedback('{$smarty.const.ACTION_DELETE}');" />{/if}    <!--    <input data-inline="true" data-icon="add-item" onclick="window.location.href='{if $Global_member.member_role_id eq $smarty.const.ROLE_CUSTOMER}{$page_url}&mode={$smarty.const.MODE_CREATE}{else}{$website}/user/login.php?prev_page=FEEDBACK{/if}'" type="button" value="{$_lang.tbl_feedback.CREATE.BTN_LBL}"/>    -->{if $smarty.session[$smarty.const.SES_TABLE]>0}    <input data-inline="true" data-icon="add-item" onclick="window.location.href='{$page_url}&mode={$smarty.const.MODE_CREATE}';" type="button" value="{$_lang.tbl_feedback.CREATE.BTN_LBL}"/>{/if}</div><!-- --></div>{include file="tbl_feedback/js.tpl"}{include file="footer.tpl"}