{include file='header.tpl'}<div class="wrapper"><h1>{$_lang.tbl_feedback.feed_stats}<a href="#" style="float:right;" data-role="button" data-icon="search" data-iconpos="notext" onclick="$('#filter_tbl_feedback').toggle();"></a></h1>{**include file="dish_navbar.tpl"**}{include file="tbl_feedback/tabs.tpl"}{include file="tbl_feedback/search.tpl"}{if $search_text neq ''}	<div style="width:99%;padding:5px;border-bottom:1px dotted #ccc;font-style: italic;"><span class="info">Search results &laquo; </span><span class="notice">{$search_text}</span><span class="info">&raquo;</span></div><br>{/if}{if $error_msg}	<center>{$error_msg}</center>{/if}{include file="tbl_feedback/search.tpl"}{if $result_found gt 0 && $tbl_disheslist}	<table class="listTable">	<tr>    <th class="bigListItem">{if $fts_is_restaurant eq 1}Restaurant{else}{$_lang.tbl_dishes.title}{/if}</th>    <th style="width:5%;">        {$_lang.main.rating}	</th>	</tr>		{foreach from=$tbl_disheslist item=tbl_dishesitem}			<tr>              {if $fts_is_restaurant eq 1}                    {include file="tbl_feedback/restaurant_rating.tpl"}              {else}                    {include file="tbl_feedback/dish_rating.tpl"}              {/if}			</tr>        {/foreach}	</table>{else}	<div class="error">{$_lang.tbl_feedback.no_record_found}</div>{/if}{if $fts_is_restaurant eq 1}    <div class="line_break"></div>    <h3><b>Itemwise Rating: {$report_img.title}</b></h3>    <div class="biz_center">     {if $report_img neq "" && $report_img.img_src neq ""}    	 <img src="data:image/png;base64,{$report_img.img_src}" width="90%;"/>     {else}    	 <img src="{$website}/images/_graphics/no_record.png"/>     {/if}    </div>        <div class="line_break"></div>    <h3><b>Overall Rating: {$report_img.title}</b></h3>    <div class="biz_center">     {if $report_img neq "" && $report_img_overall.img_src neq ""}    	 <img src="data:image/png;base64,{$report_img_overall.img_src}" width="90%;"/>     {else}    	 <img src="{$website}/images/_graphics/no_record.png"/>     {/if}    </div>        {/if}{if $pagination}	<center>{$pagination}</center>{/if} <div class="biz_center"></div></div>{*include file="tbl_dishes/js.tpl"*}{include file="footercontent.tpl"}{include file="tbl_table_status_link/js.tpl"}{literal}	<script type="text/javascript">		$(function(){			$("#fts_start_date").scroller({ preset: 'date',dateFormat : '{/literal}{$smarty.const.MOBISCROL_FORMAT}{literal}',  animate: 'pop'});			$("#fts_end_date").scroller({ preset: 'date',dateFormat : '{/literal}{$smarty.const.MOBISCROL_FORMAT}{literal}',  animate: 'pop'});		})	</script>{/literal}</body></html>