{include file='header.tpl'}<div class="wrapper"><h1>{$_lang.tbl_crm.listing_title}</h1>{if $error_msg}	<div class="biz_center">{$error_msg}</div>{/if}<form name="frm_tbl_crmlist" id="frm_tbl_crmlist"  method="POST" action="{$website}/user/tbl_crm.php" >{if $result_found gt 0 && $tbl_crmlist}	<table class="listTable">		<tr>			<th colspan="2">{$_lang.tbl_crm.label.crm_cust_id}</th>			<!-- <th>{$_lang.tbl_crm.label.crm_cust_email}</th> -->			<th>{$_lang.tbl_crm.label.crm_cust_type}</th>			<th>{$_lang.tbl_crm.label.crm_is_subsribed}</th>			<th>{$_lang.main.status}</th>		</tr>		{foreach from=$tbl_crmlist item=tbl_crmitem}		<tr> 				<td style="width:2%;">                    <label for="sel_tbl_crm[{$tbl_crmitem.crm_id}]" data-mini="true" style="width:23px;"><input type="checkbox" data-inline='true' data-mini='true' id="sel_tbl_crm[{$tbl_crmitem.crm_id}]" name="sel_tbl_crm[{$tbl_crmitem.crm_id}]" />&nbsp;</label>                </td>				<td style="width:30%;">{$tbl_crmitem.customer_name} <br> {$tbl_crmitem.crm_cust_email}                    <!--                        <a href="{$page_url}?mode={$smarty.const.MODE_VIEW}&crm_id={$tbl_crmitem.crm_id}">{$tbl_crmitem.crm_cust_email}</a>                    -->                </td>						<!--                <td style="width:30%;">{$tbl_crmitem.crm_cust_email}                        <a href="{$page_url}?mode={$smarty.const.MODE_VIEW}&crm_id={$tbl_crmitem.crm_id}">{$tbl_crmitem.crm_cust_email}</a>                </td>                -->				<td style="width:20%">{if $tbl_crmitem.crm_cust_type eq $smarty.const.CUST_TYPE_LOGIN}Login{else}Cookie{/if}</td>				<td style="width:18%">{if $tbl_crmitem.crm_is_subsribed eq 1}Yes{else}No{/if}</td>  				<td style="width:5%">                    {if $tbl_crmitem.isActive eq 1}                        <a href="#" class="activeIcon" title="{$_lang.tbl_crm.ACTIVATE.BTN_LBL}"></a>                    {else}                        <a href="#" class="deactiveIcon" title="{$_lang.tbl_crm.DEACTIVATE.BTN_LBL}"></a>                    {/if}                </td>				<!--<td class="actionListItem"><a href="{$page_url}?mode={$smarty.const.MODE_UPDATE}&crm_id={$tbl_crmitem.crm_id}" class="editIcon"></a><a class="deleteIcon" href="#" onclick="deletetbl_crm({$tbl_crmitem.crm_id});"></a>{if $tbl_crmitem.isActive eq 1}<a href="{$page_url}?action={$smarty.const.ACTION_DEACTIVATE}&crm_id={$tbl_crmitem.crm_id}" class="deactiveIcon" title="{$_lang.tbl_crm.DEACTIVATE.BTN_LBL}"></a>{else}<a href="{$page_url}?action={$smarty.const.ACTION_ACTIVATE}&crm_id={$tbl_crmitem.crm_id}" class="activeIcon" title="{$_lang.tbl_crm.ACTIVATE.BTN_LBL}"></a>{/if}</td>-->			</tr>	{/foreach}	</table>{else}	<div class="error">{$_lang.tbl_crm.no_record_found}</div>{/if}	{html_input type="hidden" name="action" value=""}</form>{if $pagination}	<center>{$pagination}</center>{/if}<div class="biz_center"><input data-inline="true" data-icon="briefcase" type="button" id="sel_all_tbl_crm" name="sel_all_tbl_crm" value="{$_lang.main.toggle}" onclick="javascript:$('input[type=checkbox]').click();" /> <input data-inline="true" data-icon="inactive" type="button" value="{$_lang.tbl_crm.DEACTIVATE.BTN_LBL}" onclick="actiontbl_crm('{$smarty.const.ACTION_DEACTIVATE}');"  /><input data-inline="true" data-icon="active" type="button" value="{$_lang.tbl_crm.ACTIVATE.BTN_LBL}" onclick="actiontbl_crm('{$smarty.const.ACTION_ACTIVATE}');" /> <!--<input data-icon="add-item" data-inline="true" onclick="window.location.href='{$page_url}?mode={$smarty.const.MODE_CREATE}'" type="button" value="{$_lang.tbl_crm.CREATE.BTN_LBL}"/>--></div></div>{include file="tbl_crm/js.tpl"}{include file="footer.tpl"}