{include file="header.tpl"}<div class="wrapper"><h1>{$_lang.tbl_crm.title}</h1>{if $error_msg}	<center>{$error_msg}</center>{/if}<form name="frmupdatetbl_crm" id="frmupdatetbl_crm" onsubmit="return validatetbl_crm();" method="POST" action="{$page_url}" style="{if $isUpdate eq 1}{else}display:none;{/if}">	<input type="hidden" name="crm_id" id="crm_id" value="{$tbl_crminfo.crm_id}"/>	<div style="display:none;" class="error" id="crm_id_err"></div>	<div class="field-row">		<label for="crm_cust_id">{$_lang.tbl_crm.label.crm_cust_id}</label>		<input type="text" name="crm_cust_id" id="crm_cust_id" value="{$tbl_crminfo.crm_cust_id}"/>		<div class="error" id="crm_cust_id_err"></div>	</div>	<div class="field-row">		<label for="crm_cust_email">{$_lang.tbl_crm.label.crm_cust_email}</label>		<input type="text" name="crm_cust_email" id="crm_cust_email" value="{$tbl_crminfo.crm_cust_email}"/>		<div class="error" id="crm_cust_email_err"></div>	</div>	<div class="field-row">		<label for="crm_cust_type">{$_lang.tbl_crm.label.crm_cust_type}</label>		<input type="text" name="crm_cust_type" id="crm_cust_type" value="{$tbl_crminfo.crm_cust_type}"/>		<div class="error" id="crm_cust_type_err"></div>	</div>	<div class="field-row">		<label for="crm_is_subsribed">{$_lang.tbl_crm.label.crm_is_subsribed}</label>		<input type="text" name="crm_is_subsribed" id="crm_is_subsribed" value="{$tbl_crminfo.crm_is_subsribed}"/>		<div class="error" id="crm_is_subsribed_err"></div>	</div>	<!--	<div class="field-row">		<label for="crm_start_date">{$_lang.tbl_crm.label.crm_start_date}</label>		<input type="text" name="crm_start_date" id="crm_start_date" value="{$tbl_crminfo.crm_start_date}"/>		<div class="error" id="crm_start_date_err"></div>	</div>	-->	<!--	<div class="field-row">		<label for="crm_end_date">{$_lang.tbl_crm.label.crm_end_date}</label>		<input type="text" name="crm_end_date" id="crm_end_date" value="{$tbl_crminfo.crm_end_date}"/>		<div class="error" id="crm_end_date_err"></div>	</div>	-->	<center><input type="hidden" id="action" name="action" value="{$smarty.const.ACTION_UPDATE}"/> <input  data-inline="true" data-icon="save"  type="submit" value="{$_lang.save_lbl}"/> <input data-inline="true" data-icon="delete" type="reset" onclick="$('#tbl_crm_view').show();$('#frmupdatetbl_crm').hide();" value="{$_lang.cancel_lbl}"/></center></form><div id="tbl_crm_view" class="description" style="{if $isUpdate eq 1}display:none;{/if}">	<table class="listTable">		<tr><th class="fieldItem">{$_lang.field_title}</th><th class="valueItem">{$_lang.value_title}</th></tr>		<tr><td class="fieldItem">{$_lang.tbl_crm.label.crm_id}<i>:</i></td><td class="valueItem">{$tbl_crminfo.crm_id}</td></tr>		<tr><td class="fieldItem">{$_lang.tbl_crm.label.crm_cust_id}<i>:</i></td><td class="valueItem">{$tbl_crminfo.crm_cust_id}</td></tr>		<tr><td class="fieldItem">{$_lang.tbl_crm.label.crm_cust_email}<i>:</i></td><td class="valueItem">{$tbl_crminfo.crm_cust_email}</td></tr>		<tr><td class="fieldItem">{$_lang.tbl_crm.label.crm_cust_type}<i>:</i></td><td class="valueItem">{$tbl_crminfo.crm_cust_type}</td></tr>		<tr><td class="fieldItem">{$_lang.tbl_crm.label.crm_is_subsribed}<i>:</i></td><td class="valueItem">{$tbl_crminfo.crm_is_subsribed}</td></tr>	<!--		<tr><td class="fieldItem">{$_lang.tbl_crm.label.crm_start_date}<i>:</i></td><td class="valueItem">{$tbl_crminfo.crm_start_date}</td></tr>	-->	<!--		<tr><td class="fieldItem">{$_lang.tbl_crm.label.crm_end_date}<i>:</i></td><td class="valueItem">{$tbl_crminfo.crm_end_date}</td></tr>	-->	<tr><td class="fieldItem">{$_lang.tbl_crm.label.isActive}<i>:</i></td><td class="valueItem">{if $tbl_crminfo.isActive eq 1}{$_lang.tbl_crm.label.isActive_yes}{else}{$_lang.tbl_crm.label.isActive_no}{/if}</td></tr>	</table>	<center><input data-inline="true" data-icon="edit" type="button" value="{$_lang.tbl_crm.UPDATE.BTN_LBL}" onclick="$('#tbl_crm_view').hide();$('#frmupdatetbl_crm').show();"/><input type="button" value="{$_lang.cancel_lbl}" data-inline="true" data-icon="delete" onclick="window.location.href='{$page_url}'"/></center></div>{include file="tbl_crm/js.tpl"}</div>{include file="footer.tpl"}