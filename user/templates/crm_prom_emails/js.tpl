{literal}<script type="text/javascript">function validDtRange(frmDtId,toDtId){    if(frmDtId){}else{		frmDtId = 'search_from_dt';	}	if(toDtId){}else{		toDtId = 'search_to_dt';	}	if(new Date($('#'+ toDtId).val()).getTime() < new Date($('#' + frmDtId).val()).getTime()){	//if($('#'+ toDtId).val() < $('#' + frmDtId).val()){		alert('End date should be greter than start date');		return false;	}	return true;}function deletecrm_prom_emails(varId){	if(varId > 0){		if(confirm("{/literal}{$_lang.crm_prom_emails.DELETE.CONFIRM_MSG}{literal}")==true){			window.location.href="{/literal}{$page_url}{literal}?action=delete&crm_pr_ml_id="+varId;		}	}}function validatecrm_prom_emails(){	$("#crm_pr_ml_id_err").html("");	$("#crm_pr_ml_userid_err").html("");	$("#crm_pr_ml_promotion_err").html("");	$("#flg_send_err").html("");	$("#start_date_err").html("");	$("#end_date_err").html("");	var isErr = true;	if(elemById("action").value=="update"){		if(IsNonEmpty(elemById("crm_pr_ml_id").value)==false){			$("#crm_pr_ml_id_err").html("{/literal}{$_lang.crm_prom_emails.not_empty_msg.crm_pr_ml_id}{literal}");			isErr = false;		}	}	if(IsNonEmpty(elemById("crm_pr_ml_userid").value)==false){		$("#crm_pr_ml_userid_err").html("{/literal}{$_lang.crm_prom_emails.not_empty_msg.crm_pr_ml_userid}{literal}");		isErr = false;	}	if(IsNonEmpty(elemById("crm_pr_ml_promotion").value)==false){		$("#crm_pr_ml_promotion_err").html("{/literal}{$_lang.crm_prom_emails.not_empty_msg.crm_pr_ml_promotion}{literal}");		isErr = false;	}	if(IsNonEmpty(elemById("flg_send").value)==false){		$("#flg_send_err").html("{/literal}{$_lang.crm_prom_emails.not_empty_msg.flg_send}{literal}");		isErr = false;	}	if(IsNonEmpty(elemById("start_date").value)==false){		$("#start_date_err").html("{/literal}{$_lang.crm_prom_emails.not_empty_msg.start_date}{literal}");		isErr = false;	}	if(IsNonEmpty(elemById("end_date").value)==false){		$("#end_date_err").html("{/literal}{$_lang.crm_prom_emails.not_empty_msg.end_date}{literal}");		isErr = false;	}	if(isErr == false){		alert("{/literal}{$_lang.messages.revise_form}{literal}");	}	return isErr;}//..function</script>{/literal}