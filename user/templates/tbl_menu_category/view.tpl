{include file="header.tpl"}<div class="wrapper"><h1>{$_lang.tbl_menu_category.title}</h1>{if $error_msg}	<center>{$error_msg}</center>{/if}<form name="frmupdatetbl_menu_category" id="frmupdatetbl_menu_category" onsubmit="return validatetbl_menu_category();" method="POST" action="{$page_url}" style="{if $isUpdate eq 1}{else}display:none;{/if}">	<input type="hidden" name="mnu_cat_id" id="mnu_cat_id" value="{$tbl_menu_categoryinfo.mnu_cat_id}"/>	<div style="display:none;" class="error" id="mnu_cat_id_err"></div>	<div class="field-row">		<label for="mnu_cat_name">{$_lang.tbl_menu_category.label.mnu_cat_name}</label>		<input type="text" name="mnu_cat_name" id="mnu_cat_name" value="{$tbl_menu_categoryinfo.mnu_cat_name}"/>		<div class="error" id="mnu_cat_name_err"></div>	</div>	<div class="field-row">		<label for="mnu_cat_desc">{$_lang.tbl_menu_category.label.mnu_cat_desc}</label>		<input type="text" name="mnu_cat_desc" id="mnu_cat_desc" value="{$tbl_menu_categoryinfo.mnu_cat_desc}"/>		<div class="error" id="mnu_cat_desc_err"></div>	</div>	<!--	<div class="field-row">		<label for="mnu_cat_start_date">{$_lang.tbl_menu_category.label.mnu_cat_start_date}</label>		<input type="text" name="mnu_cat_start_date" id="mnu_cat_start_date" value="{$tbl_menu_categoryinfo.mnu_cat_start_date}"/>		<div class="error" id="mnu_cat_start_date_err"></div>	</div>	-->	<!--	<div class="field-row">		<label for="mnu_cat_end_date">{$_lang.tbl_menu_category.label.mnu_cat_end_date}</label>		<input type="text" name="mnu_cat_end_date" id="mnu_cat_end_date" value="{$tbl_menu_categoryinfo.mnu_cat_end_date}"/>		<div class="error" id="mnu_cat_end_date_err"></div>	</div>	-->	<input type="hidden" id="action" name="action" value="{$smarty.const.ACTION_UPDATE}"/> <input class="fleft" type="submit" value="{$_lang.save_lbl}"/> <input class="fright" type="reset" onclick="$('#tbl_menu_category_view').show();$('#frmupdatetbl_menu_category').hide();" value="{$_lang.cancel_lbl}"/></form><div id="tbl_menu_category_view" class="description" style="{if $isUpdate eq 1}display:none;{/if}">	<table class="listTable">		<tr class="biz_hidden"><th class="fieldItem">{$_lang.field_title}</th><th class="valueItem">{$_lang.value_title}</th></tr>		<tr><td class="fieldItem">{$_lang.tbl_menu_category.label.mnu_cat_id}<i>:</i></td><td class="valueItem">{$tbl_menu_categoryinfo.mnu_cat_id}</td></tr>		<tr><td class="fieldItem">{$_lang.tbl_menu_category.label.mnu_cat_name}<i>:</i></td><td class="valueItem">{$tbl_menu_categoryinfo.mnu_cat_name}</td></tr>		<tr><td class="fieldItem">{$_lang.tbl_menu_category.label.mnu_cat_desc}<i>:</i></td><td class="valueItem">{$tbl_menu_categoryinfo.mnu_cat_desc}</td></tr>	<!--		<tr><td class="fieldItem">{$_lang.tbl_menu_category.label.mnu_cat_start_date}<i>:</i></td><td class="valueItem">{$tbl_menu_categoryinfo.mnu_cat_start_date}</td></tr>	-->	<!--		<tr><td class="fieldItem">{$_lang.tbl_menu_category.label.mnu_cat_end_date}<i>:</i></td><td class="valueItem">{$tbl_menu_categoryinfo.mnu_cat_end_date}</td></tr>	-->	<tr><td class="fieldItem">{$_lang.tbl_menu_category.label.isActive}<i>:</i></td><td class="valueItem">{if $tbl_menu_categoryinfo.isActive eq 1}{$_lang.tbl_menu_category.label.isActive_yes}{else}{$_lang.tbl_menu_category.label.isActive_no}{/if}</td></tr>	</table>	<input class="fleft" type="button" value="{$_lang.tbl_menu_category.UPDATE.BTN_LBL}" onclick="$('#tbl_menu_category_view').hide();$('#frmupdatetbl_menu_category').show();"/><input type="button" value="{$_lang.cancel_lbl}" class="fright"  onclick="window.location.href='{$page_url}'"/></div>{include file="tbl_menu_category/js.tpl"}</div>{include file="footer.tpl"}