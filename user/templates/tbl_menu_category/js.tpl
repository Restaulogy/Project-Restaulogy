{literal}<script type="text/javascript">function deletetbl_menu_category(varId){	if(varId > 0){		if(confirm("{/literal}{$_lang.tbl_menu_category.DELETE.CONFIRM_MSG}{literal}")==true){			window.location.href="{/literal}{$page_url}{literal}?action=delete&mnu_cat_id="+varId;		}	}}function validatetbl_menu_category(){	$("#mnu_cat_id_err").html("");	$("#mnu_cat_name_err").html("");	$("#mnu_cat_desc_err").html("");	$("#mnu_cat_start_date_err").html("");	$("#mnu_cat_end_date_err").html("");	var isErr = true;	if(elemById("action").value=="update"){		if(IsNonEmpty(elemById("mnu_cat_id").value)==false){			$("#mnu_cat_id_err").html("{/literal}{$_lang.tbl_menu_category.not_empty_msg.mnu_cat_id}{literal}");			isErr = false;		}	}	if(IsNonEmpty(elemById("mnu_cat_name").value)==false){		$("#mnu_cat_name_err").html("{/literal}{$_lang.tbl_menu_category.not_empty_msg.mnu_cat_name}{literal}");		isErr = false;	}	if(IsNonEmpty(elemById("mnu_cat_desc").value)==false){		$("#mnu_cat_desc_err").html("{/literal}{$_lang.tbl_menu_category.not_empty_msg.mnu_cat_desc}{literal}");		isErr = false;	}/*	if(IsNonEmpty(elemById("mnu_cat_start_date").value)==false){		$("#mnu_cat_start_date_err").html("{/literal}{$_lang.tbl_menu_category.not_empty_msg.mnu_cat_start_date}{literal}");		isErr = false;	}*//*	if(IsNonEmpty(elemById("mnu_cat_end_date").value)==false){		$("#mnu_cat_end_date_err").html("{/literal}{$_lang.tbl_menu_category.not_empty_msg.mnu_cat_end_date}{literal}");		isErr = false;	}*/	if(isErr == false){		alert("{/literal}{$_lang.messages.revise_form}{literal}");	}	return isErr;}//..function</script>{/literal}