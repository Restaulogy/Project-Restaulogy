{include file="header.tpl"}<div class="wrapper"><h1>{$_lang.biz_checkins.title}</h1>{if $error_msg}	<center>{$error_msg}</center>{/if}<form name="frmupdatebiz_checkins" id="frmupdatebiz_checkins" onsubmit="return validatebiz_checkins();" method="POST" action="{$page_url}" style="{if $isUpdate eq 1}{else}display:none;{/if}">	<input type="hidden" name="chkin_id" id="chkin_id" value="{$biz_checkinsinfo.chkin_id}"/>	<div style="display:none;" class="error" id="chkin_id_err"></div>	<div class="field-row">		<label for="chkin_buss_id">{$_lang.biz_checkins.label.chkin_buss_id}</label>		<input type="text" name="chkin_buss_id" id="chkin_buss_id" value="{$biz_checkinsinfo.chkin_buss_id}"/>		<div class="error" id="chkin_buss_id_err"></div>	</div>	<div class="field-row">		<label for="chkin_rwd_id">{$_lang.biz_checkins.label.chkin_rwd_id}</label>		<input type="text" name="chkin_rwd_id" id="chkin_rwd_id" value="{$biz_checkinsinfo.chkin_rwd_id}"/>		<div class="error" id="chkin_rwd_id_err"></div>	</div>	<div class="field-row">		<label for="chkin_user_id">{$_lang.biz_checkins.label.chkin_user_id}</label>		<input type="text" name="chkin_user_id" id="chkin_user_id" value="{$biz_checkinsinfo.chkin_user_id}"/>		<div class="error" id="chkin_user_id_err"></div>	</div>	<div class="field-row">		<label for="chkin_session">{$_lang.biz_checkins.label.chkin_session}</label>		<input type="text" name="chkin_session" id="chkin_session" value="{$biz_checkinsinfo.chkin_session}"/>		<div class="error" id="chkin_session_err"></div>	</div>	<div class="field-row">		<label for="chkin_points">{$_lang.biz_checkins.label.chkin_points}</label>		<input type="text" name="chkin_points" id="chkin_points" value="{$biz_checkinsinfo.chkin_points}"/>		<div class="error" id="chkin_points_err"></div>	</div>	<div class="field-row">		<label for="chkin_date">{$_lang.biz_checkins.label.chkin_date}</label>		<input type="text" name="chkin_date" id="chkin_date" value="{$biz_checkinsinfo.chkin_date}"/>		<div class="error" id="chkin_date_err"></div>	</div>	<center><input type="hidden" id="action" name="action" value="{$smarty.const.ACTION_UPDATE}"/> <input  data-inline="true" data-icon="save"  type="submit" value="{$_lang.save_lbl}"/> <input data-inline="true" data-icon="delete" type="reset" onclick="$('#biz_checkins_view').show();$('#frmupdatebiz_checkins').hide();" value="{$_lang.cancel_lbl}"/></center></form><div id="biz_checkins_view" class="description" style="{if $isUpdate eq 1}display:none;{/if}">	<table class="listTable">		<tr><th class="fieldItem">{$_lang.field_title}</th><th class="valueItem">{$_lang.value_title}</th></tr>		<tr><td class="fieldItem">{$_lang.biz_checkins.label.chkin_id}<i>:</i></td><td class="valueItem">{$biz_checkinsinfo.chkin_id}</td></tr>		<tr><td class="fieldItem">{$_lang.biz_checkins.label.chkin_buss_id}<i>:</i></td><td class="valueItem">{$biz_checkinsinfo.chkin_buss_id}</td></tr>		<tr><td class="fieldItem">{$_lang.biz_checkins.label.chkin_rwd_id}<i>:</i></td><td class="valueItem">{$biz_checkinsinfo.chkin_rwd_id}</td></tr>		<tr><td class="fieldItem">{$_lang.biz_checkins.label.chkin_user_id}<i>:</i></td><td class="valueItem">{$biz_checkinsinfo.chkin_user_id}</td></tr>		<tr><td class="fieldItem">{$_lang.biz_checkins.label.chkin_session}<i>:</i></td><td class="valueItem">{$biz_checkinsinfo.chkin_session}</td></tr>		<tr><td class="fieldItem">{$_lang.biz_checkins.label.chkin_points}<i>:</i></td><td class="valueItem">{$biz_checkinsinfo.chkin_points}</td></tr>		<tr><td class="fieldItem">{$_lang.biz_checkins.label.chkin_date}<i>:</i></td><td class="valueItem">{$biz_checkinsinfo.chkin_date}</td></tr>	<tr><td class="fieldItem">{$_lang.biz_checkins.label.isActive}<i>:</i></td><td class="valueItem">{if $biz_checkinsinfo.isActive eq 1}{$_lang.biz_checkins.label.isActive_yes}{else}{$_lang.biz_checkins.label.isActive_no}{/if}</td></tr>	</table>	<center><input data-inline="true" data-icon="edit" type="button" value="{$_lang.biz_checkins.UPDATE.BTN_LBL}" onclick="$('#biz_checkins_view').hide();$('#frmupdatebiz_checkins').show();"/><input type="button" value="{$_lang.cancel_lbl}" data-inline="true" data-icon="delete" onclick="window.location.href='{$page_url}'"/></center></div>{include file="biz_checkins/js.tpl"}</div>{include file="footer.tpl"}