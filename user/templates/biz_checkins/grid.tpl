{include file='header.tpl'}<div class="wrapper"><h1>{$_lang.biz_checkins.listing_title}</h1>{if $rpt_to_show=='actvty_log'}    {include file="tbl_qrcode_log/tabs.tpl"}{else}    {include file='biz_rewards/tabs.tpl'}{/if}<a href="#" style="float:right;" data-role="button" data-icon="search" data-iconpos="notext" onclick="$('#filter_for_checkin').toggle();"></a>{if $filt_text neq ''}	<div style="width:99%;padding:5px;border-bottom:1px dotted #ccc;font-style: italic;"><span class="info">Search results &laquo; </span><span class="notice">{$filt_text}</span><span class="info">&raquo;</span></div><br>{/if}{if $error_msg}	<center>{$error_msg}</center>{/if}{include file='biz_checkins/search.tpl'}<div style='display:none;float:right;padding-right:8px;'>    <!-- <input type="button" data-icon="add-item" data-inline="true" name="add_visit" onclick="$('#popupAddVisit').popup('open');" value="Add Visit" /> -->     {if $smarty.session[$smarty.const.SES_REWARD].id gt 0}         {if $isCustomer gt 0}            {if $_RWD_CONFIG_TYPE eq 'CUSTOMER'}                  {if $server_validated eq 1}                    <input type="button" data-icon="add-item" data-inline="true" name="add_visit" onclick="$('#popupAddVisit').popup('open');" value="{$_lang.biz_checkins.create_title}" />                   {else}                         <input type="button" data-icon="add-item" data-inline="true" name="add_visit" onclick="$('#cust_popupServerPin').popup('open');" value="{$_lang.biz_checkins.create_title}" />                   {/if}            {elseif $_RWD_CONFIG_TYPE eq 'RESTAURANT'}                   <input type="button" data-icon="add-item" data-inline="true" name="add_visit" onclick="post_for_add_vist(1);" value="{$_lang.biz_checkins.create_title}" />            {/if}    	 {else}               <input type="button" data-icon="add-item" data-inline="true" name="add_visit" onclick="$('#popupAddVisit').popup('open');" value="{$_lang.biz_checkins.create_title}" />    	 {/if}	{/if}</div>{if $result_found gt 0 && $biz_checkinslist}	<table class="biz_data_grid">		<tr>            <th class="{if $smarty.get.sort_on eq 'chkin_date'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}">Name</th>            <th class="{if $smarty.get.sort_on eq 'chkin_date'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}">Phone</th>			<th class="{if $smarty.get.sort_on eq 'chkin_date'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}">Award Points</th>			<th class="{if $smarty.get.sort_on eq 'chkin_date'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}">Promotion</th>			<th class="{if $smarty.get.sort_on eq 'chkin_date'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}">Redeem Points</th>			<!--<th class="{if $smarty.get.sort_on eq 'server_name'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}">Server</th>-->			<th class="{if $smarty.get.sort_on eq 'chkin_invoice'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}">Invoice</th>            <th class="{if $smarty.get.sort_on eq 'chkin_date'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}">Posted</th>            <th class="{if $smarty.get.sort_on eq 'chkin_edit_commnt'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}">Comment</th>            {if $rpt_to_show neq 'actvty_log'}            	<th >Action</th>            {/if}            </tr>		{foreach from=$biz_checkinslist item=biz_checkinsitem}		<tr class="{cycle values="odd,even"}">            <td>{$biz_checkinsitem.name}</td>            <td>{$biz_checkinsitem.phone}</td>            <!--<td>{if $biz_checkinsitem.usr_without_email eq 0}{$biz_checkinsitem.email}{/if} </td>-->            <td>{$biz_checkinsitem.award_points}</td>            <td>{$biz_checkinsitem.promotion}</td>            <td>{$biz_checkinsitem.reward_point}</td>            <!--<td>{$biz_checkinsitem.server_name}</td>-->            <td>{$biz_checkinsitem.chkin_invoice}</td>			<td>{$biz_checkinsitem.posted_on|date_format:$smarty.const.HTML5_DATETIME_FORMAT}</td>			<td>{$biz_checkinsitem.chkin_edit_commnt}</td>		{if $rpt_to_show neq 'actvty_log'} 			<td>			{if $biz_checkinsitem.chkin_id gt 0}                <input type="button" data-icon="add-item" data-mini="true" data-inline="true" name="edit_award" onclick="edit_award_point({$biz_checkinsitem.chkin_id},{$biz_checkinsitem.award_points},'{$biz_checkinsitem.chkin_edit_commnt}')" value="Edit" />            {else}                &nbsp;            {/if}            </td>        {/if}              </tr>	{/foreach}		<tfoot>			<tr>				<td colspan="10">					# {$result_found}&nbsp;&nbsp;&nbsp;{if $pagination neq ""}{$pagination}{/if}					<select onchange="changePage('{$navigationURL}',this.value,{$smarty.request.limit});">					{if $allPageCount gt 1}						{for $foo=1 to $allPageCount}							<option value="{$foo}" {if $foo eq $currentPage}selected="selected"{/if}>{$foo}</option>						{/for}					{else}						<option value="1" disabled="disabled">1</option>					{/if}					</select>				</td>			</tr>		</tfoot>	</table>{else}	<div class="error">{$_lang.biz_checkins.no_record_found}</div>{/if}{*include file="biz_checkins/claim_history.tpl"*}</div>{include file="biz_checkins/js.tpl"}{include file="biz_checkins/edit_award_pin.tpl"}{*include file="footer.tpl"*}{*include file="biz_rewards/show_server_code.tpl"*}{*include file="ask_cust_email.tpl"*}{*include file="biz_rewards/add_visit.tpl"*}{*include file="biz_rewards/cust_server_pin.tpl"*}{include file="footercontent.tpl"}{literal}<script type="text/javascript">      $(function(){  $("#search_from_dt, #search_to_dt").scroller({ display:'bubble', preset: 'date', dateFormat: 'mm/dd/yyyy', timeWheels: 'yyyymmdd', animate: 'slidevertical'});});</script>{/literal}{if $isCustomer gt 0}    {literal}    <script type="text/javascript">    {/literal}    {if ($smarty.session[$smarty.const.SES_REWARD]) && ($smarty.session[$smarty.const.SES_REWARD].email neq '')}    {else}       	 {literal}    	 	$(function(){ askCustEmail(); });    	 {/literal}    {/if}    {literal}    </script>    {/literal}{/if}</body></html>