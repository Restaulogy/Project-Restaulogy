{include file="header.tpl"}<div class="wrapper"><h1>{$_lang.biz_checkins.create_title}</h1>{if $error_msg}	<center>{$error_msg}</center>{/if}<form name="frmcreatebiz_checkins" id="frmcreatebiz_checkins" onsubmit="return validatebiz_checkins();" method="POST" action="{$page_url}">	<input type="hidden" name="Array" id="Array" value="0"/>	<div style="display:none;" class="error" id="Array_err"></div>	<div class="field-row">		<label for="chkin_buss_id">{$_lang.biz_checkins.label.chkin_buss_id}</label>		<input maxlength="20" id="chkin_buss_id" type="text" value="{$smarty.post.chkin_buss_id}" name="chkin_buss_id"/> 		<div class="error" id="chkin_buss_id_err"></div>	</div>	<div class="field-row">		<label for="chkin_rwd_id">{$_lang.biz_checkins.label.chkin_rwd_id}</label>		<input maxlength="20" id="chkin_rwd_id" type="text" value="{$smarty.post.chkin_rwd_id}" name="chkin_rwd_id"/> 		<div class="error" id="chkin_rwd_id_err"></div>	</div>	<div class="field-row">		<label for="chkin_user_id">{$_lang.biz_checkins.label.chkin_user_id}</label>		<input maxlength="20" id="chkin_user_id" type="text" value="{$smarty.post.chkin_user_id}" name="chkin_user_id"/> 		<div class="error" id="chkin_user_id_err"></div>	</div>	<div class="field-row">		<label for="chkin_session">{$_lang.biz_checkins.label.chkin_session}</label>		<input maxlength="20" id="chkin_session" type="text" value="{$smarty.post.chkin_session}" name="chkin_session"/> 		<div class="error" id="chkin_session_err"></div>	</div>	<div class="field-row">		<label for="chkin_points">{$_lang.biz_checkins.label.chkin_points}</label>		<input maxlength="6,2" id="chkin_points" type="text" value="{$smarty.post.chkin_points}" name="chkin_points"/> 		<div class="error" id="chkin_points_err"></div>	</div>	<div class="field-row">		<label for="chkin_date">{$_lang.biz_checkins.label.chkin_date}</label>		<input  id="chkin_date" type="text" value="{$smarty.post.chkin_date}" name="chkin_date"/> 		<div class="error" id="chkin_date_err"></div>	</div>	<center><input type="hidden" id="action" name="action" value="{$smarty.const.ACTION_CREATE}"/><input data-inline="true" data-icon="save" type="submit" value="{$_lang.save_lbl}"/> <input data-inline="true" data-icon="delete" type="reset" value="{$_lang.cancel_lbl}" onclick="window.location.href='{$page_url}'"/></center></form>{include file="biz_checkins/js.tpl"}</div>{include file="footer.tpl"}