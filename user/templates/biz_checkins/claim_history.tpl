<h1>{$_lang.crm_prom_emails.listing_title}</h1>{if $result_found_his gt 0 && $crm_prom_emailslist}	<table class="biz_data_grid">		<tr>			<th class="{if $smarty.get.sort_on eq 'crm_pr_ml_userid'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=crm_pr_ml_userid&sort_by={$new_sort}">{$_lang.crm_prom_emails.label.crm_pr_ml_userid}</a></th>			<th class="{if $smarty.get.sort_on eq 'crm_pr_ml_promotion'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=crm_pr_ml_promotion&sort_by={$new_sort}">{$_lang.crm_prom_emails.label.crm_pr_ml_promotion}</a></th>            <th class="{if $smarty.get.sort_on eq 'end_date'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=end_date&sort_by={$new_sort}">{$_lang.crm_prom_emails.label.end_date}</a></th>			<!-- <th class="action_header">Action</th> -->        </tr>		{foreach from=$crm_prom_emailslist item=crm_prom_emailsitem}		<tr class="{cycle values="odd,even"}">			<td>            <!--            <a href="{$page_url}?mode={$smarty.const.MODE_VIEW}&crm_pr_ml_id={$crm_prom_emailsitem.crm_pr_ml_id}">{$crm_prom_emailsitem.crm_cust_email}</a>            -->            {if $crm_prom_emailsitem.usr_without_email eq 0}{$crm_prom_emailsitem.crm_cust_email}{/if}            </td>			<td><a href="{$website}/modules/business_listing/show.php?show_type=PR&promoid={$crm_prom_emailsitem.crm_pr_ml_promotion}&lid={$crm_prom_emailsitem.list_id}" target="_blank">{$crm_prom_emailsitem.title}</a></td>			<td>{$crm_prom_emailsitem.end_date}</td>			            </tr>	{/foreach}		<tfoot>			<tr>				<td colspan="6">					# {$result_found_his}&nbsp;&nbsp;&nbsp;{if $pagination_his neq ""}{$pagination_his}{/if}					<select onchange="changePage('{$navigationURL}',this.value,{$smarty.request.limit_his});">					{if $allPageCount_his gt 1}						{for $foo=1 to $allPageCount_his}							<option value="{$foo}" {if $foo eq $currentPage_his}selected="selected"{/if}>{$foo}</option>						{/for}					{else}						<option value="1" disabled="disabled">1</option>					{/if}					</select>				</td>            </tr>		</tfoot>	</table>{else}	<div class="error">{$_lang.crm_prom_emails.no_record_found}</div>{/if}