{literal}<script type="text/javascript">function edit_award_point(chkin_id,chkin_points,chkin_edit_commnt){    if(chkin_id>0){        $('#chkin_id').val(chkin_id);        $('#chkin_points').val(chkin_points);        $('#chkin_edit_commnt').val(chkin_edit_commnt);        $('#popupCheckIn').popup('open');    }else{        alert('Please select the award record first');    }}function validDtRange(frmDtId,toDtId){    if(frmDtId){}else{		frmDtId = 'search_from_dt';	}	if(toDtId){}else{		toDtId = 'search_to_dt';	}	if(new Date($('#'+ toDtId).val()).getTime() < new Date($('#' + frmDtId).val()).getTime()){	//if($('#'+ toDtId).val() < $('#' + frmDtId).val()){		alert('End date should be greter than start date');		return false;	}	return true;}function deletebiz_checkins(varId){	if(varId > 0){		if(confirm("{/literal}{$_lang.biz_checkins.DELETE.CONFIRM_MSG}{literal}")==true){			window.location.href="{/literal}{$page_url}{literal}?action=delete&chkin_id="+varId;		}	}}function validatebiz_checkins(){	$("#chkin_id_err").html("");	$("#chkin_buss_id_err").html("");	$("#chkin_rwd_id_err").html("");	$("#chkin_user_id_err").html("");	$("#chkin_session_err").html("");	$("#chkin_points_err").html("");	$("#chkin_date_err").html("");		$("#chkin_edit_commnt_err").html("");		var isErr = true;	if(elemById("action").value=="update"){		if(IsNonEmpty(elemById("chkin_id").value)==false){			$("#chkin_id_err").html("{/literal}{$_lang.biz_checkins.not_empty_msg.chkin_id}{literal}");			isErr = false;		}	}/*	if(IsNonEmpty(elemById("chkin_buss_id").value)==false){		$("#chkin_buss_id_err").html("{/literal}{$_lang.biz_checkins.not_empty_msg.chkin_buss_id}{literal}");		isErr = false;	}	if(IsNonEmpty(elemById("chkin_rwd_id").value)==false){		$("#chkin_rwd_id_err").html("{/literal}{$_lang.biz_checkins.not_empty_msg.chkin_rwd_id}{literal}");		isErr = false;	}	if(IsNonEmpty(elemById("chkin_user_id").value)==false){		$("#chkin_user_id_err").html("{/literal}{$_lang.biz_checkins.not_empty_msg.chkin_user_id}{literal}");		isErr = false;	}	if(IsNonEmpty(elemById("chkin_session").value)==false){		$("#chkin_session_err").html("{/literal}{$_lang.biz_checkins.not_empty_msg.chkin_session}{literal}");		isErr = false;	}*/	if(IsNonEmpty(elemById("chkin_points").value)==false){		$("#chkin_points_err").html("{/literal}{$_lang.biz_checkins.not_empty_msg.chkin_points}{literal}");		isErr = false;	}else{        if(IsNumeric(elemById("chkin_points").value)==false){    		$("#chkin_points_err").html("{/literal}{$_lang.biz_checkins.input_err_msg.chkin_points}{literal}");    		isErr = false;    	}    }		if(IsNonEmpty(elemById("chkin_edit_commnt").value)==false){		$("#chkin_edit_commnt_err").html("{/literal}{$_lang.biz_checkins.not_empty_msg.chkin_edit_commnt}{literal}");		isErr = false;	}/*	if(IsNonEmpty(elemById("chkin_date").value)==false){		$("#chkin_date_err").html("{/literal}{$_lang.biz_checkins.not_empty_msg.chkin_date}{literal}");		isErr = false;	}*/	if(isErr == false){		alert("{/literal}{$_lang.messages.revise_form}{literal}");	}	return isErr;}//..function</script>{/literal}