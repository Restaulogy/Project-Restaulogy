{literal}<script type="text/javascript">function deletetbl_order_details(varId){	if(varId > 0){		if(confirm("{/literal}{$_lang.tbl_order_details.DELETE.CONFIRM_MSG}{literal}")==true){			window.location.href="{/literal}{$page_url}{literal}?action=delete&ord_dtl_id="+varId;		}	}}function validatetbl_order_details(){	$("#ord_dtl_id_err").html("");	$("#ord_dtl_order_id_err").html("");	$("#ord_dtl_sbmenu_dish_id_err").html("");	$("#ord_dtl_quantity_err").html("");	$("#ord_dtl_price_err").html("");	var isErr = true;	if(elemById("action").value=="update"){		if(IsNonEmpty(elemById("ord_dtl_id").value)==false){			$("#ord_dtl_id_err").html("{/literal}{$_lang.tbl_order_details.not_empty_msg.ord_dtl_id}{literal}");			isErr = false;		}	}	if(IsNonEmpty(elemById("ord_dtl_order_id").value)==false){		$("#ord_dtl_order_id_err").html("{/literal}{$_lang.tbl_order_details.not_empty_msg.ord_dtl_order_id}{literal}");		isErr = false;	}	if(IsNonEmpty(elemById("ord_dtl_sbmenu_dish_id").value)==false){		$("#ord_dtl_sbmenu_dish_id_err").html("{/literal}{$_lang.tbl_order_details.not_empty_msg.ord_dtl_sbmenu_dish_id}{literal}");		isErr = false;	}	if(IsNonEmpty(elemById("ord_dtl_quantity").value)==false){		$("#ord_dtl_quantity_err").html("{/literal}{$_lang.tbl_order_details.not_empty_msg.ord_dtl_quantity}{literal}");		isErr = false;	}	if(IsNonEmpty(elemById("ord_dtl_price").value)==false){		$("#ord_dtl_price_err").html("{/literal}{$_lang.tbl_order_details.not_empty_msg.ord_dtl_price}{literal}");		isErr = false;	}	if(isErr == false){		alert("{/literal}{$_lang.messages.revise_form}{literal}");	}	return isErr;}//..function</script>{/literal}