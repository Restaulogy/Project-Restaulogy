{include file='header.tpl'}<div class="wrapper">{include file="tbl_tip/tabs.tpl"}<h1>{$_lang.tbl_tip.listing_title}</h1>{if $error_msg}	<center>{$error_msg}</center>{/if}<form name="frm_tbl_tip" id="frm_tbl_tip"  method="POST" action="{$page_url}" >{if $result_found gt 0 && $tbl_tiplist}	<table class="listTable">		<tr>				<th style="width:5%;"></th>				<th style="width:90%;">{$_lang.tbl_tip.title}</th>				<th style="width:5%;"></th>		<!--<th class="actionListItem"></th>-->					</tr>		{foreach from=$tbl_tiplist item=tbl_tipitem}			<tr>				<td style="width:5%;">					<label for="sel_tip[{$tbl_tipitem.tip_id}]" data-mini="true" style="width:23px;"><input type="checkbox" data-inline='true' data-mini='true' id="sel_tip[{$tbl_tipitem.tip_id}]" name="sel_tip[{$tbl_tipitem.tip_id}]" />&nbsp;</label>				</td>				<td style="width:95%;"><a href="{$page_url}?mode={$smarty.const.MODE_UPDATE}&tip_id={$tbl_tipitem.tip_id}">{$tbl_tipitem.tip_percent} {$_lang.percent_mark}</a></td>				<td style="width:5%;">                {if $tbl_tipitem.isActive eq 1}                   <a href="#" class="activeIcon" title="{$_lang.tbl_tip.ACTIVATE.BTN_LBL}"></a>                {else}                    <a href="#" class="deactiveIcon" title="{$_lang.tbl_tip.DEACTIVATE.BTN_LBL}"></a>                {/if}                </td>				<!--<td class="actionListItem"><a href="{$page_url}?mode={$smarty.const.MODE_UPDATE}&tip_id={$tbl_tipitem.tip_id}" class="editIcon"></a><a class="deleteIcon" href="#" onclick="deletetbl_tip({$tbl_tipitem.tip_id});"></a>{if $tbl_tipitem.isActive eq 1}<a href="{$page_url}?action={$smarty.const.ACTION_DEACTIVATE}&tip_id={$tbl_tipitem.tip_id}" class="deactiveIcon" title="{$_lang.tbl_tip.DEACTIVATE.BTN_LBL}"></a>{else}<a href="{$page_url}?action={$smarty.const.ACTION_ACTIVATE}&tip_id={$tbl_tipitem.tip_id}" class="activeIcon" title="{$_lang.tbl_tip.ACTIVATE.BTN_LBL}"></a>{/if}</td>-->			</tr>	{/foreach}	</table>{else}	<div class="error">{$_lang.tbl_tip.no_record_found}</div>{/if}{if $pagination}	<div class="biz_center">{$pagination}</div>{/if}<input type="hidden" id="action" name="{$smarty.const.ACTION_TITLE}"/></form><div class="biz_center"> {if $result_found gt 0 && $tbl_tiplist}	{jqmbutton icon="briefcase" value="{$_lang.main.toggle}" onclick="javascript:$('input[type=checkbox]').click();"} 	{jqmbutton type="delete" onclick="actiontbl_tip('{$smarty.const.ACTION_DELETE}');"}	{jqmbutton icon="inactive" value="{$_lang.tbl_tip.DEACTIVATE.BTN_LBL}" onclick="actiontbl_tip('{$smarty.const.ACTION_DEACTIVATE}');"}	{jqmbutton icon="active" value="{$_lang.tbl_tip.ACTIVATE.BTN_LBL}" onclick="actiontbl_tip('{$smarty.const.ACTION_ACTIVATE}');"}{/if}	{jqmbutton icon="add-item" value="{$_lang.tbl_tip.CREATE.BTN_LBL}" onclick="window.location.href='{$page_url}?mode={$smarty.const.MODE_CREATE}'"}  	</div></div>{include file="tbl_tip/js.tpl"}{include file="footer.tpl"}