{include file="header.tpl"}<div class="wrapper">{include file="tbl_tip/tabs.tpl"}<h1>{$_lang.tbl_tip.title}</h1>{if $error_msg}	<center>{$error_msg}</center>{/if}<form name="frmupdatetbl_tip" id="frmupdatetbl_tip" onsubmit="return validatetbl_tip();" method="POST" action="{$page_url}" style="{if $isUpdate eq 1}{else}display:none;{/if}">	<input type="hidden" name="tip_id" id="tip_id" value="{$tbl_tipinfo.tip_id}"/>	<div style="display:none;" class="error" id="tip_id_err"></div>	<div class="field-row">		<label for="tip_percent">{$_lang.tbl_tip.label.tip_percent}</label>		<input type="text" name="tip_percent" id="tip_percent" value="{$tbl_tipinfo.tip_percent}"/>		<div class="error" id="tip_percent_err"></div>	</div>	<!--	<div class="field-row">		<label for="tip_start_date">{$_lang.tbl_tip.label.tip_start_date}</label>		<input type="text" name="tip_start_date" id="tip_start_date" value="{$tbl_tipinfo.tip_start_date}"/>		<div class="error" id="tip_start_date_err"></div>	</div>	-->	<!--	<div class="field-row">		<label for="tip_end_date">{$_lang.tbl_tip.label.tip_end_date}</label>		<input type="text" name="tip_end_date" id="tip_end_date" value="{$tbl_tipinfo.tip_end_date}"/>		<div class="error" id="tip_end_date_err"></div>	</div>	-->	<center><input type="hidden" id="action" name="action" value="{$smarty.const.ACTION_UPDATE}"/> <input  data-inline="true" data-icon="save"  type="submit" value="{$_lang.save_lbl}"/> <input type="reset" value="{$_lang.cancel_lbl}" data-inline="true" data-icon="delete" onclick="window.location.href='{$page_url}'"/></center></form><div id="tbl_tip_view" class="description" style="{if $isUpdate eq 1}display:none;{/if}">	<table class="listTable">		<tr><th class="fieldItem">{$_lang.field_title}</th><th class="valueItem">{$_lang.value_title}</th></tr>		<tr><td class="fieldItem">{$_lang.tbl_tip.label.tip_id}<i>:</i></td><td class="valueItem">{$tbl_tipinfo.tip_id}</td></tr>		<tr><td class="fieldItem">{$_lang.tbl_tip.label.tip_percent}<i>:</i></td><td class="valueItem">{$tbl_tipinfo.tip_percent}</td></tr>	<!--		<tr><td class="fieldItem">{$_lang.tbl_tip.label.tip_start_date}<i>:</i></td><td class="valueItem">{$tbl_tipinfo.tip_start_date}</td></tr>	-->	<!--		<tr><td class="fieldItem">{$_lang.tbl_tip.label.tip_end_date}<i>:</i></td><td class="valueItem">{$tbl_tipinfo.tip_end_date}</td></tr>	-->	<tr><td class="fieldItem">{$_lang.tbl_tip.label.isActive}<i>:</i></td><td class="valueItem">{if $tbl_tipinfo.isActive eq 1}{$_lang.tbl_tip.label.isActive_yes}{else}{$_lang.tbl_tip.label.isActive_no}{/if}</td></tr>	</table>	<center><input data-inline="true" data-icon="edit" type="button" value="{$_lang.tbl_tip.UPDATE.BTN_LBL}" onclick="$('#tbl_tip_view').hide();$('#frmupdatetbl_tip').show();"/><input type="button" value="{$_lang.cancel_lbl}" data-inline="true" data-icon="delete" onclick="window.location.href='{$page_url}'"/></center></div>{include file="tbl_tip/js.tpl"}</div>{include file="footer.tpl"}