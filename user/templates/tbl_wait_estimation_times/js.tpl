{literal}<script type="text/javascript">function actiontbl_wait_estimation_times(action){  	var varId = $("input:checked").length;	var isValid = 0;	if(varId > 0){	if(action != ""){		switch (action) {			case '{/literal}{$smarty.const.ACTION_DELETE}{literal}':			if(confirm("{/literal}{$_lang.tbl_wait_estimation_times.DELETE.CONFIRM_MSG}{literal}")==true){					isValid = 1;			}  			break;						case '{/literal}{$smarty.const.ACTION_DEACTIVATE}{literal}':			isValid = 1;			break;						case '{/literal}{$smarty.const.ACTION_ACTIVATE}{literal}':			isValid = 1; 			break; 		}		  		if(isValid){		 	$('#action').val(action);		  $('#frm_tbl_wait_estimation_times').submit();		}				}	}else{			alert("{/literal}{$_lang.main.select_ids.empty}{literal}"); 	}	}function deletetbl_wait_estimation_times(varId){	if(varId > 0){		if(confirm("{/literal}{$_lang.tbl_wait_estimation_times.DELETE.CONFIRM_MSG}{literal}")==true){			window.location.href="{/literal}{$page_url}{literal}?action=delete&wt_est_time_id="+varId;		}	}}function rangeValidate(range){	var reg1 = /([0-9])\-([0-9])/;	var reg2 = /([0-9])\+$/;	var res = false;	if((range) && (range.indexOf('.') == -1)){		if(reg1.test(range) || reg2.test(range)){   			res = true;		}	} 	return res;}function validatetbl_wait_estimation_times(){	$("#wt_est_time_id_err").html("");	$("#wt_est_time_party_size_err").html("");	$("#wt_est_time_table_type_err").html("");	$("#wt_est_time_value_err").html("");	$("#wt_est_time_start_date_err").html("");	$("#wt_est_time_end_date_err").html("");	var isErr = true;	if(elemById("action").value=="update"){		if(IsNonEmpty(elemById("wt_est_time_id").value)==false){			$("#wt_est_time_id_err").html("{/literal}{$_lang.tbl_wait_estimation_times.not_empty_msg.wt_est_time_id}{literal}");			isErr = false;		}	}	if(IsNonEmpty(elemById("wt_est_time_party_size").value)==false){		$("#wt_est_time_party_size_err").html("{/literal}{$_lang.tbl_wait_estimation_times.not_empty_msg.wt_est_time_party_size}{literal}");		isErr = false;	}else{		if(rangeValidate(elemById("wt_est_time_party_size").value)==false){			$("#wt_est_time_party_size_err").html("{/literal}{$_lang.tbl_wait_estimation_times.msg.not_proper_party_size}{literal}");			isErr = false;		}		}	 	if(IsNonEmpty(elemById("wt_est_time_table_type").value)==false){		$("#wt_est_time_table_type_err").html("{/literal}{$_lang.tbl_wait_estimation_times.not_empty_msg.wt_est_time_table_type}{literal}");		isErr = false;	}	if(IsNonEmpty(elemById("wt_est_time_value").value)==false){		$("#wt_est_time_value_err").html("{/literal}{$_lang.tbl_wait_estimation_times.not_empty_msg.wt_est_time_value}{literal}");		isErr = false;	}/*	if(IsNonEmpty(elemById("wt_est_time_start_date").value)==false){		$("#wt_est_time_start_date_err").html("{/literal}{$_lang.tbl_wait_estimation_times.not_empty_msg.wt_est_time_start_date}{literal}");		isErr = false;	}*//*	if(IsNonEmpty(elemById("wt_est_time_end_date").value)==false){		$("#wt_est_time_end_date_err").html("{/literal}{$_lang.tbl_wait_estimation_times.not_empty_msg.wt_est_time_end_date}{literal}");		isErr = false;	}*/	if(isErr == false){		alert("{/literal}{$_lang.messages.revise_form}{literal}");	}	return isErr;}//..function	 </script>{/literal}