{include file="header.tpl"}<div class="wrapper"><h1>"{$table_type_info.tbl_type_name}" - {$_lang.tbl_wait_estimation_times.create_title}</h1>{if $error_msg}	<center>{$error_msg}</center>{/if}<form name="frmcreatetbl_wait_estimation_times" id="frmcreatetbl_wait_estimation_times" onsubmit="return validatetbl_wait_estimation_times();" method="POST" action="{$page_url}">	<input type="hidden" name="wt_est_time_id" id="wt_est_time_id" value="0"/>	<div style="display:none;" class="error" id="wt_est_time_id_err"></div>	<div class="field-row">		<label for="wt_est_time_party_size">{$_lang.tbl_wait_estimation_times.label.wt_est_time_party_size}</label>		<input type="text" name="wt_est_time_party_size" id="wt_est_time_party_size" value="{$smarty.post.wt_est_time_party_size}"/>		<div class="error" id="wt_est_time_party_size_err"></div>	</div>	<div class="field-row">		<label for="wt_est_time_table_type">{$_lang.tbl_wait_estimation_times.label.wt_est_time_table_type}</label>{if $smarty.session[$smarty.const.SES_TABLE_TYPE] gt 0}	<input type="hidden" name="wt_est_time_table_type" id="wt_est_time_table_type" value="{$smarty.session[$smarty.const.SES_TABLE_TYPE]}"/> 	{$table_type_info.tbl_type_name}{else}	<select name="wt_est_time_table_type" id="wt_est_time_table_type">			<option value="">Select Table Type</option>			{foreach $table_types as $tbl_type}				<option value='{$tbl_type@key}' {if $smarty.post.wt_est_time_table_type eq $tbl_type@key}selected="selected"{/if}>{$tbl_type}</option>			{/foreach}		</select>	{/if}		 				<div class="error" id="wt_est_time_table_type_err"></div>	</div>	<div class="field-row">		<label for="wt_est_time_value">{$_lang.tbl_wait_estimation_times.label.wt_est_time_value}</label>		<select name="wt_est_time_value" id="wt_est_time_value">		<option value="">Select Time</option>		{for $var=1 to 120}			<option value="{$var}" {if $smarty.post.wt_est_time_value eq $var}selected="selected"{/if}>{$var}</option>		{/for}		</select>		<div class="error" id="wt_est_time_value_err"></div>	</div>	<!--	<div class="field-row">		<label for="wt_est_time_start_date">{$_lang.tbl_wait_estimation_times.label.wt_est_time_start_date}</label>		<input type="text" name="wt_est_time_start_date" id="wt_est_time_start_date" value="{$smarty.post.wt_est_time_start_date}"/>		<div class="error" id="wt_est_time_start_date_err"></div>	</div>	-->	<!--	<div class="field-row">		<label for="wt_est_time_end_date">{$_lang.tbl_wait_estimation_times.label.wt_est_time_end_date}</label>		<input type="text" name="wt_est_time_end_date" id="wt_est_time_end_date" value="{$smarty.post.wt_est_time_end_date}"/>		<div class="error" id="wt_est_time_end_date_err"></div>	</div>	-->	 <div class="biz_center">			<input type="hidden" id="action" name="action" value="{$smarty.const.ACTION_CREATE}"/><input data-inline="true" data-icon="save" type="submit" value="{$_lang.save_lbl}"/> 	<input data-inline="true" data-icon="delete" type="reset" value="{$_lang.cancel_lbl}" onclick="window.location.href='{$page_url}'"/>			</div>	</form></div> {include file="footercontent.tpl"}{include file="tbl_wait_estimation_times/js.tpl"}</body></html>