{literal}<script type="text/javascript">function deletetbl_order_details_options(varId){	if(varId > 0){		if(confirm("{/literal}{$_lang.tbl_order_details_options.DELETE.CONFIRM_MSG}{literal}")==true){			window.location.href="{/literal}{$page_url}{literal}?action=delete&ord_det_opt_id="+varId;		}	}}function validatetbl_order_details_options(){	$("#ord_det_opt_id_err").html("");	$("#ord_det_opt_order_detail_err").html("");	$("#ord_det_opt_optionvalue_err").html("");	$("#ord_det_opt_qty_err").html("");	$("#ord_det_opt_price_err").html("");	var isErr = true;	if(elemById("action").value=="update"){		if(IsNonEmpty(elemById("ord_det_opt_id").value)==false){			$("#ord_det_opt_id_err").html("{/literal}{$_lang.tbl_order_details_options.not_empty_msg.ord_det_opt_id}{literal}");			isErr = false;		}	}	if(IsNonEmpty(elemById("ord_det_opt_order_detail").value)==false){		$("#ord_det_opt_order_detail_err").html("{/literal}{$_lang.tbl_order_details_options.not_empty_msg.ord_det_opt_order_detail}{literal}");		isErr = false;	}	if(IsNonEmpty(elemById("ord_det_opt_optionvalue").value)==false){		$("#ord_det_opt_optionvalue_err").html("{/literal}{$_lang.tbl_order_details_options.not_empty_msg.ord_det_opt_optionvalue}{literal}");		isErr = false;	}	if(IsNonEmpty(elemById("ord_det_opt_qty").value)==false){		$("#ord_det_opt_qty_err").html("{/literal}{$_lang.tbl_order_details_options.not_empty_msg.ord_det_opt_qty}{literal}");		isErr = false;	}	if(IsNonEmpty(elemById("ord_det_opt_price").value)==false){		$("#ord_det_opt_price_err").html("{/literal}{$_lang.tbl_order_details_options.not_empty_msg.ord_det_opt_price}{literal}");		isErr = false;	}	if(isErr == false){		alert("{/literal}{$_lang.messages.revise_form}{literal}");	}	return isErr;}//..function</script>{/literal}