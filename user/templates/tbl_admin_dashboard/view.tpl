{include file="header.tpl"}<div class="wrapper"><h1>{$_lang.tbl_admin_dashboard.title}</h1>{if $error_msg}	<center>{$error_msg}</center>{/if}<form name="frmupdatetbl_admin_dashboard" id="frmupdatetbl_admin_dashboard" onsubmit="return validatetbl_admin_dashboard();" method="POST" action="{$page_url}" style="{if $isUpdate eq 1}{else}display:none;{/if}">	<input type="hidden" name="admdash_id" id="admdash_id" value="{$tbl_admin_dashboardinfo.admdash_id}"/>	<div style="display:none;" class="error" id="admdash_id_err"></div>	<div class="field-row">		<label for="admdash_name">{$_lang.tbl_admin_dashboard.label.admdash_name}</label>		<input type="text" name="admdash_name" id="admdash_name" value="{$tbl_admin_dashboardinfo.admdash_name}"/>		<div class="error" id="admdash_name_err"></div>	</div>	<div class="field-row">		<label for="admdash_lnk">{$_lang.tbl_admin_dashboard.label.admdash_lnk}</label>		<input type="text" name="admdash_lnk" id="admdash_lnk" value="{$tbl_admin_dashboardinfo.admdash_lnk}"/>		<div class="error" id="admdash_lnk_err"></div>	</div>	<div class="field-row">		<label for="admdash_img">{$_lang.tbl_admin_dashboard.label.admdash_img}</label>		<input type="text" name="admdash_img" id="admdash_img" value="{$tbl_admin_dashboardinfo.admdash_img}"/>		<div class="error" id="admdash_img_err"></div>	</div>		<div class="field-row">		<label for="admdash_for_admin">{$_lang.tbl_admin_dashboard.label.admdash_for_admin}</label>		<input name="admdash_for_admin" id="admdash_for_admin" type="checkbox" value="1" {if $tbl_admin_dashboardinfo.admdash_for_admin eq 1} checked="checked" {/if} />		<div class="error" id="admdash_for_admin_err"></div>	</div>		<div class="field-row">		<label for="admdash_for_mngr">{$_lang.tbl_admin_dashboard.label.admdash_for_mngr}</label>		<input id="admdash_for_mngr" name="admdash_for_mngr" type="checkbox" value="1" {if $tbl_admin_dashboardinfo.admdash_for_mngr eq 1} checked="checked" {/if} />		<div class="error" id="admdash_for_mngr_err"></div>	</div>		<!--	<div class="field-row">		<label for="admdash_start_date">{$_lang.tbl_admin_dashboard.label.admdash_start_date}</label>		<input type="text" name="admdash_start_date" id="admdash_start_date" value="{$tbl_admin_dashboardinfo.admdash_start_date}"/>		<div class="error" id="admdash_start_date_err"></div>	</div>	-->	<!--	<div class="field-row">		<label for="admdash_end_date">{$_lang.tbl_admin_dashboard.label.admdash_end_date}</label>		<input type="text" name="admdash_end_date" id="admdash_end_date" value="{$tbl_admin_dashboardinfo.admdash_end_date}"/>		<div class="error" id="admdash_end_date_err"></div>	</div>	-->	<center><input type="hidden" id="action" name="action" value="{$smarty.const.ACTION_UPDATE}"/> <input  data-inline="true" data-icon="save"  type="submit" value="{$_lang.save_lbl}"/> <input data-inline="true" data-icon="delete" type="reset" onclick="$('#tbl_admin_dashboard_view').show();$('#frmupdatetbl_admin_dashboard').hide();" value="{$_lang.cancel_lbl}"/></center></form><div id="tbl_admin_dashboard_view" class="description" style="{if $isUpdate eq 1}display:none;{/if}">	<table class="listTable">		<tr><th class="fieldItem">{$_lang.field_title}</th><th class="valueItem">{$_lang.value_title}</th></tr>		<tr><td class="fieldItem">{$_lang.tbl_admin_dashboard.label.admdash_id}<i>:</i></td><td class="valueItem">{$tbl_admin_dashboardinfo.admdash_id}</td></tr>		<tr><td class="fieldItem">{$_lang.tbl_admin_dashboard.label.admdash_name}<i>:</i></td><td class="valueItem">{$tbl_admin_dashboardinfo.admdash_name}</td></tr>		<tr><td class="fieldItem">{$_lang.tbl_admin_dashboard.label.admdash_lnk}<i>:</i></td><td class="valueItem">{$tbl_admin_dashboardinfo.admdash_lnk}</td></tr>		<tr><td class="fieldItem">{$_lang.tbl_admin_dashboard.label.admdash_img}<i>:</i></td><td class="valueItem">{$tbl_admin_dashboardinfo.admdash_img}</td></tr>		<tr><td class="fieldItem">{$_lang.tbl_admin_dashboard.label.admdash_for_admin}<i>:</i></td><td class="valueItem">{$tbl_admin_dashboardinfo.admdash_for_admin}</td></tr>		<tr><td class="fieldItem">{$_lang.tbl_admin_dashboard.label.admdash_for_mngr}<i>:</i></td><td class="valueItem">{$tbl_admin_dashboardinfo.admdash_for_mngr}</td></tr>	<!--		<tr><td class="fieldItem">{$_lang.tbl_admin_dashboard.label.admdash_start_date}<i>:</i></td><td class="valueItem">{$tbl_admin_dashboardinfo.admdash_start_date}</td></tr>	-->	<!--		<tr><td class="fieldItem">{$_lang.tbl_admin_dashboard.label.admdash_end_date}<i>:</i></td><td class="valueItem">{$tbl_admin_dashboardinfo.admdash_end_date}</td></tr>	-->	<tr><td class="fieldItem">{$_lang.tbl_admin_dashboard.label.isActive}<i>:</i></td><td class="valueItem">{if $tbl_admin_dashboardinfo.isActive eq 1}{$_lang.tbl_admin_dashboard.label.isActive_yes}{else}{$_lang.tbl_admin_dashboard.label.isActive_no}{/if}</td></tr>	</table>	<center><input data-inline="true" data-icon="edit" type="button" value="{$_lang.tbl_admin_dashboard.UPDATE.BTN_LBL}" onclick="$('#tbl_admin_dashboard_view').hide();$('#frmupdatetbl_admin_dashboard').show();"/><input type="button" value="{$_lang.cancel_lbl}" data-inline="true" data-icon="delete" onclick="window.location.href='{$page_url}'"/></center></div>{include file="tbl_admin_dashboard/js.tpl"}</div>{include file="footer.tpl"}