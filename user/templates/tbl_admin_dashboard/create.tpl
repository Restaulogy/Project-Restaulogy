{include file="header.tpl"}<div class="wrapper"><h1>{$_lang.tbl_admin_dashboard.create_title}</h1>{if $error_msg}	<center>{$error_msg}</center>{/if}<form name="frmcreatetbl_admin_dashboard" id="frmcreatetbl_admin_dashboard" onsubmit="return validatetbl_admin_dashboard();" method="POST" action="{$page_url}">	<input type="hidden" name="admdash_id" id="admdash_id" value="0"/>	<div style="display:none;" class="error" id="admdash_id_err"></div>	<div class="field-row">		<label for="admdash_name">{$_lang.tbl_admin_dashboard.label.admdash_name}</label>		<input maxlength="150" id="admdash_name" type="text" value="{$smarty.post.admdash_name}" name="admdash_name"/> 		<div class="error" id="admdash_name_err"></div>	</div>	<div class="field-row">		<label for="admdash_lnk">{$_lang.tbl_admin_dashboard.label.admdash_lnk}</label>		<input maxlength="150" id="admdash_lnk" type="text" value="{$smarty.post.admdash_lnk}" name="admdash_lnk"/> 		<div class="error" id="admdash_lnk_err"></div>	</div>	<div class="field-row">		<label for="admdash_img">{$_lang.tbl_admin_dashboard.label.admdash_img}</label>		<input maxlength="150" id="admdash_img" type="text" value="{$smarty.post.admdash_img}" name="admdash_img"/> 		<div class="error" id="admdash_img_err"></div>	</div>		<div class="field-row">		<label for="admdash_for_admin">{$_lang.tbl_admin_dashboard.label.admdash_for_admin}</label>		<input id="admdash_for_admin" type="checkbox" value="1" {if $smarty.post.admdash_for_admin eq 1} checked="checked" {/if} name="admdash_for_admin"/>		<div class="error" id="admdash_for_admin_err"></div>	</div>		<div class="field-row">		<label for="admdash_for_mngr">{$_lang.tbl_admin_dashboard.label.admdash_for_mngr}</label>		<input id="admdash_for_mngr" type="checkbox" value="1" {if $smarty.post.admdash_for_mngr eq 1} checked="checked" {/if} name="admdash_for_mngr"/>		<div class="error" id="admdash_for_mngr_err"></div>	</div>	<!--	<div class="field-row">		<label for="admdash_start_date">{$_lang.tbl_admin_dashboard.label.admdash_start_date}</label>		<input  id="admdash_start_date" type="text" value="{$smarty.post.admdash_start_date}" name="admdash_start_date"/> 		<div class="error" id="admdash_start_date_err"></div>	</div>	-->	<!--	<div class="field-row">		<label for="admdash_end_date">{$_lang.tbl_admin_dashboard.label.admdash_end_date}</label>		<input  id="admdash_end_date" type="text" value="{$smarty.post.admdash_end_date}" name="admdash_end_date"/> 		<div class="error" id="admdash_end_date_err"></div>	</div>	-->	<center><input type="hidden" id="action" name="action" value="{$smarty.const.ACTION_CREATE}"/><input data-inline="true" data-icon="save" type="submit" value="{$_lang.save_lbl}"/> <input data-inline="true" data-icon="delete" type="reset" value="{$_lang.cancel_lbl}" onclick="window.location.href='{$page_url}'"/></center></form>{include file="tbl_admin_dashboard/js.tpl"}</div>{include file="footer.tpl"}