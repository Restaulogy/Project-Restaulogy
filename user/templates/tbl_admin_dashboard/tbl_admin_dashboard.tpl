{include file='header.tpl'}<div class="wrapper"><h1>{$_lang.tbl_admin_dashboard.listing_title}</h1>{if $error_msg}	<center>{$error_msg}</center>{/if}{if $result_found gt 0 && $tbl_admin_dashboardlist}	<table class="listTable">		<tr><th class="bigListItem">{$_lang.tbl_admin_dashboard.title}</th>		<th class="actionListItem"></th></tr>		{foreach from=$tbl_admin_dashboardlist item=tbl_admin_dashboarditem}			<tr>				<td class="bigListItem"><a href="{$page_url}?mode={$smarty.const.MODE_VIEW}&admdash_id={$tbl_admin_dashboarditem.admdash_id}">{$tbl_admin_dashboarditem.admdash_name}</a></td>				<td class="actionListItem"><a href="{$page_url}?mode={$smarty.const.MODE_UPDATE}&admdash_id={$tbl_admin_dashboarditem.admdash_id}" class="editIcon"></a><a class="deleteIcon" href="#" onclick="deletetbl_admin_dashboard({$tbl_admin_dashboarditem.admdash_id});"></a>{if $tbl_admin_dashboarditem.isActive eq 1}<a href="{$page_url}?action={$smarty.const.ACTION_DEACTIVATE}&admdash_id={$tbl_admin_dashboarditem.admdash_id}" class="deactiveIcon" title="{$_lang.tbl_admin_dashboard.DEACTIVATE.BTN_LBL}"></a>{else}<a href="{$page_url}?action={$smarty.const.ACTION_ACTIVATE}&admdash_id={$tbl_admin_dashboarditem.admdash_id}" class="activeIcon" title="{$_lang.tbl_admin_dashboard.ACTIVATE.BTN_LBL}"></a>{/if}</td>			</tr>	{/foreach}	</table>{else}	<div class="error">{$_lang.tbl_admin_dashboard.no_record_found}</div>{/if}{if $pagination}	<center>{$pagination}</center>{/if}<center><input data-icon="add-item" data-inline="true" onclick="window.location.href='{$page_url}?mode={$smarty.const.MODE_CREATE}'" type="button" value="{$_lang.tbl_admin_dashboard.CREATE.BTN_LBL}"/></center></div>{include file="tbl_admin_dashboard/js.tpl"}{include file="footer.tpl"}