{include file='header.tpl'}<div class="wrapper"><h1>{$_lang.tbl_admin_dashboard.listing_title}</h1>{if $error_msg}	<center>{$error_msg}</center>{/if}{if $result_found gt 0 && $tbl_admin_dashboardlist}	<table class="biz_data_grid">		<tr>			<th class="{if $smarty.get.sort_on eq 'admdash_name'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=admdash_name&sort_by={$new_sort}">{$_lang.tbl_admin_dashboard.label.admdash_name}</a></th>			<th class="{if $smarty.get.sort_on eq 'admdash_lnk'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=admdash_lnk&sort_by={$new_sort}">{$_lang.tbl_admin_dashboard.label.admdash_lnk}</a></th>			<th class="{if $smarty.get.sort_on eq 'admdash_img'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=admdash_img&sort_by={$new_sort}">{$_lang.tbl_admin_dashboard.label.admdash_img}</a></th>			<th class="{if $smarty.get.sort_on eq 'admdash_for_admin'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=admdash_for_admin&sort_by={$new_sort}">{$_lang.tbl_admin_dashboard.label.admdash_for_admin}</a></th>			<th class="{if $smarty.get.sort_on eq 'admdash_for_mngr'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=admdash_for_mngr&sort_by={$new_sort}">{$_lang.tbl_admin_dashboard.label.admdash_for_mngr}</a></th>			<th class="action_header">Action</th>		</tr>		{foreach from=$tbl_admin_dashboardlist item=tbl_admin_dashboarditem}		<tr class="{cycle values="odd,even"}">			<td><a href="{$page_url}?mode={$smarty.const.MODE_UPDATE}&admdash_id={$tbl_admin_dashboarditem.admdash_id}">{$tbl_admin_dashboarditem.admdash_name}</a></td>			<td>{$tbl_admin_dashboarditem.admdash_lnk}</td>			<td>{$tbl_admin_dashboarditem.admdash_img}</td>			<td>{$tbl_admin_dashboarditem.admdash_for_admin}</td>			<td>{$tbl_admin_dashboarditem.admdash_for_mngr}</td>			<td><a class="deleteIcon" href="#" onclick="deletetbl_admin_dashboard({$tbl_admin_dashboarditem.admdash_id});"></a>{if $tbl_admin_dashboarditem.isActive eq 1}<a href="{$page_url}?action={$smarty.const.ACTION_DEACTIVATE}&admdash_id={$tbl_admin_dashboarditem.admdash_id}" class="deactiveIcon" title="{$_lang.tbl_admin_dashboard.DEACTIVATE.BTN_LBL}"></a>{else}<a href="{$page_url}?action={$smarty.const.ACTION_ACTIVATE}&admdash_id={$tbl_admin_dashboarditem.admdash_id}" class="activeIcon" title="{$_lang.tbl_admin_dashboard.ACTIVATE.BTN_LBL}"></a>{/if}</td>		</tr>	{/foreach}		<tfoot>			<tr>				<td colspan="8">					# {$result_found}&nbsp;&nbsp;&nbsp;{if $pagination neq ""}{$pagination}{/if}					<select onchange="changePage('{$navigationURL}',this.value,{$smarty.request.limit});">					{if $allPageCount gt 1}						{for $foo=1 to $allPageCount}							<option value="{$foo}" {if $foo eq $currentPage}selected="selected"{/if}>{$foo}</option>						{/for}					{else}						<option value="1" disabled="disabled">1</option>					{/if}					</select>				</td>			</tr>		</tfoot>	</table>{else}	<div class="error">{$_lang.tbl_admin_dashboard.no_record_found}</div>{/if}<center><input data-icon="add-item" data-inline="true" onclick="window.location.href='{$page_url}?mode={$smarty.const.MODE_CREATE}'" type="button" value="{$_lang.tbl_admin_dashboard.CREATE.BTN_LBL}"/></center></div>{include file="tbl_admin_dashboard/js.tpl"}{include file="footer.tpl"}