<div ><h1>{$_lang.biz_rewards.redeem_hist}</h1>{if $redeem_hist }	<table class="biz_data_grid">		<tr>			<th >{$_lang.biz_rewards.label.redeem_promotion}</th>			<th >{$_lang.biz_rewards.label.redeem_dt}</th>			<th >{$_lang.biz_rewards.label.redeem_pt}</th>			<th >{$_lang.biz_checkins.label.chkin_invoice}</th>        </tr>		{foreach from=$redeem_hist item=redeem_histitem}    		<tr class="{cycle values="odd,even"}">                <td>{$redeem_histitem.title}</td>    			<td>{$redeem_histitem.redimed_date|date_format:$smarty.const.HTML5_DATETIME_FORMAT}</td>    			<td>{$redeem_histitem.rwd_points}</td>    			<td>{$redeem_histitem.rwd_invoice}</td>            </tr>        {/foreach}	</table>{else}	<div class="error">{$_lang.biz_rewards.no_redeem_history}</div>{/if}</div>