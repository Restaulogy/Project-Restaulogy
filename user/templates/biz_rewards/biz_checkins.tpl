<div ><h1>{$_lang.biz_checkins.award_history}</h1>{if $biz_checkinslist}	<table class="biz_data_grid">		<tr>			<th class="{if $smarty.get.sort_on eq 'chkin_date'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}">{$_lang.biz_checkins.label.chkin_date}</th>			<th class="{if $smarty.get.sort_on eq 'chkin_points'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}">{$_lang.biz_checkins.label.chkin_points}            </th>            <th >{$_lang.biz_checkins.label.chkin_invoice}</th>            </tr>		{foreach from=$biz_checkinslist item=biz_checkinsitem}		    <tr class="{cycle values="odd,even"}">			<td>{$biz_checkinsitem.chkin_date|date_format:$smarty.const.HTML5_DATETIME_FORMAT}</td>			<td>{$biz_checkinsitem.chkin_points}</td>			<td>{$biz_checkinsitem.chkin_invoice}</td>            </tr>	    {/foreach}	</table>{else}	<div class="error">{$_lang.biz_checkins.no_record_found}</div>{/if}</div>