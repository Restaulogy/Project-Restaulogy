{include file='header.tpl'}<div class="wrapper"><h1>{$_lang.biz_rewards.listing_title}</h1>{if $error_msg}	<center>{$error_msg}</center>{/if}{if $result_found gt 0 && $biz_rewardslist}	<table class="biz_data_grid">		<tr>			<th class="{if $smarty.get.sort_on eq 'rwd_buss_id'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=rwd_buss_id&sort_by={$new_sort}">{$_lang.biz_rewards.label.rwd_buss_id}</a></th>			<th class="{if $smarty.get.sort_on eq 'rwd_cup_id'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=rwd_cup_id&sort_by={$new_sort}">{$_lang.biz_rewards.label.rwd_cup_id}</a></th>			<th class="{if $smarty.get.sort_on eq 'rwd_type'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=rwd_type&sort_by={$new_sort}">{$_lang.biz_rewards.label.rwd_type}</a></th>			<th class="{if $smarty.get.sort_on eq 'rwd_one_point_val'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=rwd_one_point_val&sort_by={$new_sort}">{$_lang.biz_rewards.label.rwd_one_point_val}</a></th>			<th class="{if $smarty.get.sort_on eq 'rwd_point_limit'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=rwd_point_limit&sort_by={$new_sort}">{$_lang.biz_rewards.label.rwd_point_limit}</a></th>			<th class="{if $smarty.get.sort_on eq 'rwd_check_in'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=rwd_check_in&sort_by={$new_sort}">{$_lang.biz_rewards.label.rwd_check_in}</a></th>			<th class="{if $smarty.get.sort_on eq 'rwd_is_quest_asc'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=rwd_is_quest_asc&sort_by={$new_sort}">{$_lang.biz_rewards.label.rwd_is_quest_asc}</a></th>			<th class="{if $smarty.get.sort_on eq 'rwd_coupon_id'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=rwd_coupon_id&sort_by={$new_sort}">{$_lang.biz_rewards.label.rwd_coupon_id}</a></th>			<th class="{if $smarty.get.sort_on eq 'rwd_start_dt'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=rwd_start_dt&sort_by={$new_sort}">{$_lang.biz_rewards.label.rwd_start_dt}</a></th>			<th class="{if $smarty.get.sort_on eq 'rwd_end_dt'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=rwd_end_dt&sort_by={$new_sort}">{$_lang.biz_rewards.label.rwd_end_dt}</a></th>			<th class="{if $smarty.get.sort_on eq 'rwd_created_dt'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=rwd_created_dt&sort_by={$new_sort}">{$_lang.biz_rewards.label.rwd_created_dt}</a></th>			<th class="{if $smarty.get.sort_on eq 'rwd_updated_dt'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=rwd_updated_dt&sort_by={$new_sort}">{$_lang.biz_rewards.label.rwd_updated_dt}</a></th>			<th class="action_header">Action</th>		</tr>		{foreach from=$biz_rewardslist item=biz_rewardsitem}		<tr class="{cycle values="odd,even"}">			<td><a href="{$page_url}?mode={$smarty.const.MODE_VIEW}&rwd_id={$biz_rewardsitem.rwd_id}">{$biz_rewardsitem.rwd_buss_id}</a></td>			<td>{$biz_rewardsitem.rwd_cup_id}</td>			<td>{$biz_rewardsitem.rwd_type}</td>			<td>{$biz_rewardsitem.rwd_one_point_val}</td>			<td>{$biz_rewardsitem.rwd_point_limit}</td>			<td>{$biz_rewardsitem.rwd_check_in}</td>			<td>{$biz_rewardsitem.rwd_is_quest_asc}</td>			<td>{$biz_rewardsitem.rwd_coupon_id}</td>			<td>{$biz_rewardsitem.rwd_start_dt}</td>			<td>{$biz_rewardsitem.rwd_end_dt}</td>			<td>{$biz_rewardsitem.rwd_created_dt}</td>			<td>{$biz_rewardsitem.rwd_updated_dt}</td>			<td><a href="{$page_url}?mode={$smarty.const.MODE_UPDATE}&rwd_id={$biz_rewardsitem.rwd_id}" class="editIcon"></a><a class="deleteIcon" href="#" onclick="deletebiz_rewards({$biz_rewardsitem.rwd_id});"></a>{if $biz_rewardsitem.isActive eq 1}<a href="{$page_url}?action={$smarty.const.ACTION_DEACTIVATE}&rwd_id={$biz_rewardsitem.rwd_id}" class="deactiveIcon" title="{$_lang.biz_rewards.DEACTIVATE.BTN_LBL}"></a>{else}<a href="{$page_url}?action={$smarty.const.ACTION_ACTIVATE}&rwd_id={$biz_rewardsitem.rwd_id}" class="activeIcon" title="{$_lang.biz_rewards.ACTIVATE.BTN_LBL}"></a>{/if}</td>		</tr>	{/foreach}		<tfoot>			<tr>				<td colspan="13">					# {$result_found}&nbsp;&nbsp;&nbsp;{if $pagination neq ""}{$pagination}{/if}					<select onchange="changePage('{$navigationURL}',this.value,{$smarty.request.limit});">					{if $allPageCount gt 1}						{for $foo=1 to $allPageCount}							<option value="{$foo}" {if $foo eq $currentPage}selected="selected"{/if}>{$foo}</option>						{/for}					{else}						<option value="1" disabled="disabled">1</option>					{/if}					</select>				</td>			</tr>		</tfoot>	</table>{else}	<div class="error">{$_lang.biz_rewards.no_record_found}</div>{/if}<center><input data-icon="add-item" data-inline="true" onclick="window.location.href='{$page_url}?mode={$smarty.const.MODE_CREATE}'" type="button" value="{$_lang.biz_rewards.CREATE.BTN_LBL}"/></center></div>{include file="biz_rewards/js.tpl"}{include file="footer.tpl"}