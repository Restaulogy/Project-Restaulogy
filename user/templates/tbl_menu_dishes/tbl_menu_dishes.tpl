{include file='header.tpl'}<div class="wrapper"><h1>{$_lang.tbl_menu_dishes.listing_title}</h1>{if $error_msg}	<center>{$error_msg}</center>{/if}{if $result_found gt 0 && $tbl_menu_disheslist}	<table class="listTable">		<tr><th class="bigListItem">{$_lang.tbl_menu_dishes.title}</th>		<th class="actionListItem"></th></tr>		{foreach from=$tbl_menu_disheslist item=tbl_menu_dishesitem}			<tr>				<td class="bigListItem"><a href="{$page_url}?mode={$smarty.const.MODE_VIEW}&mnu_dish_id={$tbl_menu_dishesitem.mnu_dish_id}">{$tbl_menu_dishesitem.mnu_dish_menu_sub_menu_link}</a></td>				<td class="actionListItem"><a href="{$page_url}?mode={$smarty.const.MODE_UPDATE}&mnu_dish_id={$tbl_menu_dishesitem.mnu_dish_id}" class="editIcon"></a><a class="deleteIcon" href="#" onclick="deletetbl_menu_dishes({$tbl_menu_dishesitem.mnu_dish_id});"></a>{if $tbl_menu_dishesitem.isActive eq 1}<a href="{$page_url}?action={$smarty.const.ACTION_DEACTIVATE}&mnu_dish_id={$tbl_menu_dishesitem.mnu_dish_id}" class="deactiveIcon" title="{$_lang.tbl_menu_dishes.DEACTIVATE.BTN_LBL}"></a>{else}<a href="{$page_url}?action={$smarty.const.ACTION_ACTIVATE}&mnu_dish_id={$tbl_menu_dishesitem.mnu_dish_id}" class="activeIcon" title="{$_lang.tbl_menu_dishes.ACTIVATE.BTN_LBL}"></a>{/if}</td>			</tr>	{/foreach}	</table>{else}	<div class="error">{$_lang.tbl_menu_dishes.no_record_found}</div>{/if}{if $pagination}	<center>{$pagination}</center>{/if}<input onclick="window.location.href='{$page_url}?mode={$smarty.const.MODE_CREATE}'" type="button" value="{$_lang.tbl_menu_dishes.CREATE.BTN_LBL}"/></div>{include file="tbl_menu_dishes/js.tpl"}{include file="footer.tpl"}