{include file='header.tpl'}<div class="wrapper"><h1>{$_lang.tbl_menu_dishes.listing_title}</h1>{if $error_msg}	<center>{$error_msg}</center>{/if}{if $result_found gt 0 && $tbl_menu_disheslist}	<table class="biz_data_grid">		<tr>			<th class="{if $smarty.get.sort_on eq 'mnu_dish_menu_sub_menu_link'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=mnu_dish_menu_sub_menu_link&sort_by={$new_sort}">{$_lang.tbl_menu_dishes.label.mnu_dish_menu_sub_menu_link}</a></th>			<th class="{if $smarty.get.sort_on eq 'mnu_dish_menu_id'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=mnu_dish_menu_id&sort_by={$new_sort}">{$_lang.tbl_menu_dishes.label.mnu_dish_menu_id}</a></th>			<th class="{if $smarty.get.sort_on eq 'mnu_dish_dish_id'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=mnu_dish_dish_id&sort_by={$new_sort}">{$_lang.tbl_menu_dishes.label.mnu_dish_dish_id}</a></th>			<th class="{if $smarty.get.sort_on eq 'mnu_dish_price'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=mnu_dish_price&sort_by={$new_sort}">{$_lang.tbl_menu_dishes.label.mnu_dish_price}</a></th>			<th class="{if $smarty.get.sort_on eq 'mnu_dish_display_order'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=mnu_dish_display_order&sort_by={$new_sort}">{$_lang.tbl_menu_dishes.label.mnu_dish_display_order}</a></th>			<th class="{if $smarty.get.sort_on eq 'mnu_dish_desc'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=mnu_dish_desc&sort_by={$new_sort}">{$_lang.tbl_menu_dishes.label.mnu_dish_desc}</a></th>			<th class="action_header">Action</th>		</tr>		{foreach from=$tbl_menu_disheslist item=tbl_menu_dishesitem}		<tr class="{cycle values="odd,even"}">			<td><a href="{$page_url}?mode={$smarty.const.MODE_VIEW}&mnu_dish_id={$tbl_menu_dishesitem.mnu_dish_id}">{$tbl_menu_dishesitem.mnu_dish_menu_sub_menu_link}</a></td>			<td>{$tbl_menu_dishesitem.mnu_dish_menu_id}</td>			<td>{$tbl_menu_dishesitem.mnu_dish_dish_id}</td>			<td>{$tbl_menu_dishesitem.mnu_dish_price}</td>			<td>{$tbl_menu_dishesitem.mnu_dish_display_order}</td>			<td>{$tbl_menu_dishesitem.mnu_dish_desc}</td>			<td><a href="{$page_url}?mode={$smarty.const.MODE_UPDATE}&mnu_dish_id={$tbl_menu_dishesitem.mnu_dish_id}" class="editIcon"></a><a class="deleteIcon" href="#" onclick="deletetbl_menu_dishes({$tbl_menu_dishesitem.mnu_dish_id});"></a>{if $tbl_menu_dishesitem.isActive eq 1}<a href="{$page_url}?action={$smarty.const.ACTION_DEACTIVATE}&mnu_dish_id={$tbl_menu_dishesitem.mnu_dish_id}" class="deactiveIcon" title="{$_lang.tbl_menu_dishes.DEACTIVATE.BTN_LBL}"></a>{else}<a href="{$page_url}?action={$smarty.const.ACTION_ACTIVATE}&mnu_dish_id={$tbl_menu_dishesitem.mnu_dish_id}" class="activeIcon" title="{$_lang.tbl_menu_dishes.ACTIVATE.BTN_LBL}"></a>{/if}</td>		</tr>	{/foreach}		<tfoot>			<tr>				<td colspan="9">					# {$result_found}&nbsp;&nbsp;&nbsp;{if $pagination neq ""}{$pagination}{/if}					<select onchange="changePage('{$navigationURL}',this.value,{$smarty.request.limit});">					{if $allPageCount gt 1}						{for $foo=1 to $allPageCount}							<option value="{$foo}" {if $foo eq $currentPage}selected="selected"{/if}>{$foo}</option>						{/for}					{else}						<option value="1" disabled="disabled">1</option>					{/if}					</select>				</td>			</tr>		</tfoot>	</table>{else}	<div class="error">{$_lang.tbl_menu_dishes.no_record_found}</div>{/if}<input onclick="window.location.href='{$page_url}?mode={$smarty.const.MODE_CREATE}'" type="button" value="{$_lang.tbl_menu_dishes.CREATE.BTN_LBL}"/></div>{include file="tbl_menu_dishes/js.tpl"}{include file="footer.tpl"}