{include file='header.tpl'}<div class="wrapper"><h1>{$_lang.tbl_staff.listing_title}</h1>{if $error_msg}	<center>{$error_msg}</center>{/if}<form name="frm_tbl_stafflist" id="frm_tbl_stafflist"  method="POST" action="{$page_url}" >{if $result_found gt 0 && $tbl_stafflist}	<table class="listTable">		<tr>        <th class="bigListItem" colspan="2">{$_lang.tbl_staff.title}</th>        <th>{$_lang.tbl_staff.staff_role}</th>        <th style="width:5%;">                {$_lang.main.status}		</th>		</tr>		{foreach from=$tbl_stafflist item=tbl_staffitem}			<tr>			<td style="width:2%;">			<label for="sel_tbl_staff[{$tbl_staffitem.staff_id}]" data-mini="true" style="width:23px;"><input type="checkbox" data-inline='true' data-mini='true' id="sel_tbl_staff[{$tbl_staffitem.staff_id}]" name="sel_tbl_staff[{$tbl_staffitem.staff_id}]" />&nbsp;</label>            </td>				<td class="bigListItem"><a href="{$page_url}?mode={$smarty.const.MODE_UPDATE}&staff_id={$tbl_staffitem.staff_id}">{$tbl_staffitem.staff_full_name}</a></td>			<td >{$tbl_staffitem.member_role_name}</td>			<td >                {if $tbl_staffitem.isActive eq 1}                    <a href="#" class="activeIcon" title="{$_lang.tbl_staff.ACTIVATE.BTN_LBL}"></a>                {else}                    <a href="#" class="deactiveIcon" title="{$_lang.tbl_staff.DEACTIVATE.BTN_LBL}"></a>                {/if}			</td>						<!--            <td class="actionListItem"><a href="{$page_url}?mode={$smarty.const.MODE_VIEW}&staff_id={$tbl_staffitem.staff_id}" class="detailIcon"></a>            <a href="{$page_url}?mode={$smarty.const.MODE_UPDATE}&staff_id={$tbl_staffitem.staff_id}" class="editIcon"></a>            <a class="deleteIcon" href="#" onclick="deletetbl_staff({$tbl_staffitem.staff_id});"></a>            {if $tbl_staffitem.isActive eq 1}<a href="{$page_url}?action={$smarty.const.ACTION_DEACTIVATE}&staff_id={$tbl_staffitem.staff_id}" class="deactiveIcon" title="{$_lang.tbl_staff.DEACTIVATE.BTN_LBL}"></a>            {else}<a href="{$page_url}?action={$smarty.const.ACTION_ACTIVATE}&staff_id={$tbl_staffitem.staff_id}" class="activeIcon" title="{$_lang.tbl_staff.ACTIVATE.BTN_LBL}"></a>            {/if}            </td>            -->            		</tr>	{/foreach}	</table>{else}	<div class="error">{$_lang.tbl_staff.no_record_found}</div>{/if}{if $pagination}	<center>{$pagination}</center>{/if}<input type='hidden' name='action' id='action' value='' /></form><div class="biz_center">{if $result_found gt 0 && $tbl_stafflist}        <input data-inline="true" data-icon="briefcase" type="button" id="sel_all_tbl_staff" name="sel_all_tbl_staff" value="{$_lang.main.toggle}" onclick="javascript:$('input[type=checkbox]').click();" />        <input data-inline="true" data-icon="inactive" type="button" value="{$_lang.tbl_dishes.DEACTIVATE.BTN_LBL}" onclick="actiontbl_staff('{$smarty.const.ACTION_DEACTIVATE}');"  />		<input data-inline="true" data-icon="active" type="button" value="{$_lang.tbl_dishes.ACTIVATE.BTN_LBL}" onclick="actiontbl_staff('{$smarty.const.ACTION_ACTIVATE}');"  />    <!--        <input data-inline="true" data-icon="delete" type="button" id="del_sel_all_tbl_staff" name="del_sel_all_tbl_staff" value="{$_lang.tbl_staff.DELETE.BTN_LBL}" onclick="deletetbl_staff({$tbl_staffitem.tbl_sts_id});" />    -->{/if} {jqmbutton onclick="window.location.href='{$page_url}?mode={$smarty.const.MODE_CREATE}'" icon="add-item" value="{$_lang.tbl_staff.CREATE.BTN_LBL}"} </div></div>{include file="tbl_staff/js.tpl"}{include file="footer.tpl"}