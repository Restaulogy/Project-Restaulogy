{include file='header.tpl'}<div class="wrapper"><h1>{$_lang.tbl_staff.banned_users}</h1>{if $error_msg}	<center>{$error_msg}</center>{/if}<form name="frm_tbl_stafflist" id="frm_tbl_stafflist"  method="POST" action="{$page_url}" >{if $result_found gt 0 && $tbl_stafflist}	<table class="listTable">		<tr>        <th class="bigListItem" colspan="2">{$_lang.tbl_staff.crm_list}</th>        		</tr>		{foreach from=$tbl_stafflist item=tbl_staffitem}			<tr>            <td style="width:2%;">			<label for="sel_tbl_staff[{$tbl_staffitem.staff_id}]" data-mini="true" style="width:23px;"><input type="checkbox" data-inline='true' data-mini='true' id="sel_tbl_staff[{$tbl_staffitem.staff_id}]" name="sel_tbl_staff[{$tbl_staffitem.staff_id}]" />&nbsp;</label>            </td>            			<td class="bigListItem"><a href="{$page_url}?mode={$smarty.const.MODE_UPDATE}&staff_id={$tbl_staffitem.staff_id}">{$tbl_staffitem.staff_full_name}</a></td>						<!--			<td >                <input type="button" value="{$_lang.tbl_staff.UNBANN_USER.BTN_LBL}" data-inline="true" data-icon="save" onclick="location.href='{$page_url}?action=UNBANN_USER&staff_member_id={$tbl_staffitem.staff_member_id}';" />			</td>			-->		</tr>	{/foreach}	</table>{else}	<div class="error">{$_lang.tbl_staff.no_record_found}</div>{/if}{if $pagination}	<center>{$pagination}</center>{/if}<input type='hidden' name='action' id='action' value='' /><div class="biz_center">    {if $result_found gt 0 && $tbl_stafflist}        <input data-inline="true" data-icon="briefcase" type="button" id="sel_all_tbl_staff" name="sel_all_tbl_staff" value="{$_lang.main.toggle}" onclick="javascript:$('input[type=checkbox]').click();" />        <input data-inline="true" data-icon="save" type="button" id="del_sel_all_tbl_staff" name="del_sel_all_tbl_staff" value="{$_lang.tbl_staff.UNBANN_USER.BTN_LBL}" onclick="softdeletetbl_staff();" />    {/if}</div></form></div>{include file="tbl_staff/js.tpl"}{include file="footer.tpl"}