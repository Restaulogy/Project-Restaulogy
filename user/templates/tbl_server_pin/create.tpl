{include file="header.tpl"}<div class="wrapper"><h1>{$_lang.tbl_server_pin.create_title}</h1>{if $error_msg}	<center>{$error_msg}</center>{/if}<form name="frmcreatetbl_server_pin" id="frmcreatetbl_server_pin" onsubmit="return validatetbl_server_pin();" method="POST" action="{$page_url}">	<input type="hidden" name="Array" id="Array" value="0"/>	<div style="display:none;" class="error" id="Array_err"></div>	<div class="biz_hidden">		<label for="srv_pin_restaurant">{$_lang.tbl_server_pin.label.srv_pin_restaurant}</label>		<input id="srv_pin_restaurant" type="text" value="{$smarty.session[$smarty.const.SES_RESTAURANT]}" name="srv_pin_restaurant"/>		<div class="error" id="srv_pin_restaurant_err"></div>	</div>	<div class="field-row">		<label for="srv_pin_password">{$_lang.tbl_server_pin.label.srv_pin_password}</label>		<input id="srv_pin_password" type="text" value="{$smarty.post.srv_pin_password}" name="srv_pin_password"/>		<div class="error" id="srv_pin_password_err"></div>	</div>		<div class="field-row">		<label for="srv_pin_server">{$_lang.tbl_server_pin.label.srv_pin_server}</label>		 <select name="srv_pin_server" id="srv_pin_server">			 <option value="">Select Server</option>			  {foreach $lst_servers as $val}			 		<option value="{$val@key}" {if $val@key eq $smarty.request.srv_pin_server}selected="selected"{/if}>{$val}</option>			  {/foreach}			 </select>		<div class="error" id="srv_pin_server_err"></div>	</div>	<!--	<div class="field-row">		<label for="srv_pin_start_date">{$_lang.tbl_server_pin.label.srv_pin_start_date}</label>		<input  id="srv_pin_start_date" type="text" value="{$smarty.post.srv_pin_start_date}" name="srv_pin_start_date"/> 		<div class="error" id="srv_pin_start_date_err"></div>	</div>	-->	<!--	<div class="field-row">		<label for="srv_pin_end_date">{$_lang.tbl_server_pin.label.srv_pin_end_date}</label>		<input  id="srv_pin_end_date" type="text" value="{$smarty.post.srv_pin_end_date}" name="srv_pin_end_date"/> 		<div class="error" id="srv_pin_end_date_err"></div>	</div>	-->	<div class="biz_center">    <input type="hidden" id="action" name="action" value="{$smarty.const.ACTION_CREATE}"/>    <input data-inline="true" data-icon="save" type="submit" value="{$_lang.save_lbl}"/>    <input data-inline="true" data-icon="delete" type="reset" value="{$_lang.cancel_lbl}" onclick="window.location.href='{$page_url}'"/>    </div></form>{include file="tbl_server_pin/js.tpl"}</div>{include file="footer.tpl"}