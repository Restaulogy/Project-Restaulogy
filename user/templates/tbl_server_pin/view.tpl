{include file="header.tpl"}<div class="wrapper"><h1>{$_lang.tbl_server_pin.title}</h1>{if $error_msg}	<center>{$error_msg}</center>{/if}<form name="frmupdatetbl_server_pin" id="frmupdatetbl_server_pin" onsubmit="return validatetbl_server_pin();" method="POST" action="{$page_url}" style="{if $isUpdate eq 1}{else}display:none;{/if}">	<input type="hidden" name="srv_pin_id" id="srv_pin_id" value="{$tbl_server_pininfo.srv_pin_id}"/>	<div style="display:none;" class="error" id="srv_pin_id_err"></div>	<div class="biz_hidden">		<label for="srv_pin_restaurant">{$_lang.tbl_server_pin.label.srv_pin_restaurant}</label>		<input type="text" name="srv_pin_restaurant" id="srv_pin_restaurant" value="{$tbl_server_pininfo.srv_pin_restaurant}"/>		<div class="error" id="srv_pin_restaurant_err"></div>	</div>	<div class="field-row">		<label for="srv_pin_password">{$_lang.tbl_server_pin.label.srv_pin_password}</label>		<input type="text" name="srv_pin_password" id="srv_pin_password" value="{$tbl_server_pininfo.srv_pin_password}"/>		<div class="error" id="srv_pin_password_err"></div>	</div>		<div class="field-row">		<label for="srv_pin_server">{$_lang.tbl_server_pin.label.srv_pin_server}</label>		 <select name="srv_pin_server" id="srv_pin_server">			 <option value="">Select Server</option>			  {foreach $lst_servers as $val}			 		<option value="{$val@key}" {if $val@key eq $tbl_server_pininfo.srv_pin_server}selected="selected"{/if}>{$val}</option>			  {/foreach}			 </select>		<div class="error" id="srv_pin_server_err"></div>	</div>	<!--	<div class="field-row">		<label for="srv_pin_start_date">{$_lang.tbl_server_pin.label.srv_pin_start_date}</label>		<input type="text" name="srv_pin_start_date" id="srv_pin_start_date" value="{$tbl_server_pininfo.srv_pin_start_date}"/>		<div class="error" id="srv_pin_start_date_err"></div>	</div>	-->	<!--	<div class="field-row">		<label for="srv_pin_end_date">{$_lang.tbl_server_pin.label.srv_pin_end_date}</label>		<input type="text" name="srv_pin_end_date" id="srv_pin_end_date" value="{$tbl_server_pininfo.srv_pin_end_date}"/>		<div class="error" id="srv_pin_end_date_err"></div>	</div>	-->	<div class="biz_center">    <input type="hidden" id="action" name="action" value="{$smarty.const.ACTION_UPDATE}"/> <input  data-inline="true" data-icon="save"  type="submit" value="{$_lang.save_lbl}"/> <input data-inline="true" data-icon="delete" type="button" onclick="window.location.href='{$page_url}'" value="{$_lang.cancel_lbl}"/>    </div></form><div id="tbl_server_pin_view" class="description" style="{if $isUpdate eq 1}display:none;{/if}">	<table class="listTable">		<tr><th class="fieldItem">{$_lang.field_title}</th><th class="valueItem">{$_lang.value_title}</th></tr>		<tr><td class="fieldItem">{$_lang.tbl_server_pin.label.srv_pin_id}<i>:</i></td><td class="valueItem">{$tbl_server_pininfo.srv_pin_id}</td></tr>		<tr><td class="fieldItem">{$_lang.tbl_server_pin.label.srv_pin_restaurant}<i>:</i></td><td class="valueItem">{$tbl_server_pininfo.srv_pin_restaurant}</td></tr>		<tr><td class="fieldItem">{$_lang.tbl_server_pin.label.srv_pin_password}<i>:</i></td><td class="valueItem">{$tbl_server_pininfo.srv_pin_password}</td></tr>	<!--		<tr><td class="fieldItem">{$_lang.tbl_server_pin.label.srv_pin_start_date}<i>:</i></td><td class="valueItem">{$tbl_server_pininfo.srv_pin_start_date}</td></tr>	-->	<!--		<tr><td class="fieldItem">{$_lang.tbl_server_pin.label.srv_pin_end_date}<i>:</i></td><td class="valueItem">{$tbl_server_pininfo.srv_pin_end_date}</td></tr>	-->	<tr><td class="fieldItem">{$_lang.tbl_server_pin.label.isActive}<i>:</i></td><td class="valueItem">{if $tbl_server_pininfo.isActive eq 1}{$_lang.tbl_server_pin.label.isActive_yes}{else}{$_lang.tbl_server_pin.label.isActive_no}{/if}</td></tr>	</table>	<center><input data-inline="true" data-icon="edit" type="button" value="{$_lang.tbl_server_pin.UPDATE.BTN_LBL}" onclick="$('#tbl_server_pin_view').hide();$('#frmupdatetbl_server_pin').show();"/><input type="button" value="{$_lang.cancel_lbl}" data-inline="true" data-icon="delete" onclick="window.location.href='{$page_url}'"/></center></div>{include file="tbl_server_pin/js.tpl"}</div>{include file="footer.tpl"}