{literal}<script type="text/javascript">function actiontbl_server_pin(action){	var varId = $("input:checked").length;	var isValid = 0;	if(varId > 0){	if(action != ""){		switch (action) {			case '{/literal}{$smarty.const.ACTION_DELETE}{literal}':			     if(confirm("{/literal}{$_lang.tbl_server_pin.DELETE.CONFIRM_MSG}{literal}")==true){					isValid = 1;				 }			break;			case '{/literal}{$smarty.const.ACTION_DEACTIVATE}{literal}':			         isValid = 1;			break;			case '{/literal}{$smarty.const.ACTION_ACTIVATE}{literal}':					isValid = 1;			break;		}		if(isValid){		 	$('#action').val(action);		    $('#frm_tbl_server_pin').submit();		}	} }else{			alert("{/literal}{$_lang.main.select_ids.empty}{literal}"); }}function deletetbl_server_pin(varId){	if(varId > 0){		if(confirm("{/literal}{$_lang.tbl_server_pin.DELETE.CONFIRM_MSG}{literal}")==true){			window.location.href="{/literal}{$page_url}{literal}?action=delete&srv_pin_id="+varId;		}	}}function validatetbl_server_pin(){	$("#srv_pin_id_err").html("");	$("#srv_pin_restaurant_err").html("");	$("#srv_pin_password_err").html("");	$("#srv_pin_start_date_err").html("");	$("#srv_pin_end_date_err").html("");	var isErr = true;	if(elemById("action").value=="update"){		if(IsNonEmpty(elemById("srv_pin_id").value)==false){			$("#srv_pin_id_err").html("{/literal}{$_lang.tbl_server_pin.not_empty_msg.srv_pin_id}{literal}");			isErr = false;		}	}	if(IsNonEmpty(elemById("srv_pin_restaurant").value)==false){		$("#srv_pin_restaurant_err").html("{/literal}{$_lang.tbl_server_pin.not_empty_msg.srv_pin_restaurant}{literal}");		isErr = false;	}	if(IsNonEmpty(elemById("srv_pin_password").value)==false){		$("#srv_pin_password_err").html("{/literal}{$_lang.tbl_server_pin.not_empty_msg.srv_pin_password}{literal}");		isErr = false;	}/*	if(IsNonEmpty(elemById("srv_pin_start_date").value)==false){		$("#srv_pin_start_date_err").html("{/literal}{$_lang.tbl_server_pin.not_empty_msg.srv_pin_start_date}{literal}");		isErr = false;	}*//*	if(IsNonEmpty(elemById("srv_pin_end_date").value)==false){		$("#srv_pin_end_date_err").html("{/literal}{$_lang.tbl_server_pin.not_empty_msg.srv_pin_end_date}{literal}");		isErr = false;	}*/	if(isErr == false){		alert("{/literal}{$_lang.messages.revise_form}{literal}");	}	return isErr;}//..function</script>{/literal}