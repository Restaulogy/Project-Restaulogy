{include file="header.tpl"}<div class="wrapper"><h1>{$_lang.tbl_orders.create_title}</h1>{if $error_msg}	<center>{$error_msg}</center>{/if}<form name="frmcreatetbl_orders" id="frmcreatetbl_orders" onsubmit="return validatetbl_orders();" method="POST" action="{$page_url}">	<input type="hidden" name="order_id" id="order_id" value="0"/>	<div style="display:none;" class="error" id="order_id_err"></div>	<div class="field-row">		<label for="order_table_id">{$_lang.tbl_orders.label.order_table_id}</label>		<input type="text" name="order_table_id" id="order_table_id" value="{$smarty.post.order_table_id}"/>		<div class="error" id="order_table_id_err"></div>	</div>	<div class="field-row">		<label for="order_emp_id">{$_lang.tbl_orders.label.order_emp_id}</label>		<input type="text" name="order_emp_id" id="order_emp_id" value="{$smarty.post.order_emp_id}"/>		<div class="error" id="order_emp_id_err"></div>	</div>	<div class="field-row">		<label for="order_customer_id">{$_lang.tbl_orders.label.order_customer_id}</label>		<input type="text" name="order_customer_id" id="order_customer_id" value="{$smarty.post.order_customer_id}"/>		<div class="error" id="order_customer_id_err"></div>	</div>	<div class="field-row">		<label for="order_customer_name">{$_lang.tbl_orders.label.order_customer_name}</label>		<input type="text" name="order_customer_name" id="order_customer_name" value="{$smarty.post.order_customer_name}"/>		<div class="error" id="order_customer_name_err"></div>	</div>	<div class="field-row">		<label for="order_note">{$_lang.tbl_orders.label.order_note}</label>		<input type="text" name="order_note" id="order_note" value="{$smarty.post.order_note}"/>		<div class="error" id="order_note_err"></div>	</div>	<div class="field-row">		<label for="order_created_on">{$_lang.tbl_orders.label.order_created_on}</label>		<input type="text" name="order_created_on" id="order_created_on" value="{$smarty.post.order_created_on}"/>		<div class="error" id="order_created_on_err"></div>	</div>	<div class="field-row">		<label for="order_completed_on">{$_lang.tbl_orders.label.order_completed_on}</label>		<input type="text" name="order_completed_on" id="order_completed_on" value="{$smarty.post.order_completed_on}"/>		<div class="error" id="order_completed_on_err"></div>	</div>	<div class="field-row">		<label for="order_status_id">{$_lang.tbl_orders.label.order_status_id}</label>		<input type="text" name="order_status_id" id="order_status_id" value="{$smarty.post.order_status_id}"/>		<div class="error" id="order_status_id_err"></div>	</div>	<input type="hidden" id="action" name="action" value="{$smarty.const.ACTION_CREATE}"/><input class="fleft" type="submit" value="{$_lang.save_lbl}"/> <input  class="fright" type="reset" value="{$_lang.cancel_lbl}" onclick="window.location.href='{$page_url}'"/></form> </div>{include file="footercontent.tpl"}{include file="tbl_orders/js.tpl"} </body></html>