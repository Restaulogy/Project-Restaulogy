{include file='header.tpl'}<div class="wrapper"><h1>{$_lang.tbl_orders.listing_title}</h1>{if $error_msg}	<center>{$error_msg}</center>{/if}{if $result_found gt 0 && $tbl_orderslist}	<table class="listTable">		<tr><th class="bigListItem">{$_lang.tbl_orders.title}</th>		<th class="actionListItem"></th></tr>		{foreach from=$tbl_orderslist item=tbl_ordersitem}			<tr>				<td class="bigListItem"><a href="{$page_url}?mode={$smarty.const.MODE_VIEW}&order_id={$tbl_ordersitem.order_id}">{$tbl_ordersitem.order_table_id}</a></td>				<td class="actionListItem"><a href="{$page_url}?mode={$smarty.const.MODE_UPDATE}&order_id={$tbl_ordersitem.order_id}" class="editIcon"></a><a class="deleteIcon" href="#" onclick="deletetbl_orders({$tbl_ordersitem.order_id});"></a>{if $tbl_ordersitem.isActive eq 1}<a href="{$page_url}?action={$smarty.const.ACTION_DEACTIVATE}&order_id={$tbl_ordersitem.order_id}" class="deactiveIcon" title="{$_lang.tbl_orders.DEACTIVATE.BTN_LBL}"></a>{else}<a href="{$page_url}?action={$smarty.const.ACTION_ACTIVATE}&order_id={$tbl_ordersitem.order_id}" class="activeIcon" title="{$_lang.tbl_orders.ACTIVATE.BTN_LBL}"></a>{/if}</td>			</tr>	{/foreach}	</table>{else}	<div class="error">{$_lang.tbl_orders.no_record_found}</div>{/if}{if $pagination}	<center>{$pagination}</center>{/if}<input onclick="window.location.href='{$page_url}?mode={$smarty.const.MODE_CREATE}'" type="button" value="{$_lang.tbl_orders.CREATE.BTN_LBL}"/></div>{include file="footercontent.tpl"}{include file="tbl_orders/js.tpl"} </body></html>