{include file="header.tpl"}<div class="wrapper"><h1>{$_lang.tbl_table_shift_assignment.create_title}</h1>{if $error_msg}	<center>{$error_msg}</center>{/if} <form name="frmcreatetbl_table_shift_assignment" id="frmcreatetbl_table_shift_assignment" onsubmit="return validatetbl_table_shift_assignment();" method="POST" action="{$page_url}">	<input type="hidden" name="tbl_sft_id" id="tbl_sft_id" value="0"/>	<div style="display:none;" class="error" id="tbl_sft_id_err"></div>	<!--	<label for="tbl_sft_restaurant_id">{$_lang.tbl_table_shift_assignment.label.tbl_sft_restaurant_id}</label>	<input type="text" name="tbl_sft_restaurant_id" id="tbl_sft_restaurant_id" value="{$smarty.post.tbl_sft_restaurant_id}"/>	<div class="error" id="tbl_sft_restaurant_id_err"></div>	-->	<label for="tbl_sft_shift_id">{$_lang.tbl_table_shift_assignment.label.tbl_sft_shift_id}</label>	<select name="tbl_sft_shift_id" id="tbl_sft_shift_id" value="tbl_sft_shift_id">		<option value="">Select Shift</option>		{if $shifts} 			{foreach $shifts as $value}				<option value="{$value@key}" {if $smarty.post.tbl_sft_shift_id eq $value@key}selected="selected"{/if}>{$value}</option>			{/foreach}		{/if}  	</select>	<div class="error" id="tbl_sft_shift_id_err"></div>	<label for="tbl_sft_emp_id">{$_lang.tbl_table_shift_assignment.label.tbl_sft_emp_id}</label>	<select name="tbl_sft_emp_id" id="tbl_sft_emp_id" value="tbl_sft_emp_id">		<option value="">Select Employee</option>		{if $employees}			 			{foreach $employees as $value}				<option value="{$value@key}" {if $smarty.post.tbl_sft_emp_id eq $value@key}selected="selected"{/if}>{$value}</option>			{/foreach} 		{/if}  	</select>	<div class="error" id="tbl_sft_emp_id_err"></div>	<label for="tbl_sft_table_id">{$_lang.tbl_table_shift_assignment.label.tbl_sft_table_id}</label>	<select name="tbl_sft_table_id" id="tbl_sft_table_id" value="tbl_sft_table_id">		<option value="">Select Table</option>		{if $tables}			{foreach $tables as $value}				<option value="{$value@key}" {if $smarty.post.tbl_sft_table_id eq $value@key}selected="selected"{/if}>{$value}</option>			{/foreach} 		{/if}  	</select>	<div class="error" id="tbl_sft_table_id_err"></div>	<label for="tbl_sft_period_from">{$_lang.tbl_table_shift_assignment.label.tbl_sft_period_from}</label>	<input type="text" readonly="readonly" name="tbl_sft_period_from" id="tbl_sft_period_from" value="{$smarty.post.tbl_sft_period_from|date_format:$smarty.const.HTML5_DATE_FORMAT}"/>	<div class="error" id="tbl_sft_period_from_err"></div>	<label for="tbl_sft_period_to">{$_lang.tbl_table_shift_assignment.label.tbl_sft_period_to}</label>	<input type="text" readonly="readonly" name="tbl_sft_period_to" id="tbl_sft_period_to" value="{$smarty.post.tbl_sft_period_to|date_format:$smarty.const.HTML5_DATE_FORMAT}"/>	<div class="error" id="tbl_sft_period_to_err"></div>	<!--	<label for="tb_sft_created_on">{$_lang.tbl_table_shift_assignment.label.tb_sft_created_on}</label>	<input type="date" name="tb_sft_created_on" id="tb_sft_created_on" value="{$smarty.post.tb_sft_created_on|date_format:$smarty.const.HTML5_DATE_FORMAT}"/>	<div class="error" id="tb_sft_created_on_err"></div>	<label for="tb_sft_terminated_on">{$_lang.tbl_table_shift_assignment.label.tb_sft_terminated_on}</label>	<input type="date" name="tb_sft_terminated_on" id="tb_sft_terminated_on" value="{$smarty.post.tb_sft_terminated_on|date_format:$smarty.const.HTML5_DATE_FORMAT}" />	<div class="error" id="tb_sft_terminated_on_err"></div>	-->    <center>	<input type="hidden" id="action" name="action" value="{$smarty.const.ACTION_CREATE}"/><input data-inline="true" data-icon="save" type="submit" value="{$_lang.save_lbl}" /> <input data-inline="true" data-icon="delete" type="reset" value="{$_lang.cancel_lbl}" onclick="window.location.href='{$page_url}'"/>  </center>	</form>{include file="tbl_table_shift_assignment/js.tpl"}</div>{include file="footercontent.tpl"}{literal}<script>$(function(){	$("#tbl_sft_period_from").scroller({ preset: 'date', dateFormat: 'mm/dd/yyyy', timeWheels: 'mmddyyyy', animate: 'pop'});	$("#tbl_sft_period_to").scroller({ preset: 'date', dateFormat: 'mm/dd/yyyy', timeWheels: 'mmddyyyy', animate: 'pop'});	})</script>{/literal}