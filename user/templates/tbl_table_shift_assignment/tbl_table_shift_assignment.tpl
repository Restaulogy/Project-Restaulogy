{include file='header.tpl'}<div class="wrapper"><h1>{$_lang.tbl_table_shift_assignment.listing_title}</h1>{if $error_msg}	<center>{$error_msg}</center>{/if}{if $result_found gt 0 && $tbl_table_shift_assignmentlist}	<table class="listTable">		<tr><th class="bigListItem">{$_lang.tbl_table_shift_assignment.title}</th>		<th class="actionListItem"></th></tr>		{foreach from=$tbl_table_shift_assignmentlist item=tbl_table_shift_assignmentitem}			<tr>				<td class="bigListItem"><a href="{$page_url}?mode={$smarty.const.MODE_VIEW}&tbl_sft_id={$tbl_table_shift_assignmentitem.tbl_sft_id}">{$tbl_table_shift_assignmentitem.tbl_sft_restaurant_id}</a></td>				<td class="actionListItem"><a href="{$page_url}?mode={$smarty.const.MODE_UPDATE}&tbl_sft_id={$tbl_table_shift_assignmentitem.tbl_sft_id}" class="editIcon"></a><a class="deleteIcon" href="#" onclick="deletetbl_table_shift_assignment({$tbl_table_shift_assignmentitem.tbl_sft_id});"></a>{if $tbl_table_shift_assignmentitem.isActive eq 1}<a href="{$page_url}?action={$smarty.const.ACTION_DEACTIVATE}&tbl_sft_id={$tbl_table_shift_assignmentitem.tbl_sft_id}" class="deactiveIcon" title="{$_lang.tbl_table_shift_assignment.DEACTIVATE.BTN_LBL}"></a>{else}<a href="{$page_url}?action={$smarty.const.ACTION_ACTIVATE}&tbl_sft_id={$tbl_table_shift_assignmentitem.tbl_sft_id}" class="activeIcon" title="{$_lang.tbl_table_shift_assignment.ACTIVATE.BTN_LBL}"></a>{/if}</td>			</tr>	{/foreach}	</table>{else}	<div class="error">{$_lang.tbl_table_shift_assignment.no_record_found}</div>{/if}{if $pagination}	<center>{$pagination}</center>{/if}<input onclick="window.location.href='{$page_url}?mode={$smarty.const.MODE_CREATE}'" type="button" value="{$_lang.tbl_table_shift_assignment.CREATE.BTN_LBL}"/></div>{include file="tbl_table_shift_assignment/js.tpl"}{include file="footer.tpl"}