nt.not_empty_msg.tbl_sft_period_to}{literal}");		isErr = false;		compare_date = 0;	}else{		if(elemById("tbl_sft_period_to").value < elemById("tbl_sft_period_from").value){			$("#tbl_sft_period_to_err").html("{/literal}{$_lang.messages.validation.gt_others_time|sprintf:$_lang.tbl_table_shift_assignment.label.tbl_sft_period_from}{literal}");			isErr = false;		}	}	    /*	if(IsNonEmpty(elemById("tb_sft_created_on").value)==false){		$("#tb_sft_created_on_err").html("{/literal}{$_lang.tbl_table_shift_assignment.not_empty_msg.tb_sft_created_on}{literal}");		isErr = false;	}	if(IsNonEmpty(elemById("tb_sft_terminated_on").value)==false){		$("#tb_sft_terminated_on_err").html("{/literal}{$_lang.tbl_table_shift_assignment.not_empty_msg.tb_sft_terminated_on}{literal}");		isErr = false;	}*/	if(isErr == false){		alert("{/literal}{$_lang.messages.revise_form}{literal}");	}	return isErr;}//..functionfunction deletetbl_table_shift_assignment(varId){	if(varId > 0){		if(confirm("{/literal}{$_lang.tbl_table_shift_assignment.DELETE.CONFIRM_MSG}{literal}")==true){			window.location.href="{/literal}{$page_url}{literal}?action=delete&tbl_sft_id="+varId;		}	}}</script>{/literal}