{include file='header.tpl'}<div class="wrapper"><h1>{$_lang.tbl_dish_attrib.listing_title}</h1>{if $error_msg}	<center>{$error_msg}</center>{/if}{if $result_found gt 0 && $tbl_dish_attriblist}<form name="frm_tbl_dish_attrib" id="frm_tbl_dish_attrib" method="POST" action="{$page_url}">	<table class="listTable">		<tr>        <th class="bigListItem" colspan="2">{$_lang.tbl_dish_attrib.title}</th>        <th style="width:5%;">                {$_lang.main.status}		</th>        <!-- <th class="actionListItem"></th> -->        </tr>		{foreach from=$tbl_dish_attriblist item=tbl_dish_attribitem}			<tr>                <td style="width:3%;" >					 <label for="sel_dish_attrib[{$tbl_dish_attribitem.dish_attrib_id}]" data-mini="true" style="width:23px;"><input type="checkbox" data-inline='true' data-mini='true' id="sel_dish_attrib[{$tbl_dish_attribitem.dish_attrib_id}]" name="sel_dish_attrib[{$tbl_dish_attribitem.dish_attrib_id}]" />&nbsp;</label>			    </td>			    				<td ><a href="{$page_url}?mode={$smarty.const.MODE_UPDATE}&dish_attrib_id={$tbl_dish_attribitem.dish_attrib_id}">{$tbl_dish_attribitem.dish_attrib_name}</a></td>								<td >                    {if $tbl_dish_attribitem.isActive eq 1}                            <a href="#" class="activeIcon" title="{$_lang.tbl_dish_attrib.ACTIVATE.BTN_LBL}"></a>                    {else}                            <a href="#" class="deactiveIcon" title="{$_lang.tbl_dish_attrib.DEACTIVATE.BTN_LBL}"></a>                    {/if}                </td>								<!--				<td class="actionListItem">                <a href="{$page_url}?mode={$smarty.const.MODE_UPDATE}&dish_attrib_id={$tbl_dish_attribitem.dish_attrib_id}" class="editIcon"></a><a class="deleteIcon" href="#" onclick="deletetbl_dish_attrib({$tbl_dish_attribitem.dish_attrib_id});"></a>                {if $tbl_dish_attribitem.isActive eq 1}<a href="{$page_url}?action={$smarty.const.ACTION_DEACTIVATE}&dish_attrib_id={$tbl_dish_attribitem.dish_attrib_id}" class="deactiveIcon" title="{$_lang.tbl_dish_attrib.DEACTIVATE.BTN_LBL}"></a>{else}<a href="{$page_url}?action={$smarty.const.ACTION_ACTIVATE}&dish_attrib_id={$tbl_dish_attribitem.dish_attrib_id}" class="activeIcon" title="{$_lang.tbl_dish_attrib.ACTIVATE.BTN_LBL}"></a>{/if}</td>				-->			</tr>	{/foreach}	</table>{else}	<div class="error">{$_lang.tbl_dish_attrib.no_record_found}</div>{/if}{if $pagination}	<center>{$pagination}</center>{/if} <input type="hidden" id="action" name="{$smarty.const.ACTION_TITLE}"/></form><div class="biz_center">{if $result_found gt 0 && $tbl_dish_attriblist} 	<input data-inline="true" data-icon="briefcase" type="button" value="{$_lang.main.toggle}" onclick="javascript:$('input[type=checkbox]').click();" />    <input data-inline="true" data-icon="inactive" type="button" value="{$_lang.tbl_dishes.DEACTIVATE.BTN_LBL}" onclick="actiontbl_dish_attrib('{$smarty.const.ACTION_DEACTIVATE}');"  />    <input data-inline="true" data-icon="active" type="button" value="{$_lang.tbl_dishes.ACTIVATE.BTN_LBL}" onclick="actiontbl_dish_attrib('{$smarty.const.ACTION_ACTIVATE}');" />    <input data-inline="true" data-icon="delete" type="button" value="{$_lang.tbl_dishes.DELETE.BTN_LBL}" onclick="actiontbl_dish_attrib('{$smarty.const.ACTION_DELETE}');" />{/if}<input data-icon="add-item" data-inline="true" onclick="window.location.href='{$page_url}?mode={$smarty.const.MODE_CREATE}'" type="button" value="{$_lang.tbl_dish_attrib.CREATE.BTN_LBL}"/></div></div>{include file="tbl_dish_attrib/js.tpl"}{include file="footer.tpl"}