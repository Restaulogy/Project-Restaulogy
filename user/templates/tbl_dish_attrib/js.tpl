{literal}<script type="text/javascript">function actiontbl_dish_attrib(action){	var varId = $("input:checked").length;	var isValid = 0;	if(varId > 0){	if(action != ""){		switch (action) {			case '{/literal}{$smarty.const.ACTION_DELETE}{literal}':			if(confirm("{/literal}{$_lang.tbl_dish_attrib.DELETE.CONFIRM_MSG}{literal}")==true){				isValid = 1;			}			break;			case '{/literal}{$smarty.const.ACTION_DEACTIVATE}{literal}':			isValid = 1;			break;			case '{/literal}{$smarty.const.ACTION_ACTIVATE}{literal}':			isValid = 1;			break;		}		    	if(isValid){    	   $('#action').val(action);    	   $('#frm_tbl_dish_attrib').submit();    	}	}	}else{			alert("{/literal}{$_lang.main.select_ids.empty}{literal}"); 	}}function validateImage(){	 var field = document.getElementById('dish_attrib_img');	 var mnu_err = $('#dish_attrib_img_err');	 mnu_err.html('').trigger('refresh');      if(!IsNonEmpty(field.value)){         mnu_err.html('Please Upload a file.').trigger('refresh');         return false;      }else{		  if ((IsFileType(field.value, 'png') || IsFileType(field.value, 'gif') || IsFileType(field.value, 'jpg')) == false){            mnu_err.html('Your uploaded file must be in .gif|.png|.jpg Format\nPlease select again.').trigger('refresh');            return false; 		 }else{            //$('#frmupdatetbl_dish_attrib').submit();            return true; 		 }      }	return true;}function deletetbl_dish_attrib(varId){	if(varId > 0){		if(confirm("{/literal}{$_lang.tbl_dish_attrib.DELETE.CONFIRM_MSG}{literal}")==true){			window.location.href="{/literal}{$page_url}{literal}?action=delete&dish_attrib_id="+varId;		}	}}function validatetbl_dish_attrib(){	$("#dish_attrib_id_err").html("");	$("#dish_attrib_name_err").html("");	$("#dish_attrib_desc_err").html("");	$("#dish_attrib_img_err").html("");	$("#dish_attrib_start_date_err").html("");	$("#dish_attrib_end_date_err").html("");	var isErr = true;	if(elemById("action").value=="UPDATE"){		if(IsNonEmpty(elemById("dish_attrib_id").value)==false){			$("#dish_attrib_id_err").html("{/literal}{$_lang.tbl_dish_attrib.not_empty_msg.dish_attrib_id}{literal}");			isErr = false;		}	}		if(IsNonEmpty(elemById("dish_attrib_name").value)==false){		$("#dish_attrib_name_err").html("{/literal}{$_lang.tbl_dish_attrib.not_empty_msg.dish_attrib_name}{literal}");		isErr = false;	}	if(IsNonEmpty(elemById("dish_attrib_desc").value)==false){		$("#dish_attrib_desc_err").html("{/literal}{$_lang.tbl_dish_attrib.not_empty_msg.dish_attrib_desc}{literal}");		isErr = false;	}	    if(elemById("action").value!="UPDATE"){    	if(IsNonEmpty(elemById("dish_attrib_img").value)==false){    		$("#dish_attrib_img_err").html("{/literal}{$_lang.tbl_dish_attrib.not_empty_msg.dish_attrib_img}{literal}");    		isErr = false;    	}    	    	if(validateImage()==false){            isErr = false;        }	}/*	if(IsNonEmpty(elemById("dish_attrib_start_date").value)==false){		$("#dish_attrib_start_date_err").html("{/literal}{$_lang.tbl_dish_attrib.not_empty_msg.dish_attrib_start_date}{literal}");		isErr = false;	}*//*	if(IsNonEmpty(elemById("dish_attrib_end_date").value)==false){		$("#dish_attrib_end_date_err").html("{/literal}{$_lang.tbl_dish_attrib.not_empty_msg.dish_attrib_end_date}{literal}");		isErr = false;	}*/	if(isErr == false){		alert("{/literal}{$_lang.messages.revise_form}{literal}");	}	return isErr;}//..function</script>{/literal}