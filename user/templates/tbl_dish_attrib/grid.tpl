{include file='header.tpl'}<div class="wrapper"><h1>{$_lang.tbl_dish_attrib.listing_title}</h1>{if $error_msg}	<center>{$error_msg}</center>{/if}{if $result_found gt 0 && $tbl_dish_attriblist}	<table class="biz_data_grid">		<tr>			<th class="{if $smarty.get.sort_on eq 'dish_attrib_name'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=dish_attrib_name&sort_by={$new_sort}">{$_lang.tbl_dish_attrib.label.dish_attrib_name}</a></th>			<th class="{if $smarty.get.sort_on eq 'dish_attrib_desc'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=dish_attrib_desc&sort_by={$new_sort}">{$_lang.tbl_dish_attrib.label.dish_attrib_desc}</a></th>			<th class="{if $smarty.get.sort_on eq 'dish_attrib_img'}active_{if $smarty.get.sort_by eq 'DESC'}desc{else}asc{/if}{/if}"><a href="{$page_url}?sort_on=dish_attrib_img&sort_by={$new_sort}">{$_lang.tbl_dish_attrib.label.dish_attrib_img}</a></th>			<th class="action_header">Action</th>		</tr>		{foreach from=$tbl_dish_attriblist item=tbl_dish_attribitem}		<tr class="{cycle values="odd,even"}">			<td><a href="{$page_url}?mode={$smarty.const.MODE_VIEW}&dish_attrib_id={$tbl_dish_attribitem.dish_attrib_id}">{$tbl_dish_attribitem.dish_attrib_name}</a></td>			<td>{$tbl_dish_attribitem.dish_attrib_desc}</td>			<td>{$tbl_dish_attribitem.dish_attrib_img}</td>			<td><a href="{$page_url}?mode={$smarty.const.MODE_UPDATE}&dish_attrib_id={$tbl_dish_attribitem.dish_attrib_id}" class="editIcon"></a><a class="deleteIcon" href="#" onclick="deletetbl_dish_attrib({$tbl_dish_attribitem.dish_attrib_id});"></a>{if $tbl_dish_attribitem.isActive eq 1}<a href="{$page_url}?action={$smarty.const.ACTION_DEACTIVATE}&dish_attrib_id={$tbl_dish_attribitem.dish_attrib_id}" class="deactiveIcon" title="{$_lang.tbl_dish_attrib.DEACTIVATE.BTN_LBL}"></a>{else}<a href="{$page_url}?action={$smarty.const.ACTION_ACTIVATE}&dish_attrib_id={$tbl_dish_attribitem.dish_attrib_id}" class="activeIcon" title="{$_lang.tbl_dish_attrib.ACTIVATE.BTN_LBL}"></a>{/if}</td>		</tr>	{/foreach}		<tfoot>			<tr>				<td colspan="6">					# {$result_found}&nbsp;&nbsp;&nbsp;{if $pagination neq ""}{$pagination}{/if}					<select onchange="changePage('{$navigationURL}',this.value,{$smarty.request.limit});">					{if $allPageCount gt 1}						{for $foo=1 to $allPageCount}							<option value="{$foo}" {if $foo eq $currentPage}selected="selected"{/if}>{$foo}</option>						{/for}					{else}						<option value="1" disabled="disabled">1</option>					{/if}					</select>				</td>			</tr>		</tfoot>	</table>{else}	<div class="error">{$_lang.tbl_dish_attrib.no_record_found}</div>{/if}<center><input data-icon="add-item" data-inline="true" onclick="window.location.href='{$page_url}?mode={$smarty.const.MODE_CREATE}'" type="button" value="{$_lang.tbl_dish_attrib.CREATE.BTN_LBL}"/></center></div>{include file="tbl_dish_attrib/js.tpl"}{include file="footer.tpl"}