{include file='header.tpl'}<div class="wrapper"><h1>{$_lang.tbl_role_functionality.listing_title}</h1>{if $error_msg}	<center>{$error_msg}</center>{/if}{if $result_found gt 0 && $tbl_role_functionalitylist}	<table class="listTable">		<tr><th class="bigListItem" >{$_lang.tbl_role_functionality.title}</th>		</tr>		{foreach from=$tbl_role_functionalitylist item=tbl_role_functionalityitem}			<tr>				<td class="bigListItem"><a href="{$page_url}?mode={$smarty.const.MODE_UPDATE}&rl_fn_id={$tbl_role_functionalityitem.rl_fn_id}">{$tbl_role_functionalityitem.member_role_name}</a></td>				<!--<td class="actionListItem"><a href="{$page_url}?mode={$smarty.const.MODE_UPDATE}&rl_fn_id={$tbl_role_functionalityitem.rl_fn_id}" class="editIcon"></a><a class="deleteIcon" href="#" onclick="deletetbl_role_functionality({$tbl_role_functionalityitem.rl_fn_id});"></a>{if $tbl_role_functionalityitem.isActive eq 1}<a href="{$page_url}?action={$smarty.const.ACTION_DEACTIVATE}&rl_fn_id={$tbl_role_functionalityitem.rl_fn_id}" class="deactiveIcon" title="{$_lang.tbl_role_functionality.DEACTIVATE.BTN_LBL}"></a>{else}<a href="{$page_url}?action={$smarty.const.ACTION_ACTIVATE}&rl_fn_id={$tbl_role_functionalityitem.rl_fn_id}" class="activeIcon" title="{$_lang.tbl_role_functionality.ACTIVATE.BTN_LBL}"></a>{/if}</td>-->			</tr>	{/foreach}	</table>{else}	<div class="error">{$_lang.tbl_role_functionality.no_record_found}</div>{/if}{if $pagination}	<center>{$pagination}</center>{/if}<center><input data-icon="add-item" data-inline="true" onclick="window.location.href='{$page_url}?mode={$smarty.const.MODE_CREATE}'" type="button" value="{$_lang.tbl_role_functionality.CREATE.BTN_LBL}"/></center></div>{include file="tbl_role_functionality/js.tpl"}{include file="footer.tpl"}