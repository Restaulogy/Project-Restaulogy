{include file="header.tpl"}<div class="wrapper"><h1>{$_lang.tbl_role_functionality.title}</h1>{if $error_msg}	<center>{$error_msg}</center>{/if}<form name="frmupdatetbl_role_functionality" id="frmupdatetbl_role_functionality" onsubmit="return validatetbl_role_functionality();" method="POST" action="{$page_url}" style="{if $isUpdate eq 1}{else}display:none;{/if}">	<input type="hidden" name="rl_fn_id" id="rl_fn_id" value="{$tbl_role_functionalityinfo.rl_fn_id}"/>	<div style="display:none;" class="error" id="rl_fn_id_err"></div>			<div class="field-row" >		<label for="rl_fn_role">{$_lang.tbl_role_functionality.label.rl_fn_role}</label>				{$tbl_role_functionalityinfo.member_role_name}		<div style='display:none;'>		<select name='rl_fn_role' id='rl_fn_role'>			<option value="">Select Role</option>			{if $act_roles} 				{foreach from=$act_roles item=role}					<option value="{$role.member_role_id}" {if $tbl_role_functionalityinfo.rl_fn_role eq $role.member_role_id}selected="selected"{/if}>{$role.member_role_name}</option>				{/foreach}			{/if}  				</select> 		</div>				<div class="error" id="rl_fn_role_err"></div>	</div>	<div class="field-row">		<label for="rl_fn_tablests_live">{$_lang.tbl_role_functionality.label.rl_fn_tablests_live}</label>		<input type="checkbox" name="rl_fn_tablests_live" id="rl_fn_tablests_live" {if $tbl_role_functionalityinfo.rl_fn_tablests_live eq 1} checked="checked" {/if} value="1"/>		<div class="error" id="rl_fn_tablests_live_err"></div>	</div>	<div class="field-row">		<label for="rl_fn_tablests_expired">{$_lang.tbl_role_functionality.label.rl_fn_tablests_expired}</label>		<input type="checkbox" name="rl_fn_tablests_expired" id="rl_fn_tablests_expired" {if $tbl_role_functionalityinfo.rl_fn_tablests_expired eq 1} checked="checked" {/if} value="1" />		<div class="error" id="rl_fn_tablests_expired_err"></div>	</div>	<div class="field-row">		<label for="rl_fn_request_live">{$_lang.tbl_role_functionality.label.rl_fn_request_live}</label>		<input type="checkbox" name="rl_fn_request_live" id="rl_fn_request_live" {if $tbl_role_functionalityinfo.rl_fn_request_live eq 1} checked="checked" {/if} value="1"/>		<div class="error" id="rl_fn_request_live_err"></div>	</div>	<div class="field-row">		<label for="rl_fn_request_expired">{$_lang.tbl_role_functionality.label.rl_fn_request_expired}</label>		<input type="checkbox" name="rl_fn_request_expired" id="rl_fn_request_expired" {if $tbl_role_functionalityinfo.rl_fn_request_expired eq 1} checked="checked" {/if} value="1" />		<div class="error" id="rl_fn_request_expired_err"></div>	</div>	<div class="field-row">		<label for="rl_fn_order_live">{$_lang.tbl_role_functionality.label.rl_fn_order_live}</label>		<input type="checkbox" name="rl_fn_order_live" id="rl_fn_order_live" {if $tbl_role_functionalityinfo.rl_fn_order_live eq 1} checked="checked" {/if} value="1" />		<div class="error" id="rl_fn_order_live_err"></div>	</div>	<div class="field-row">		<label for="rl_fn_order_expired">{$_lang.tbl_role_functionality.label.rl_fn_order_expired}</label>		<input type="checkbox" name="rl_fn_order_expired" id="rl_fn_order_expired" {if $tbl_role_functionalityinfo.rl_fn_order_expired eq 1} checked="checked" {/if} value="1" />		<div class="error" id="rl_fn_order_expired_err"></div>	</div>	<div class="field-row">		<label for="rl_fn_promotion_live">{$_lang.tbl_role_functionality.label.rl_fn_promotion_live}</label>		<input type="checkbox" name="rl_fn_promotion_live" id="rl_fn_promotion_live" {if $tbl_role_functionalityinfo.rl_fn_promotion_live eq 1} checked="checked" {/if} value="1" />		<div class="error" id="rl_fn_promotion_live_err"></div>	</div>	<div class="field-row">		<label for="rl_fn_promotion_expired">{$_lang.tbl_role_functionality.label.rl_fn_promotion_expired}</label>		<input type="checkbox" name="rl_fn_promotion_expired" id="rl_fn_promotion_expired" {if $tbl_role_functionalityinfo.rl_fn_promotion_expired eq 1} checked="checked" {/if} value="1" />		<div class="error" id="rl_fn_promotion_expired_err"></div>	</div>	<div class="field-row">		<label for="rl_fn_payment_process">{$_lang.tbl_role_functionality.label.rl_fn_payment_process}</label>		<input type="checkbox" name="rl_fn_payment_process" id="rl_fn_payment_process" {if $tbl_role_functionalityinfo.rl_fn_payment_process eq 1} checked="checked" {/if} value="1" />		<div class="error" id="rl_fn_payment_process_err"></div>	</div>	<!--	<div class="field-row">		<label for="rl_fn_start_date">{$_lang.tbl_role_functionality.label.rl_fn_start_date}</label>		<input type="checkbox" name="rl_fn_start_date" id="rl_fn_start_date" value="{$tbl_role_functionalityinfo.rl_fn_start_date}"/>		<div class="error" id="rl_fn_start_date_err"></div>	</div>	-->	<!--	<div class="field-row">		<label for="rl_fn_end_date">{$_lang.tbl_role_functionality.label.rl_fn_end_date}</label>		<input type="checkbox" name="rl_fn_end_date" id="rl_fn_end_date" value="{$tbl_role_functionalityinfo.rl_fn_end_date}"/>		<div class="error" id="rl_fn_end_date_err"></div>	</div>	-->	<center>			<input type="hidden" id="action" name="action" value="{$smarty.const.ACTION_UPDATE}"/> 		{if $tbl_role_functionalityinfo.isActive eq 1}		<input type="button" value="{$_lang.tbl_role_functionality.DEACTIVATE.BTN_LBL}" onclick="location.href='{$page_url}?action=deactivate&rl_fn_id={$tbl_role_functionalityinfo.rl_fn_id}';" data-inline="true" data-icon="check" title="{$_lang.tbl_role_functionality.DEACTIVATE.BTN_LBL}" />	{else}		<input type="button" value="{$_lang.tbl_role_functionality.ACTIVATE.BTN_LBL}" onclick="location.href='{$page_url}?action=activate&rl_fn_id={$tbl_role_functionalityinfo.rl_fn_id}';" data-inline="true" data-icon="check" title="{$_lang.tbl_role_functionality.ACTIVATE.BTN_LBL}" />	{/if}		<input data-inline="true" data-icon="save"  type="submit" value="{$_lang.save_lbl}"/> 	<!--<input data-inline="true" data-icon="delete" type="reset" onclick="$('#tbl_role_functionality_view').show();$('#frmupdatetbl_role_functionality').hide();" value="{$_lang.cancel_lbl}"/>-->	<input type="button" value="{$_lang.cancel_lbl}" data-inline="true" data-icon="delete" onclick="window.location.href='{$page_url}'"/>			</center></form><div id="tbl_role_functionality_view" class="description" style="{if $isUpdate eq 1}display:none;{/if}">	<table class="listTable">		<tr><th class="fieldItem">{$_lang.field_title}</th><th class="valueItem">{$_lang.value_title}</th></tr>		<tr><td class="fieldItem">{$_lang.tbl_role_functionality.label.rl_fn_id}<i>:</i></td><td class="valueItem">{$tbl_role_functionalityinfo.rl_fn_id}</td></tr>		<tr><td class="fieldItem">{$_lang.tbl_role_functionality.label.rl_fn_role}<i>:</i></td><td class="valueItem">{$tbl_role_functionalityinfo.rl_fn_role}</td></tr>		<tr><td class="fieldItem">{$_lang.tbl_role_functionality.label.rl_fn_tablests_live}<i>:</i></td><td class="valueItem">{$tbl_role_functionalityinfo.rl_fn_tablests_live}</td></tr>		<tr><td class="fieldItem">{$_lang.tbl_role_functionality.label.rl_fn_tablests_expired}<i>:</i></td><td class="valueItem">{$tbl_role_functionalityinfo.rl_fn_tablests_expired}</td></tr>		<tr><td class="fieldItem">{$_lang.tbl_role_functionality.label.rl_fn_request_live}<i>:</i></td><td class="valueItem">{$tbl_role_functionalityinfo.rl_fn_request_live}</td></tr>		<tr><td class="fieldItem">{$_lang.tbl_role_functionality.label.rl_fn_request_expired}<i>:</i></td><td class="valueItem">{$tbl_role_functionalityinfo.rl_fn_request_expired}</td></tr>		<tr><td class="fieldItem">{$_lang.tbl_role_functionality.label.rl_fn_order_live}<i>:</i></td><td class="valueItem">{$tbl_role_functionalityinfo.rl_fn_order_live}</td></tr>		<tr><td class="fieldItem">{$_lang.tbl_role_functionality.label.rl_fn_order_expired}<i>:</i></td><td class="valueItem">{$tbl_role_functionalityinfo.rl_fn_order_expired}</td></tr>		<tr><td class="fieldItem">{$_lang.tbl_role_functionality.label.rl_fn_promotion_live}<i>:</i></td><td class="valueItem">{$tbl_role_functionalityinfo.rl_fn_promotion_live}</td></tr>		<tr><td class="fieldItem">{$_lang.tbl_role_functionality.label.rl_fn_promotion_expired}<i>:</i></td><td class="valueItem">{$tbl_role_functionalityinfo.rl_fn_promotion_expired}</td></tr>		<tr><td class="fieldItem">{$_lang.tbl_role_functionality.label.rl_fn_payment_process}<i>:</i></td><td class="valueItem">{$tbl_role_functionalityinfo.rl_fn_payment_process}</td></tr>	<!--		<tr><td class="fieldItem">{$_lang.tbl_role_functionality.label.rl_fn_start_date}<i>:</i></td><td class="valueItem">{$tbl_role_functionalityinfo.rl_fn_start_date}</td></tr>	-->	<!--		<tr><td class="fieldItem">{$_lang.tbl_role_functionality.label.rl_fn_end_date}<i>:</i></td><td class="valueItem">{$tbl_role_functionalityinfo.rl_fn_end_date}</td></tr>	-->	<tr><td class="fieldItem">{$_lang.tbl_role_functionality.label.isActive}<i>:</i></td><td class="valueItem">{if $tbl_role_functionalityinfo.isActive eq 1}{$_lang.tbl_role_functionality.label.isActive_yes}{else}{$_lang.tbl_role_functionality.label.isActive_no}{/if}</td></tr>	</table>	<center>			<input data-inline="true" data-icon="edit" type="button" value="{$_lang.tbl_role_functionality.UPDATE.BTN_LBL}" onclick="$('#tbl_role_functionality_view').hide();$('#frmupdatetbl_role_functionality').show();"/>			<input type="button" value="{$_lang.cancel_lbl}" data-inline="true" data-icon="delete" onclick="window.location.href='{$page_url}'"/>			</center></div>{include file="tbl_role_functionality/js.tpl"}</div>{include file="footer.tpl"}