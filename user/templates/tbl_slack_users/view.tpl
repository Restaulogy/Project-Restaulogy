{include file="header.tpl"}<div class="wrapper"><h1>{$_lang.tbl_slack_users.title}</h1>{if $error_msg}	<center>{$error_msg}</center>{/if}<form name="frmupdatetbl_slack_users" id="frmupdatetbl_slack_users" onsubmit="return validatetbl_slack_users();" method="POST" action="{$page_url}" style="{if $isUpdate eq 1}{else}display:none;{/if}">	<input type="hidden" name="slkusr_id" id="slkusr_id" value="{$tbl_slack_usersinfo.slkusr_id}"/>	<div style="display:none;" class="error" id="slkusr_id_err"></div>	<div class="field-row">		<label for="slkusr_phone">{$_lang.tbl_slack_users.label.slkusr_phone}</label>		<input type="text" name="slkusr_phone" id="slkusr_phone" value="{$tbl_slack_usersinfo.slkusr_phone}"/>		<div class="error" id="slkusr_phone_err"></div>	</div>	<div class="field-row">		<label for="slkusr_webhook">{$_lang.tbl_slack_users.label.slkusr_webhook}</label>		<input type="text" name="slkusr_webhook" id="slkusr_webhook" value="{$tbl_slack_usersinfo.slkusr_webhook}"/>		<div class="error" id="slkusr_webhook_err"></div>	</div>	<!--	<div class="field-row">		<label for="slkusr_start_date">{$_lang.tbl_slack_users.label.slkusr_start_date}</label>		<input type="text" name="slkusr_start_date" id="slkusr_start_date" value="{$tbl_slack_usersinfo.slkusr_start_date}"/>		<div class="error" id="slkusr_start_date_err"></div>	</div>	-->	<!--	<div class="field-row">		<label for="slkusr_end_date">{$_lang.tbl_slack_users.label.slkusr_end_date}</label>		<input type="text" name="slkusr_end_date" id="slkusr_end_date" value="{$tbl_slack_usersinfo.slkusr_end_date}"/>		<div class="error" id="slkusr_end_date_err"></div>	</div>	-->	<center><input type="hidden" id="action" name="action" value="{$smarty.const.ACTION_UPDATE}"/> <input  data-inline="true" data-icon="save"  type="submit" value="{$_lang.save_lbl}"/> <input data-inline="true" data-icon="delete" type="reset" onclick="$('#tbl_slack_users_view').show();$('#frmupdatetbl_slack_users').hide();" value