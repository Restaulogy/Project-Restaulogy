{include file='header.tpl'}<div class="wrapper"><h1>{$_lang.tbl_slack_users.listing_title}</h1>{if $error_msg}	<center>{$error_msg}</center>{/if}{if $result_found gt 0 && $tbl_slack_userslist}	<table class="listTable">		<tr><th class="bigListItem">{$_lang.tbl_slack_users.title}</th>		<th class="actionListItem"></th></tr>		{foreach from=$tbl_slack_userslist item=tbl_slack_usersitem}			<tr>				<td class="bigListItem"><a href="{$page_url}?mode={$smarty.const.MODE_VIEW}&slkusr_id={$tbl_slack_usersitem.slkusr_id}">{$tbl_slack_usersitem.slkusr_phone}</a></td>				<td class="actionListItem"><a href="{$page_url}?mode={$smarty.const.MODE_UPDATE}&slkusr_id={$tbl_slack_usersitem.slkusr_id}" class="editIcon"></a><a class="deleteIcon" href="#" onclick="deletetbl_slack_users({$tbl_slack_usersitem.slkusr_id});"></a>{if $tbl_slack_usersitem.isActive eq 1}<a href="{$page_url}?action={$smarty.const.ACTION_DEACTIVATE}&slkusr_id={$tbl_slack_usersitem.slkusr_id}" class="deactiveIcon" title="{$_lang.tbl_slack_users.DEACTIVATE.BTN_LBL}"></a>{else}<a href="{$page_url}?action={$smarty.const.ACTION_ACTIVATE}&slkusr_id={$tbl_slack_usersitem.slkusr_id}" class="activeIcon" title="{$_lang.tbl_slack_users.ACTIVATE.BTN_LBL}"></a>{/if}</td>			</tr>	{/foreach}	</table>{else}	<div class="error">{$_lang.tbl_slack_users.no_record_found}</div>{/if}{if $pagination}	<center>{$pagination}</center>{/if}<center><input data-icon="add-item" data-inline="true" onclick="window.location.href='{$page_url}?mode={$smarty.const.MODE_CREATE}'" type="button" value="{$_lang.tbl_slack_users.CREATE.BTN_LBL}"/></center></div>{include file="tbl_slack_users/js.tpl"}{include file="footer.tpl"}