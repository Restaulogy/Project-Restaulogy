{literal}<script type="text/javascript">function deletemember_role(varId){	if(varId > 0){		if(confirm("{/literal}{$_lang.member_role.DELETE.CONFIRM_MSG}{literal}")==true){			window.location.href="{/literal}{$page_url}{literal}?action=delete&member_role_id="+varId;		}	}}function validatemember_role(){	var isErr = true;	if(IsNonEmpty(elemById("member_role_id"))==false){		$("#member_role_id_err").html("{/literal}{$_lang.member_role.not_empty_msg.member_role_id}{literal}");		isErr = false;	}	if(IsNonEmpty(elemById("member_role_name"))==false){		$("#member_role_name_err").html("{/literal}{$_lang.member_role.not_empty_msg.member_role_name}{literal}");		isErr = false;	}	/*	if(IsNonEmpty(elemById("member_role_start_date"))==false){		$("#member_role_start_date_err").html("{/literal}{$_lang.member_role.not_empty_msg.member_role_start_date}{literal}");		isErr = false;	}	if(IsNonEmpty(elemById("member_role_end_date"))==false){		$("#member_role_end_date_err").html("{/literal}{$_lang.member_role.not_empty_msg.member_role_end_date}{literal}");		isErr = false;	}	*/	if(isErr == false){		alert("{/literal}{$_lang.messages.revise_form}{literal}");	}	return isErr;}//..function</script>{/literal}