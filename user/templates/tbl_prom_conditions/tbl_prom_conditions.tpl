{include file='header.tpl'}<div class="wrapper"><h1>{$_lang.tbl_prom_conditions.listing_title}</h1>{if $error_msg}	<div class="biz_center">{$error_msg}</div><!--/center-->{/if}{if $result_found gt 0 && $tbl_prom_conditionslist}	<table class="listTable">		<tr><th class="bigListItem">{$_lang.tbl_prom_conditions.title}</th>		<th class="actionListItem"></th></tr>		{foreach from=$tbl_prom_conditionslist item=tbl_prom_conditionsitem}			<tr>				<td class="bigListItem"><a href="{$website}/user/tbl_prom_cond_details.php?promotion={$prmcon_promotion}&prcndtl_condition={$tbl_prom_conditionsitem.prmcon_id}">{$tbl_prom_conditionsitem.prmcon_title}</a></td>								<td class="actionListItem">				<a href="{$page_url}&mode={$smarty.const.MODE_UPDATE}&prmcon_id={$tbl_prom_conditionsitem.prmcon_id}" class="editIcon"></a>                <a class="deleteIcon" href="#" onclick="deletetbl_prom_conditions({$tbl_prom_conditionsitem.prmcon_id});"></a>                {if $tbl_prom_conditionsitem.isActive eq 1}                <a href="{$page_url}action={$smarty.const.ACTION_DEACTIVATE}&prmcon_id={$tbl_prom_conditionsitem.prmcon_id}" class="deactiveIcon" title="{$_lang.tbl_prom_conditions.DEACTIVATE.BTN_LBL}"></a>                {else}                <a href="{$page_url}action={$smarty.const.ACTION_ACTIVATE}&prmcon_id={$tbl_prom_conditionsitem.prmcon_id}" class="activeIcon" title="{$_lang.tbl_prom_conditions.ACTIVATE.BTN_LBL}"></a>				{/if}				</td>			</tr>	{/foreach}	</table>{else}	<div class="error">{$_lang.tbl_prom_conditions.no_record_found}</div>{/if}{if $pagination}	<div class="biz_center">{$pagination}</div><!--/center-->{/if}<div class="biz_center">    <input data-icon="add-item" data-inline="true" onclick="window.location.href='{$website}/user/tbl_prom_cond_details.php?promotion={$prmcon_promotion}&mode={$smarty.const.MODE_CREATE}'" type="button" value="{$_lang.tbl_prom_conditions.CREATE.BTN_LBL}"/></div><!--/center--></div>{include file="tbl_prom_conditions/js.tpl"}{include file="footer.tpl"}