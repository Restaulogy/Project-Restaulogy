{literal}<script type="text/javascript">function deletetbl_prom_conditions(varId){	if(varId > 0){		if(confirm("{/literal}{$_lang.tbl_prom_conditions.DELETE.CONFIRM_MSG}{literal}")==true){			window.location.href="{/literal}{$page_url}{literal}&action=delete&prmcon_id="+varId;		}	}}function validatetbl_prom_conditions(){	$("#prmcon_id_err").html("");	$("#prmcon_promotion_err").html("");	$("#prmcon_title_err").html("");	$("#prmcon_start_date_err").html("");	$("#prmcon_end_date_err").html("");	var isErr = true;	if(elemById("action").value=="update"){		if(IsNonEmpty(elemById("prmcon_id").value)==false){			$("#prmcon_id_err").html("{/literal}{$_lang.tbl_prom_conditions.not_empty_msg.prmcon_id}{literal}");			isErr = false;		}	}	if(IsNonEmpty(elemById("prmcon_promotion").value)==false){		$("#prmcon_promotion_err").html("{/literal}{$_lang.tbl_prom_conditions.not_empty_msg.prmcon_promotion}{literal}");		isErr = false;	}	if(IsNonEmpty(elemById("prmcon_title").value)==false){		$("#prmcon_title_err").html("{/literal}{$_lang.tbl_prom_conditions.not_empty_msg.prmcon_title}{literal}");		isErr = false;	}/*	if(IsNonEmpty(elemById("prmcon_start_date").value)==false){		$("#prmcon_start_date_err").html("{/literal}{$_lang.tbl_prom_conditions.not_empty_msg.prmcon_start_date}{literal}");		isErr = false;	}*//*	if(IsNonEmpty(elemById("prmcon_end_date").value)==false){		$("#prmcon_end_date_err").html("{/literal}{$_lang.tbl_prom_conditions.not_empty_msg.prmcon_end_date}{literal}");		isErr = false;	}*/	if(isErr == false){		alert("{/literal}{$_lang.messages.revise_form}{literal}");	}	return isErr;}//..function</script>{/literal}